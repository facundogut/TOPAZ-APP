<?xml version="1.0" encoding="UTF-8"?>
<web-app 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://java.sun.com/xml/ns/javaee" 
 xmlns:web="http://java.sun.com/xml/ns/javaee"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
  id="WebApp_ID" 
  version="2.5">
  <display-name>test</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MiddlewareService</servlet-name>
    <servlet-class>servlets.MiddlewareConnector</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MiddlewareService</servlet-name>
    <url-pattern>/MiddlewareService</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DispatcherService</servlet-name>
    <servlet-class>servlets.PageDispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DispatcherService</servlet-name>
    <url-pattern>/DispatcherService</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>PosicionCliente.html</welcome-file>
    <welcome-file>PosicionCliente.htm</welcome-file>
    <welcome-file>PosicionCliente.jsp</welcome-file>
  </welcome-file-list>
</web-app>