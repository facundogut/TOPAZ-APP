<div class="frame row">		
	{{view contentBinding="this" templateName="FrameHeaderView" class="header-frame col-sm-12"}}
	
	<div class="content-frame">
		{{#if links}} 
		{{#view "frameLinks"}}
		<div {{bind-attr class=":contextMenu isCollapsed:collapse"}}>
			<ul>
				<li><a class="btnLinks ion-link" alt="Links" title="Links"></a></li>
			</ul>
		</div>
		{{/view}}
		{{/if}}
	  	<div {{bind-attr class=":table-responsive isCollapsed:collapse"}}>
			{{#each data}}
			<div class="row">
				{{#each}}
					{{#unless hidden}}
						{{#view "formField"}}
	                       <label class="label-color col-sm-12 col-xs-12">{{label}}</label>
	                       <label class="label-normal col-sm-12 col-xs-12">{{encodeMyString value}}</label>
						{{/view}}
					{{/unless}}
				{{/each}}
            </div>
            {{/each}}
		</div>
	</div>
</div>
