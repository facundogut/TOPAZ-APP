<?xml version="1.0" encoding="UTF-8"?>
<process-definition-descriptor name="Solicitud">
	<prompt>Solicitud</prompt>
	<description>Solicitud</description>
	<variables> 
		<variable-mapping id="SOLICITUD" variable-name="v_solicitud">
			<prompt>Numero Solicitud</prompt>
			<type name="numeric">
				<attribute name="length">15</attribute>
			</type>
		</variable-mapping>	
		<variable-mapping id="CLIENTE" variable-name="v_cliente">
			<prompt>Nombre Cliente</prompt>
			<type name="numeric">
				<attribute name="length">15</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="MONEDA" variable-name="v_moneda">
			<prompt>Moneda</prompt>
			<type name="numeric">
				<attribute name="length">5</attribute>
			</type>
		</variable-mapping>
	</variables>
	<keys>
		<variable-mapping-id id="SOLICITUD"></variable-mapping-id>
		<variable-mapping-id id="CLIENTE"></variable-mapping-id>
		<variable-mapping-id id="MONEDA"></variable-mapping-id>
	</keys>
	<basic-mappings>
		<variable-mapping-id id="SOLICITUD"/>
		<variable-mapping-id id="CLIENTE"/>
		<variable-mapping-id id="MONEDA"/>
	</basic-mappings>
</process-definition-descriptor>
