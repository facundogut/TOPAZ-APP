<?xml version="1.0" encoding="UTF-8"?>
<process-definition-descriptor name="SolicitudCreditosEmpresas">
	<prompt>Solicitud de Creditos Empresas</prompt>
	<description>Solicitud de Creditos Empresas</description>
	<variables>
		<variable-mapping id="NROSOLICITUD" variable-name="NROSOLICITUD">
			<prompt>Nro. Solicitud</prompt>
			<type name="numeric">
				<attribute name="length">12</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NOMBRE" variable-name="NOMBRE">
			<prompt>Nombre</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
	    <variable-mapping id="NUMERO_CLIENTE" variable-name="NUMERO_CLIENTE">
			<prompt>Numero Cliente</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="CUIT" variable-name="CUIT">
			<prompt>CUIT</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="MONEDA" variable-name="MONEDA">
			<prompt>Moneda</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="IMPORTE" variable-name="IMPORTE">
			<prompt>Importe Solicitado</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
	</variables>
	<keys>
		<variable-mapping-id id="NROSOLICITUD"></variable-mapping-id>
	</keys>
	<basic-mappings>
		<variable-mapping-id id="NROSOLICITUD"/>
		<variable-mapping-id id="NUMERO_CLIENTE"/>
		<variable-mapping-id id="CUIT"/>
		<variable-mapping-id id="NOMBRE"/>
		<variable-mapping-id id="MONEDA"/>
		<variable-mapping-id id="IMPORTE"/>
	</basic-mappings>
</process-definition-descriptor>
