<process-definition-descriptor name="SolicitudGarantias">
	<prompt>Solicitudes de Garantias</prompt>
	<description>Solicitudes de Garantias</description>
	<variables>
		<variable-mapping id="NUMERO_GARANTIA" variable-name="NUMERO_GARANTIA">
			<prompt>Numero de Solicitud</prompt>
			<type name="numeric">
				<attribute name="length">12</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NUMERO_CLIENTE" variable-name="NUMERO_CLIENTE">
			<prompt>Numero Cliente</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NRO_DOCUMENTO" variable-name="NRO_DOCUMENTO">
			<prompt>Documento</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NOMBRE" variable-name="NOMBRE">
			<prompt>Nombre Cliente</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="BENEFICIARIO" variable-name="BENEFICIARIO">
			<prompt>Beneficiario</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
				<variable-mapping id="CLASIFICACION" variable-name="CLASIFICACION">
			<prompt>Clasificacion</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="MONEDA" variable-name="MONEDA">
			<prompt>Moneda</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="IMPORTE" variable-name="IMPORTE">
			<prompt>Importe Solicitado</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
	</variables>
	<keys>
		<variable-mapping-id id="NUMERO_GARANTIA"></variable-mapping-id>
	</keys>
	<basic-mappings>
		<variable-mapping-id id="NUMERO_GARANTIA"/>
		<variable-mapping-id id="NUMERO_CLIENTE"/>
		<variable-mapping-id id="NRO_DOCUMENTO"/>
		<variable-mapping-id id="NOMBRE"/>
		<variable-mapping-id id="BENEFICIARIO"/>
		<variable-mapping-id id="CLASIFICACION"/>
		<variable-mapping-id id="MONEDA"/>
		<variable-mapping-id id="IMPORTE"/>
	</basic-mappings>
	
	<state-mappings state-name="Completar Datos Gtia. Con Asesoria Notarial">
		<prompt>Completar Datos Gtia. Con Asesoria Notarial</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

	<state-mappings state-name="Completar Datos Gtia. Sin Asesoria Notarial">
		<prompt>Completar Datos Gtia. Sin Asesoria Notarial</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

	<state-mappings state-name="Regularizar Documentacion">
		<prompt>Regularizar Documentacion</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

	<state-mappings state-name="Informe Legal">
		<prompt>Informe Legal</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

	<state-mappings state-name="Preconst. Gtia Con Asesoria Not.">
		<prompt>Preconst. Gtia Con Asesoria Not.</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

	<state-mappings state-name="Preconst. Gtia Sin Asesoria Not.">
		<prompt>Preconst. Gtia Sin Asesoria Not.</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

	<state-mappings state-name="Constitucion Garantia">
		<prompt>Constitucion Garantia</prompt>
		<filter-branch branch-variable-name="SUC"/>
	</state-mappings>

</process-definition-descriptor>
