<process-definition-descriptor name="AltaConvenioPago">
	<prompt>Alta de Convenios de Pago</prompt>
	<description>Alta de Convenios de Pago</description>
	<variables>
		<variable-mapping id="NRO_CONVENIO" variable-name="NRO_CONVENIO">
			<prompt>Nro. Convenio</prompt>
			<type name="numeric">
				<attribute name="length">15</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NOMBRE_CONVENIO" variable-name="NOMBRE_CONVENIO">
			<prompt>Nombre</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="CUIT" variable-name="CUIT">
			<prompt>CUIT</prompt>
			<type name="numeric">
				<attribute name="length">11</attribute>
			</type>
		</variable-mapping>
				<variable-mapping id="CLIENTE" variable-name="CLIENTE">
			<prompt>Cliente</prompt>
			<type name="numeric">
				<attribute name="length">12</attribute>
			</type>
		</variable-mapping>
	</variables>
	<keys>
		<variable-mapping-id id="NRO_CONVENIO"></variable-mapping-id>
	</keys>
	<basic-mappings>
		<variable-mapping-id id="NRO_CONVENIO"/>
		<variable-mapping-id id="NOMBRE_CONVENIO"/>
		<variable-mapping-id id="CUIT"/>
		<variable-mapping-id id="CLIENTE"/>
	</basic-mappings>
</process-definition-descriptor>
