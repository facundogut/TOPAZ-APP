<?xml version="1.0" encoding="UTF-8"?>
<process-definition-descriptor name="SolicitudLeasing">
	<prompt>Solicitud de Creditos Leasing</prompt>
	<description>Solicitud de Creditos Leasing</description>
	<variables>
		<variable-mapping id="NRO_SOLICITUD" variable-name="NRO_SOLICITUD">
			<prompt>Nro. Solicitud</prompt>
			<type name="numeric">
				<attribute name="length">12</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NOMBRE" variable-name="NOMBRE">
			<prompt>Nombre</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
	    <variable-mapping id="NUMERO_CLIENTE" variable-name="NUMERO_CLIENTE">
			<prompt>Numero Cliente</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="DOCUMENTO" variable-name="DOCUMENTO">
			<prompt>Documento</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="MONEDA" variable-name="MONEDA">
			<prompt>Moneda</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="IMPORTE" variable-name="IMPORTE">
			<prompt>Valor Total del Bien</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="DESC_BIEN" variable-name="DESC_BIEN">
			<prompt>BIEN</prompt>
			<type name="text">
				<attribute name="length">100</attribute>
			</type>
		</variable-mapping>
	</variables>
	<keys>
		<variable-mapping-id id="NRO_SOLICITUD"></variable-mapping-id>
	</keys>
	<basic-mappings>
		<variable-mapping-id id="NRO_SOLICITUD"/>
		<variable-mapping-id id="NUMERO_CLIENTE"/>
		<variable-mapping-id id="DOCUMENTO"/>
		<variable-mapping-id id="NOMBRE"/>
		<variable-mapping-id id="MONEDA"/>
		<variable-mapping-id id="IMPORTE"/>
		<variable-mapping-id id="DESC_BIEN"/>
	</basic-mappings>
</process-definition-descriptor>
