<?xml version="1.0" encoding="UTF-8"?>
<process-definition-descriptor name="IngresoListaLote">
	<prompt>Flujo de Carga de Lista o Lote</prompt>
	<description>Flujo de Carga de Lista o Lote</description>
	<variables>
		<variable-mapping id="NUMERO_LISTA" variable-name="NUMERO_LISTA">
			<prompt>Nro. Lista</prompt>
			<type name="numeric">
				<attribute name="length">10</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NOMBRE" variable-name="NOMBRE">
			<prompt>Nombre Cliente</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="NUMERO_CLIENTE" variable-name="NUMERO_CLIENTE">
			<prompt>Numero Cliente</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="CUIT" variable-name="CUIT">
			<prompt>CUIT</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="MONEDA" variable-name="MONEDA">
			<prompt>Moneda</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
		<variable-mapping id="IMPORTE" variable-name="IMPORTE">
			<prompt>Importe Solicitado</prompt>
			<type name="text">
				<attribute name="length">45</attribute>
			</type>
		</variable-mapping>
	</variables>
	<keys>
		<variable-mapping-id id="NUMERO_LISTA"></variable-mapping-id>
	</keys>
	<basic-mappings>
		<variable-mapping-id id="NUMERO_LISTA"/>
		<variable-mapping-id id="NOMBRE"/>
		<variable-mapping-id id="NUMERO_CLIENTE"/>
		<variable-mapping-id id="CUIT"/>
		<variable-mapping-id id="MONEDA"/>
		<variable-mapping-id id="IMPORTE"/>
	</basic-mappings>
	<state-mappings state-name="Controles Certificados DGI">
		<prompt>Controles Certificados DGI</prompt>
		<description>Controles Certificados DGI</description>
	</state-mappings>
	<state-mappings state-name="Controles Documentos">
		<prompt>Controles Documentos</prompt>
		<description>Controles Documentos</description>
	</state-mappings>
	<state-mappings state-name="Confirmar Lista_Lote">
		<prompt>Confirmar Lista o Lote</prompt>
		<description>Confirmar Lista o Lote</description>
	</state-mappings>
	<state-mappings state-name="Aprobar Lista_Lote">
		<prompt>Aprobar Lista o Lote</prompt></state-mappings>
	<state-mappings state-name="Fin Flujo">
		<prompt>Fin Ingreso Documentos</prompt></state-mappings>
</process-definition-descriptor>
