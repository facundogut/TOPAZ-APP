<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<map>
    <query>
        <class-name>topsystems.topaz.server.alias.ResultAliasImpl</class-name>
        <query-name>query.vo_AliasCuenta</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
		select 
		JTS_OID, 
		TIPO_PRODUCTO,
		TIPO_DOC,
		DOCUMENTO,
		ALIAS,
		TIPO_ALIAS,
		DATA_ALTA,
		DATA_BAJA from vta_alias where 
		DATA_BAJA &gt;= ? and 
		TIPO_PRODUCTO = ? and
		TIPO_DOC = ? and
		DOCUMENTO = ? and 
		ALIAS = ? and	
		TZ_LOCK = 0
	</sentence>
        <attribute>
            <attribute-name>saldoJtsOid</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipoProducto</attribute-name>
            <column-name>TIPO_PRODUCTO</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipoDocumento</attribute-name>
            <column-name>TIPO_DOC</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>documento</attribute-name>
            <column-name>DOCUMENTO</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>alias</attribute-name>
            <column-name>ALIAS</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipoAlias</attribute-name>
            <column-name>TIPO_ALIAS</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>dataAlta</attribute-name>
            <column-name>DATA_ALTA</column-name>
            <column-type>Timestamp</column-type>
        </attribute>
        <attribute>
            <attribute-name>dataBaja</attribute-name>
            <column-name>DATA_BAJA</column-name>
            <column-type>Timestamp</column-type>
        </attribute>
    </query>
    <query>
        <class-name>topsystems.topaz.server.alias.ResultAliasImpl</class-name>
        <query-name>query.vo_AliasCuentaDefault</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
		select 
		JTS_OID, 
		TIPO_PRODUCTO,
		TIPO_DOC,
		DOCUMENTO,
		ALIAS,
		TIPO_ALIAS,
		DATA_ALTA,
		DATA_BAJA from vta_alias where 
		DATA_BAJA &gt;= ? and 
		TIPO_PRODUCTO = ? and
		TIPO_DOC = ? and
		DOCUMENTO = ? and 
		JTS_OID = ? and
		TIPO_ALIAS = 'D' and	
		TZ_LOCK = 0
	</sentence>
        <attribute>
            <attribute-name>saldoJtsOid</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipoProducto</attribute-name>
            <column-name>TIPO_PRODUCTO</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipoDocumento</attribute-name>
            <column-name>TIPO_DOC</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>documento</attribute-name>
            <column-name>DOCUMENTO</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>alias</attribute-name>
            <column-name>ALIAS</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipoAlias</attribute-name>
            <column-name>TIPO_ALIAS</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>dataAlta</attribute-name>
            <column-name>DATA_ALTA</column-name>
            <column-type>Timestamp</column-type>
        </attribute>
        <attribute>
            <attribute-name>dataBaja</attribute-name>
            <column-name>DATA_BAJA</column-name>
            <column-type>Timestamp</column-type>
        </attribute>
    </query>
</map>
