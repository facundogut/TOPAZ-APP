        <attribute>
            <attribute-name>tipoGarantia</attribute-name>
            <column-name>TIPOGARANTIA</column-name>
            <key>primary</key>
        </attribute>
        <attribute>
            <attribute-name>plaza</attribute-name>
            <column-name>PLAZA</column-name>
            <key>primary</key>
        </attribute>
        <attribute>
            <attribute-name>conLinea</attribute-name>
            <column-name>CONLINEA</column-name>
