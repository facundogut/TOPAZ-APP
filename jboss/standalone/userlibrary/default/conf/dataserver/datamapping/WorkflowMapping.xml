    <query>
        <class-name>topsystems.bpm.server.automaticprocess.objectsdomain.ExpirationTimeVO</class-name>
        <query-name>query.ExpirationTime</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
				SELECT PROCESS_NAME, STATE_NAME, CONDITION, CANT_HOURS FROM JBPM_EXPIRATION_TIME 
				ORDER BY PROCESS_NAME, STATE_NAME, ORDINAL
			</sentence>
        <attribute>
            <attribute-name>processName</attribute-name>
            <column-name>PROCESS_NAME</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>stateName</attribute-name>
            <column-name>STATE_NAME</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>condition</attribute-name>
            <column-name>CONDITION</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>cantHours</attribute-name>
            <column-name>CANT_HOURS</column-name>
            <column-type>String</column-type>
        </attribute>
    </query>
    <query>
        <class-name>topsystems.bpm.server.roles.QueryTopazUsuarioRoles</class-name>
        <query-name>query.QueryTopazUsuarioRoles</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
				SELECT R.ROLE_NAME, R.DATA_TYPE, R.DATA
				FROM TOPAZ_ROLES_ASSIGNMENT R
				INNER JOIN TOPAZ_USUARIO_ROLES T ON R.ROLE_NAME = T.ROLE_NAME			           
	                       AND R.TZ_LOCK = 0
	                       AND T.TZ_LOCK = 0
						   AND T.USUARIO = ?					   
			</sentence>
        <attribute>
            <attribute-name>roleName</attribute-name>
            <column-name>ROLE_NAME</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>tipo</attribute-name>
            <column-name>DATA_TYPE</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>usuarioGrupo</attribute-name>
            <column-name>DATA</column-name>
            <column-type>String</column-type>
        </attribute>
    </query>
</map>
