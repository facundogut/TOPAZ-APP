<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<map>
    <query>
        <class-name>topsystems.ws.services.errors.codes.ErrorCodeWSVO</class-name>
        <query-name>query.vo_ErrorCodeWS</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
		SELECT CODIGO, CODIGO_HTTP, USA_MENSAJE, MENSAJE
		FROM ERROR_CODE_WS 
		WHERE TZ_LOCK = 0
		</sentence>
        <attribute>
            <attribute-name>errorCode</attribute-name>
            <column-name>CODIGO</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>httpCode</attribute-name>
            <column-name>CODIGO_HTTP</column-name>
            <column-type>int</column-type>
        </attribute>
        <attribute>
            <attribute-name>useMessage</attribute-name>
            <column-name>USA_MENSAJE</column-name>
            <column-type>int</column-type>
        </attribute>
        <attribute>
            <attribute-name>message</attribute-name>
            <column-name>MENSAJE</column-name>
            <column-type>String</column-type>
        </attribute>
    </query>
</map>
