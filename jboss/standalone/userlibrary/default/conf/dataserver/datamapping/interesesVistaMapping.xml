        <query-name>query.QuerySaldosCobro</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
					SELECT S.C1620 AS FECHA_INICIO,
						 S.C1604 AS SALDO_ACTUAL,
						 S.JTS_OID,
						 S.C1665 AS CONTRACUENTA,
						 S.C1813 AS FECHA_DEVENGADO,
						 S.C1820 AS DEVENGADO_COBRO,
						 S.C1822 AS FECHA_COBRO,
						 S.C1632 AS TASA,
						 V.Valor_Vigencia  AS FECHA_VALOR_VIGENCIA,
						 V.FECHA_VENCIMIENTO AS FECHA_VENCIMIENTO_ACUERDO
					  FROM SALDOS S
					 INNER JOIN CRE_PRODUCTOS_SOBREGIRO C ON S.PRODUCTO = C.PRODUCTO
												 AND C.TIPO = 'C'
												 AND S.C1728 IN ('N','V')
												 AND S.TZ_LOCK = 0
												 AND C.TZ_LOCK = 0
					 INNER JOIN VTA_SOBREGIROS V ON S.OPERACION = V.NRO_AUTORIZACION     
												 AND V.TZ_LOCK = 0                          
												 AND (S.JTS_OID = ? OR -1 = ?) 
			</sentence>
