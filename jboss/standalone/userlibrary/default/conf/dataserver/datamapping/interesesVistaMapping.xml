    <query>
        <class-name>topsystems.automaticprocess.historicocliente.valueobjects.QueryRecalculoHistoricoIntereses</class-name>
        <query-name>query.QueryRecalculoHistoricoIntereses</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>

				SELECT s.JTS_OID

				  FROM Saldos s

				 INNER JOIN CLI_Clientes c ON s.C1803 = c.codigocliente

				 INNER JOIN PLANCTAS pl ON pl.C6326 = s.C1730

				 INNER JOIN VTA_DEFINICION_VISTA vd ON s.PRODUCTO = vd.Producto

								   AND s.Moneda = vd.moneda

								   AND s.C1772 = vd.grupo

				 INNER JOIN VTA_GRUPOS gr ON gr.cod_grupo = vd.grupo

				  LEFT OUTER JOIN VTA_SOBREGIROS va ON va.estado = 1

								   AND s.JTS_OID = va.JTS_OID_SALDO

								   AND va.TZ_LOCK = 0

				 WHERE s.c1785 in (2, 3)

				   AND ((s.JTS_OID &gt;= ? AND s.JTS_OID &lt;= ?) OR - 1 = ?)

				   AND pl.C6318 in ('M', 'D')

				   AND (s.tz_lock = 0)

				   AND (vd.tz_lock = 0)

				   AND (c.tz_lock = 0) AND ((gr.apertura_cli = 'S' AND s.jts_oid = vd.saldo_jts_oid) 

				   OR  (gr.apertura_cli &lt;&gt; 'S'))

			</sentence>
        <attribute>
            <attribute-name>saldo_jts_oid</attribute-name>
            <column-name>JTS_OID </column-name>
            <column-type>long</column-type>
        </attribute>
    </query>
