    <query>
        <class-name>topsystems.automaticprocess.interesesvista.objectsdomain.QueryPromedioCalculoInteres</class-name>
        <query-name>query.QueryPromedioCalculoInteres</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>

				SELECT SUM(PROMEDIO) / ? AS PROMEDIO, COUNT(*) AS REGISTROS FROM(

				   SELECT CASE

					 WHEN 'C' = ? THEN

				   CASE WHEN 

					 GRL_SALDOS_DIARIOS.SALDO_CON_FECHA_VALOR_COBRO &lt; 0 THEN

					 GRL_SALDOS_DIARIOS.SALDO_CON_FECHA_VALOR_COBRO ELSE

					 0

					 END

							 ELSE

							   CASE WHEN 

					 GRL_SALDOS_DIARIOS.SALDO_CON_FECHA_VALOR_PAGO &gt; 0 THEN

					 GRL_SALDOS_DIARIOS.SALDO_CON_FECHA_VALOR_PAGO ELSE

					 0

					 END 

				   END AS PROMEDIO  FROM GRL_SALDOS_DIARIOS

					 WHERE FECHA &gt;= ?

					   AND FECHA &lt;= ?

					AND GRL_SALDOS_DIARIOS.SALDOS_JTS_OID = ?) T

			</sentence>
        <attribute>
            <attribute-name>promedio</attribute-name>
            <column-name>PROMEDIO</column-name>
            <column-type>String</column-type>
        </attribute>
        <attribute>
            <attribute-name>registros</attribute-name>
            <column-name>REGISTROS</column-name>
            <column-type>long</column-type>
        </attribute>
    </query>
