        <class-name>topsystems.automaticprocess.interesesvista.objectsdomain.InteresesVistaInputImpl</class-name>
        <query-name>query.InteresesVistaInput</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>

						SELECT 
							s.C1772 tipocta,
							vd.GRUPO,
							vd.IMP_MIN_PAGO,
							vd.MODALIDADPAGO,
							vd.VALORMODALIDADPAGO,
							vd.MODALIDADENFERIADOPAGO,
							vd.MODALIDADTIPOPERIODOPAGO,
							vd.ALGORITMOPAGO,
							vd.CODIGORANGOPAGO,
							vd.DIFERENCIALNORESIDENTEPAGO,
							vd.IMP_MIN_COBRO,
							vd.MODALIDADENFERIADOCOBRO,
							vd.MODALIDADTIPOPERIODOCOBRO,
							vd.MODALIDADCOBRO,
							vd.VALORMODALIDADCOBRO,
							vd.ALGORITMOCOBRO,
							vd.criterio_min_cobro,
							vd.CODIGORANGOCOBRO,
							vd.DIFERENCIALNORESIDENTECOBRO,
							s.C1785 AS TIPO,
							c.codigoresidencia AS codresidencia,
							va.cod_rango AS codigorangoacuerdo,
							s.C1604 sdoactual,
							s.Sucursal,
							s.Producto,
							s.Cuenta,
							s.C1730 rubrocontable,
							s.Moneda,
							s.Operacion,
							s.Ordinal,
							s.JTS_OID,
							vd.MESES_COBRO,
							vd.MESES_PAGO,
							pl.C6318 AS TIPO_DEVENGADO,
							c.IVA
						FROM Saldos s
						INNER JOIN CLI_Clientes c ON 
							s.C1803 = c.codigocliente
						INNER JOIN PLANCTAS pl ON 
							pl.C6326 = s.C1730
						INNER JOIN VTA_DEFINICION_VISTA vd ON 
							s.PRODUCTO = vd.Producto
							AND s.Moneda = vd.moneda
							AND s.C1772 = vd.grupo
						INNER JOIN VTA_GRUPOS gr ON 
							gr.cod_grupo = vd.grupo
						LEFT OUTER JOIN VTA_SOBREGIROS va ON 
							va.estado = 1
							AND s.JTS_OID = va.JTS_OID_SALDO
							AND va.TZ_LOCK = 0
						WHERE 
							s.c1785 in (2, 3)
							AND (s.jts_oid = ? OR (-1 = ?))
							AND pl.C6318 in ('M', 'D')
							AND (s.tz_lock = 0)
							AND (vd.tz_lock = 0)
							AND (c.tz_lock = 0) 
							AND ( 
								(gr.apertura_cli = 'S' AND s.jts_oid = vd.saldo_jts_oid)
								OR  (gr.apertura_cli &lt;&gt; 'S' AND vd.grupo = 
									(select 
										case when exists(
											select null
											from saldos s2
											INNER JOIN CLI_Clientes c2 ON 
												s2.C1803 = c2.codigocliente
											INNER JOIN VTA_DEFINICION_VISTA vd2 ON 
												s2.PRODUCTO = vd2.Producto
												AND s2.Moneda = vd2.moneda
												AND s2.C1772 = vd2.grupo
												AND vd2.grupo = c2.segmentocliente
											INNER JOIN VTA_GRUPOS gr2 ON 
												gr2.cod_grupo = vd2.grupo
											WHERE 
												(s2.jts_oid = s.jts_oid)
												AND (s2.tz_lock = 0) 
												AND (vd2.tz_lock = 0) 
												AND (c2.tz_lock = 0)
												AND (gr2.apertura_cli &lt;&gt; 'S')
										) then 
											convert(CHAR,c2.segmentocliente)
										else '0'
										end as segmentocliente
									from 
										CLI_Clientes c2 
									where 
										c2.codigocliente = c.codigocliente
									)
									)
								) 
					</sentence>
