<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<map>
    <query>
        <class-name>topsystems.ws.services.general.channels.ChannelDefinitionVO</class-name>
        <query-name>query.api.ChannelXName</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
				SELECT TZ_LOCK, CODIGO, NOMBRE 
				FROM CANALES 
				WHERE NOMBRE = ?
				AND TZ_LOCK = 0
			</sentence>
        <attribute>
            <attribute-name>tz_lock</attribute-name>
            <column-name>TZ_LOCK</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>code</attribute-name>
            <column-name>CODIGO</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>name</attribute-name>
            <column-name>NOMBRE</column-name>
            <column-type>string</column-type>
        </attribute>
    </query>
</map>
