	<query>
		<class-name>posicion.objectsdomain.QueryPosicionPersona</class-name>
		<query-name>query.vo_QueryPosicionPersona</query-name>
		<database-name>TOP/CLIENTES</database-name>
		<sentence>
			SELECT S.C1803 AS CLIENTE,
			       S.JTS_OID AS SALDO_JTS_OID,
			       C.TITULARIDAD,
			       S.C1730 AS RUBRO,
			       S.MONEDA,
			       S.C1604 AS SALDO,
			       S.C1609 AS INTERES,
			       S.C1610 AS INTERESCANCELADO,
			       S.C1614 AS MORA,
             		       S.C1694 AS VISUALIZABLE
			  FROM SALDOS S
			 INNER JOIN CLI_CLIENTEPERSONA C ON S.C1803 = C.CODIGOCLIENTE 
			 AND C.NUMEROPERSONA = ? AND S.TZ_LOCK = 0 AND C.TZ_LOCK = 0
		</sentence>
