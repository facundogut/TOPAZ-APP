	<query>
		<class-name>posicion.objectsdomain.QueryPosicionGarantiaPersona</class-name>
		<query-name>query.vo_QueryPosicionGarantiaPersona</query-name>
		<database-name>TOP/CLIENTES</database-name>
		<sentence>
			SELECT TPO_PERSONA AS TIPOGARANTE,
			       CRE_GARANTIAPERSONAS.NUM_GARANTIA,
			       CRE_GARANTIASRECIBIDAS.PAIS_GARANTIA,
			       CRE_GARANTIASRECIBIDAS.TIPOGARANTIA,
			       CRE_GARANTIASRECIBIDAS.SUBTIPOGARANTIA,
			       CRE_GARANTIASRECIBIDAS.COD_SUBCLAGARANTIA,
			       CRE_GARANTIASRECIBIDAS.IMPORTECONTRACTUAL,
			       CRE_GARANTIASRECIBIDAS.MONEDA_GARANTIA,
			       CRE_CLASGARANTIAS.AUTOLIQUIDABLE
			  FROM CRE_GARANTIAPERSONAS
			 INNER JOIN CRE_GARANTIASRECIBIDAS ON CRE_GARANTIAPERSONAS.NUM_GARANTIA =
							      CRE_GARANTIASRECIBIDAS.NUM_GARANTIA
			 INNER JOIN CRE_CLASGARANTIAS ON CRE_GARANTIASRECIBIDAS.PAIS_GARANTIA =
							 CRE_CLASGARANTIAS.PAIS_GARANTIA
						     AND CRE_GARANTIASRECIBIDAS.TIPOGARANTIA =
							 CRE_CLASGARANTIAS.TIPOGARANTIA
						     AND CRE_GARANTIASRECIBIDAS.SUBTIPOGARANTIA =
							 CRE_CLASGARANTIAS.SUBTIPOGTIA
						     AND CRE_GARANTIASRECIBIDAS.COD_SUBCLAGARANTIA =
							 CRE_CLASGARANTIAS.COD_SUBCLA_GARANTIA
						     WHERE CRE_GARANTIAPERSONAS.NUM_PERSONA = ? 	
							 AND CRE_GARANTIAPERSONAS.TZ_LOCK = 0
							 AND CRE_GARANTIASRECIBIDAS.TZ_LOCK = 0
							 AND CRE_CLASGARANTIAS.TZ_LOCK = 0
                  
		</sentence>
