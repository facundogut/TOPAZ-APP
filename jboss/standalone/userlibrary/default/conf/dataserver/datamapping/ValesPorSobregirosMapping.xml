			<column-name>fecha_proceso</column-name>
			<key>primary</key>
		</attribute>
		<attribute>
			<attribute-name>jts_oid_saldo</attribute-name>
			<column-name>jts_oid_saldo</column-name>
			<key>primary</key>
		</attribute>
		<attribute>
			<attribute-name>cant_dias_sobregiro</attribute-name>
			<column-name>cant_dias_sobregiro</column-name>
		</attribute>
		<attribute>
			<attribute-name>valor_sobregiro</attribute-name>
			<column-name>valor_sobregiro</column-name>
		</attribute>
		<attribute>
			<attribute-name>causa_vale</attribute-name>
			<column-name>causa_vale</column-name>
		</attribute>
	</class>
