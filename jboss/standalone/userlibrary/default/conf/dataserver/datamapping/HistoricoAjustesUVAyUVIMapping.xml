    <query>
        <class-name>topsystems.automaticprocess.ajustesuvauvi.QueryAjustesSaldosUVAyUVI</class-name>
        <query-name>query.QueryAjustesSaldosUVAyUVI</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
				SELECT MOV.SALDO_JTS_OID, MOV.FECHAPROCESO, MOV.SUCURSAL, MOV.ASIENTO, MOV.TRREAL, MOV.ORDINAL, MOV.TRVIRTUAL, MOV.TRTIPO, MOV.FECHAVALOR, 
					   MOV.MONEDA, MOV.DEBITOCREDITO, MOV.CAPITALREALIZADO, E.FECHAPROCESO_ORIGINAL, E.ASIENTO_ORIGINAL, E.SUCURSAL_ORIGINAL, MOV.HORASISTEMA,
					   SUB.SUB_ASIENTO_ORIGINAL, MOV.SUB_ASIENTO
				FROM
					(SELECT M.FECHAPROCESO, M.SUCURSAL, M.ASIENTO, M.TRREAL, M.ORDINAL, M.TRVIRTUAL, M.TRTIPO, M.FECHAVALOR, 
							M.MONEDA, M.DEBITOCREDITO, M.CAPITALREALIZADO, M.SALDO_JTS_OID, M.HORASISTEMA, M.SUB_ASIENTO 
					FROM MOVIMIENTOS_CONTABLES M INNER JOIN ASIENTOS A ON M.FECHAPROCESO=A.FECHAPROCESO AND M.ASIENTO=A.ASIENTO AND M.SUCURSAL=A.SUCURSAL 
					WHERE PRODUCTO IN (SELECT C6250 FROM PRODUCTOS WHERE C6252 IN (3,4)) 
						  AND MONEDA IN (999,988) 
						  AND M.FECHAPROCESO&gt;(SELECT FECHAPROCESOANTERIOR FROM PARAMETROS) 
						  AND M.FECHAPROCESO&lt;= (SELECT FECHAPROCESO FROM PARAMETROS) 
						  AND A.ESTADO=77) MOV	
					LEFT JOIN CON_ASIENTOS_EXTORNADOS E ON MOV.FECHAPROCESO=E.FECHAPROCESO AND MOV.ASIENTO=E.ASIENTO AND MOV.SUCURSAL=E.SUCURSAL 
					LEFT JOIN CON_SUBASIENTOS_EXTORNADOS SUB ON E.FECHAPROCESO=SUB.FECHAPROCESO AND E.ASIENTO=SUB.ASIENTO AND E.SUCURSAL=SUB.SUCURSAL
					WHERE MOV.SALDO_JTS_OID NOT IN (SELECT SALDO_JTS_OID FROM CON_PROCESADO_UVA_UVI WHERE FECHAPROCESO = (SELECT FECHAPROCESO FROM PARAMETROS))
					ORDER BY MOV.SALDO_JTS_OID, MOV.FECHAPROCESO, MOV.HORASISTEMA ASC, MOV.ASIENTO, MOV.TRREAL
			</sentence>
