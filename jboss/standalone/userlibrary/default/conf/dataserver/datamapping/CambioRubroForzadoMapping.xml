	<!-- QUERY'S -->
	<query>
	<class-name>topsystems.automaticprocess.objectsdomain.JtsOidGenericObject</class-name>
	<query-name>query.CambioRubroForzado</query-name>
	<database-name>TOP/CLIENTES</database-name>
	<sentence>
	SELECT S.JTS_OID
	FROM   SALDOS S
	WHERE  S.C1785 = 5 AND (C1604 &lt; 0 OR C1608 &gt; 0) 
	AND S.C1728 &lt;&gt; 'C'
	AND	S.TZ_LOCK = 0 AND (S.C1734 &lt;&gt; ' ' AND S.C1734 IS NOT NULL)
	</sentence>
	<attribute>
		<attribute-name>JTS_OID</attribute-name>
		<column-name>JTS_OID</column-name>
		<column-type>long</column-type>
	</attribute>
	</query>
