		<database-name>TOP/CLIENTES</database-name>
		<sentence>
			select codlineafinan, montolinfin, montodesemb 
			from lf_lineasfinanciamiento 
			where 
			tz_lock = 0 and 
			estado = 'A' and 
			moneda = ? and
			(? between montomin and montomax) and
			(? between pzodesde and pzohasta) and
			(? between tasaanualmin and tasaanualmax) and
			(tipocliente = ? or tipocliente = 'A') and
			(sexo = ? or sexo = 'T') order by prioridad desc
		</sentence>
