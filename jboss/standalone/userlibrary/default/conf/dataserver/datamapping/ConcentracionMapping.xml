<map>
    <query>
        <class-name>limitecredito.objectsdomain.LibradorCheque</class-name>
        <query-name>query.LibradorCheque</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>		
			select
			28 as banco,
			ch.sucursal as sucursal,
			ch.cuenta as cuenta
			from 
			CHE_CHEQUES ch, saldos s, CLI_CLIENTES cli
			where
			ch.tz_lock = 0 and
			s.tz_lock = 0 and
			cli.tz_lock = 0 and
			ch.JTS_ORIGEN = s.jts_oid and
			s.c1803 = cli.CODIGOCLIENTE and
			s.c1785=6 AND
			ch.nro_solicitud = ?
			union
			select
			chs.banco_girado as banco,
			chs.sucursal_banco_girado as sucursal,
			chs.numerico_cuenta_giradora as cuenta
			from
			CLE_CHEQUES_SALIENTE chs
			where
			chs.tz_lock = 0 and
			chs.nro_solicitud = ?      
		</sentence>
        <attribute>
            <attribute-name>banco</attribute-name>
            <column-name>banco</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>sucursal</attribute-name>
            <column-name>sucursal</column-name>
            <column-type>long</column-type>
        </attribute>
        <attribute>
            <attribute-name>cuenta</attribute-name>
            <column-name>cuenta</column-name>
            <column-type>long</column-type>
        </attribute>
    </query>
