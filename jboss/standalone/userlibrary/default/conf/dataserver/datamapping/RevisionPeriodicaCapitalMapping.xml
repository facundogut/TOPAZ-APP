    <query>
        <class-name>topsystems.automaticprocess.objectsdomain.JtsOidGenericObject</class-name>
        <query-name>query.vo_SaldosRevisionPeriodicaCapital</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
			select JTS_OID 
			       from saldos
			       where 
			       c1621 &lt;= (select FechaProceso from parametros) and 
			       (c1604 &lt; 0 and C1785 = 5) and 
			       saldos.Tz_lock = 0 and producto = ? and C1771 = ?
		</sentence>
        <attribute>
            <attribute-name>JTS_OID</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>long</column-type>
        </attribute>
    </query>
