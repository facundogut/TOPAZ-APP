    <query>
        <class-name>topsystems.automaticprocess.objectsdomain.JtsOidGenericObject</class-name>
        <query-name>query.vo_saldosDobleCronograma</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
			SELECT S.JTS_OID
			FROM SALDOS AS S
			WHERE
				S.ORDINAL = ? AND
				S.CUENTA = ? AND
				S.MONEDA = ? AND
				S.SUCURSAL = ? AND
				(S.[C1604] &lt; 0 OR S.[C1608] &gt; 0) AND
				S.[producto] = (SELECT P.[prodblcro] FROM [PRODUCTOS] AS P WHERE P.[C6250] = ?)
        </sentence>
        <attribute>
            <attribute-name>JTS_OID</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>double</column-type>
        </attribute>
    </query>
