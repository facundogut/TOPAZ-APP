<map>
    <query>
        <class-name>topsystems.automaticprocess.objectsdomain.JtsOidGenericObject</class-name>
        <query-name>query.vo_clientesCategorizar</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
			SELECT
				S.JTS_OID,
				S.PRODUCTO
			FROM SALDOS AS S WITH (nolock)
			WHERE
				S.TZ_LOCK = 0
				AND ((
						S.C1604 &lt; 0
						AND S.C1785 IN (5)
						AND S.C1734 IN ('C','Z')
						AND S.C1734 &lt;&gt; s.C1728
					)
					OR (
						S.C1604 &gt; 0
						AND S.C1785 = 4
						AND S.C1734 IN ('I')
						AND S.C1734 &lt;&gt; s.C1728
						AND S.PRODUCTO &lt;&gt; (SELECT NUMERICO FROM PARAMETROSGENERALES WHERE CODIGO = 708)
						AND S.MONEDA IN (
							SELECT M.C6399
							FROM MONEDAS AS M
							WHERE
								M.C6403 &lt;&gt; 'I'
								AND ((M.TZ_LOCK &lt; 100000000000000 OR M.TZ_LOCK &gt;= 200000000000000)
									AND (M.TZ_LOCK &lt; 300000000000000 OR M.TZ_LOCK &gt;= 400000000000000)
								)
						)
					)
				)
			ORDER BY S.C1785 DESC
        </sentence>
        <attribute>
            <attribute-name>JTS_OID</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>double</column-type>
        </attribute>
    </query>
    <query>
        <class-name>topsystems.automaticprocess.objectsdomain.JtsOidGenericObject</class-name>
        <query-name>query.vo_clientesCategorizarMensual</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
			SELECT S.JTS_OID, S.PRODUCTO
			FROM SALDOS AS S
			WHERE S.PRODUCTO IN(4202,4203,4301,4302,4303,4101,4102,4103,4105,4106,4205,4206,4201) AND
				 S.C1604 &lt;&gt; 0  AND 
				 S.TZ_LOCK = 0 
			ORDER BY c1785 DESC
        </sentence>
        <attribute>
            <attribute-name>JTS_OID</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>double</column-type>
        </attribute>
    </query>
    <query>
        <class-name>topsystems.automaticprocess.objectsdomain.JtsOidGenericObject</class-name>
        <query-name>query.vo_clientesCategorizarVista</query-name>
        <database-name>TOP/CLIENTES</database-name>
        <sentence>
			SELECT S.JTS_OID, S.PRODUCTO
			FROM SALDOS AS S
			WHERE 
				S.C1651 &lt;&gt; '1' -- cuenta no cancelada
				AND S.C1785 IN (2, 3) -- sea cuenta vista ahorro y cta cte
				AND S.TZ_LOCK = 0
				AND (S.C3877 &lt; (SELECT FECHAPROCESO FROM PARAMETROS) OR S.C3877 is null)
				AND S.C1734 &lt;&gt; S.c1728 
				AND S.C1734 &lt;&gt; ' ' 
			ORDER  BY C1785 DESC
        </sentence>
        <attribute>
            <attribute-name>JTS_OID</attribute-name>
            <column-name>JTS_OID</column-name>
            <column-type>double</column-type>
        </attribute>
    </query>
