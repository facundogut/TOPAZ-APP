<?xml version="1.0" encoding="UTF-8"?>
<TableRequestConfig>

        <!-- TableRequest loaders -->

        <TableRequestProcessor>
                <name>xmlDirProcessor</name>
                <className>com.topsystems.jtopaz.tablerequest.processor.xml.XMLDirTableRequestProcessor</className>
                <!-- Si no se define la property "dir", se toma por defecto TOPAZ_HOME/user_library/TOPAZ_USER_HOME/tablerequests -->
                <TableRequestProcessorProperty>
                        <name>path</name>
                        <value>{{ param_files.table_request_config.ope_path }}</value>
                </TableRequestProcessorProperty>
        </TableRequestProcessor>
</TableRequestConfig>
