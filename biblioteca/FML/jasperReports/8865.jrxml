	<queryString>
		<![CDATA[SELECT 
S.JTS_OID,
S.SUCURSAL,
U.NOMBRESUCURSAL,
S.C1803 AS CLIENTE,
S.PRODUCTO,
S.MONEDA, 
S.CUENTA,
S.OPERACION,
S.ORDINAL,
CASE
	WHEN EXISTS (SELECT * FROM PYF_APODERADOS WHERE ID_ENTIDAD = S.JTS_OID AND MONTO_MAX_INDIV <> 0) THEN 'INDISTINTA'
	ELSE 'CONJUNTA'
END AS MODALIDAD_CUENTA,
CP.NUMEROPERSONA,
E.EMAIL,
L.DESCRIPCION_DIM3 AS LOCALIDAD_SUC
FROM SALDOS AS S
INNER JOIN PRODUCTOS AS P ON P.C6250 = S.PRODUCTO AND P.TZ_LOCK = 0
LEFT JOIN CLI_CLIENTEPERSONA AS CP ON S.C1803 = CP.CODIGOCLIENTE AND CP.TITULARIDAD = 'T' AND CP.TZ_LOCK = 0
LEFT JOIN CLI_DIRECCIONES AS D ON CP.NUMEROPERSONA = D.ID AND D.TZ_LOCK = 0
LEFT JOIN CLI_EMAILS AS E ON D.ID = E.ID AND E.TIPO IN ('PE', 'LE') AND E.TZ_LOCK = 0
AND E.ORDINAL = (SELECT MIN(ORDINAL) FROM CLI_EMAILS WHERE ID = E.ID AND TIPO = E.TIPO AND TZ_LOCK = 0)
LEFT JOIN SUCURSALES AS U ON S.SUCURSAL = U.SUCURSAL AND U.TZ_LOCK = 0
LEFT JOIN CLI_DIRECCIONES AS DI ON $P{TOPAZ_SUCURSAL_SISTEMA} = DI.ID  AND DI.FORMATO = 'S' AND DI.TIPODIRECCION = 'R' AND DI.TZ_LOCK = 0
LEFT JOIN CLI_LOCALIDADES AS L ON DI.PAIS = L.CODIGOPAIS AND DI.PROVINCIA = L.DIM1 AND DI.DEPARTAMENTO = L.DIM2
AND DI.LOCALIDAD = L.DIM3 AND L.TZ_LOCK = 0
WHERE S.TZ_LOCK = 0 AND S.JTS_OID = $P{JTS_OID}
GROUP BY S.JTS_OID, S.SUCURSAL, U.NOMBRESUCURSAL, S.C1803, S.PRODUCTO, S.MONEDA, S.CUENTA,
S.OPERACION, S.ORDINAL, CP.NUMEROPERSONA, E.EMAIL, L.DESCRIPCION_DIM3, E.TIPO
ORDER BY E.TIPO]]>
	</queryString>
