	<subDataset name="datasetTablaCaja" uuid="a0ca8d98-0277-4370-aa0b-82c92725a272">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Adaptador NBCH"/>
		<queryString>
			<![CDATA[SELECT CT.DESCRIPCION+' '+CAST(CT.LARGO AS VARCHAR(8))+' x '+CAST(CT.ANCHO AS VARCHAR(8))+' x '+CAST(CT.ALTO AS VARCHAR(8)) AS DESCR,
CI.IMPORTE_APLICAR
FROM COF_COFRES_TIPO CT
INNER JOIN CI_CARGOS_TARIFAS CI ON CT.TIPO = CI.SEGMENTO 
WHERE CI.MONEDA = 80 AND CI.ID_CARGO = 210;]]>
		</queryString>
		<field name="DESCR" class="java.lang.String"/>
		<field name="IMPORTE_APLICAR" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[java.lang.System.getProperty("Topaz.reportesJR")]]></defaultValueExpression>
	</parameter>
	<parameter name="GENERA_EXCEL" class="java.lang.Boolean" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA[false]]></defaultValueExpression>
	</parameter>
	<parameter name="NRO_CONTRATO" class="java.math.BigDecimal">
		<parameterDescription><![CDATA[Nro. Contrato Caja Seg.:]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SET
LANGUAGE Spanish;

SELECT 
	CC.CODIGO AS CONTRATO,
	C.NUMERO_COFRE,
	CC.FECHA_ALTA,
	DATEPART(year, CC.FECHA_ALTA) AS ANIO_ALTA,
	DATEPART(m, CC.FECHA_ALTA) AS MES_ALTA,
	DATENAME(MONTH, CC.FECHA_ALTA) AS MES_NOMBRE_ALTA,
	DATEPART(d, CC.FECHA_ALTA) AS DIA_ALTA,
	DATEPART(year, GETDATE()) AS ANIO_ACTUAL,
	DATEPART(m, GETDATE()) AS MES_ACTUAL,
	DATENAME(MONTH, GETDATE()) AS MES_NOMBRE,
	DATEPART(d, GETDATE()) AS DIA_ACTUAL,
	CASE
		WHEN PJ.NUMEROPERSONAJURIDICA IS NULL 
			THEN (	ISNULL(NULLIF(PRIMERNOMBRE, ''),'') 
					+ ISNULL(NULLIF(' '+SEGUNDONOMBRE, ''),'') 
					+ ISNULL(NULLIF(' '+APELLIDOMATERNO, ''),'') 
					+ ISNULL(NULLIF(' '+APELLIDOPATERNO, ''),'') 
				)
		ELSE PJ.RAZONSOCIAL
	END NOMBRE_PERSONA,
	ISNULL(CD.NUM_DOC_FISICO, 'N/A') AS NRO_DNI,
	CD.NUMERODOCUMENTO AS CUIL,
	D.CALLE + ' ' + ISNULL(NULLIF(CAST(D.NUMERO AS VARCHAR(8)), ''),'S/N') AS DIRECCION,
	CT.DESCRIPCION AS TIPO_COFRE,
	SUC.NOMBRESUCURSAL,
	CE.EMAIL,
	P.DESCRIPCION AS PROVINCIA
FROM COF_COFRES_CONTRATOS AS CC WITH (nolock)
INNER JOIN SALDOS AS S WITH (nolock) ON
	S.SUCURSAL = CC.SUCURSAL_DEBITO
	AND S.PRODUCTO = CC.PRODUCTO_DEBITO
	AND S.CUENTA = CC.CUENTA_DEBITO
	AND S.MONEDA = CC.MONEDA
	AND S.OPERACION = CC.OPERACION_DEBITO
	AND S.ORDINAL = CC.ORDINAL_DEBITO
	AND S.TZ_LOCK = 0
INNER JOIN COF_COFRES AS C WITH (nolock) ON
	C.CODIGO = CC.CODIGO_COFRE
	AND C.TZ_LOCK = 0
INNER JOIN COF_COFRES_TIPO AS CT WITH (nolock) ON
	CT.TIPO = C.TIPO
	AND CT.TZ_LOCK = 0
INNER JOIN SUCURSALES AS SUC WITH (nolock) ON
	C.DEPENDENCIA = SUC.SUCURSAL
	AND SUC.TZ_LOCK = 0	
INNER JOIN CLI_DIRECCIONES AS DIR_SUC WITH(nolock) ON
	SUC.SUCURSAL = DIR_SUC.ID
	AND DIR_SUC.FORMATO = 'S'
	AND DIR_SUC.TIPODIRECCION = 'R'
	AND DIR_SUC.TZ_LOCK = 0
INNER JOIN CLI_PROVINCIAS AS P WITH(nolock) ON
	DIR_SUC.PROVINCIA = P.DIM1
	AND P.TZ_LOCK = 0
INNER JOIN CLI_CLIENTES AS CLI WITH (nolock) ON
	CLI.CODIGOCLIENTE = CC.CLIENTE
	AND CLI.TZ_LOCK = 0
INNER JOIN CLI_CLIENTEPERSONA AS CP WITH (nolock) ON
	CP.CODIGOCLIENTE = CLI.CODIGOCLIENTE
	AND CP.TITULARIDAD = 'T'
	AND CP.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASFISICAS AS PF WITH (nolock) ON
	PF.NUMEROPERSONAFISICA = CP.NUMEROPERSONA
	AND PF.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASJURIDICAS AS PJ WITH (nolock) ON
	PJ.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA
	AND PJ.TZ_LOCK = 0
INNER JOIN CLI_DocumentosPFPJ AS CD WITH (nolock) ON
	CD.NUMEROPERSONAFJ = CP.NUMEROPERSONA
	AND CD.TIPODOCUMENTO = (SELECT
								TOP 1 TIPODOCUMENTO
							FROM CLI_DocumentosPFPJ
							WHERE 
								NUMEROPERSONAFJ = CD.NUMEROPERSONAFJ
								AND TZ_LOCK = 0
							)
	AND CD.NUMERODOCUMENTO = (	SELECT
									TOP 1 NUMERODOCUMENTO
								FROM CLI_DocumentosPFPJ
								WHERE
									NUMEROPERSONAFJ = CD.NUMEROPERSONAFJ
									AND TIPODOCUMENTO = CD.TIPODOCUMENTO
									AND TZ_LOCK = 0
								)
	AND CD.TZ_LOCK = 0
INNER JOIN CLI_DIRECCIONES AS D WITH (nolock) ON
	D.ID = CP.NUMEROPERSONA
	AND D.FORMATO =
	(CASE
		WHEN CD.TIPOPERSONA = 'F' 
			THEN 'PF'
		ELSE 'PJ'
	END)
	AND D.TIPODIRECCION IN ('L','R','PR')
	AND D.ORDINAL_DIR = (	SELECT
								TOP 1 ORDINAL_DIR
							FROM CLI_DIRECCIONES
							WHERE
								ID = D.ID
								AND FORMATO = D.FORMATO
								AND TIPODIRECCION = D.TIPODIRECCION
								AND TZ_LOCK = 0
						)
	AND D.TZ_LOCK = 0
LEFT JOIN GRL_ESTADOS_DE_CUENTA AS GED WITH(nolock) ON
	GED.SUCURSAL = S.SUCURSAL
	AND GED.PRODUCTO = S.PRODUCTO
	AND GED.MONEDA = S.MONEDA
	AND GED.CUENTA = S.CUENTA
	AND GED.OPERACION = S.OPERACION
	AND GED.ORDINAL = S.ORDINAL
	AND GED.TIPO_EMISION = (SELECT
								TOP 1 TIPO_EMISION
							FROM GRL_ESTADOS_DE_CUENTA
							WHERE
								SUCURSAL = GED.SUCURSAL
								AND PRODUCTO = GED.PRODUCTO
								AND MONEDA = GED.MONEDA
								AND CUENTA = GED.CUENTA
								AND OPERACION = GED.OPERACION
								AND ORDINAL = GED.ORDINAL
								AND TZ_LOCK = 0
							)
	AND GED.TZ_LOCK = 0
LEFT JOIN CLI_EMAILS AS CE WITH(nolock) ON
	CE.ID = CP.NUMEROPERSONA
	AND CE.FORMATO = GED.FORMATO_MAIL
	AND CE.TIPO = GED.TIPO_MAIL
	AND CE.ORDINAL = GED.ORDINAL_MAIL
	AND CE.TZ_LOCK = 0
WHERE
	CC.TZ_LOCK = 0
	AND CC.CODIGO = $P{NRO_CONTRATO}]]>
	</queryString>
	<field name="CONTRATO" class="java.math.BigDecimal"/>
	<field name="NUMERO_COFRE" class="java.math.BigDecimal"/>
