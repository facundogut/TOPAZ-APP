	<queryString>
		<![CDATA[SELECT 
	S.SUCURSAL,
	C.CODIGOCLIENTE,
	CP.NUMEROPERSONA,
	CONCAT_WS(' ', DS.CALLE, ISNULL(NULLIF(CAST(DS.NUMERO AS VARCHAR), '0'),'S/N')) AS DIRSUC, 
	C.NOMBRECLIENTE,
	CONCAT_WS(' ', D.CALLE, ISNULL(NULLIF(CAST(D.NUMERO AS VARCHAR), '0'),'S/N')) AS DIRCLI,
	L.DESCRIPCION_DIM3 AS LOCALIDAD,
	D.CPA_NUEVO AS CP,
	U.NOMBRESUCURSAL AS NOMSUC,
	S.CUENTA,
	CC.FECHA_CANCELACION
FROM
	SALDOS AS S WITH (nolock)
INNER JOIN SUCURSALES AS U WITH (nolock) ON
	U.SUCURSAL = S.SUCURSAL
	AND U.TZ_LOCK = 0
INNER JOIN CLI_DIRECCIONES AS DS WITH (nolock) ON
	DS.TZ_LOCK = 0
	AND DS.ID = U.SUCURSAL
	AND DS.FORMATO = 'S'
	AND DS.TIPODIRECCION = 'R'
	AND DS.ORDINAL_DIR IN (	SELECT
								TOP 1 ORDINAL_DIR
							FROM 
								CLI_DIRECCIONES
							WHERE
								ID = DS.ID
								AND FORMATO = DS.FORMATO
								AND TIPODIRECCION = DS.TIPODIRECCION
								AND TZ_LOCK = 0
							)
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON
	C.CODIGOCLIENTE = S.C1803
	AND C.TZ_LOCK = 0
INNER JOIN VW_CLIENTES_PERSONAS AS CP WITH (nolock) ON
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE
	AND CP.TITULARIDAD = 'T'
INNER JOIN CLI_DIRECCIONES AS D WITH (nolock) ON
	D.TZ_LOCK = 0
	AND D.ID = CP.NUMEROPERSONA
	AND D.FORMATO =
				CASE
					WHEN CP.TIPOPERSONA = 'F'
						THEN 'PF'
					ELSE 'PJ'
				END
	AND D.TIPODIRECCION =
				CASE
					WHEN CP.TIPOPERSONA = 'F'
						THEN 'PR'
					ELSE 'L'
				END
	AND D.ORDINAL_DIR IN (	SELECT
								TOP 1 ORDINAL_DIR
							FROM 
								CLI_DIRECCIONES
							WHERE
								ID = D.ID
								AND FORMATO = D.FORMATO
								AND TIPODIRECCION = D.TIPODIRECCION
								AND TZ_LOCK = 0
							)
INNER JOIN CLI_LOCALIDADES AS L WITH (nolock) ON
	L.CODIGOPAIS = D.PAIS
	AND L.DIM1 = D.PROVINCIA
	AND L.DIM2 = D.DEPARTAMENTO
	AND L.DIM3 = D.LOCALIDAD
	AND L.TZ_LOCK = 0
LEFT JOIN CV_CANCELACION_CUENTAS AS CC WITH (nolock) ON
	CC.SALDO_JTS_OID = S.JTS_OID
	AND CC.TZ_LOCK = 0
WHERE
	S.TZ_LOCK = 0
	AND S.JTS_OID = $P{JTSOID}]]>
	</queryString>
