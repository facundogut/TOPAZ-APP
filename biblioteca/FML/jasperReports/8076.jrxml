	<queryString>
		<![CDATA[SELECT concat(pp.C2300,' - ',convert(varchar,pp.C2302,103)) AS COLUMNA1,
       FORMAT(pp.C2304,'#,0.00') AS COLUMNA2,
       FORMAT(pp.C2305,'#,0.00') AS COLUMNA3,
       (SELECT FORMAT(isnull(sum(Importe_gasto),0),'#,0.00')
        FROM GASTOS_POR_CUOTA h WITH(NOLOCK)
        WHERE h.SALDOS_JTS_OID = pp.SALDO_JTS_OID
          AND NUMERO_CUOTA=C2300 AND h.TZ_LOCK = 0)  AS COLUMNA4,
         FORMAT(pp.C2304+pp.C2305+(SELECT isnull(sum(Importe_gasto),0)
        FROM GASTOS_POR_CUOTA h WITH(NOLOCK)
        WHERE h.SALDOS_JTS_OID = pp.SALDO_JTS_OID
          AND NUMERO_CUOTA=C2300 AND h.TZ_LOCK = 0),'#,0.00') AS COLUMNA5,
        sc.C1704 AS N_SOLICITUD
 FROM PLANPAGOS pp WITH(NOLOCK)
 INNER JOIN SALDOS sc WITH(NOLOCK) ON pp.SALDO_JTS_OID = sc.JTS_OID
 WHERE pp.SALDO_JTS_OID= $P{JTS_OID}]]>
	</queryString>
