	<parameter name="NUMERO_SOLICITUD" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
ref.NUMERO_SOLICITUD,
       cl.NOMBRECLIENTE AS NOMBRE_CLIENTE,
       vc.NUMERODOCUMENTO AS CUIT,
       cp.NUMEROPERSONA,
       (SELECT o.DESCRIPCION FROM OPCIONES o WITH(NOLOCK)
         WHERE o.NUMERODECAMPO = 1357
           AND o.OPCIONINTERNA = cl.IVA) AS COND_AFIP,
       CONCAT(ca.CODIGO_ACT_AFIP,' - ',ca.DESCRIPCION) AS COD_ACTIVIDAD,
       (SELECT TOP 1 NUMERO FROM CLI_TELEFONOS WITH(NOLOCK)
         WHERE ID = cp.NUMEROPERSONA AND TZ_LOCK = 0
           AND FORMATO = (CASE WHEN cl.TIPO='F'THEN'PF'
                               WHEN cl.TIPO='J'THEN'PJ'
                               ELSE 'Sin telefono' END)
           ORDER BY ID desc) AS TELEFONO,
 	   CONCAT(vc.calle,' ',vc.numero) DOMICILIO,
       vc.CPA_NUEVO AS COD_POST,
       vc.LOCALIDAD AS LOCALIDAD,
       vc.PROVINCIA AS PROVINCIA ,
ref.CONCEPTO,
ref.OPERACION,
ref.IMPORTE_CANCELADO,
cl.TIPO,
vc.NUMERODOCUMENTO AS CUIT,
cl.NOMBRECLIENTE
FROM VW_CRE_SOL_ASISTENCIAS_REF ref WITH(NOLOCK)
INNER JOIN SALDOS sal WITH(NOLOCK)
ON sal.C1704 = ref.NUMERO_SOLICITUD
INNER JOIN CLI_CLIENTES cl WITH(NOLOCK) ON sal.C1803 = cl.CODIGOCLIENTE AND cl.TZ_LOCK = 0
INNER JOIN CLI_ClientePersona cp WITH(NOLOCK) ON cl.CODIGOCLIENTE = cp.CODIGOCLIENTE AND cp.TITULARIDAD = 'T' AND cp.TZ_LOCK = 0
INNER JOIN VW_CLI_DIRECCIONES vc WITH(NOLOCK) ON cp.CODIGOCLIENTE = vc.CODIGOCLIENTE AND (vc.tipodireccion = 'L'OR vc.tipodireccion='PR')
  INNER JOIN CLI_Cod_Act_AFIP ca WITH(NOLOCK) ON sal.C1723 = ca.CODIGO_ACT_AFIP
WHERE ref.NUMERO_SOLICITUD =$P{NUMERO_SOLICITUD}
]]>
	</queryString>
