	<queryString>
		<![CDATA[SELECT
U.NOMBRESUCURSAL AS SUC_EMISION,
CONCAT(DU.CALLE + ' ', DU.NUMERO) AS DIRECCION_SUCURSAL,
V.NOMBRE_CUENTA,
P.C6251 AS TIPO_CUENTA,
M.C6400 AS MONEDA,
SU.NOMBRESUCURSAL AS SUC_CUENTA,
S.CUENTA,
V.CTA_CBU,
S.C1803 AS CLIENTE
FROM SALDOS AS S
INNER JOIN VTA_SALDOS AS V ON S.JTS_OID = V.JTS_OID_SALDO AND V.TZ_LOCK = 0
LEFT JOIN SUCURSALES AS U ON U.SUCURSAL = $P{TOPAZ_SUCURSAL_SISTEMA}  AND U.TZ_LOCK = 0
LEFT JOIN CLI_DIRECCIONES AS DU ON U.SUCURSAL = DU.ID AND DU.FORMATO = 'S' 
AND DU.TIPODIRECCION = 'R' AND DU.ORDINAL_DIR = 0 AND DU.TZ_LOCK = 0
LEFT JOIN PRODUCTOS AS P ON S.PRODUCTO = P.C6250 AND P.TZ_LOCK = 0
LEFT JOIN MONEDAS AS M ON S.MONEDA = M.C6399 AND M.TZ_LOCK = 0
LEFT JOIN SUCURSALES SU ON S.SUCURSAL = SU.SUCURSAL AND SU.TZ_LOCK = 0
WHERE S.TZ_LOCK = 0 AND S.JTS_OID = $P{JTS_OID}]]>
	</queryString>
