	<parameter name="JTS_OID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
A.CODIGO_PERSONA_CLIENTE,
A.FUENTE_INGRESO,
A.EMPRESA,
D.DESCRIPCION AS ACTIVIDAD,
R.DESCRIPCION AS PROFESION,
A.RUC_INDEPENDIENTE
FROM SALDOS AS S
INNER JOIN CLI_CLIENTEPERSONA AS C ON S.C1803 = C.CODIGOCLIENTE AND C.TITULARIDAD = 'T' AND C.TZ_LOCK = 0
INNER JOIN CLI_PERSONASFISICAS AS P ON C.NUMEROPERSONA = P.NUMEROPERSONAFISICA AND P.TZ_LOCK = 0
INNER JOIN CLI_ACTIVIDAD_ECONOMICA AS A ON P.NUMEROPERSONAFISICA = A.CODIGO_PERSONA_CLIENTE AND A.TZ_LOCK = 0
LEFT JOIN CLI_Cod_Act_AFIP D ON A.CODIGO_ACTIVIDAD = D.CODIGO_ACT_AFIP AND D.TZ_LOCK = 0
LEFT JOIN CLI_PROFESIONES AS R ON P.COD_PROFESION = R.PROFESION AND R.TZ_LOCK = 0
WHERE S.JTS_OID = $P{JTS_OID}
AND S.TZ_LOCK = 0]]>
	</queryString>
