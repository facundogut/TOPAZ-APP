	<queryString>
		<![CDATA[SELECT
DISTINCT S.JTS_OID
FROM SALDOS AS S
INNER JOIN CLI_CLIENTEPERSONA AS CP ON S.C1803 = CP.CODIGOCLIENTE AND CP.TZ_LOCK = 0
INNER JOIN CLI_INTEGRANTESPJ AS I ON CP.NUMEROPERSONA = I.NUMEROPERSONAJURIDICA AND I.TZ_LOCK = 0 
AND (I.CODIGOCARGO = 'REP' OR I.CODIGOCARGO IN (
SELECT CARGO FROM CLI_ROLES_OBL_CARGO AS C WHERE C.OBLIGATORIO = 'S' AND C.TZ_LOCK = 0 AND C.TIPO_SOCIEDAD IN (
SELECT TIPOSOCIEDAD FROM CLI_PERSONASJURIDICAS AS PJ WHERE PJ.NUMEROPERSONAJURIDICA = I.NUMEROPERSONAJURIDICA AND
PJ.TZ_LOCK = 0)))
INNER JOIN CLI_PERSONASFISICAS AS PF ON I.NUMEROPERSONAFISICA = PF.NUMEROPERSONAFISICA AND PF.TZ_LOCK = 0
INNER JOIN CLI_DOCUMENTOSPFPJ AS D ON PF.NUMEROPERSONAFISICA = D.NUMEROPERSONAFJ AND D.TZ_LOCK = 0
WHERE S.TZ_LOCK = 0 AND S.JTS_OID = $P{JTS_OID}]]>
	</queryString>
