	<subDataset name="DATADETALLE2">
		<queryString>
			<![CDATA[SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)) AS CAPITAL, sum(INTERESPAGADO) AS INTERES, sum(MORAPAGADA) AS MORA
FROM VBS_PAYS_DETAIL
WHERE CUOTA = 1 AND TIPO='P' AND SALDOS_JTS_OID=$F{JTS}]]>
		</queryString>
		<field name="CAPITAL" class="java.math.BigDecimal"/>
		<field name="INTERES" class="java.math.BigDecimal"/>
		<field name="MORA" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="DATADETALLE">
		<parameter name="NUMEROCUOTA" class="java.lang.String"/>
		<parameter name="JTSOID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
(SELECT NROCUOTA FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS NCUOTA,
(SELECT FECHAVTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS FVENC,
(SELECT CAPITAL FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS CAPITAL,
(SELECT INTERES FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS INTERES,
(SELECT GASTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS GASTO,
(SELECT CAPITAL+INTERES+GASTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS TOTAL,
(SELECT FECHAVALOR FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS FECHAVALOR,
(SELECT DATEDIFF(d,a.FECHAVALOR,b.FECHAVTO) FROM VBS_PAYS_DETAIL a, V_SALDOS_PLANPAGOS b WHERE a.CUOTA=p.CUOTA AND a.TIPO='P' AND a.SALDOS_JTS_OID=p.SALDOS_JTS_OID AND b.IDJTSOID=vsp.IDJTSOID AND b.NROCUOTA=vsp.NROCUOTA) AS DIAS,
(SELECT FECHAPROCESOMOV FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS FECHAPROCMOV,
(SELECT NROASIENTOMOV FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS ASIENTO,
(SELECT SUCURSALMVTO FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS SUC,
(SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)) FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CAPITALP,
(SELECT sum(INTERESPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS INTERESP,
(SELECT sum(MORAPAGADA) FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS MORAP,
(SELECT sum(CAPITALPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CARGOSP,
((SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)) FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)+(SELECT sum(CAPITALPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = p.CUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)) AS TOTALP
FROM VBS_PAYS_DETAIL p, V_SALDOS_PLANPAGOS vsp
WHERE p.SALDOS_JTS_OID=$P{JTSOID} AND vsp.IDJTSOID=$P{JTSOID} AND
p.CUOTA=$P{NUMEROCUOTA} AND vsp.NROCUOTA=$P{NUMEROCUOTA}]]>
		</queryString>
		<field name="NCUOTA" class="java.lang.Integer"/>
		<field name="FVENC" class="java.util.Date"/>
		<field name="CAPITAL" class="java.math.BigDecimal"/>
		<field name="INTERES" class="java.math.BigDecimal"/>
		<field name="GASTO" class="java.math.BigDecimal"/>
		<field name="TOTAL" class="java.math.BigDecimal"/>
		<field name="FECHAVALOR" class="java.util.Date"/>
		<field name="DIAS" class="java.lang.Integer"/>
		<field name="FECHAPROCMOV" class="java.util.Date"/>
		<field name="ASIENTO" class="java.lang.Integer"/>
		<field name="SUC" class="java.lang.Integer"/>
		<field name="CAPITALP" class="java.math.BigDecimal"/>
		<field name="INTERESP" class="java.math.BigDecimal"/>
		<field name="MORAP" class="java.math.BigDecimal"/>
		<field name="CARGOSP" class="java.math.BigDecimal"/>
		<field name="TOTALP" class="java.math.BigDecimal"/>
		<group name="NCUOTA">
			<groupExpression><![CDATA[$F{NCUOTA}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="NPRESTAMO" class="java.lang.String"/>
	<queryString>
		<![CDATA[
SELECT DISTINCT vsp.NROPRESTAMO, s.C1604 AS SALDO,s.C1601 AS MONTO,s.C1728 AS ESTADO,vsp.NROCLIENTE,s.SUCURSAL,vsp.ANALISTA,vsp.NROPRODUCTO,s.C1642*s.C1644 AS PLAZO,
vsp.FECDESEM, vsp.NROSOLICITUD,vsp.MONEDA,vsp.TASAINT,s.C4765*s.C1642 AS PLAZOGRACIA, s.JTS_OID AS JTS, s.C1644 AS TOTALCUOTAS, vsp.NROCUOTA AS CUOTA
FROM (V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN VBS_PAYS_DETAIL vpd ON vsp.IDJTSOID=vpd.SALDOS_JTS_OID AND vsp.NROCUOTA=vpd.CUOTA), CO_PRODUCTOS p, SALDOS s WHERE
(vsp.NroProducto = p.C6250) and
vsp.TipoProd = 5 and
((p.C7996 = 2 and
(p.ProDblCro<>0 or p.C8007 = 0)) or
(p.C7996 <> 2 )) and
(s.JTS_OID = vsp.IDJTSOID) and
(s.TZ_LOCK = 0 AND s.TZ_LOCK NOT BETWEEN 300000000000000  AND  399999999999999) AND
s.CUENTA=$P{NPRESTAMO}]]>
	</queryString>
