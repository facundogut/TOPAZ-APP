	<subDataset name="Table Dataset 1"/>
	<subDataset name="Nombre Deudor">
		<parameter name="Id_Solicitud" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT c.C1000 AS Nombre FROM CL_CLIENTES c , SL_SOLICITUDCREDITO s
WHERE C0902=s.C5187
AND s.C5000=$P{Id_Solicitud}
AND (s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000) AND(c.tz_lock < 300000000000000 OR c.tz_lock >= 400000000000000) AND (c.tz_lock < 100000000000000 OR c.tz_lock >= 200000000000000)]]>
		</queryString>
		<field name="Nombre" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DocDeudor">
		<parameter name="Id_Solicitud" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT  td.DESCRIPCION+' No. '+d.NRODOCUMENTO AS cedula  FROM CL_CLIENTES c ,CL_RELPERDOC  d , SL_SOLICITUDCREDITO s , CL_TIPOSDOCUMENTO td
WHERE C0902=s.C5187 AND d.IDPERSONA=C0902 AND d.Principal='S' AND  td.TIPODOC=d.TIPODOC
AND s.C5000=$P{Id_Solicitud}
AND (s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000) AND(c.tz_lock < 300000000000000 OR c.tz_lock >= 400000000000000) AND (c.tz_lock < 100000000000000 OR c.tz_lock >= 200000000000000) AND
(d.tz_lock < 300000000000000 OR d.tz_lock >= 400000000000000) AND (d.tz_lock < 100000000000000 OR d.tz_lock >= 200000000000000)]]>
		</queryString>
		<field name="cedula" class="java.lang.String"/>
	</subDataset>
	<subDataset name="NombreFiador">
		<parameter name="NroGarantia" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT f.NOMBRE FROM GR_GARANTIAS g, VCL_AYUDAPERSONASNATURALES f WHERE g.IDPERSONATITULAR=f.IDPERSONA and g.NroGarantia=$P{NroGarantia}]]>
		</queryString>
		<field name="NOMBRE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="CedulaFiador">
		<parameter name="NroGarantia" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT t.DESCRIPCION+' No. '+d.NRODOCUMENTO as cedula FROM GR_GARANTIAS g, VCL_AYUDAPERSONASNATURALES f ,CL_RELPERDOC d, CL_TIPOSDOCUMENTO t WHERE g.IDPERSONATITULAR=f.IDPERSONA and g.NroGarantia=$P{NroGarantia} AND d.IDPERSONA=f.IDPERSONA AND  t.TIPODOC=d.TIPODOC]]>
		</queryString>
		<field name="cedula" class="java.lang.String"/>
	</subDataset>
