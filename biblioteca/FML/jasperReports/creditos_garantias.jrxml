	<queryString>
		<![CDATA[SELECT gr.Familia as familia, gr.Monto_Utilizado as monto_utilizado, gr.Linea as producto, 
s.C1601 AS cant_orig, s.C1604 as saldo_prestamo, s.OPERACION as operacion, s.ORDINAL as desglose, CONVERT(NUMERIC, s.C1803) AS cliente 
FROM VW_GARANTIASPRESTAMOS gr WITH(NOLOCK) 
JOIN SALDOS s WITH(NOLOCK) ON gr.JTS_OID = s.JTS_OID
 WHERE gr.Garantia = $P{garantia} 
 AND (1=$P{TODOS_FAMILIA} OR gr.Familia = $P{FAMILIA})
 AND (1=$P{TODOS_PRODUCTO} OR gr.Linea = $P{PRODUCTO})]]>
