	<parameter name="TOPAZ_FECHA_SISTEMA" class="java.util.Date" isForPrompting="false"/>
	<parameter name="FECHAP" class="java.util.Date"/>
	<parameter name="FECHAPROCESO" class="java.util.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{FECHAP} == null ? $P{TOPAZ_FECHA_SISTEMA} : $P{FECHAP}]]></defaultValueExpression>
	</parameter>
	<parameter name="TOPAZ_INICIALES_USUARIO" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT G.RUBROCONTABLE AS RUBRO,
			       S.MONEDA AS CODIGOMONEDA,
			       S.SUCURSAL,
			       S.OPERACION,
			       S.ORDINAL,
			       G.SALDO_AL_CORTE AS SALDO_ACTUAL,
			       S.c1621 AS FECHA_ALTA,
			       S.c1628 AS FECHA_PROX_VENCIMIENTO,
			       S.c1632 AS TASA_INTERES,
			       G.INT_A_LIQUIDAR AS INTERES_A_LIQUIDAR,
			       S.C1627 AS FECHA_VENCIMIENTO,
			       S.C1803 AS CLIENTE,
			       S.CUENTA,
			       C.C1000,
			       S.C9314 AS TIPOPRODUCTO,
			       G.FECHA AS FECHA_PROCESO,
			       S.PRODUCTO
			  FROM SALDOS S INNER JOIN SALDOS_DIARIOS G ON G.SALDO_JTS_OID = S.JTS_OID
			  AND G.FECHA = isnull($P{FECHAPROCESO},$P{TOPAZ_FECHA_SISTEMA}) AND ((S.C1651 != '1' AND S.C9314 IN (4,5,6,7)) OR (S.C1604 != 0 OR G.INT_A_LIQUIDAR != 0 AND S.C9314 IN (0,1,2,3)))
			  LEFT JOIN CL_CLIENTES C ON S.C1803 = C.C0902
			 ORDER BY S.C1730, S.MONEDA, S.SUCURSAL, S.C9314]]>
	</queryString>
