	<subDataset name="Direccion">
		<parameter name="ID_SOLICITUD" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT  dim1.C1481+', '+dim2.C1207+', '+dim3.C1216+', '+dim4.NOMDIM4+', '+d.REFERENCIA as DIR
	FROM SL_SOLICITUDCREDITO s, TC_DIRECCIONES d,
	TC_DIR_DIM1 dim1,TC_DIR_DIM2 dim2,TC_DIR_DIM3 dim3,TC_DIR_DIM4 dim4
	WHERE d.ID=s.C5187 AND d.FORMATO='P' AND d.TIPODIRECCION='R' AND
	dim1.C1480=d.DIM1 AND
	dim2.C1205=d.DIM1 AND dim2.C1206=d.DIM2 AND
	dim3.C1212=d.DIM1 AND dim3.C1214=d.DIM2 AND dim3.C1215=d.DIM3 AND
	dim4.CODDIM1=d.DIM1 AND dim4.CODDIM2=d.DIM2 AND dim4.CODDIM3=d.DIM3 AND dim4.CODDIM4=d.DIM4
	AND s.C5000=$P{ID_SOLICITUD}*100 AND
	(s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000) AND
	(d.tz_lock < 300000000000000 OR d.tz_lock >= 400000000000000) AND (d.tz_lock < 100000000000000 OR d.tz_lock >= 200000000000000)]]>
		</queryString>
		<field name="DIR" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataMoneda">
		<parameter name="codigo" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT (c6401) simbolo FROM CO_MONEDAS WHERE C6399 =$P{codigo}]]>
		</queryString>
		<field name="simbolo" class="java.lang.String"/>
	</subDataset>
	<parameter name="NRO_SOLICITUD" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[Select C5036, MONEDA from SL_SOLICITUDCREDITO where C5000 =$P{NRO_SOLICITUD}*100]]>
	</queryString>
	<field name="C5036" class="java.math.BigDecimal"/>
	<field name="MONEDA" class="java.math.BigDecimal"/>
