	<queryString>
		<![CDATA[SELECT
s.SUCURSAL,
s.CUENTA,
s.PRODUCTO,
s.MONEDA,
s.OPERACION,
m.CAPITALREALIZADO,
m.FECHAPROCESO,
sc.C6020 AS NOMBRE_SUCURSAL,
p.C6251 AS NOMBRE_PRODUCTO,
mo.C6400 AS NOMBRE_MONEDA
FROM MOVIMIENTOS_CONTABLES m
JOIN ASIENTOS a ON a.ASIENTO = m.ASIENTO AND a.SUCURSAL = m.SUCURSAL
					AND a.FECHAPROCESO = m.FECHAPROCESO
JOIN SALDOS s ON m.SUCURSAL_CUENTA = s.SUCURSAL AND m.MONEDA = s.MONEDA
				AND m.CUENTA = s.CUENTA AND m.OPERACION_CUENTA = s.OPERACION
				AND m.PRODUCTO = s.PRODUCTO AND m.ORDINAL_CUENTA = s.ORDINAL
JOIN TC_SUCURSALES sc ON sc.C5834 = s.SUCURSAL
JOIN CO_PRODUCTOS p ON p.C6250 = s.PRODUCTO
JOIN CO_MONEDAS mo ON mo.C6399 = s.MONEDA
WHERE
-- Oper Devengamiento de Comisiones (8646)
m.OPERACION = 8699
--
AND m.DEBITOCREDITO = 'D'
AND a.ESTADO = 77
AND a.TZ_LOCK = 0
AND sc.TZ_LOCK = 0
AND s.TZ_LOCK = 0
AND p.TZ_LOCK = 0
AND mo.TZ_LOCK = 0
AND $P{FECHA} = m.FECHAPROCESO
order by s.SUCURSAL, s.PRODUCTO, s.MONEDA]]>
	</queryString>
