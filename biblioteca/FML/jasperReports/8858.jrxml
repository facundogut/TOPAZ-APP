				<text><![CDATA[RECIBO. NOTIFICACIONES.]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="20" width="555" height="330" uuid="e69ca56e-08ce-4cd0-adf0-84188ab55462">
