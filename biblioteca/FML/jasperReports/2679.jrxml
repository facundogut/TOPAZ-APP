	<queryString>
		<![CDATA[SET Language 'Spanish';

SELECT ROUND(conv.Plazo, 0) AS plazo,
	   day((SELECT fechaproceso FROM PARAMETROS)) AS dia,
	   DATENAME(month,(SELECT fechaproceso FROM PARAMETROS)) AS mes,
  	   year((SELECT fechaproceso FROM PARAMETROS))	AS año,
	   conv.NomConvRec AS nombreconvenio,
       CUIT AS CUIT,
       CLI.NOMBRECLIENTE AS nombrecliente,
       conv.Id_ConvPadre
FROM CONV_CONVENIOS_REC conv WITH (nolock)
 
INNER JOIN CLI_CLIENTES AS CLI WITH (nolock) ON
          CLI.CODIGOCLIENTE = conv.Cliente
WHERE Id_ConvRec = $P{NRO_CONVENIO}]]>
	</queryString>
