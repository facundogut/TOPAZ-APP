	<parameter name="FechaReporte" class="java.util.Date"/>
	<queryString>
		<![CDATA[DECLARE @FechaCorte  AS SmallDatetime  =$P{FechaReporte}
DECLARE @FechaInicio AS SmallDatetime =  CONVERT( SMALLDATETIME, ('01' +'/'+ convert(nvarchar,MONTH(@FechaCorte)) +'/' + CONVERT(NVARCHAR, YEAR(@FechaCorte))))

DECLARE @FechaIni    AS SmallDatetime =  CONVERT(VarChar(12), DATEADD(yy,DATEDIFF(yy,0,getdate()), 0), 103)

SELECT reporte.UsrGteSucursal
       ,reporte.Id_Region
       ,reporte.Region
       ,reporte.CodSucursal
       ,reporte.Sucursal
       ,reporte.ID_Persona
       ,reporte.Cedula
       ,reporte.CodigoCargo
       ,reporte.Cargo
       ,reporte.Gerente
       ,reporte.Salario_Basico
       ,reporte.Monto_Incentivo
       ,reporte.Salario_Incentivo_Base
       ,reporte.Porc_SalBasico
       ,reporte.Porc_Incentivo
       ,reporte.Metas_Colocaciones
       ,reporte.Colocaciones
       ,reporte.Porc_Cumplimiento_Colocaciones
       ,reporte.Porc_Peso_Colocaciones
       ,reporte.Meta_Cartera_Activa
       ,reporte.Saldo_Cartera
       ,reporte.Porc_Cumplimiento_Cartera_Activa
       ,reporte.Porc_Peso_CarteraActiva
       ,reporte.Meta_Clientes_Activos
       ,reporte.Clientes_Activos
       ,reporte.Porc_Cumplimiento_Clientes_Activos
       ,reporte.Meta_Estructura_Cartera
       ,reporte.Estructura_Cartera
       ,reporte.Porc_Cumplimiento_Estructura_Cartera
       ,reporte.Porc_Peso_ClientesActivos
       ,reporte.Porc_Peso_Estructura_Cartera
       ,reporte.CER1
       ,reporte.Mora
       ,reporte.Porc_Mora_Deducible
       ,reporte.Porc_Neto_Colocaciones
       ,reporte.Porc_Neto_Cartera_Activa
       ,reporte.Porc_Cond_Cump_Colocaciones
       ,reporte.Porc_Neto_Total
       ,reporte.Porc_Neto_Estructura_Cartera
       ,reporte.Porc_Neto_Clientes_Activos
       ,reporte.Porc_Cond_Cump_Cartera_Activa
       ,reporte.Porc_Cond_Cump_Clientes_Activos
       ,reporte.Condicion_Colocaciones
       ,reporte.Condicion_Cartera_Activa
       ,reporte.Porc_Cond_Cump_Estr_Cartera
       ,reporte.Condicion_Clientes_Activos
       ,reporte.Condicion_Estructura_Cartera
       ,reporte.Inc_Colocaciones
       ,reporte.Inc_Cartera_Activa
       ,reporte.Inc_Clientes_Activos
       ,reporte.Inc_Estructura_Cartera
       ,'Total_Incentivos' = (reporte.Inc_Colocaciones + reporte.Inc_Cartera_Activa + reporte.Inc_Clientes_Activos + reporte.Inc_Estructura_Cartera)
       ,'Castigo'          = (reporte.Inc_Colocaciones + reporte.Inc_Cartera_Activa + reporte.Inc_Clientes_Activos + reporte.Inc_Estructura_Cartera) * reporte.Porc_Mora_Deducible
       ,'Incentivo_Pago'   = (reporte.Inc_Colocaciones + reporte.Inc_Cartera_Activa + reporte.Inc_Clientes_Activos + reporte.Inc_Estructura_Cartera) - ((reporte.Inc_Colocaciones + reporte.Inc_Cartera_Activa + reporte.Inc_Clientes_Activos + reporte.Inc_Estructura_Cartera) * reporte.Porc_Mora_Deducible)

FROM (
SELECT  Final.UsrGteSucursal
       ,Final.Id_Region
       ,Final.Region
       ,Final.CodSucursal
       ,Final.Sucursal
       ,Final.ID_Persona
       ,Final.Cedula
       ,Final.CodigoCargo
       ,Final.Cargo
       ,Final.Gerente
       ,Final.Salario_Basico
       ,Final.Monto_Incentivo
       ,Final.Salario_Incentivo_Base
       ,Final.Porc_SalBasico
       ,Final.Porc_Incentivo
       ,Final.Metas_Colocaciones
       ,Final.Colocaciones
       ,Final.Porc_Cumplimiento_Colocaciones
       ,Final.Porc_Peso_Colocaciones
       ,Final.Meta_Cartera_Activa
       ,Final.Saldo_Cartera
       ,Final.Porc_Cumplimiento_Cartera_Activa
       ,Final.Porc_Peso_CarteraActiva
       ,Final.Meta_Clientes_Activos
       ,Final.Clientes_Activos
       ,Final.Porc_Cumplimiento_Clientes_Activos
       ,Final.Porc_Cumplimiento_Estructura_Cartera
       ,Final.Estructura_Cartera
       ,Final.Meta_Estructura_Cartera
       ,Final.Porc_Peso_ClientesActivos
       ,Final.Porc_Peso_Estructura_Cartera
       ,Final.CER1
       ,Final.Mora
       ,Final.Porc_Mora_Deducible
       ,Final.Porc_Neto_Colocaciones
       ,Final.Porc_Neto_Cartera_Activa
       ,Final.Porc_Neto_Clientes_Activos
       ,Final.Porc_Neto_Estructura_Cartera
       ,Final.Porc_Neto_Total

       ,Final.Porc_Cond_Cump_Colocaciones
       ,Final.Porc_Cond_Cump_Cartera_Activa
       ,Final.Porc_Cond_Cump_Clientes_Activos
       ,Final.Porc_Cond_Cump_Estr_Cartera

       ,Final.Condicion_Colocaciones
       ,Final.Condicion_Cartera_Activa
       ,Final.Condicion_Clientes_Activos
       ,Final.Condicion_Estructura_Cartera

      ,'Inc_Colocaciones'       = Case When Final.Condicion_Colocaciones       = 'CUMPLE' Then (Final.Salario_Incentivo_Base * Final.Porc_Neto_Colocaciones     * Final.Porc_Incentivo) Else 0 End
      ,'Inc_Cartera_Activa'     = Case When Final.Condicion_Cartera_Activa     = 'CUMPLE' Then (Final.Salario_Incentivo_Base * Final.Porc_Neto_Cartera_Activa   * Final.Porc_Incentivo) Else 0 End
      ,'Inc_Clientes_Activos'   = Case When Final.Condicion_Clientes_Activos   = 'CUMPLE' Then (Final.Salario_Incentivo_Base * Final.Porc_Neto_Clientes_Activos * Final.Porc_Incentivo) Else 0 End
      ,'Inc_Estructura_Cartera' = Case When Final.Condicion_Estructura_Cartera = 'CUMPLE' Then (Final.Salario_Incentivo_Base * Porc_Neto_Estructura_Cartera     * Final.Porc_Incentivo) Else 0 End


FROM (
SELECT  Total.UsrGteSucursal
       ,Total.Id_Region
       ,Total.Region
       ,Total.CodSucursal
       ,Total.Sucursal
       ,Total.ID_Persona
       ,Total.Cedula
       ,Total.CodigoCargo
       ,Total.Cargo
       ,Total.Gerente
       ,Total.Salario_Basico
       ,Total.Monto_Incentivo
       ,Total.Salario_Incentivo_Base
       ,Total.Porc_SalBasico
       ,Total.Porc_Incentivo
       ,Total.Metas_Colocaciones
       ,Total.Colocaciones
       ,Total.Porc_Cumplimiento_Colocaciones
       ,Total.Porc_Peso_Colocaciones
       ,Total.Meta_Cartera_Activa
       ,Total.Saldo_Cartera
       ,Total.Porc_Cumplimiento_Cartera_Activa
       ,Total.Porc_Peso_CarteraActiva
       ,Total.Meta_Clientes_Activos
       ,Total.Clientes_Activos
       ,Total.Porc_Cumplimiento_Clientes_Activos
       ,Total.Porc_Peso_ClientesActivos
       ,Total.Meta_Estructura_Cartera
       ,Total.Estructura_Cartera
       ,Total.Porc_Cumplimiento_Estructura_Cartera
       ,Total.Porc_Peso_Estructura_Cartera
       ,Total.CER1
       ,Total.Mora
       ,Total.Porc_Mora_Deducible
       ,'Porc_Neto_Colocaciones'        = (Total.Porc_Cumplimiento_Colocaciones       * Total.Porc_Peso_Colocaciones)
       ,'Porc_Neto_Cartera_Activa'      = (Total.Porc_Cumplimiento_Cartera_Activa     * Total.Porc_Peso_CarteraActiva)
       ,'Porc_Neto_Clientes_Activos'    = (Total.Porc_Cumplimiento_Clientes_Activos   * Total.Porc_Peso_ClientesActivos)
       ,'Porc_Neto_Estructura_Cartera'  = (Total.Porc_Cumplimiento_Estructura_Cartera * Total.Porc_Peso_Estructura_Cartera)
       ,'Porc_Neto_Total'               = (Total.Porc_Cumplimiento_Colocaciones       * Total.Porc_Peso_Colocaciones)       +
                                          (Total.Porc_Cumplimiento_Cartera_Activa     * Total.Porc_Peso_CarteraActiva)      +
                                          (Total.Porc_Cumplimiento_Clientes_Activos   * Total.Porc_Peso_ClientesActivos)    +
                                          (Total.Porc_Cumplimiento_Estructura_Cartera * Total.Porc_Peso_Estructura_Cartera)
        ,Total.Porc_Cond_Cump_Colocaciones
        ,Total.Porc_Cond_Cump_Cartera_Activa
        ,Total.Porc_Cond_Cump_Clientes_Activos
        ,Total.Porc_Cond_Cump_Estr_Cartera

        ,'Condicion_Colocaciones'       = Case When Total.Porc_Cumplimiento_Colocaciones       >= Total.Porc_Cond_Cump_Colocaciones       Then 'CUMPLE' Else 'NO CUMPLE' END
        ,'Condicion_Cartera_Activa'     = Case When Total.Porc_Cumplimiento_Cartera_Activa     >= Total.Porc_Cond_Cump_Cartera_Activa     Then 'CUMPLE' Else 'NO CUMPLE' END
        ,'Condicion_Clientes_Activos'   = Case When Total.Porc_Cumplimiento_Clientes_Activos   >= Total.Porc_Cond_Cump_Clientes_Activos   Then 'CUMPLE' Else 'NO CUMPLE' END
        ,'Condicion_Estructura_Cartera' = Case When Total.Porc_Cumplimiento_Estructura_Cartera >= Total.Porc_Cond_Cump_Estr_Cartera       Then 'CUMPLE' Else 'NO CUMPLE' End

FROM(
	/*  Porcentaje  */
SELECT  Gtes.UsuarioTopaz                AS UsrGteSucursal
       ,CarSucur.Id_Region
       ,CarSucur.Region
       ,CarSucur.Sucursal_Prestamo        AS CodSucursal
       ,CarSucur.Desc_Sucursal_Prestamo   AS Sucursal
       ,Gtes.IdPersona                    AS ID_Persona
       ,Gtes.Cedula                       AS Cedula
       ,Gtes.CodigoCargo                  AS CodigoCargo
       ,Gtes.Desccargo                    AS Cargo
       ,Gtes.Nombre                       AS Gerente
       ,'Salario_Incentivo_Base' = (Select Incentivo_Base
									  From Inc_Parametros_Cargos
									  Where Codcargo    = Gtes.CodigoCargo
										And CodRegion   = CarSucur.Id_Region
										And CodSucursal = CarSucur.Sucursal_Prestamo
										And Tz_Lock    = 0)
        ,Gtes.SalarioMensual  AS Salario_Basico
        ,'Monto_Incentivo' = (Select Incentivo_Base * Porc_Incentivo
                              From Inc_Parametros_Cargos
					          Where Codcargo    = Gtes.CodigoCargo
					            And CodRegion   = CarSucur.Id_Region
					            And CodSucursal = CarSucur.Sucursal_Prestamo
					            And Tz_Lock    = 0)
		,'Porc_SalBasico' = (Select Porc_SalBasico
								From Inc_Parametros_Cargos
								Where Codcargo   = Gtes.CodigoCargo
								And CodRegion  = CarSucur.Id_Region
								And CodSucursal = CarSucur.Sucursal_Prestamo
								And Tz_Lock    = 0)
	   ,'Porc_Incentivo' = (Select Porc_Incentivo
                              From Inc_Parametros_Cargos
					          Where Codcargo   = Gtes.CodigoCargo
					            And CodRegion  = CarSucur.Id_Region
					            And CodSucursal = CarSucur.Sucursal_Prestamo
					            And Tz_Lock    = 0)
       ,CarSucur.Metas_Colocaciones       AS Metas_Colocaciones
       ,CarSucur.Colocaciones             AS Colocaciones
       ,'Porc_Cumplimiento_Colocaciones'  = (CarSucur.Colocaciones / CarSucur.Metas_Colocaciones)
       ,'Porc_Peso_Colocaciones' =  (Select Peso
									   From dbo.Inc_Peso
									  Where Codvariable ='G'
									    And CodCargo    = GTES.CodigoCargo
									    And CodRegion   = CarSucur.Id_Region
									    And TZ_LOCK     = 0)
       ,CarSucur.Meta_Cartera_Activa      AS Meta_Cartera_Activa
       ,CarSucur.Saldo_Cartera            AS Saldo_Cartera
       ,'Porc_Cumplimiento_Cartera_Activa'      = (CarSucur.Saldo_Cartera / CarSucur.Meta_Cartera_Activa)
       ,'Porc_Peso_CarteraActiva' =  (Select Peso
									   From dbo.Inc_Peso
									  Where Codvariable ='A'
									    And CodCargo    = GTES.CodigoCargo
									    And CodRegion   = CarSucur.Id_Region
									    And TZ_LOCK     = 0)
       ,CarSucur.Meta_Clientes_Activos    AS Meta_Clientes_Activos
       ,CarSucur.Clientes_Activos         AS Clientes_Activos
       ,'Porc_Cumplimiento_Clientes_Activos'    = (CarSucur.Clientes_Activos / CarSucur.Meta_Clientes_Activos)
       ,'Porc_Peso_ClientesActivos' =  (Select Peso
									   From dbo.Inc_Peso
									  Where Codvariable ='B'
									    And CodCargo    = GTES.CodigoCargo
									    And CodRegion   = CarSucur.Id_Region
									    And TZ_LOCK     = 0)
       ,CarSucur.Meta_Estructura_Cartera  AS Meta_Estructura_Cartera
       ,CarSucur.Estructura_Cartera       AS Estructura_Cartera
       ,'Porc_Cumplimiento_Estructura_Cartera'  = (CarSucur.Estructura_Cartera / CarSucur.Meta_Estructura_Cartera)
       ,'Porc_Peso_Estructura_Cartera' =  (Select Peso
										   From dbo.Inc_Peso
										  Where Codvariable ='E'
											And CodCargo    = GTES.CodigoCargo
											And CodRegion   = CarSucur.Id_Region
											And TZ_LOCK     = 0)
       ,CarSucur.CER1                     AS CER1
       ,CarSucur.Mora                     AS Mora
       ,'Porc_Mora_Deducible' = (Select Incentivo
								  From [dbo].[INC_RANGOS] ir
								 Where ir.CodVariable = 'F'
								   And ir.CodRegion   = 0
								   And ir.CodCargo    = GTES.CodigoCargo
								   And ir.Tz_Lock     = 0
								   And ((CarSucur.CER1 /CarSucur.Saldo_Cartera)* 100.00) >= ir.MINIMO
								   And ((CarSucur.CER1/CarSucur.Saldo_Cartera) * 100.00  <= ir.MAXIMO))

	   ,'Porc_Cond_Cump_Colocaciones'  = (Select Porc_Cumplimiento
										   From Inc_Condicion_Cumplimiento ic
										  Where CodVariable = 'G'
											And CodRegion   = CarSucur.Id_Region
											And TZ_Lock     = 0)

	    ,'Porc_Cond_Cump_Cartera_Activa'  = (Select Porc_Cumplimiento
											  From Inc_Condicion_Cumplimiento ic
											 Where CodVariable = 'A'
											   And CodRegion   = CarSucur.Id_Region
											   And TZ_Lock     = 0)

		 ,'Porc_Cond_Cump_Clientes_Activos'  =    (Select Porc_Cumplimiento
													 From Inc_Condicion_Cumplimiento ic
													Where CodVariable = 'B'
													  And CodRegion   = CarSucur.Id_Region
													  And TZ_Lock     = 0)

		 ,'Porc_Cond_Cump_Estr_Cartera'  = (Select Porc_Cumplimiento
										     From Inc_Condicion_Cumplimiento ic
										    Where CodVariable = 'E'
											  And CodRegion   = CarSucur.Id_Region
											  And TZ_Lock     = 0)
FROM(
SELECT Detalle.Id_Region
       ,Detalle.Region
       ,Detalle.Sucursal_Prestamo
       ,Detalle.Desc_Sucursal_Prestamo
       ,'Metas_Colocaciones' = (Select Sum(Metas)
                                From [dbo].[INC_METASVARIABLES] im
                               Where im.CodSucursal = Detalle.Sucursal_Prestamo
                                 And im.CodVariable = 'G'
                                 And CodRegion      = 0
                                 And im.Mes         = MONTH(@FechaCorte)
                                 And im.Anio        = YEAR(@FechaCorte))
       /* CONSULTA A LA TABLA SALDOS */
       ,'Colocaciones' = (SELECT SUM(CapitalOriginal) AS TOTAL_DESEMBOLSADO
                          FROM  (SELECT ts.C6020 AS AGENCIA
                                        ,s.C1601 As CapitalOriginal
                                   FROM dbo.SALDOS s WITH(NOLOCK)
                                        INNER JOIN dbo.TC_SUCURSALES ts WITH(NOLOCK) ON s.SUCURSAL = ts.C6021
                                        INNER JOIN dbo.RH_FUNCIONARIO rf WITH(NOLOCK) ON rf.CODFUNCIONARIO = s.C1670
                                     WHERE s.SUCURSAL = Detalle.SUCURSAL_PRESTAMO
                                     AND s.C1620 BETWEEN @FechaIni AND @FechaCorte
									 AND s.C1620   > '30/09/2014'
									 AND s.C9314 = 5
									 AND s.TZ_LOCK = 0
                                     --Filtro que excluye los créditos extornado ()
                                     AND Not EXISTS(SELECT sre.SALDOSJTSOIDEXT FROM SL_RASTROS_EXTORNOS  sre WITH(NOLOCK) WHERE sre.SALDOSJTSOIDEXT = S.JTS_OID)
                                     )As Consulta
                                     Group By Agencia)
	    ,'Meta_Cartera_Activa' = (Select Sum(Metas)
                                    From [dbo].[INC_METASVARIABLES] im
	                               Where im.CodSucursal = Detalle.Sucursal_Prestamo
									 And im.CodVariable = 'A'
									 And im.CodRegion   = 0
									 And im.Mes         = MONTH(@FechaCorte)
									 And im.Anio        = YEAR(@FechaCorte))
       ,Sum(Detalle.Saldo_Cartera)  AS Saldo_Cartera
       ,'Meta_Clientes_Activos' = (Select Sum(Metas)
                                     From [dbo].[INC_METASVARIABLES] im
                                    Where im.CodSucursal = Detalle.Sucursal_Prestamo
                                      And im.CodVariable = 'B'
                                      And im.CodRegion   = 0
                                      And im.Mes         = MONTH(@FechaCorte)
                                      And im.Anio        = YEAR(@FechaCorte))
       ,Count(Distinct CodCliente)  AS Clientes_Activos
       ,'Meta_Estructura_Cartera' = (Select Sum(Metas)
                                       From [dbo].[INC_METASVARIABLES] im
                                      Where im.CodSucursal = Detalle.Sucursal_Prestamo
										And im.CodVariable In ('E')
										And CODREGION = 0
										And im.Mes    = MONTH(@FechaCorte)
										And im.Anio   = YEAR(@FechaCorte))
       ,100.00                      AS Estructura_Cartera
       ,Sum(CER1) As CER1
       ,Mora = (Sum(CER1)/Sum(Detalle.Saldo_Cartera)) * 100

FROM (
SELECT trs.CodRegion          AS Id_Region
       ,Region                 AS Region
       ,egp.Sucursal_Prestamo
       ,egp.Desc_Sucursal_Prestamo
       ,egp.Codigo_Cliente     AS CodCliente
       ,'Saldo_Cartera'      = Saldo_MO
       ,Case When egp.Dias_Atraso > 0 Then egp.Saldo_MO Else 0 End As 'CER1'
  FROM [dbo].[FN_EGP_FUNDESER_UNIVERSO] (@FechaCorte) egp
       INNER JOIN TC_REL_REGION_SUCURSAL trs   ON trs.CodSucursal      = egp.SUCURSAL_PRESTAMO
       INNER JOIN TC_REGIONES tr               ON tr.CodRegion         = trs.CodRegion
WHERE Not Exists(Select sre.SALDOSJTSOIDEXT From SL_Rastros_Extornos sre WITH(NOLOCK) Where sre.SaldosJTSOIDEXT = egp.JTS_OID)
)detalle
GROUP BY  Detalle.Id_Region
         ,Detalle.Region
         ,Detalle.Sucursal_Prestamo
         ,Desc_Sucursal_Prestamo
  )CarSucur
INNER JOIN
(
SELECT GTES_NOMINA.IdPersona
       ,GTES_NOMINA.Cedula
       ,GTES_NOMINA.CodigoCargo
       ,GTES_NOMINA.Desccargo
       ,GTES_NOMINA.Nombre
       ,GTES_NOMINA.SalarioMensual
       ,rf.CodAgencia
       ,ar.UsuarioTopaz
  FROM
 (--Lista de Gerentes segun NOMINA.SA
 Select cr.IdPersona
     ,(REPLACE(e.Cedula,'-' ,'')) Cedula
     ,e.CodCargo    AS CodigoCargo
     ,c.Descripcion AS Desccargo
     ,e.Nombre
     ,e.SalarioMensual
  From CL_RELPERDOC cr inner Join VINC_NOMINA e On (REPLACE(e.Cedula,'-' ,'')) collate Modern_Spanish_CI_AS IN (ISNULL(cr.NRODOCUMENTO,''))
       Inner Join VINC_NOMINA_CARGOS c  On e.CodCargo = c.CodCargo
        and c.CodCargo IN (23,36,117)
 )GTES_NOMINA
/* EL RELACION PARA OBTENER LA SUCURSAL DEL GERENTE */
left Join RH_FUNCIONARIO       rf ON rf.IdPersona      = GTES_NOMINA.IdPersona
left Join AU_RELFUNCIONARIOUSR ar ON rf.CODFUNCIONARIO = ar.CODFUNCIONARIO
WHERE rf.IDPERSONA IS NOT NULL
  AND rf.FUNCESTADO = 'A'
  AND rf.TZ_LOCK    =  0
)GTES ON CarSucur.SUCURSAL_PRESTAMO =GTES.CODAGENCIA
   ) Total
      ) Final
)Reporte
--WHERE Reporte.UsrGteSucursal =@Usuario
ORDER BY Reporte.Id_Region, Reporte.Sucursal]]>
