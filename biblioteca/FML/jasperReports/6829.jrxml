	<parameter name="MES" class="java.lang.String"/>
	<parameter name="ANIO" class="java.lang.String"/>
	<parameter name="JTS_CUENTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT hv.CONCEPTO AS EXPEDIENTE, cli.NOMBRECLIENTE, hv.MONTO AS MONTOTOTAL, hv.FECHA_VALOR AS FECHA,
hv.FECHA_PROCESADO AS FECHAPAGO,
CASE WHEN CODIGO_TRANSACCION = 3021 THEN 40 ELSE 701 END AS CODIGO,
CASE WHEN CODIGO_TRANSACCION = 3021 THEN 'DEBEXCE' ELSE 'RECFDOS' END AS CONCEPTO
FROM HISTORIA_VISTA hv
JOIN SALDOS s ON hv.SALDO_JTS_OID = s.JTS_OID AND s.TZ_LOCK = 0
JOIN ASIENTOS a ON hv.ASIENTO = a.ASIENTO AND hv.FECHA_PROCESADO = a.FECHAPROCESO AND hv.SUCURSAL = a.SUCURSAL AND a.ESTADO = 77
JOIN BS_HISTORIA_PLAZO hp ON hv.ASIENTO = hp.NROASIENTOMOV AND hv.FECHA_PROCESADO = hp.FECHAPROCESOMOV AND hv.SUCURSAL = hp.SUCURSALMOV
JOIN SALDOS sp ON hp.SALDOS_JTS_OID = sp.JTS_OID AND sp.TZ_LOCK = 0
JOIN CLI_CLIENTES cli ON sp.C1803 = cli.CODIGOCLIENTE AND cli.TZ_LOCK = 0
WHERE CODIGO_TRANSACCION IN (3021,3024) AND
FORMAT(hv.FECHA_VALOR,'MM') = RIGHT('00' + Ltrim(Rtrim($P{MES})),2) AND FORMAT(hv.FECHA_VALOR,'yyyy') = $P{ANIO} AND
hv.SALDO_JTS_OID = $P{JTS_CUENTA}
ORDER BY hv.JTS_OID]]>
	</queryString>
	<field name="EXPEDIENTE" class="java.lang.String"/>
	<field name="NOMBRECLIENTE" class="java.lang.String"/>
	<field name="MONTOTOTAL" class="java.math.BigDecimal"/>
	<field name="FECHA" class="java.sql.Timestamp"/>
	<field name="FECHAPAGO" class="java.sql.Timestamp"/>
	<field name="CODIGO" class="java.lang.Integer"/>
	<field name="CONCEPTO" class="java.lang.String"/>
	<variable name="MONTOTOTAL_1" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{CODIGO} == 701 ? $F{MONTOTOTAL} : 0.0]]></variableExpression>
	</variable>
