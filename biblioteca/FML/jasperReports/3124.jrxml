	<queryString>
		<![CDATA[WITH 
x AS (SELECT n FROM (VALUES (1),(2),(3),(4),(5),(6),(7),(8),(9)) v(n))
,ULTIMODIA AS (SELECT dbo.ULTIMODIAHABIL($P{FECHA}) AS DIA)
,ANTERIOR AS (
	SELECT
		dbo.diaHabil(DATEADD(dd,(-1 * MIN(ones.n)),U.DIA),-1) AS DIA
	FROM x ones, ULTIMODIA U
	WHERE
		(((DATEPART(DW,dbo.diaHabil(DATEADD(dd,(-1 * (ones.n)),U.DIA),-1)) + @@DATEFIRST + 6 - 7 ) % 7) + 1) NOT IN (1,7)
	GROUP BY DIA
)
,SALDOS_BLOQUEADOS AS (
	SELECT 
		A.DIA,
		S.JTS_OID,
		IIF(B.COD_BLOQUEO IS NULL, IIF(BL.CODBLOQUEO IS NULL, 1, 0), 0) AS MULTIPLICADOR_BLOQUEO
	FROM 
		ANTERIOR AS A WITH (nolock)
	INNER JOIN SALDOS AS S WITH (nolock) ON
		S.TZ_LOCK = 0
	INNER JOIN GRL_SALDOS_DIARIOS AS GN WITH (nolock) ON
		GN.SALDOS_JTS_OID = S.JTS_OID
		AND GN.TZ_LOCK = 0
		AND GN.TIPO_SALDO_FONDO = 2
		AND GN.FECHA = (	SELECT MAX(FECHA) FROM GRL_SALDOS_DIARIOS WITH (nolock) 
						WHERE SALDOS_JTS_OID = GN.SALDOS_JTS_OID AND FECHA <= A.DIA AND TZ_LOCK = 0)
	LEFT JOIN GRL_BLOQUEOS AS B WITH (nolock) ON
		B.SALDO_JTS_OID = S.JTS_OID
		AND (B.ESTADO = 1
			 OR (B.ESTADO = 2 AND B.FECHA_MODIFICACION > A.DIA)
		)
		AND ((B.MANEJA_VENCIMIENTO = 1 
				AND B.FECHA_VENCIMIENTO IS NOT NULL 
				AND B.FECHA_VENCIMIENTO >= A.DIA
			 )
			  OR B.MANEJA_VENCIMIENTO = 0
		)
		AND B.FECHA_VIGENCIA <= A.DIA
		AND B.TZ_LOCK = 0
	LEFT JOIN GRL_COD_BLOQUEOS AS CB WITH (nolock) ON
		CB.COD_BLOQUEO = B.COD_BLOQUEO
		AND CB.ACCIONES_DEBITO IN (2, 3)
		AND CB.TZ_LOCK = 0
	LEFT JOIN CLI_BLOQUEOS AS BL WITH (nolock) ON
		BL.NROCLIENTE = S.C1803
		AND BL.FECHABLOQUEO <= A.DIA
		AND BL.CODBLOQUEO = 'B'
		AND BL.TZ_LOCK = 0
		AND NOT EXISTS (SELECT 1 
						FROM CLI_BLOQUEOS
						WHERE	
							NROCLIENTE = BL.NROCLIENTE
							AND (
								(FECHABLOQUEO > BL.FECHABLOQUEO)
								OR (FECHABLOQUEO = BL.FECHABLOQUEO AND HORABLOQUEO > BL.HORABLOQUEO)
							)
							AND FECHABLOQUEO <= A.DIA
							AND CODBLOQUEO = ''
							AND TZ_LOCK = 0
						)
)
SELECT 
	U.DIA AS ULTIMO, 
	CONVERT(DATETIME,A.DIA,103) AS PENULTIMO,
	S.MONEDA AS COD_MONEDA,
	M.C6400 AS NOM_MONEDA,
	CASE
	    WHEN P.C6252=2 THEN 'CC'
	    WHEN P.C6252=3 THEN 'AC'
	END AS PRODUCTO,
	P.C6251 AS NOM_PRODUCTO,
	S.SUCURSAL,
	SC.NOMBRESUCURSAL AS NOM_SUCURSAL,
	S.CUENTA,
	C.NOMBRECLIENTE AS NOM_CLIENTE,
	CAST((SELECT TOP 1 PORCENTAJE 
			FROM VTA_PARAM_FUCO WITH (nolock) 
			WHERE FECHA_DESDE < CONVERT(DATETIME,A.DIA,103)
			ORDER BY FECHA_DESDE DESC
			) AS NUMERIC(15,2)) AS PORCENTAJE,
	CAST((D1.SALDO_AJUSTADO) AS NUMERIC(15,2)) * (-1) AS TOTAL_RUBRO,
	CAST((((SELECT TOP 1 PORCENTAJE 
			FROM VTA_PARAM_FUCO WITH (nolock) 
			WHERE FECHA_DESDE < CONVERT(DATETIME,A.DIA,103)
			ORDER BY FECHA_DESDE DESC
			) * D2.SALDO_AJUSTADO) / 100 * MULTIPLICADOR_BLOQUEO) AS NUMERIC(15,2)) AS SALDOS,
	V.NOMBRE_CUENTA,
	CAST(D2.SALDO_AJUSTADO * MULTIPLICADOR_BLOQUEO AS NUMERIC(15,2)) AS SALDO_ACTUAL	
FROM 
	ULTIMODIA AS U
CROSS JOIN ANTERIOR AS A
CROSS JOIN SALDOS AS S WITH (nolock)
INNER JOIN GRL_SALDOS_DIARIOS AS GN WITH (nolock) ON
	GN.SALDOS_JTS_OID = S.JTS_OID
	AND GN.TZ_LOCK = 0
	AND (  (GN.TIPO_SALDO_FONDO = 1
			AND GN.FECHA = (	SELECT MAX(FECHA) FROM GRL_SALDOS_DIARIOS WITH (nolock) 
					WHERE SALDOS_JTS_OID = GN.SALDOS_JTS_OID AND FECHA <= U.DIA AND TZ_LOCK = 0)
		)
		OR (GN.TIPO_SALDO_FONDO = 2
			AND GN.FECHA = (	SELECT MAX(FECHA) FROM GRL_SALDOS_DIARIOS WITH (nolock) 
					WHERE SALDOS_JTS_OID = GN.SALDOS_JTS_OID AND FECHA <= A.DIA AND TZ_LOCK = 0)
		)
	)
LEFT JOIN MONEDAS AS M WITH (nolock) ON 
	S.MONEDA = M.C6399 
	AND M.TZ_LOCK = 0
LEFT JOIN PRODUCTOS AS P WITH (nolock) ON 
	S.PRODUCTO = P.C6250 
	AND P.TZ_LOCK = 0
LEFT JOIN SUCURSALES AS SC WITH (nolock) ON 
	S.SUCURSAL = SC.SUCURSAL 
	AND SC.TZ_LOCK = 0
LEFT JOIN CLI_CLIENTES AS C WITH (nolock) ON 
	S.C1803 = C.CODIGOCLIENTE 
	AND C.TZ_LOCK = 0
LEFT JOIN VTA_SALDOS AS V WITH (nolock) ON 
	S.JTS_OID = V.JTS_OID_SALDO 
	AND V.TZ_LOCK = 0
LEFT JOIN GRL_SALDOS_DIARIOS AS D1 WITH (nolock) ON 
	S.JTS_OID = D1.SALDOS_JTS_OID 
	AND S.TIPO_SALDO_FONDO = 1
LEFT JOIN GRL_SALDOS_DIARIOS AS D2 WITH (nolock) ON 
	S.JTS_OID = D2.SALDOS_JTS_OID
	AND D2.TIPO_SALDO_FONDO = 2
LEFT JOIN SALDOS_BLOQUEADOS AS B WITH (nolock) ON
	B.JTS_OID = S.JTS_OID
WHERE
	S.TZ_LOCK = 0
	AND (D1.FECHA = (	SELECT MAX(FECHA) FROM GRL_SALDOS_DIARIOS WITH (nolock) 
					WHERE SALDOS_JTS_OID = GN.SALDOS_JTS_OID AND FECHA <= U.DIA AND TZ_LOCK = 0)
		OR D2.FECHA = (	SELECT MAX(FECHA) FROM GRL_SALDOS_DIARIOS WITH (nolock) 
					WHERE SALDOS_JTS_OID = GN.SALDOS_JTS_OID AND FECHA <= A.DIA AND TZ_LOCK = 0)
	)]]>
	</queryString>
