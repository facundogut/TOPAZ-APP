	<queryString>
		<![CDATA[SELECT  
	FECHA_DEL_SALDO, 
	FECHA, 
	RUBROCONTABLE,
	LEFT(DESCRIPCION_CUENTA,15) AS DESCRIPCION_CUENTA,
	CASE WHEN ELEMENTO_PATRIMONIAL='Activo' THEN
		'ACT'
	WHEN ELEMENTO_PATRIMONIAL='Pasivo' THEN
		'PAS'
	WHEN ELEMENTO_PATRIMONIAL='Patrimonio' THEN
		'PAT'
	WHEN ELEMENTO_PATRIMONIAL='Contingencia Deudoras' THEN
		'CD'
	WHEN ELEMENTO_PATRIMONIAL='Contingencia Acreedoras' THEN
		'CA'
	WHEN ELEMENTO_PATRIMONIAL='Cuentas de Orden Deudoras' THEN
		'COD'
	WHEN ELEMENTO_PATRIMONIAL='Cuentas de Orden Acreedoras' THEN
		'COA'
	WHEN ELEMENTO_PATRIMONIAL='Perdidas' THEN
		'PER'
	ELSE 
		'GAN'
	END AS	ELEMENTO_PATRIMONIAL,
	CASE WHEN MONETARIO='No Monetaria' THEN
		'NM'
		ELSE
			'M'
	END AS MONETARIO,
	CASE 
		WHEN MONETARIO='No Monetaria' THEN
			(TASA)
		ELSE
			(TASA -1)
	END AS TASA,
	CASE WHEN MONETARIO='No Monetaria' THEN 
		SALDO_AJUSTADO 
	ELSE 
		0
	END AS "Saldo ajustado",
	AJUSTE_NM,
	COMPROBACION_MONETARIA,
	SALDO
FROM CO_REPO_HIS_AJUSTE_X_INFLACION
WHERE FECHA =  $P{FECHA_AJUSTE}
ORDER BY RUBROCONTABLE]]>
	</queryString>
