	<queryString>
		<![CDATA[SELECT 1 AS ORDINAL, 'Personas Jurídicas con estado no autorizada o no activa.' AS DESCRIPCION, COUNT(PJ.NUMEROPERSONAJURIDICA) AS CANTIDAD FROM CLI_PERSONASJURIDICAS AS PJ WHERE PJ.TZ_LOCK != 0 OR PJ.ESTADO = 1  

UNION ALL

SELECT 2 AS ORDINAL, 'Personas Jurídicas autorizadas o activas.' AS DESCRIPCION, COUNT(NUMEROPERSONAJURIDICA) AS CANTIDAD FROM CLI_PERSONASJURIDICAS WHERE ESTADO = 0 AND TZ_LOCK = 0 

UNION ALL

SELECT 3 AS ORDINAL, 'Personas Jurídicas con estado inhabilitado por el BCRA.' AS DESCRIPCION, COUNT(NUMEROPERSONAJURIDICA) AS CANTIDAD FROM CLI_PERSONASJURIDICAS WHERE ESTADO = 1 AND MOTIVO_INHABILITADO = 1 

UNION ALL

SELECT 4 AS ORDINAL, 'Personas Jurídicas con estado inhabilitado UIF.' AS DESCRIPCION, COUNT(NUMEROPERSONAJURIDICA) AS CANTIDAD FROM CLI_PERSONASJURIDICAS WHERE ESTADO = 1 AND MOTIVO_INHABILITADO = 3

UNION ALL

SELECT 5 AS ORDINAL, 'Personas Jurídicas con estado inhabilitado terrorismo.' AS DESCRIPCION, COUNT(NUMEROPERSONAJURIDICA) AS CANTIDAD FROM CLI_PERSONASJURIDICAS WHERE ESTADO = 1 AND MOTIVO_INHABILITADO = 4 

UNION ALL

SELECT 6 AS ORDINAL, 'Personas Jurídicas con estado inhabilitación administrativa.' AS DESCRIPCION, COUNT(NUMEROPERSONAJURIDICA) AS CANTIDAD FROM CLI_PERSONASJURIDICAS WHERE ESTADO = 1 AND MOTIVO_INHABILITADO = 5

ORDER BY ORDINAL]]>
	</queryString>
