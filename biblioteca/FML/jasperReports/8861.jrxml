		<defaultValueExpression><![CDATA[java.lang.System.getProperty("Topaz.reportesJR")]]></defaultValueExpression>
	</parameter>
	<parameter name="TOPAZ_FECHA_SISTEMA" class="java.util.Date" isForPrompting="false"/>
	<parameter name="TOPAZ_SUCURSAL_SISTEMA" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT 
S.JTS_OID,
P.NUMEROPERSONA,
PF.NUMEROPERSONAFISICA,
PJ.NUMEROPERSONAJURIDICA,
A.NROPERSONA,
A.ID_SOLICITANTE,
I.NUMEROPERSONAFISICA AS INTEGRANTE
FROM
SALDOS AS S
INNER JOIN CLI_CLIENTES AS C ON S.C1803 = C.CODIGOCLIENTE AND C.TZ_LOCK = 0
INNER JOIN CLI_CLIENTEPERSONA AS P ON C.CODIGOCLIENTE = P.CODIGOCLIENTE AND P.TZ_LOCK = 0 AND P.TITULARIDAD = 'T'
LEFT JOIN CLI_PERSONASFISICAS AS PF ON P.NUMEROPERSONA = PF.NUMEROPERSONAFISICA AND PF.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASJURIDICAS AS PJ ON P.NUMEROPERSONA = PJ.NUMEROPERSONAJURIDICA AND PJ.TZ_LOCK = 0
LEFT JOIN SOLICAPERTCTAVISTA AS A ON S.SUCURSAL = A.SUCURSAL AND S.PRODUCTO = A.PRODUCTO
AND S.CUENTA = A.CUENTA AND S.MONEDA = A.MONEDA AND S.TZ_LOCK = 0
LEFT JOIN CLI_INTEGRANTESPJ AS I ON P.NUMEROPERSONA = I.NUMEROPERSONAJURIDICA AND I.TZ_LOCK = 0 
WHERE S.TZ_LOCK = 0 AND S.JTS_OID = $P{JTS_OID}
GROUP BY S.JTS_OID, P.NUMEROPERSONA, PF.NUMEROPERSONAFISICA, PJ.NUMEROPERSONAJURIDICA, A.NROPERSONA, A.ID_SOLICITANTE, I.NUMEROPERSONAFISICA]]>
	</queryString>
