	<variable name="NroPersona" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{INTEGRANTE} != null ? $F{INTEGRANTE} :
$F{NUMEROPERSONAJURIDICA} == null && $F{NUMEROPERSONAFISICA} != null && $F{ID_SOLICITANTE} != null && !$F{NROPERSONA}.equals($F{ID_SOLICITANTE}) ? $F{ID_SOLICITANTE} :
$F{NUMEROPERSONAJURIDICA} == null && $F{NUMEROPERSONAFISICA} != null && $F{NROPERSONA}.equals($F{ID_SOLICITANTE}) ? $F{NROPERSONA} : null]]></variableExpression>
	</variable>
	<variable name="Caracter_Invocado" class="java.lang.String">
