		<queryString>
			<![CDATA[SELECT c.C1000 AS Nombre, ' ,'+td.DESCRIPCION+' No. '+d.NRODOCUMENTO AS cedula  FROM CL_CLIENTES c ,CL_RELPERDOC  d , SL_SOLICITUDCREDITO s , CL_TIPOSDOCUMENTO td WHERE C0902=s.C5187 AND d.IDPERSONA=C0902 AND d.Principal='S' AND  td.TIPODOC=d.TIPODOC AND s.C5000=$P{ID_SOLICITUD} AND (s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000) AND(c.tz_lock < 300000000000000 OR c.tz_lock >= 400000000000000) AND (c.tz_lock < 100000000000000 OR c.tz_lock >= 200000000000000) AND (d.tz_lock < 300000000000000 OR d.tz_lock >= 400000000000000) AND (d.tz_lock < 100000000000000 OR d.tz_lock >= 200000000000000)]]>
		</queryString>
		<field name="Nombre" class="java.lang.String"/>
		<field name="cedula" class="java.lang.String"/>
	</subDataset>
