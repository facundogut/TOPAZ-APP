				</textElement>
				<text><![CDATA[5.4.13. Recomendaciones para el uso de cajeros automáticos.]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="435" width="555" height="40" uuid="acf8bde6-23e5-41da-b60b-020ec80037b5">
					<property name="com.jaspersoft.studio.unit.height" value="pixel"/>
