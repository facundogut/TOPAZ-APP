	<subDataset name="Cuotas">
		<parameter name="JTS_OID" class="java.lang.Double"/>
		<queryString>
			<![CDATA[SELECT count(*) AS CuotasVenc FROM BS_PLANPAGOS
		WHERE SALDO_JTS_OID=$P{JTS_OID} AND C2302<(SELECT fechaproceso FROM parametros) AND NOT (C2309=0 AND C2310=0 AND C2311=0)]]>
		</queryString>
		<field name="CuotasVenc" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="APagar">
		<parameter name="JTS_OID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT sum(j2.Apagar+isnull(hd.MORA_Devengada,0)) AS Apagar
FROM
   (SELECT j.JTS AS JTS, sum(j.SalCap+j.salInt+j.gast)AS APagar
    FROM (SELECT pp.SALDO_JTS_OID AS jts ,pp.C2309 AS SalCap , pp.C2310 AS SAlInt ,isnull(gpc.IMPORTE_GASTO,0) AS gast
          FROM BS_PLANPAGOS pp LEFT JOIN GASTOS_POR_CUOTA gpc  ON pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
          WHERE SALDO_JTS_OID=$P{JTS_OID} AND C2302<(SELECT fechaproceso FROM parametros)
          AND NOT (C2309=0 AND C2310=0 AND C2311=0)
         ) AS j
   GROUP BY j.JTS
   ) AS j2 LEFT JOIN HISTORICO_DEVENGAMIENTO hd ON j2.JTS=hd.SALDO_JTS_OID AND hd.FECHA=(SELECT FechaProceso FROM Parametros)]]>
		</queryString>
		<field name="Apagar" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="MontoPrincipal">
		<parameter name="JTS_OID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT sum(C2309) AS MontoPrincipal FROM BS_PLANPAGOS
WHERE saldo_JTS_OID=$P{JTS_OID} AND C2302<(SELECT fechaproceso FROM parametros) AND NOT (C2309=0 AND C2310=0 AND C2311=0)]]>
		</queryString>
		<field name="MontoPrincipal" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="TotalMontoPrincipal">
		<parameter name="Mon" class="java.math.BigDecimal"/>
		<parameter name="Suc" class="java.math.BigDecimal"/>
		<parameter name="CodFunc" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT sum(C2309) AS MontoPrincTotal FROM BS_PLANPAGOS pp
WHERE pp.C2302<(SELECT fechaproceso FROM parametros) AND NOT (pp.C2309=0 AND pp.C2310=0 AND pp.C2311=0) AND EXISTS (SELECT * FROM SALDOS s WHERE s.JTS_OID=pp.saldo_jts_oid AND s.ESTADOATRASO<>'C'   AND s.C1728<>'C' AND S.SUCURSAL=$P{Suc} AND s.MONEDA=$P{Mon} and s.C1670=$P{CodFunc})]]>
		</queryString>
		<field name="MontoPrincTotal" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="FechaUltPago">
		<parameter name="JTS_OID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT Max(FechaValor) as FECHA FROM BS_HISTORIA_PLAZO WHERE saldos_jts_oid=$P{JTS_OID} AND TIPOMOV='P']]>
		</queryString>
		<field name="FECHA" class="java.sql.Timestamp"/>
	</subDataset>
	<subDataset name="NombreMoneda">
		<parameter name="Moneda" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT C6400,C6401 FROM CO_MONEDAS where C6399=$P{Moneda}]]>
		</queryString>
		<field name="C6400" class="java.lang.String"/>
		<field name="C6401" class="java.lang.String"/>
	</subDataset>
	<subDataset name="TotalSC">
		<parameter name="Mon" class="java.math.BigDecimal"/>
		<parameter name="Suc" class="java.math.BigDecimal"/>
		<parameter name="CodFunc" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT -sum(s.C1604) as Tot FROM SALDOS s join CL_CLIENTES c on c.C0902=s.C1803
WHERE s.C9314=5 AND s.C1628<(SELECT fechaproceso FROM parametros) AND s.C1604<0 AND s.ESTADOATRASO<>'C' AND s.C1728<>'C' AND s.moneda=$P{Mon} AND s.SUCURSAL=$P{Suc} and s.C1670=$P{CodFunc}
 and (s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000)
	 and (c.tz_lock < 300000000000000 OR c.tz_lock >= 400000000000000) AND (c.tz_lock < 100000000000000 OR c.tz_lock >= 200000000000000)]]>
		</queryString>
		<field name="Tot" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="TelCli">
		<parameter name="NROCLIENTE" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT Tel1 FROM TC_DIRECCIONES WHERE FORMATO='P' AND TIPODIRECCION='R' AND ID=$P{NROCLIENTE}]]>
		</queryString>
		<field name="Tel1" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Obtern Num Cred">
		<parameter name="JTS" class="java.math.BigDecimal"/>
		<parameter name="NROCLIENTE" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT dbo.Obtener_Nro_Credito($P{NROCLIENTE},$P{JTS}) as NroCred]]>
		</queryString>
		<field name="NroCred" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="TOPAZ_FECHA_SISTEMA" class="java.util.Date" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT  c.C1000 AS NomCli,
        s.CUENTA AS NoCredito,
s.sucursal as numcuc,
        C1601 AS MontoDesemblosado,
        C1604 AS SaldoCredito,s.JTS_OID ,
             f.Nombre AS Funcionario,
       F.codfuncionario as codFunc,
       SUC.descripcion AS sucursal,
       c.c0902 AS NumCli,
       (CASE p.C6259 WHEN 'F' THEN s.C1642
       			   WHEN 'H' THEN s.C1642
       			   ELSE null
       	   End) AS periodicidad,
       DATEDIFF ( dd , s.C1628, (SELECT fechaproceso FROM parametros) ) AS DiasAtraso,
       s.moneda as Moneda
FROM (SALDOS s JOIN CL_CLIENTES c ON c.c0902=s.C1803), RH_FUNCIONARIO f, SUCURSALESSC suc, CO_PRODUCTOS p
WHERE s.C9314=5 AND s.C1628<(SELECT fechaproceso FROM parametros) AND s.C1604<0  AND s.ESTADOATRASO<>'C' AND s.C1728<>'C'
	 AND  F.codfuncionario=s.C1670
	 AND s.SUCURSAL=SUC.sucursal
	 and (s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000)
	 and (c.tz_lock < 300000000000000 OR c.tz_lock >= 400000000000000) AND (c.tz_lock < 100000000000000 OR c.tz_lock >= 200000000000000)
	 AND p.C6250=s.PRODUCTO
ORDER BY SUC.descripcion, s.moneda,F.codfuncionario

]]>
	</queryString>
	<field name="NomCli" class="java.lang.String"/>
	<field name="NoCredito" class="java.math.BigDecimal"/>
	<field name="numcuc" class="java.math.BigDecimal"/>
	<field name="MontoDesemblosado" class="java.math.BigDecimal"/>
	<field name="SaldoCredito" class="java.math.BigDecimal"/>
	<field name="JTS_OID" class="java.math.BigDecimal"/>
	<field name="Funcionario" class="java.lang.String"/>
	<field name="codFunc" class="java.math.BigDecimal"/>
	<field name="sucursal" class="java.lang.String"/>
	<field name="NumCli" class="java.lang.Long"/>
	<field name="periodicidad" class="java.math.BigDecimal"/>
	<field name="DiasAtraso" class="java.lang.Integer"/>
	<field name="Moneda" class="java.math.BigDecimal"/>
