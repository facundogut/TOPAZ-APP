	<subDataset name="Fecha">
		<queryString>
			<![CDATA[SELECT
	(SELECT lower(dbo.NumeroALetrasEntero(DATEPART(dd,(SELECT FECHAPROCESO FROM PARAMETROS))))) AS DIA,
	(SELECT CASE (DATEPART(mm,(SELECT FECHAPROCESO FROM PARAMETROS)))
		WHEN 1 THEN 'Enero'
		WHEN 2 THEN 'Febrero'
		WHEN 1 THEN 'Marzo'
		WHEN 2 THEN 'Abril'
		WHEN 1 THEN 'Mayo'
		WHEN 2 THEN 'Junio'
		WHEN 1 THEN 'Julio'
		WHEN 2 THEN 'Agosto'
		WHEN 1 THEN 'Septiembre'
		WHEN 2 THEN 'Octubre'
		WHEN 1 THEN 'Noviembre'
		WHEN 2 THEN 'Diciembre'
		end
	) AS MES,
	(SELECT lower(dbo.NumeroALetrasEntero(DATEPART(yy,(SELECT FECHAPROCESO FROM PARAMETROS))))) AS ANIO]]>
		</queryString>
		<field name="DIA" class="java.lang.String"/>
		<field name="MES" class="java.lang.String"/>
		<field name="ANIO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Integrantes del grupo">
		<parameter name="codigo" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT ('  ' + t.DESCRIPCION) documento,('  ' + C1406+' '+c1407+' '+c1404+' '+c1405)nombre,' ' as firma FROM CL_PERSONASFISICAS p, CL_RELPERDOC d,CL_TIPOSDOCUMENTO t WHERE p.IDPERSONA IN (SELECT C3199 FROM CL_RELGSCLTE where C3198=$P{codigo} )
AND d.IDPERSONA = p.IDPERSONA AND d.PRINCIPAL = 'S' AND t.TIPODOC = d.TIPODOC]]>
		</queryString>
		<field name="documento" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
		<field name="firma" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataFundeser">
		<parameter name="sucursal" class="java.lang.Integer"/>
		<parameter name="codigo" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT (gerente_suc ) NOMBRE1 FROM PARAM_CONTRATOS_POR_SUC_2 where sucursal = $P{sucursal} and codigo =$P{codigo}]]>
		</queryString>
		<field name="NOMBRE1" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataDireccion">
		<parameter name="suc" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT dd.C1216 dir FROM TC_DIRECCIONES d , TC_DIR_DIM3 dd WHERE d.ID=$P{suc} and d.FORMATO='S' AND d.TIPODIRECCION ='R' AND dd.C1212 = d.DIM1 AND dd.C1214= d.DIM2 AND dd.C1215 = d.DIM3]]>
		</queryString>
		<field name="dir" class="java.lang.String"/>
	</subDataset>
