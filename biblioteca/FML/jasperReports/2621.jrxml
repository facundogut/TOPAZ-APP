	<queryString>
		<![CDATA[
SELECT
cli.NOMBRECLIENTE, e.NUMEROPERSONA,
ISNULL(dfj_cuit.NUMERODOCUMENTO,'') AS CUIT,
ISNULL(dfj_cuil.NUMERODOCUMENTO,'') AS CUIL,
ISNULL(dfj_dni.NUMERODOCUMENTO,'') AS DNI,
CONVERT(VARCHAR(10),a.FECHA_COBRANZA,103) AS FECHA_COBRANZA,cli.NOMBRECLIENTE AS NOMBRE_EMPRESA, c.NomConvRec,
d.CUENTA, prod.C6251 AS TIPO_CUENTA, suc.NOMBRESUCURSAL, suc.SUCURSAL, CONCAT(SUBSTRING(a.PERIODO,5,6),'/',SUBSTRING(a.PERIODO,1,4)) AS PERIODO,
a.IMPORTE, loc.DESCRIPCION_DIM3 AS LOCALIDAD, dire.CALLE, (CASE  WHEN dire.APARTAMENTO IS NULL THEN ''ELSE CONCAT(' apto ',dire.APARTAMENTO) END) as APTO,
(CASE WHEN dire.PISO IS NULL THEN '' ELSE CONCAT(' piso ', dire.PISO) END) AS PISO, ISNULL(CAST(dire.NUMERO AS VARCHAR),'') AS NUMERO


FROM REC_DET_DEBITOSAUTOMATICOS a WITH (NOLOCK)
JOIN REC_CAB_DEBITOSAUTOMATICOS b WITH (NOLOCK) ON a.ID_CABEZAL = b.ID
JOIN CONV_CONVENIOS_REC c WITH (NOLOCK) ON c.Id_ConvRec = b.CONVENIO
/* DATOS CLIENTE */
JOIN SALDOS d WITH (NOLOCK) ON a.JTS_DEBITO = d.JTS_OID AND d.TZ_LOCK = 0
JOIN PRODUCTOS prod WITH (NOLOCK) ON d.PRODUCTO = prod.C6250 AND prod.TZ_LOCK = 0
JOIN SUCURSALES suc WITH (NOLOCK) ON d.SUCURSAL = suc.SUCURSAL AND suc.TZ_LOCK = 0
JOIN CLI_ClientePersona e WITH (NOLOCK) ON e.CODIGOCLIENTE = d.C1803 AND  e.TITULARIDAD = 'T' AND e.TZ_LOCK = 0
JOIN CLI_CLIENTES cli WITH (NOLOCK) ON cli.CODIGOCLIENTE = d.C1803 AND cli.TZ_LOCK = 0
LEFT JOIN CLI_DocumentosPFPJ dfj_cuit WITH (NOLOCK) ON dfj_cuit.NUMEROPERSONAFJ = e.NUMEROPERSONA AND dfj_cuit.TIPODOCUMENTO = 'CUIT' AND dfj_cuit.TZ_LOCK = 0
LEFT JOIN CLI_DocumentosPFPJ dfj_cuil WITH (NOLOCK) ON dfj_cuil.NUMEROPERSONAFJ = e.NUMEROPERSONA AND dfj_cuil.TIPODOCUMENTO = 'CUIL' AND dfj_cuil.TZ_LOCK = 0
LEFT JOIN CLI_DocumentosPFPJ dfj_dni WITH (NOLOCK) ON dfj_dni.NUMEROPERSONAFJ = e.NUMEROPERSONA AND dfj_dni.TIPODOCUMENTO = 'DNI' AND dfj_dni.TZ_LOCK = 0
LEFT JOIN CLI_DIRECCIONES dire WITH (NOLOCK) ON dire.ID = $P{ID_DIR} AND dire.FORMATO =$P{FORMATO_DIR} AND dire.TIPODIRECCION = $P{TIPO_DIR} AND ORDINAL_DIR = $P{ORDINAL_DIR} AND dire.TZ_LOCK = 0
LEFT JOIN CLI_LOCALIDADES loc WITH (NOLOCK) ON loc.CODIGOPAIS = $P{CODIGOPAIS} AND loc.DIM1 = $P{DIM1} AND loc.DIM2 = $P{DIM2} AND loc.DIM3 = $P{DIM3} AND loc.CODIGO_POSTAL =$P{CODIGO_POSTAL} AND loc.TZ_LOCK = 0
/* DATOS EMPRESA */
LEFT JOIN CLI_CLIENTES j WITH (NOLOCK) ON c.Cliente = j.CODIGOCLIENTE AND j.TZ_LOCK = 0
WHERE a.ID_CABEZAL = $P{ID_CABEZAL} AND a.ID_LINEA = $P{ID_LINEA}]]>
