	<parameter name="idPersona" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT t.RowNum
, t.numero
, isnull( convert(varchar(50),t.IDPERSONA),'') AS IDPERSONA
, isnull(t.CARGO,'') AS CARGO
, isnull( t.PROFESION,'') AS PROFESION
, isnull( convert(NVARCHAR(50),t.ANTIGUEDAD),'') AS ANTIGUEDAD
, isnull( t.NOMBRENEGOCIO,'') AS NOMBRENEGOCIO
, t.PROMVTAS
, isnull( t.TEL1,'') AS TEL1
, isnull( t.EMAIL,'') AS EMAIL
, isnull( t.WEB,'') AS WEB
, isnull( t.DIRECCION ,'') AS DIRECCION
,ISNULL(act.C1266,'') AS DES_ACTIVIDAD_ECON
 FROM(
select ROW_NUMBER() OVER (ORDER BY idpersona) AS RowNum
,*
 from  (SELECT $P{idPersona}
 AS numero) b

LEFT JOIN vlc_ActividadEconomicaPN v on v.idpersona =b.numero) AS t
LEFT JOIN CL_NEGOCIOS cn ON t.IDPERSONA =cn.IDPERSONA
LEFT JOIN TC_ACIVIDADESCIIU act ON act.C1265 =cn.CODACTINTERNO
 WHERE rownum=1]]>
