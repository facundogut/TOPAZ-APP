	<parameter name="NRO_DOCUMENTO" class="java.lang.String"/>
	<parameter name="TIPO_DOCUMENTO" class="java.lang.String"/>
	<parameter name="CODIGO_CLIENTE" class="java.math.BigDecimal"/>
	<parameter name="TIPO_PERSONA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT  D.TIPODOCUMENTO AS TIPODOC,
        D.NUMERODOCUMENTO AS NUMERODOC,
        D.NUMEROPERSONAFJ AS NUMEROPERSONA,
        D.TIPOPERSONA,
        ISNULL(PF.PRIMERNOMBRE, '') + ' ' + ISNULL(PF.APELLIDOPATERNO, '') + ' ' + ISNULL(PF.APELLIDOMATERNO, '') AS NOMBRE,
		(SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 1506 AND IDIOMA = 'E' AND OPCIONINTERNA = CP.TITULARIDAD) AS TITULARIDAD,
        C.CODIGOCLIENTE,
        C.NOMBRECLIENTE,
        (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 33366 AND IDIOMA = 'E' AND OPCIONINTERNA = PF.ESTADO) AS ESTADO,
        'Física' AS TIPOPER,
        (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 1040 AND IDIOMA = 'E' AND OPCIONINTERNA = C.TIPO) AS TIPOCLI
FROM    CLI_CLIENTES AS C,
        CLI_CLIENTEPERSONA AS CP,
        CLI_PERSONASFISICAS AS PF,
        CLI_DocumentosPFPJ AS D,
        OPCIONES AS O
WHERE         (   (
            $P{CODIGO_CLIENTE} != 0
            AND
            C.CODIGOCLIENTE = $P{CODIGO_CLIENTE}
          )
          OR
          (
            $P{TIPO_DOCUMENTO} != ''
            AND
            D.TIPODOCUMENTO = $P{TIPO_DOCUMENTO}
            AND
            D.NUMERODOCUMENTO = $P{NRO_DOCUMENTO}
          )
          OR
          (
            $P{CODIGO_CLIENTE} = 0
            AND
            $P{TIPO_DOCUMENTO} = ''
          )
        )
        AND
        (C.CODIGOCLIENTE = CP.CODIGOCLIENTE)
        AND
        (
          PF.NUMEROPERSONAFISICA = CP.NUMEROPERSONA
          AND
          PF.NUMEROPERSONAFISICA = D.NUMEROPERSONAFJ
        )
        AND
        (C.TZ_LOCK = 0)
        AND
        (CP.TZ_LOCK = 0)
        AND
        (PF.TZ_LOCK = 0)
        AND
        (D.TZ_LOCK = 0)
        AND
        (O.NUMERODECAMPO = 33366 AND O.OPCIONINTERNA = PF.ESTADO)
-------------------------------------------------------------------
UNION ALL
-------------------------------------------------------------------
SELECT  D.TIPODOCUMENTO AS TIPODOC,
        D.NUMERODOCUMENTO AS NUMERODOC,
        D.NUMEROPERSONAFJ AS NUMEROPERSONA,
        D.TIPOPERSONA,
        PJ.RAZONSOCIAL AS NOMBRE,
       (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 1506 AND IDIOMA = 'E' AND OPCIONINTERNA = CP.TITULARIDAD) AS TITULARIDAD,
        C.CODIGOCLIENTE,
        C.NOMBRECLIENTE,
        (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 33367 AND IDIOMA = 'E' AND OPCIONINTERNA = PJ.ESTADO) AS ESTADO,
        'Jurídica' AS TIPOPER,
        (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 1040 AND IDIOMA = 'E' AND OPCIONINTERNA = C.TIPO) AS TIPOCLI
FROM    CLI_CLIENTES AS C,
        CLI_CLIENTEPERSONA AS CP,
        CLI_PERSONASJURIDICAS AS PJ,
        CLI_DocumentosPFPJ AS D,
        OPCIONES AS O
WHERE   (
          (
            $P{CODIGO_CLIENTE} != 0
            AND
            C.CODIGOCLIENTE = $P{CODIGO_CLIENTE}
          )
          OR
          (
            $P{TIPO_DOCUMENTO} != ''
            AND
            D.TIPODOCUMENTO = $P{TIPO_DOCUMENTO}
            AND
            D.NUMERODOCUMENTO = $P{NRO_DOCUMENTO}
          )
          OR
          (
            $P{CODIGO_CLIENTE} = 0
            AND
            $P{TIPO_DOCUMENTO} = ''
          )
        )
        AND
        (C.CODIGOCLIENTE = CP.CODIGOCLIENTE)
        AND
        (
          PJ.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA
          AND
          PJ.NUMEROPERSONAJURIDICA = D.NUMEROPERSONAFJ
        )
        AND
        (C.TZ_LOCK = 0)
        AND
        (CP.TZ_LOCK = 0)
        AND
        (PJ.TZ_LOCK = 0)
        AND
        (D.TZ_LOCK = 0)
        AND
        (O.NUMERODECAMPO = 33367 AND O.OPCIONINTERNA = PJ.ESTADO)
ORDER BY  C.CODIGOCLIENTE ASC]]>
	</queryString>
