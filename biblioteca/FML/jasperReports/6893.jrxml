	<queryString>
		<![CDATA[SELECT e.nro_cliente, doc.NUMERODOC, cl.NOMBRECLIENTE, e.emisor_certificado, isnull(e.fecha_alta, 01/01/1800) as fecha_alta , cl.CATEGORIARESULTANTE, isnull(d.DEUDA, 0) AS DEUDA, a.id, opc.descripcion AS motivo, a.decreto_provincial, a.detalle, a.fecha_vig_desde, a.fecha_vig_hasta, a.decreto_nacional, convert(varchar(20),$P{FECHA_DESDE}, 103) as fecha_desde_parm, convert(varchar(20),$P{FECHA_HASTA}, 103) as fecha_hasta_parm, convert(varchar(10),$P{ID_EMERGENCIA}) as id_emergenc_parm
FROM CRE_CERTIFICADOS_EMERGENCIA e WITH(NOLOCK)
INNER JOIN CRE_EMERGENCIA_AGRO a WITH(NOLOCK) ON e.id_emergencia=a.id AND a.TZ_LOCK=0
INNER JOIN CLI_CLIENTES cl WITH(NOLOCK) ON cl.CODIGOCLIENTE= e.nro_cliente AND cl.TZ_LOCK=0
INNER JOIN VW_CLI_X_DOC doc WITH(NOLOCK) ON cl.CODIGOCLIENTE=doc.CODIGOCLIENTE
INNER JOIN OPCIONES opc WITH(NOLOCK) ON opc.numerodecampo = 43448 AND a.motivo = opc.opcioninterna
LEFT JOIN (SELECT CLIENTE,  sum(SALDO_DEUDA) AS DEUDA FROM VW_DEUDA_CLIENTES GROUP BY CLIENTE) d ON d.CLIENTE=cl.CODIGOCLIENTE
WHERE e.TZ_LOCK=0 AND ($P{FECHA_DESDE} = '18000101' OR e.fecha_alta >= $P{FECHA_DESDE}) AND ($P{FECHA_HASTA} = '18000101' OR e.fecha_alta <= $P{FECHA_HASTA}) AND ($P{ID_EMERGENCIA} = -1 OR a.id = $P{ID_EMERGENCIA})
GROUP BY a.id, e.nro_cliente, doc.NUMERODOC, cl.NOMBRECLIENTE, e.emisor_certificado, e.fecha_alta, cl.CATEGORIARESULTANTE, d.DEUDA, a.motivo, a.decreto_provincial, a.detalle, a.fecha_vig_desde, a.fecha_vig_hasta, a.decreto_nacional, opc.descripcion
ORDER BY a.id,d.DEUDA DESC]]>
	</queryString>
