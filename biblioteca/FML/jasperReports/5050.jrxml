	<subDataset name="Cuentas">
		<parameter name="Cte" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT
     idcliente,
     inst_financiera,
     tipo_cta,
     moneda,
     saldo_prom
     ,isnull(cm.C6400,'') AS DES_MONEDA
FROM
     (SELECT $P{Cte}
      AS numero) b LEFT JOIN "CL_PERFIL_INF_PN_CTAS" CL_PERFIL_INF_PN_CTAS on idcliente =b.numero
      LEFT JOIN CO_MONEDAS cm ON cm.C6399 =moneda]]>
		</queryString>
		<field name="idcliente" class="java.lang.Long"/>
		<field name="inst_financiera" class="java.lang.String"/>
		<field name="tipo_cta" class="java.lang.String"/>
		<field name="moneda" class="java.math.BigDecimal"/>
		<field name="saldo_prom" class="java.math.BigDecimal"/>
		<field name="DES_MONEDA" class="java.lang.String"/>
	</subDataset>
	<parameter name="IdPersona" class="java.math.BigDecimal"/>
