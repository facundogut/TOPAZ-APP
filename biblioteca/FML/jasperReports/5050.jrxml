	<parameter name="Sucursal" class="java.lang.String"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[java.lang.System.getProperty("Topaz.reportesJR")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[DECLARE @DesSucursal VARCHAR(100)
SET @DesSucursal=''
IF (ISNUMERIC ($P{Sucursal})=1)
BEGIN
	SET @DesSucursal = (SELECT isnull(TS.C6020,'')  FROM TC_SUCURSALES ts WHERE TS.C6021=$P{Sucursal} )

END
select PF.*
,isnull(TS.C6020,@DesSucursal) AS C6020
, (SELECT p.FECHAPROCESO FROM PARAMETROS p )  AS FECHAPROCESO
 from cl_personasfisicas PF
LEFT JOIN CL_CLIENTES cc ON PF.IDPERSONA=CC.C0902
LEFT JOIN USUARIOS u ON CC.UsuarioAlta =U.INICIALES
LEFT JOIN TC_SUCURSALES ts ON U.NROSUCURSAL =TS.C6021
 where idpersona =$P{IdPersona}]]>
	</queryString>
