	<subDataset name="DATADETALLE">
		<parameter name="NUMEROCUOTA" class="java.lang.String"/>
		<parameter name="JTSOID" class="java.math.BigDecimal"/>
		<parameter name="HP_JTS_OID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
(SELECT NROCUOTA FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS NCUOTA,
(SELECT CONVERT(VARCHAR(10),FECHAVTO,103) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS FVENC,
(SELECT CAPITAL FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS CAPITAL,
(SELECT INTERES FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS INTERES,
(SELECT GASTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS GASTO,
(SELECT CAPITAL+INTERES+GASTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS TOTAL,
(SELECT TOP 1 CONVERT(VARCHAR(10),FECHAVALOR,103) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID ORDER BY FECHAVALOR DESC) AS FECHAVALOR,
(SELECT TOP 1 DATEDIFF(d,a.FECHAVALOR,b.FECHAVTO) FROM VBS_PAYS_DETAIL a, V_SALDOS_PLANPAGOS b WHERE a.CUOTA=vsp.NROCUOTA AND a.TIPO='P' AND a.SALDOS_JTS_OID=p.SALDOS_JTS_OID AND b.IDJTSOID=vsp.IDJTSOID AND b.NROCUOTA=vsp.NROCUOTA AND a.HP_JTS_OID=p.HP_JTS_OID) AS DIAS,
(SELECT TOP 1 CONVERT(VARCHAR(10),FECHAPROCESOMOV,103) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND  HP_JTS_OID=p.HP_JTS_OID ORDER BY FECHAPROCESOMOV DESC) AS FECHAPROCMOV,
(SELECT TOP 1 NROASIENTOMOV FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND  HP_JTS_OID=p.HP_JTS_OID ORDER BY NROASIENTOMOV DESC) AS ASIENTO,
(SELECT TOP 1 SUCURSALMVTO FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID ORDER BY SUCURSALMVTO DESC) AS SUC,
(SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID) AS CAPITALP,
(SELECT sum(INTERESPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND  HP_JTS_OID=p.HP_JTS_OID) AS INTERESP,
(SELECT sum(MORAPAGADA) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND  HP_JTS_OID=p.HP_JTS_OID) AS MORAP,
(SELECT CASE
	((SELECT isnull((sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID)+(SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND  HP_JTS_OID=p.HP_JTS_OID))
	WHEN 0 THEN NULL
	ELSE (SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID)
	END) AS CARGOSP,


  	(SELECT CASE
		((SELECT isnull((sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND  HP_JTS_OID=p.HP_JTS_OID)+(SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID  AND HP_JTS_OID=p.HP_JTS_OID))
		WHEN 0 THEN NULL
		ELSE  ((SELECT isnull((sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID)+(SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID AND HP_JTS_OID=p.HP_JTS_OID))
		END
	)AS TOTALP,

  	(SELECT CASE

  	WHEN vsp.CUENTASISTANT = 0 THEN
  		( SELECT CASE
  			WHEN  vsp.SDOCAPITAL<>0 then
  		   (SELECT sum(isnull((pp.C2309+pp.C2310+pp.C2312+g.IMPORTE_GASTO),0)) FROM BS_PLANPAGOS pp, GASTOS_POR_CUOTA g WHERE  g.SALDOS_JTS_OID = vsp.IDJTSOID  AND pp.SALDO_JTS_OID=vsp.IDJTSOID AND pp.C2300 = vsp.NROCUOTA AND g.NUMERO_CUOTA = vsp.NROCUOTA)
  			ELSE 0
  			END )

  	ELSE
  		(SELECT isnull((pp.C2309+pp.C2310+pp.C2312),0) FROM BS_PLANPAGOS pp WHERE pp.C2300 = vsp.NROCUOTA  AND pp.SALDO_JTS_OID=vsp.IDJTSOID)
	END
	) AS SaldoCta

FROM V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN (SELECT * FROM VBS_PAYS_DETAIL pd WHERE pd.HP_JTS_OID=$P{HP_JTS_OID}) AS p ON vsp.IDJTSOID=p.SALDOS_JTS_OID

WHERE vsp.IDJTSOID=$P{JTSOID} AND vsp.NROCUOTA=$P{NUMEROCUOTA}]]>
		</queryString>
		<field name="NCUOTA" class="java.math.BigDecimal"/>
		<field name="FVENC" class="java.lang.String"/>
		<field name="CAPITAL" class="java.math.BigDecimal"/>
		<field name="INTERES" class="java.math.BigDecimal"/>
		<field name="GASTO" class="java.math.BigDecimal"/>
		<field name="TOTAL" class="java.math.BigDecimal"/>
		<field name="FECHAVALOR" class="java.lang.String"/>
		<field name="DIAS" class="java.lang.Integer"/>
		<field name="FECHAPROCMOV" class="java.lang.String"/>
		<field name="ASIENTO" class="java.math.BigDecimal"/>
		<field name="SUC" class="java.math.BigDecimal"/>
		<field name="CAPITALP" class="java.math.BigDecimal"/>
		<field name="INTERESP" class="java.math.BigDecimal"/>
		<field name="MORAP" class="java.math.BigDecimal"/>
		<field name="CARGOSP" class="java.math.BigDecimal"/>
		<field name="TOTALP" class="java.math.BigDecimal"/>
		<field name="SaldoCta" class="java.math.BigDecimal"/>
		<group name="NCUOTA">
			<groupExpression><![CDATA[$F{NCUOTA}]]></groupExpression>
		</group>
	</subDataset>
	<subDataset name="DATAFAMILIA">
		<parameter name="CODFLIA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DESCRIPCION FROM CO_FAMILIAS
WHERE CODFAMILIA=$P{CODFLIA}]]>
		</queryString>
		<field name="DESCRIPCION" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATACIIU">
		<parameter name="NROPRESTAMO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT a.C1266 AS DESCRIPCION FROM SALDOS s, SL_SOLICITUDCREDITO c, TC_ACIVIDADESCIIU a
WHERE c.C5000=s.C1704 AND
c.CODACTCIIU=a.C1265 AND
s.CUENTA=$P{NROPRESTAMO}]]>
		</queryString>
		<field name="DESCRIPCION" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAGRUPO">
		<parameter name="NROPRESTAMO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT CASE c.GRUPO
		WHEN 0 THEN 'INDIVIDUAL'
		ELSE convert(varchar(10),c.GRUPO)
	   END AS GRUPO
FROM SALDOS s, SL_SOLICITUDCREDITO c
WHERE c.C5000=s.C1704 AND
s.CUENTA=$P{NROPRESTAMO}]]>
		</queryString>
		<field name="GRUPO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATACLIENTE">
		<parameter name="CODCLI" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C1000 AS NOM FROM CL_CLIENTES
WHERE C0902=$P{CODCLI}]]>
		</queryString>
		<field name="NOM" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATASUC">
		<parameter name="NROSUC" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6020 AS NOMSUC FROM TC_SUCURSALES s
WHERE C6021=$P{NROSUC}]]>
		</queryString>
		<field name="NOMSUC" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAANALISTA">
		<parameter name="CODANALISTA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT TOP 1 NOMBRE FROM AU_RELFUNCIONARIOUSR
WHERE CODFUNCIONARIO=$P{CODANALISTA}]]>
		</queryString>
		<field name="NOMBRE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAPROD">
		<parameter name="CODPROD" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6251 AS NOMPROD FROM CO_PRODUCTOS
WHERE C6250=$P{CODPROD}]]>
		</queryString>
		<field name="NOMPROD" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAMONEDA">
		<parameter name="CODMONEDA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6400 AS NOMMON FROM CO_MONEDAS
WHERE C6399=$P{CODMONEDA}]]>
		</queryString>
		<field name="NOMMON" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATATOTAL">
		<parameter name="JTSOID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
(SELECT sum(CAPITAL) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS CAPITALTOTAL,
(SELECT sum(INTERES) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS INTERESTOTAL,
(SELECT sum(GASTO) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS GASTOTOTAL,
(SELECT sum(CAPITAL+INTERES+GASTO) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS TOTAL,
(SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CAPITALPTOTAL,
(SELECT sum(INTERESPAGADO) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS INTERESPTOTAL,
(SELECT sum(MORAPAGADA) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS MORAPTOTAL,
(SELECT CASE
	((SELECT isnull((sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)),0) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)+(SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID))
	WHEN 0 THEN NULL
	ELSE (SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)
	END) AS CARGOSPTOTAL,
(SELECT CASE
	((SELECT isnull((sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)),0) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)+(SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID))
	WHEN 0 THEN NULL
	ELSE  ((SELECT isnull((sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)),0) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)+(SELECT isnull(sum(CAPITALPAGADO),0) FROM VBS_PAYS_DETAIL WHERE TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID))
	END) AS TOTALP
FROM V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN VBS_PAYS_DETAIL p ON vsp.IDJTSOID=p.SALDOS_JTS_OID
WHERE vsp.IDJTSOID=$P{JTSOID}]]>
		</queryString>
		<field name="CAPITALTOTAL" class="java.math.BigDecimal"/>
		<field name="INTERESTOTAL" class="java.math.BigDecimal"/>
		<field name="GASTOTOTAL" class="java.math.BigDecimal"/>
		<field name="TOTAL" class="java.math.BigDecimal"/>
		<field name="CAPITALPTOTAL" class="java.math.BigDecimal"/>
		<field name="INTERESPTOTAL" class="java.math.BigDecimal"/>
		<field name="MORAPTOTAL" class="java.math.BigDecimal"/>
		<field name="CARGOSPTOTAL" class="java.math.BigDecimal"/>
		<field name="TOTALP" class="java.math.BigDecimal"/>
	</subDataset>
