	<parameter name="TOPAZ_FECHA_SISTEMA" class="java.util.Date" isForPrompting="false"/>
	<parameter name="USUARIO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT vsp.NROPRESTAMO, s.C1604*(-1) AS SALDO,s.C1601 AS MONTO,s.C1728 AS ESTADO,vsp.NROCLIENTE,s.SUCURSAL,vsp.ANALISTA,vsp.NROPRODUCTO,s.C1642*s.C1644 AS PLAZO,
vsp.FECDESEM, vsp.NROSOLICITUD,vsp.MONEDA,vsp.TASAINT,s.C4765*s.C1642 AS PLAZOGRACIA, s.JTS_OID AS JTS, s.C1644 AS TOTALCUOTAS, vsp.NROCUOTA AS CUOTA, p.C7996 AS CODFLIA, p.C8006 AS PREMIO, p.C8007 AS PORCENTAJE, vpd.HP_JTS_OID, vpd.FECHAVALOR, vpd.NROASIENTOMOV
FROM (V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN VBS_PAYS_DETAIL vpd ON vsp.IDJTSOID=vpd.SALDOS_JTS_OID AND vsp.NROCUOTA=vpd.CUOTA), CO_PRODUCTOS p, SALDOS s WHERE
(vsp.NroProducto = p.C6250) and
vsp.TipoProd = 5 and
((p.C7996 = 2 and
(p.ProDblCro<>0 or p.C8007 = 0)) or
(p.C7996 <> 2 )) and
(s.JTS_OID = vsp.IDJTSOID) and
(s.TZ_LOCK = 0 AND s.TZ_LOCK NOT BETWEEN 300000000000000  AND  399999999999999) AND
s.CUENTA=$P{NPRESTAMO}
ORDER BY vsp.NROCUOTA, vpd.FECHAVALOR, vpd.NROASIENTOMOV]]>
	</queryString>
