	<parameter name="Cliente" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT c.CLIENTE, cli.NOMBRECLIENTE, cli.TIPODOC, cli.NUMERODOC, s.C1644 AS TOTAL_CUOTAS, s.C1645 AS CUOTAS_PAGAS,
s.C1644 - s.C1645 AS CUOTA_NOPAGAS, a.NOMBRESUCURSAL, a.NOMBREPRODUCTO, a.OPERACION, a.DESGLOSE, a.VENCIMIENTO, a.MONTOORIGEN, a.SALDO
FROM CRE_CAB_CESIONCARTERA c
INNER JOIN VW_CLI_X_DOC cli ON c.CLIENTE = cli.CODIGOCLIENTE
INNER JOIN CRE_DET_CESIONCARTERA d ON c.NUMERO_CESION = d.NUMERO_CESION AND d.TZ_LOCK = 0
INNER JOIN SALDOS s ON d.JTS_OID_ASISTENCIA = s.JTS_OID AND s.TZ_LOCK = 0
INNER JOIN VW_ASISTENCIAS a ON s.JTS_OID = a.JTS_OID
WHERE c.ESTADO = 'C' AND c.TZ_LOCK = 0 AND (c.CLIENTE = $P{Cliente} or 0 = $P{Cliente})]]>
	</queryString>
