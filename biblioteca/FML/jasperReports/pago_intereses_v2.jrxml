	<queryString>
		<![CDATA[SELECT
S.C1803 AS CLIENTE,
C.C1000 AS NOMBRE,
M.CONCEPTO,
S.CUENTA,
z.cuenta AS CUENTAVISTA,
H.INTERESPAGADO AS IMPORTE,
S.C1642 AS PERIODO,
S.PRODUCTO,
H.FECHAPROCESOMOV,
P.C6251 AS PRODUCTODES
FROM BS_HISTORIA_PLAZO H, SALDOS S, CL_CLIENTES C, CO_PRODUCTOS P, MOVIMIENTOS_CONTABLES M , SALDOS z
WHERE H.SALDOS_JTS_OID = S.JTS_OID AND s.C1665 = z.jts_oid
AND C.C0902 = S.C1803
AND P.C6250 = S.PRODUCTO
AND H.NROASIENTOMOV = M.ASIENTO
AND H.FECHAPROCESOMOV = M.FECHAPROCESO
AND S.C29314 = 4
AND P.C6259 = 'P'
AND H.TIPOMOV = 'P'
AND M.OPERACION = 8621
--AND M.TIPO = 'A'
AND m.CLIENTE <> 0 AND M.DEBITOCREDITO='C'
--AND S.JTS_OID= 606458
AND H.FECHAPROCESOMOV = TO_DATE('28/04/2011' )]]>
