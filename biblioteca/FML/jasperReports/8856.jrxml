	<parameter name="JTS_OID" class="java.math.BigDecimal">
		<parameterDescription><![CDATA[JTS OID:]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT
S.C1620 AS FECHA_APERTURA,
U.NOMBRESUCURSAL,
C.TIPO AS TIPO_CLIENTE,
C.NOMBRECLIENTE,
CONCAT(PF.PRIMERNOMBRE,' '+PF.SEGUNDONOMBRE,' '+PF.APELLIDOPATERNO,' '+PF.APELLIDOMATERNO) AS NOMBRE_PF,
CONCAT(PN.PRIMERNOMBRE,' '+PN.SEGUNDONOMBRE,' '+PN.APELLIDOPATERNO,' '+PN.APELLIDOMATERNO) AS NOMBRE_INTEGRANTE,
D.NUM_DOC_FISICO,
DN.NUM_DOC_FISICO AS NUM_DOC_FISICO_INT,
D.TIPODOCUMENTO,
D.NUMERODOCUMENTO,
CASE WHEN PF.SUJETO_UIF = 'S' THEN 'S' ELSE 'N' END AS UIF_PF,
CASE WHEN PJ.SUJETO_UIF = 'S' THEN 'S' ELSE 'N' END AS UIF_PJ
FROM SALDOS AS S
INNER JOIN CLI_CLIENTES AS C ON S.C1803 = C.CODIGOCLIENTE AND C.TZ_LOCK = 0
LEFT JOIN CLI_CLIENTEPERSONA AS CP ON S.C1803 = CP.CODIGOCLIENTE AND CP.TITULARIDAD = 'T' AND CP.TZ_LOCK = 0
LEFT JOIN CLI_DOCUMENTOSPFPJ AS D ON CP.NUMEROPERSONA = D.NUMEROPERSONAFJ AND D.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASFISICAS AS PF ON CP.NUMEROPERSONA = PF.NUMEROPERSONAFISICA AND PF.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASJURIDICAS AS PJ ON CP.NUMEROPERSONA = PJ.NUMEROPERSONAJURIDICA AND PJ.TZ_LOCK = 0
LEFT JOIN CLI_INTEGRANTESPJ AS NT ON PJ.NUMEROPERSONAJURIDICA = NT.NUMEROPERSONAJURIDICA AND NT.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASFISICAS AS PN ON NT.NUMEROPERSONAFISICA = PN.NUMEROPERSONAFISICA AND PN.TZ_LOCK = 0
LEFT JOIN CLI_DOCUMENTOSPFPJ AS DN ON PN.NUMEROPERSONAFISICA = DN.NUMEROPERSONAFJ AND DN.TZ_LOCK = 0
LEFT JOIN SUCURSALES AS U ON S.SUCURSAL = U.SUCURSAL AND U.TZ_LOCK = 0
WHERE S.TZ_LOCK = 0 
AND S.JTS_OID = $P{JTS_OID}]]>
	</queryString>
