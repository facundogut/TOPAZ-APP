	<subDataset name="Cedula del Cliente">
		<parameter name="NUMCLI" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT 'NRO. DOCUMENTO '+NroDocumento as Numdoc
FROM CL_RELPERDOC WHERE PRINCIPAL='S' AND IDPERSONA=$P{NUMCLI}]]>
		</queryString>
		<field name="Numdoc" class="java.lang.String"/>
	</subDataset>
	<subDataset name="SucCli">
		<parameter name="NUMCLIENTE" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT 'FUNDESER SUCURSAL '+s.C6020 as suc FROM CL_CLIENTES c JOIN TC_SUCURSALES s ON s.C6021=c.C8425 WHERE c.C0902=$P{NUMCLIENTE}]]>
		</queryString>
		<field name="suc" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Segmentacion Cliente">
		<parameter name="NUMCLIENTE" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT 'SEGMENTACION'+c.Segmentacion as Segmentacion FROM CL_CLIENTES c WHERE c.C0902=$P{NUMCLIENTE}]]>
		</queryString>
		<field name="Segmentacion" class="java.lang.String"/>
	</subDataset>
	<parameter name="NumCli" class="java.math.BigDecimal">
		<parameterDescription><![CDATA[Numero de cliente]]></parameterDescription>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
