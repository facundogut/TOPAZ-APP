	<subDataset name="Table Dataset 1"/>
	<subDataset name="CiudadSuc">
		<parameter name="SUCURSAL" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT d3.C1216 as Ciudad
FROM TC_DIRECCIONES d JOIN tc_dir_dim3 d3 ON d.DIM1=d3.C1212 AND d.DIM2=d3.C1214 AND d.DIM3=d3.C1215
WHERE d.FORMATO='S' AND d.TIPODIRECCION='R' AND ID=$P{SUCURSAL}]]>
		</queryString>
		<field name="Ciudad" class="java.lang.String"/>
	</subDataset>
	<parameter name="SUCURSAL" class="java.lang.Integer"/>
	<parameter name="NROSOLICITUD" class="java.math.BigDecimal"/>
	<parameter name="NROGARANTIA" class="java.math.BigDecimal"/>
	<parameter name="CODGERENTE" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
	(SELECT lower(dbo.NumeroALetrasEntero(DATEPART(dd,(SELECT FECHAPROCESO FROM PARAMETROS))))) AS DIA,
	(SELECT CASE (DATEPART(mm,(SELECT FECHAPROCESO FROM PARAMETROS)))
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 1 THEN 'Marzo'
  WHEN 2 THEN 'Abril'
  WHEN 1 THEN 'Mayo'
  WHEN 2 THEN 'Junio'
  WHEN 1 THEN 'Julio'
  WHEN 2 THEN 'Agosto'
  WHEN 1 THEN 'Septiembre'
  WHEN 2 THEN 'Octubre'
  WHEN 1 THEN 'Noviembre'
  WHEN 2 THEN 'Diciembre'
  end
 ) AS MES,
	(SELECT lower(dbo.NumeroALetrasEntero(DATEPART(yy,(SELECT FECHAPROCESO FROM PARAMETROS))))) AS ANIO,
	(SELECT t.DESCRIPCION FROM CL_RELPERDOC d,CL_TIPOSDOCUMENTO t WHERE d.principal = 'S' AND d.TIPODOC = t.TIPODOC AND d.IDPERSONA =s.C5187) AS TIPODOCDEUDOR,
	(SELECT c.C1000 FROM CL_CLIENTES c WHERE c.C0902 =s.C5187) AS NOMDEUDOR,
	(SELECT Gerente_Suc FROM PARAM_CONTRATOS_POR_SUC_2 WHERE Sucursal=$P{SUCURSAL} AND Codigo=$P{CODGERENTE}) AS NOMGERENTE
FROM SL_SOLICITUDCREDITO s, GR_GARANTIAS g
WHERE s.C5000=$P{NROSOLICITUD}*100 AND g.NROGARANTIA=$P{NROGARANTIA}*100 AND
(s.tz_lock < 300000000000000 OR s.tz_lock >= 400000000000000) AND (s.tz_lock < 100000000000000 OR s.tz_lock >= 200000000000000)]]>
	</queryString>
	<field name="DIA" class="java.lang.String"/>
	<field name="MES" class="java.lang.String"/>
	<field name="ANIO" class="java.lang.String"/>
	<field name="TIPODOCDEUDOR" class="java.lang.String"/>
	<field name="NOMDEUDOR" class="java.lang.String"/>
	<field name="NOMGERENTE" class="java.lang.String"/>
