	<queryString>
		<![CDATA[SELECT
	c.C0902 AS NRO_CLIENTE,
	c.C1000 AS NOMBRE_CLIENTE,
	c.califinternafinal as CALIFICACION_INTERNA_FINAL,
	c.CalsistFinAjust as CAL_SF_AJUSTADA,
	c.CalifAlin as CALIFICACION_ALINEADA,
	s.MONEDA,
	s.C1730 AS RUBRO_CONTABLE,
	s.criterio AS TIPO_CREDITO,
	s.Producto AS PRODUCTO,
	s.CUENTA AS NRO_PMO,
	s.C1604 AS SALDO_CAPITAL,
	s.C1609-s.c1610 AS SALDO_INTERESES,
	(CASE WHEN C1628>(SELECT FECHAPROCESO FROM PARAMETROS) THEN 0 ELSE datediff(dd,C1628,(SELECT FECHAPROCESO FROM PARAMETROS)) END) AS ATRASO,
	s.CALIF_OBJ AS CALIFICACION,
	(cast((SELECT FECHAPROCESO FROM PARAMETROS) AS char) ) as FECHA
	FROM SALDOS s, CL_CLIENTES c
	WHERE s.C1803=c.C0902
	AND s.c9314=5 AND s.C1604<0
	ORDER BY c.C0902, s.CRITERIO, s.c1730]]>
