	<parameter name="CLIENTE" class="java.math.BigDecimal">
		<parameterDescription><![CDATA[Cliente:]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT
CP.CODIGOCLIENTE,
CP.NUMEROPERSONA,
CONCAT(PF.PRIMERNOMBRE + ' ', PF.APELLIDOPATERNO) AS NOMBRE,
PJ.RAZONSOCIAL,
D.TIPODOCUMENTO,
D.NUMERODOCUMENTO,
CONCAT(DI.CALLE, ' ' + CAST(DI.NUMERO AS VARCHAR)) AS DIRECCION
FROM CLI_ClientePersona AS CP
LEFT JOIN CLI_PERSONASFISICAS AS PF ON CP.NUMEROPERSONA = PF.NUMEROPERSONAFISICA AND PF.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASJURIDICAS AS PJ ON CP.NUMEROPERSONA = PJ.NUMEROPERSONAJURIDICA AND PJ.TZ_LOCK = 0
LEFT JOIN CLI_DOCUMENTOSPFPJ AS D ON CP.NUMEROPERSONA = D.NUMEROPERSONAFJ AND D.TZ_LOCK = 0
AND D.TIPODOCUMENTO = (SELECT TOP 1 TIPODOCUMENTO FROM CLI_DocumentosPFPJ WHERE NUMEROPERSONAFJ = D.NUMEROPERSONAFJ)
LEFT JOIN CLI_DIRECCIONES AS DI ON CP.NUMEROPERSONA = DI.ID AND DI.TZ_LOCK = 0
AND DI.TIPODIRECCION = (SELECT TOP 1 TIPODIRECCION FROM CLI_DIRECCIONES WHERE ID = DI.ID)
AND DI.ORDINAL_DIR = (SELECT TOP 1 ORDINAL_DIR FROM CLI_DIRECCIONES WHERE ID = DI.ID)
WHERE CP.TZ_LOCK = 0
AND CP.CODIGOCLIENTE = $P{CLIENTE}]]>
	</queryString>
