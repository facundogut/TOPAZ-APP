	<parameter name="NUMERO_CESION" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT DISTINCT c.NUMERO_CESION, cr.DESCRIPCION AS CON_RECURSOS, tc.DESCRIPCION AS TIPO_CESION,
concat(clic.TIPODOC,' ',clic.NUMERODOC) AS DOCUMENTOCOMPRANTE, clic.NOMBRECLIENTE AS NOMBRECOMPRANTE, concat(a.SUCURSAL,' - ',a.NOMBRESUCURSAL) AS SUCURSAL,
concat(a.FAMILIA,' - ',f.DESCRIPCION) AS FAMILIA, concat(a.PRODUCTO,' - ',a.NOMBREPRODUCTO) AS PRODUCTO,
a.OPERACION, a.DESGLOSE, a.SALDO, a.VENCIMIENTO, a.PORCENTAJE_PAGO, a.TASA, gd.DESCRIPCION AS GARANTIA_DESEMBOLSO,
concat(cli.TIPODOC,' ',cli.NUMERODOC) AS DOCUMENTO, cli.NOMBRECLIENTE
FROM CRE_CAB_CESIONCARTERA c
INNER JOIN CRE_DET_CESIONCARTERA d ON c.NUMERO_CESION = d.NUMERO_CESION AND d.TZ_LOCK = 0
INNER JOIN VW_CLI_X_DOC clic ON c.CLIENTE = clic.CODIGOCLIENTE
INNER JOIN VW_ASISTENCIAS a ON d.JTS_OID_ASISTENCIA = a.JTS_OID
INNER JOIN VW_CLI_X_DOC cli ON a.CLIENTE = cli.CODIGOCLIENTE
LEFT JOIN OPCIONES gd ON a.GARANTIA_DESEMBOLSO = gd.OPCIONINTERNA AND gd.NUMERODECAMPO = 43282
LEFT JOIN OPCIONES cr ON c.CON_RECURSOS = cr.OPCIONINTERNA AND cr.NUMERODECAMPO = 43820
LEFT JOIN OPCIONES tc ON c.TIPO_CESION = tc.OPCIONINTERNA AND tc.NUMERODECAMPO = 43821
INNER JOIN CRE_FAMILIAS f ON a.FAMILIA = f.FAMILIAPROD AND f.TZ_LOCK = 0
WHERE c.TZ_LOCK = 0 and c.NUMERO_CESION = $P{NUMERO_CESION}]]>
	</queryString>
