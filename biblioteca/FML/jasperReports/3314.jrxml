	<queryString>
		<![CDATA[WITH S AS (

SELECT
S.SUCURSAL AS Sucursal,
S.CUENTA AS Nro_Cuenta_Corriente,
S.PRODUCTO AS CodigoProducto,
S.C1803 AS CodigoCliente,
dpj.TIPODOCUMENTO AS Tipo_Documento,
dpj.NUMERODOCUMENTO AS Numero_Documento,
VC.NOMBRECLIENTE AS Nombre_Cliente,
(SELECT TOP 1 CDir.CALLE FROM VW_CLI_DIRECCIONES CDir WHERE CDir.NUMEROPERSONA = CP.NUMEROPERSONA AND CDIR.CODIGOCLIENTE=S.C1803) AS Calle, 
(SELECT TOP 1 CDir.DEPARTAMENTO FROM VW_CLI_DIRECCIONES CDir WHERE CDir.NUMEROPERSONA = CP.NUMEROPERSONA AND CDIR.CODIGOCLIENTE=S.C1803) AS Puerta_Dpto,
(SELECT TOP 1 CT.NUMERO FROM VW_CLI_TELEFONOS CT WHERE CT.CODIGOCLIENTE=S.C1803 AND CT.NUMEROPERSONA=CP.NUMEROPERSONA)  AS Telefono,
(SELECT TOP 1 CDI.LOCALIDAD FROM VW_CLI_DIRECCIONES CDi WHERE CDi.NUMEROPERSONA = CP.NUMEROPERSONA AND CDI.CODIGOCLIENTE=S.C1803) AS Nombre_Localidad,
M.C6401 AS Moneda, 
S.C1604 AS Saldo,
0 AS def_order
FROM SALDOS S  
INNER JOIN MONEDAS M WITH (nolock) ON M.C6399 = S.MONEDA
INNER JOIN CLI_CLIENTEPERSONA CP WITH (nolock) ON S.C1803 = CP.CODIGOCLIENTE AND CP.TITULARIDAD = 'T'
INNER JOIN CLI_DocumentosPFPJ DPJ WITH (nolock) ON CP.NUMEROPERSONA = DPJ.NUMEROPERSONAFJ 
INNER JOIN VW_CLI_X_DOC vc WITH (nolock) ON vc.CODIGOCLIENTE=CP.CODIGOCLIENTE AND vc.NUMEROPERSONA=CP.NUMEROPERSONA
INNER JOIN VW_CLI_DIRECCIONES VCD WITH (nolock) ON VCD.CODIGOCLIENTE=S.C1803 AND VCD.NUMEROPERSONA=CP.NUMEROPERSONA AND VCD.TIPODIRECCION='PR' AND VCD.FORMATO='PF'
INNER JOIN VW_CLI_TELEFONOS VCT WITH (nolock) ON VCT.CODIGOCLIENTE=S.C1803 AND VCT.NUMEROPERSONA=CP.NUMEROPERSONA AND VCT.TIPO='PE' AND VCT.FORMATO='PF'
INNER JOIN VW_CLI_CORREOS VCC WITH (nolock) ON VCC.CODIGOCLIENTE=S.C1803 AND VCC.NUMEROPERSONA=CP.NUMEROPERSONA AND VCC.TIPO='PE' AND VCC.FORMATO='PF'
WHERE 
    S.C1785 = 2 
    AND DPJ.TIPOPERSONA='F'
     AND S.TZ_LOCK=0 
     AND CP.TZ_LOCK=0 
     AND DPJ.TZ_LOCK=0 
     AND CP.TITULARIDAD='T'

 

UNION ALL

 

SELECT 
S.SUCURSAL AS Sucursal,
S.CUENTA AS Nro_Cuenta_Corriente,
S.PRODUCTO AS CodigoProducto,
S.C1803 AS CodigoCliente,
dpj.TIPODOCUMENTO AS Tipo_Documento,
dpj.NUMERODOCUMENTO AS Numero_Documento,
VC.NOMBRECLIENTE AS Nombre_Cliente,
(SELECT TOP 1 CDir.CALLE FROM VW_CLI_DIRECCIONES CDir WHERE CDir.NUMEROPERSONA = CP.NUMEROPERSONA AND CDIR.CODIGOCLIENTE=S.C1803) AS Calle, 
(SELECT TOP 1 CDir.DEPARTAMENTO FROM VW_CLI_DIRECCIONES CDir WHERE CDir.NUMEROPERSONA = CP.NUMEROPERSONA AND CDIR.CODIGOCLIENTE=S.C1803) AS Puerta_Dpto,
(SELECT TOP 1 CT.NUMERO FROM VW_CLI_TELEFONOS CT WHERE CT.CODIGOCLIENTE=S.C1803 AND CT.NUMEROPERSONA=CP.NUMEROPERSONA)  AS Telefono,
(SELECT TOP 1 CDI.LOCALIDAD FROM VW_CLI_DIRECCIONES CDi WHERE CDi.NUMEROPERSONA = CP.NUMEROPERSONA AND CDI.CODIGOCLIENTE=S.C1803) AS Nombre_Localidad,
M.C6401 AS Moneda, 
S.C1604 AS Saldo,
0 AS def_order
FROM SALDOS S  
INNER JOIN MONEDAS M WITH (nolock) ON M.C6399 = S.MONEDA
INNER JOIN CLI_CLIENTEPERSONA CP WITH (nolock) ON S.C1803 = CP.CODIGOCLIENTE AND CP.TITULARIDAD = 'T'
INNER JOIN CLI_DocumentosPFPJ DPJ WITH (nolock) ON CP.NUMEROPERSONA = DPJ.NUMEROPERSONAFJ 
INNER JOIN VW_CLI_X_DOC vc WITH (nolock) ON vc.CODIGOCLIENTE=CP.CODIGOCLIENTE AND vc.NUMEROPERSONA=CP.NUMEROPERSONA 
INNER JOIN VW_CLI_DIRECCIONES VCD WITH (nolock) ON VCD.CODIGOCLIENTE=S.C1803 AND VCD.NUMEROPERSONA=CP.NUMEROPERSONA AND VCD.TIPODIRECCION='L' AND VCD.FORMATO='PJ'
INNER JOIN VW_CLI_TELEFONOS VCT WITH (nolock) ON VCT.CODIGOCLIENTE=S.C1803 AND VCT.NUMEROPERSONA=CP.NUMEROPERSONA AND VCT.TIPO='LE' AND VCT.FORMATO='PJ'
INNER JOIN VW_CLI_CORREOS VCC WITH (nolock) ON VCC.CODIGOCLIENTE=S.C1803 AND VCC.NUMEROPERSONA=CP.NUMEROPERSONA AND VCC.TIPO='LE' AND VCC.FORMATO='PJ'
WHERE 
    S.C1785 = 2 
    AND DPJ.TIPOPERSONA='J'
    AND S.TZ_LOCK=0
    AND CP.TZ_LOCK=0 
    AND DPJ.TZ_LOCK=0 
    AND CP.TITULARIDAD='T'

 

GROUP BY S.SUCURSAL, S.CUENTA, S.PRODUCTO,S.C1803, DPJ.TIPODOCUMENTO, DPJ.NUMERODOCUMENTO, VC.NOMBRECLIENTE, VCD.CALLE,VCD.NUMERO, VCT.NUMERO, VCD.LOCALIDAD, M.C6401, S.C1604, CP.NUMEROPERSONA, DPJ.TIPOPERSONA

)

SELECT * FROM s
ORDER BY 
    CASE WHEN upper($P{ORDEN}) = 'N' THEN Nro_Cuenta_Corriente
         ELSE def_order 
    END,
    CASE WHEN upper($P{ORDEN}) <> 'N' THEN Nombre_Cliente
         ELSE CONVERT(VARCHAR, def_order)
    END,
    Sucursal, 
    CodigoProducto, 
    Moneda, 
    Saldo]]>
	</queryString>
