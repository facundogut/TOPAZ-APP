	<queryString>
		<![CDATA[SELECT concat(pp.NUMERO_CUOTA,' - ',convert(varchar,pp.FECHA_FIN,103)) AS COLUMNA1,
       FORMAT(pp.CAPITAL,'#,0.00') AS COLUMNA2,
       FORMAT(pp.INTERES,'#,0.00') AS COLUMNA3,
       (SELECT FORMAT(isnull(sum(Importe_gasto),0),'#,0.00')
        FROM GASTOS_POR_CUOTA h WITH(NOLOCK)
        WHERE h.SALDOS_JTS_OID = pp.SALDO_JTS_OID
          AND NUMERO_CUOTA=pp.NUMERO_CUOTA AND h.TZ_LOCK = 0)  AS COLUMNA4,
         FORMAT(pp.CAPITAL+pp.INTERES+(SELECT isnull(sum(Importe_gasto),0)
        FROM GASTOS_POR_CUOTA h WITH(NOLOCK)
        WHERE h.SALDOS_JTS_OID = pp.SALDO_JTS_OID
          AND NUMERO_CUOTA=pp.NUMERO_CUOTA AND h.TZ_LOCK = 0),'#,0.00') AS COLUMNA5,
        sc.C1704 AS N_SOLICITUD
 FROM PLANPAGOS_ORIGINAL pp WITH(NOLOCK)
 INNER JOIN SALDOS sc WITH(NOLOCK) ON pp.SALDO_JTS_OID = sc.JTS_OID
 WHERE pp.SALDO_JTS_OID= $P{JTS_OID}]]>
	</queryString>
