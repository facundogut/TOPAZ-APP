	<queryString>
		<![CDATA[SELECT 
P.NUMEROPERSONAFISICA,
P.PRIMERNOMBRE, 
P.SEGUNDONOMBRE, 
P.APELLIDOPATERNO,
P.APELLIDOMATERNO,
D.TIPO_DOC_FISICO, 
D.NUM_DOC_FISICO, 
D.TIPODOCUMENTO, 
D.NUMERODOCUMENTO,  
P.FECHANACIMIENTO, 
P.NACIONALIDAD,
PA.NOMBREPAIS,
L.DESCRIPCION_DIM3 AS LOCALIDAD_SUC
FROM CLI_PERSONASFISICAS AS P
INNER JOIN CLI_DOCUMENTOSPFPJ AS D ON P.NUMEROPERSONAFISICA = D.NUMEROPERSONAFJ AND D.TZ_LOCK = 0
LEFT JOIN CLI_PAISES AS PA ON D.COD_PAIS_ORIGEN = PA.CODIGOPAIS AND PA.TZ_LOCK = 0
LEFT JOIN CLI_DIRECCIONES AS DI ON $P{TOPAZ_SUCURSAL_SISTEMA} = DI.ID  AND DI.FORMATO = 'S' AND DI.TIPODIRECCION = 'R' AND DI.TZ_LOCK = 0
LEFT JOIN CLI_LOCALIDADES AS L ON DI.PAIS = L.CODIGOPAIS AND DI.PROVINCIA = L.DIM1 AND DI.DEPARTAMENTO = L.DIM2
AND DI.LOCALIDAD = L.DIM3 AND L.TZ_LOCK = 0
WHERE P.TZ_LOCK = 0 AND P.NUMEROPERSONAFISICA=$P{NUMPER}]]>
	</queryString>
