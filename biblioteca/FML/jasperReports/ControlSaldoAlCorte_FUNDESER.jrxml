	<variable name="DIFSUCURSAL" class="java.lang.Double">
		<variableExpression><![CDATA[$V{SUMAFINSUCURSAL} - $V{SUMAINISUCURSAL} + $V{SUMAALDIASUCURSAL}]]></variableExpression>
	</variable>
	<variable name="DIFMONEDA" class="java.lang.Double">
		<variableExpression><![CDATA[$V{SUMAFINMONEDA} - $V{SUMAINIMONEDA} + $V{SUMAALDIAMONEDA}]]></variableExpression>
	</variable>
	<variable name="DIFRUBRO" class="java.lang.Double">
		<variableExpression><![CDATA[$V{SUMAFINRUBRO} - $V{SUMAINIRUBRO} + $V{SUMADIARUBRO}]]></variableExpression>
	</variable>
	<variable name="DIFREGISTRO" class="java.lang.Double">
