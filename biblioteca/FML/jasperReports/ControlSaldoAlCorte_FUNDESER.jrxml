	<queryString>
		<![CDATA[SELECT SUCURSAL, MONEDA,CUENTA, OPERACION, ORDINAL, cliente, PRODUCTO, RUBROCONTABLE, SALDO_ANTERIOR,SALDO_ACTUAL,DEBITOS,CREDITOS
FROM (
                    SELECT SUCURSAL, MONEDA,CUENTA, OPERACION, ORDINAL, CLIENTE, PRODUCTO, RUBROCONTABLE,
                       SUM (SALDO_ANTERIOR) AS SALDO_ANTERIOR,
                       SUM (SALDO_ACTUAL) AS SALDO_ACTUAL,
                       SUM (DEBITOS) AS DEBITOS,
                       SUM (CREDITOS) AS CREDITOS
                           FROM
                       (
                       SELECT  S.SUCURSAL, S.MONEDA, S.CUENTA,
                               S.OPERACION, S.PRODUCTO, S.ORDINAL, s.c1803 as cliente, SD.RUBROCONTABLE AS RUBROCONTABLE,
                               SALDO_AL_CORTE AS SALDO_ANTERIOR,
                                  0 AS SALDO_ACTUAL,
                                  0 As DEBITOS,
                                  0 as CREDITOS
                       FROM  SALDOS S, SALDOS_DIARIOS SD
                       WHERE   S.JTS_OID = SD.SALDO_JTS_OID AND
                                  SD.FECHA = $P{FECHA_PROCESO}
                       UNION ALL
                       SELECT  M.SUCURSAL_CUENTA AS SUCURSAL, M.MONEDA, M.CUENTA, M.OPERACION_CUENTA AS OPERACION, M.PRODUCTO,M.ORDINAL_CUENTA AS ORDINAL, m.cliente,  M.RUBROCONTABLE,
                                               0 AS SALDO_ANTERIOR,
                                               0 AS SALDO_ACTUAL,
                                               (CASE
                           WHEN M.DEBITOCREDITO = 'D' THEN
                             M.CAPITALREALIZADO
                           ELSE
                             0
                           END) AS DEBITO,
                           (CASE
                           WHEN M.DEBITOCREDITO = 'C' THEN
                             M.CAPITALREALIZADO
                           ELSE
                             0
                           END) AS CREDITO
                       FROM MOVIMIENTOS_CONTABLES M, ASIENTOS A
                       WHERE            M.FECHAPROCESO = A.FECHAPROCESO AND
                                               M.SUCURSAL = A.SUCURSAL AND
                                               M.ASIENTO = A.ASIENTO AND
                                               A.ESTADO = 77 AND
                       M.FECHAPROCESO = $P{FECHA_PROCESO}
                       UNION ALL
                       SELECT S.SUCURSAL, S.MONEDA, S.CUENTA, S.OPERACION, S.PRODUCTO, S.ORDINAL, s.c1803 as cliente, SD.RUBROCONTABLE  AS RUBROCONTABLE,
                               0   AS SALDO_ANTERIOR,
                                  SALDO_AL_CORTE AS SALDO_ACTUAL,
                                  0 As DEBITOS, 0 as CREDITOS
                       FROM SALDOS S, SALDOS_DIARIOS SD
                       WHERE   S.JTS_OID = SD.SALDO_JTS_OID AND
                                  SD.FECHA = $P{FECHA_PROCESO}
                        ) T
                    GROUP BY RUBROCONTABLE, MONEDA, SUCURSAL, CUENTA, OPERACION, ORDINAL, cliente, PRODUCTO) T
                     WHERE  SALDO_ANTERIOR + CREDITOS - DEBITOS != SALDO_ACTUAL
		     group by PRODUCTO,MONEDA,SUCURSAL, CUENTA, OPERACION, ORDINAL, cliente, RUBROCONTABLE, SALDO_ANTERIOR,SALDO_ACTUAL,DEBITOS,CREDITOS
		     ORDER BY PRODUCTO,MONEDA, SUCURSAL]]>
	</queryString>
