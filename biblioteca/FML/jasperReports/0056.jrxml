	<queryString>
		<![CDATA[SELECT

b.TIPO_CUENTA, 
b.SUCURSAL,
b.NOMBRESUCURSAL,
b.PRODUCTO,
b.DESCRIPCION_PRODUCTO,
b.CUENTA,
b.MONEDA,
b.CLIENTE,
b.NOMBRECLIENTE,
b.PAQUETE,
b.DESCRIPCION_PAQUETE,
b.VENDEDOR,
b.COMERCIALIZADORA,
b.DESC_COMERCIALIZADORA,
b.COD_CAMPANIA

FROM

(

--ACTIVOS

SELECT

'PRÉSTAMO' AS TIPO_CUENTA, 
S.SUCURSAL,
suc.NOMBRESUCURSAL,
S.PRODUCTO,
pro.C6251 AS DESCRIPCION_PRODUCTO,
S.CUENTA,
S.MONEDA,
s.C1803 AS CLIENTE,
c.NOMBRECLIENTE,
(s.C1604 * -1) AS SALDO,
s.C1770 AS PAQUETE,
p.DESCRIPCION_PAQUETE,
sol.USU_AUTORIZA AS VENDEDOR,
v.ID AS COMERCIALIZADORA,
V.DESCRIPCION AS DESC_COMERCIALIZADORA,
(SELECT COD_CAMPANIA 
	FROM CLI_CLIENTES_PAQUETES 
	WHERE COD_PAQUETE=s.C1770  
	AND COD_CLIENTE=s.C1803 
	AND ACTIVO=1) AS COD_CAMPANIA,
s.JTS_OID
FROM SALDOS AS s WITH (nolock)
INNER JOIN CLI_CLIENTES AS c WITH (nolock) ON 
	c.CODIGOCLIENTE = s.C1803 
	AND c.TZ_LOCK =0
INNER JOIN CLI_PAQUETES AS p WITH (nolock) ON 
	p.COD_PAQUETE= s.C1770 
	AND p.TZ_LOCK =0
INNER JOIN PRODUCTOS AS pro WITH (nolock) ON 
	pro.C6250=s.PRODUCTO 
	AND pro.TZ_LOCK=0
INNER JOIN MONEDAS AS m WITH (nolock) ON 
	m.C6399 = s.MONEDA 
	AND m.TZ_LOCK=0
INNER JOIN SOLICAPERTCTAVISTA AS sol WITH (nolock) ON 
	sol.SUCURSAL=s.SUCURSAL 
	AND sol.PRODUCTO=s.PRODUCTO 
	AND sol.MONEDA=s.MONEDA 
	AND sol.CUENTA=s.CUENTA
INNER JOIN VTA_COMERCIALIZADORAS AS v WITH (nolock) ON 
	v.ID=sol.COMERCIALIZADORA
INNER JOIN SUCURSALES AS suc WITH (nolock) ON 
	suc.SUCURSAL=s.SUCURSAL 
	AND suc.TZ_LOCK=0
WHERE 
	s.TZ_LOCK =0 
	AND s.C1604<0 
	AND s.C1785=5
	AND EXISTS (SELECT 1 
							FROM CLI_CLIENTES_PAQUETES 
							WHERE COD_PAQUETE=s.C1770  
								AND COD_CLIENTE=s.C1803 
								AND ACTIVO=1 
								AND COD_CAMPANIA>=$P{CAMPANIA_DESDE}  
								AND COD_CAMPANIA<=$P{CAMPANIA_HASTA}  )
	AND c.CODIGOCLIENTE>=$P{CLIENTE_DESDE}  
	AND c.CODIGOCLIENTE <= $P{CLIENTE_HASTA}
	AND p.COD_PAQUETE>=$P{PAQUETE_DESDE}  
	AND p.COD_PAQUETE <=$P{PAQUETE_HASTA}
			   				
UNION 

--PASIVOS (Cta Vista, Caja Ahorros que no están aferradas a TJD o Cajas de Seg)

SELECT

'CTA CORRIENTE O CAJA DE AHORRO' AS TIPO_CUENTA, 
S.SUCURSAL,
suc.NOMBRESUCURSAL,
S.PRODUCTO,
pro.C6251 AS DESCRIPCION_PRODUCTO,
S.CUENTA,
S.MONEDA,
s.C1803 AS CLIENTE,
c.NOMBRECLIENTE, 
s.C1604 AS SALDO,
s.C1770 AS PAQUETE,
p.DESCRIPCION_PAQUETE,
sol.USU_AUTORIZA AS VENDEDOR,
v.ID AS COMERCIALIZADORA,
V.DESCRIPCION AS DESC_COMERCIALIZADORA,
(SELECT COD_CAMPANIA 
	FROM CLI_CLIENTES_PAQUETES 
	WHERE COD_PAQUETE=s.C1770  
	AND COD_CLIENTE=s.C1803 
	AND ACTIVO=1) AS COD_CAMPANIA,
s.JTS_OID
FROM SALDOS AS s WITH (nolock)
INNER JOIN CLI_CLIENTES AS c WITH (nolock) ON 
	c.CODIGOCLIENTE = s.C1803 
	AND c.TZ_LOCK =0
INNER JOIN CLI_PAQUETES AS p WITH (nolock) ON 
	p.COD_PAQUETE= s.C1770 
	AND p.TZ_LOCK =0
INNER JOIN PRODUCTOS AS pro WITH (nolock) ON 
	pro.C6250=s.PRODUCTO 
	AND pro.TZ_LOCK=0
INNER JOIN MONEDAS AS m WITH (nolock) ON 
	m.C6399 = s.MONEDA 
	AND m.TZ_LOCK=0
INNER JOIN SOLICAPERTCTAVISTA AS sol WITH (nolock) ON 
	sol.SUCURSAL=s.SUCURSAL 
	AND sol.PRODUCTO=s.PRODUCTO 
	AND sol.MONEDA=s.MONEDA 
	AND sol.CUENTA=s.CUENTA
INNER JOIN VTA_COMERCIALIZADORAS AS v WITH (nolock) ON 
	v.ID=sol.COMERCIALIZADORA
INNER JOIN SUCURSALES AS suc WITH (nolock) ON 
	suc.SUCURSAL=s.SUCURSAL 
	AND suc.TZ_LOCK=0
WHERE 
	s.TZ_LOCK =0 
	AND s.C1604<>0 
	AND s.C1785 IN (2,3)
	AND EXISTS (SELECT 1 
							FROM CLI_CLIENTES_PAQUETES 
							WHERE COD_PAQUETE=s.C1770  
							AND COD_CLIENTE=s.C1803 
							AND ACTIVO=1 
							AND COD_CAMPANIA>=$P{CAMPANIA_DESDE}  
							AND COD_CAMPANIA<=$P{CAMPANIA_HASTA}  )
	AND c.CODIGOCLIENTE>=$P{CLIENTE_DESDE}  
	AND c.CODIGOCLIENTE <= $P{CLIENTE_HASTA}
	AND p.COD_PAQUETE>=$P{PAQUETE_DESDE}  
	AND p.COD_PAQUETE <=$P{PAQUETE_HASTA}
				   				  
UNION  

--DPF

SELECT
'DPF' AS TIPO_CUENTA,
S.SUCURSAL,
suc.NOMBRESUCURSAL,
S.PRODUCTO,
pro.C6251 AS DESCRIPCION_PRODUCTO,
S.CUENTA,
S.MONEDA, 
s.C1803 AS CLIENTE, 
c.NOMBRECLIENTE,
s.C1604 AS SALDO,
s.C1770 AS PAQUETE,
p.DESCRIPCION_PAQUETE,
sol.USU_AUTORIZA AS VENDEDOR,
 0 AS COMERCIALIZADORA,
'NBCH' AS DESC_COMERCIALIZADORA,
(SELECT COD_CAMPANIA 
	FROM CLI_CLIENTES_PAQUETES 
	WHERE COD_PAQUETE=s.C1770  
	AND COD_CLIENTE=s.C1803 
	AND ACTIVO=1) AS COD_CAMPANIA,
s.JTS_OID
FROM SALDOS AS s WITH (nolock)
INNER JOIN CLI_CLIENTES AS c WITH (nolock) ON 
	c.CODIGOCLIENTE = s.C1803 
	AND c.TZ_LOCK =0
INNER JOIN CLI_PAQUETES AS p WITH (nolock) ON 
	p.COD_PAQUETE= s.C1770 
	AND p.TZ_LOCK =0
INNER JOIN PRODUCTOS AS pro WITH (nolock) ON 
	pro.C6250=s.PRODUCTO 
	AND pro.TZ_LOCK=0
INNER JOIN MONEDAS AS m WITH (nolock) ON 
	m.C6399 = s.MONEDA 
	AND m.TZ_LOCK=0
INNER JOIN SOLICAPERTCTAVISTA AS sol WITH (nolock) ON 
	sol.SUCURSAL=s.SUCURSAL 
	AND sol.PRODUCTO=s.PRODUCTO 
	AND sol.MONEDA=s.MONEDA 
	AND sol.CUENTA=s.CUENTA
INNER JOIN SUCURSALES AS suc WITH (nolock) ON 
	suc.SUCURSAL=s.SUCURSAL 
	AND suc.TZ_LOCK=0
WHERE 
	s.TZ_LOCK =0 
	AND s.C1604<>0 
	AND s.C1785 =4
	AND EXISTS (SELECT 1 
							FROM CLI_CLIENTES_PAQUETES 
							WHERE COD_PAQUETE=s.C1770  
							AND COD_CLIENTE=s.C1803 
							AND ACTIVO=1 
							AND COD_CAMPANIA>=$P{CAMPANIA_DESDE}  
							AND COD_CAMPANIA<=$P{CAMPANIA_HASTA}  )
	AND c.CODIGOCLIENTE>=$P{CLIENTE_DESDE}  
	AND c.CODIGOCLIENTE <= $P{CLIENTE_HASTA}
	AND p.COD_PAQUETE>=$P{PAQUETE_DESDE} 
	AND p.COD_PAQUETE <= $P{PAQUETE_HASTA}			  
				   				  
UNION

-- CTA CAJA DE SEGURIDAD

SELECT

'CTA CAJA DE SEGURIDAD' AS TIPO_CUENTA, 
S.SUCURSAL,
suc.NOMBRESUCURSAL,
S.PRODUCTO,
pro.C6251 AS DESCRIPCION_PRODUCTO,
S.CUENTA,
S.MONEDA,
s.C1803 AS CLIENTE, 
c.NOMBRECLIENTE,
s.C1604 AS SALDO,
s.C1770 AS PAQUETE,
p.DESCRIPCION_PAQUETE,
sol.USU_AUTORIZA AS VENDEDOR,
v.ID AS COMERCIALIZADORA,
V.DESCRIPCION AS DESC_COMERCIALIZADORA,
(SELECT COD_CAMPANIA 
	FROM CLI_CLIENTES_PAQUETES 
	WHERE COD_PAQUETE=s.C1770  
	AND COD_CLIENTE=s.C1803 
	AND ACTIVO=1) AS COD_CAMPANIA,
s.JTS_OID
FROM SALDOS AS s WITH (nolock)
INNER JOIN CLI_CLIENTES AS c WITH (nolock) ON 
	c.CODIGOCLIENTE = s.C1803 
	AND c.TZ_LOCK =0
INNER JOIN CLI_PAQUETES AS p WITH (nolock) ON 
	p.COD_PAQUETE= s.C1770 
	AND p.TZ_LOCK =0
INNER JOIN PRODUCTOS AS pro WITH (nolock) ON 
	pro.C6250=s.PRODUCTO 
	AND pro.TZ_LOCK=0
INNER JOIN MONEDAS AS m WITH (nolock) ON 
	m.C6399 = s.MONEDA 
	AND m.TZ_LOCK=0
INNER JOIN SOLICAPERTCTAVISTA AS sol WITH (nolock) ON 
	sol.SUCURSAL=s.SUCURSAL 
	AND sol.PRODUCTO=s.PRODUCTO 
	AND sol.MONEDA=s.MONEDA 
	AND sol.CUENTA=s.CUENTA
LEFT JOIN VTA_COMERCIALIZADORAS AS v WITH (nolock) ON 
	v.ID=sol.COMERCIALIZADORA
INNER JOIN SUCURSALES AS suc WITH (nolock) ON 
	suc.SUCURSAL=s.SUCURSAL 
	AND suc.TZ_LOCK=0
WHERE 
	s.TZ_LOCK =0 
	AND s.C1604<>0 
	AND s.C1785 IN (2,3)
	AND EXISTS (SELECT 1 
							FROM CLI_CLIENTES_PAQUETES 
							WHERE COD_PAQUETE=s.C1770  
							AND COD_CLIENTE=s.C1803 
							AND ACTIVO=1 
							AND COD_CAMPANIA>=$P{CAMPANIA_DESDE}  
							AND COD_CAMPANIA<=$P{CAMPANIA_HASTA} )
	AND EXISTS (SELECT 1 
							FROM cof_cofres_contratos 
							WHERE SUCURSAL_DEBITO=s.SUCURSAL 
							AND PRODUCTO_DEBITO=s.PRODUCTO
   							AND moneda_Debito = s.moneda 
   							AND CUENTA_DEBITO = s.CUENTA 
   							AND OPERACION_DEBITO=s.OPERACION 
   							AND ORDINAL_DEBITO =s.ORDINAL ) 
	AND c.CODIGOCLIENTE>=$P{CLIENTE_DESDE}  
	AND c.CODIGOCLIENTE <= $P{CLIENTE_HASTA}
	AND p.COD_PAQUETE>=$P{PAQUETE_DESDE}  
	AND p.COD_PAQUETE <= $P{PAQUETE_HASTA}			
   				
UNION

-- CTAS TARJETAS DE DEBITO

SELECT

'CTA TARJETAS DE DEBITO' AS TIPO_CUENTA, 
S.SUCURSAL,
suc.NOMBRESUCURSAL,
S.PRODUCTO,
pro.C6251 AS DESCRIPCION_PRODUCTO,
S.CUENTA,
S.MONEDA,
s.C1803 AS CLIENTE,
c.NOMBRECLIENTE,
s.C1604 AS SALDO,
s.C1770 AS PAQUETE,
p.DESCRIPCION_PAQUETE,
sol.USU_AUTORIZA AS VENDEDOR,
v.ID AS COMERCIALIZADORA,
V.DESCRIPCION AS DESC_COMERCIALIZADORA,
(SELECT COD_CAMPANIA 
	FROM CLI_CLIENTES_PAQUETES 
	WHERE COD_PAQUETE=s.C1770  
	AND COD_CLIENTE=s.C1803 
	AND ACTIVO=1) AS COD_CAMPANIA,
s.JTS_OID
FROM SALDOS AS s WITH (nolock)
INNER JOIN CLI_CLIENTES AS c WITH (nolock) ON 
	c.CODIGOCLIENTE = s.C1803 
	AND c.TZ_LOCK =0
INNER JOIN CLI_PAQUETES AS p WITH (nolock) ON 
	p.COD_PAQUETE= s.C1770 
	AND p.TZ_LOCK =0
INNER JOIN PRODUCTOS AS pro WITH (nolock) ON 
	pro.C6250=s.PRODUCTO 
	AND pro.TZ_LOCK=0
INNER JOIN MONEDAS AS m WITH (nolock) ON 
	m.C6399 = s.MONEDA 
	AND m.TZ_LOCK=0
INNER JOIN SOLICAPERTCTAVISTA AS sol WITH (nolock) ON 
	sol.SUCURSAL=s.SUCURSAL 
	AND sol.PRODUCTO=s.PRODUCTO 
	AND sol.MONEDA=s.MONEDA 
	AND sol.CUENTA=s.CUENTA
INNER JOIN VTA_COMERCIALIZADORAS AS v WITH (nolock) ON 
	v.ID=sol.COMERCIALIZADORA
INNER JOIN SUCURSALES AS suc WITH (nolock) ON 
	suc.SUCURSAL=s.SUCURSAL 
	AND suc.TZ_LOCK=0
WHERE 
	s.TZ_LOCK =0 
	AND s.C1604<>0 
	AND s.C1785 IN (2,3)
	AND EXISTS (SELECT 1 
							FROM CLI_CLIENTES_PAQUETES 
							WHERE COD_PAQUETE=s.C1770  
							AND COD_CLIENTE=s.C1803 
							AND ACTIVO=1 
							AND COD_CAMPANIA>=$P{CAMPANIA_DESDE}  
							AND COD_CAMPANIA<=$P{CAMPANIA_HASTA} )
	AND EXISTS (SELECT 1 
							FROM TJD_TARJETAS 
							WHERE JTS_OID_GTOS = S.JTS_OID) 
	AND c.CODIGOCLIENTE>=$P{CLIENTE_DESDE}  
	AND c.CODIGOCLIENTE <= $P{CLIENTE_HASTA}
	AND p.COD_PAQUETE>=$P{PAQUETE_DESDE}  
	AND p.COD_PAQUETE <= $P{PAQUETE_HASTA}			
	
																										) b

ORDER BY

b.SUCURSAL,
b.PRODUCTO,
b.DESCRIPCION_PRODUCTO,
b.CUENTA,
b.MONEDA,
b.PAQUETE,
b.TIPO_CUENTA,
b.COD_CAMPANIA,
b.VENDEDOR,
b.COMERCIALIZADORA,
b.DESC_COMERCIALIZADORA,
b.CLIENTE,
b.NOMBRECLIENTE]]>
	</queryString>
