	<subDataset name="Nombre">
		<parameter name="NroGarantia" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT pf.C1404+' '+pf.C1405+' '+' ,'+pf.C1406+' '+pf.C1407 AS Nombre
FROM GR_GARANTIAS g, CL_PERSONASFISICAS pf WHERE
g.IDPERSONATITULAR=pf.IDPERSONA AND g.NROGARANTIA=$P{NroGarantia}]]>
		</queryString>
		<field name="Nombre" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataGrupo">
		<parameter name="codigo" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT (C1000+',')nombre,
	CASE C1409
		WHEN 'A' THEN 'Unión de Hecho Estble, '
		WHEN 'C' THEN 'Casado/a, '
		WHEN 'D' THEN 'Divorciado/a, '
		WHEN 'S' THEN 'Soltero/a, '
		WHEN 'V' THEN 'Viudo/a, '
	END AS estCivil, (C1222 +' ' ) profesion,(t.DESCRIPCION + '  número ')DESCRIPCION,d.nrodocumento
	 FROM CL_CLIENTES cl,TC_PROFESIONES P,CL_PERSONASFISICAS C,CL_RELPERDOC d,CL_TIPOSDOCUMENTO t
WHERE cl.C0902 IN (SELECT C3199 FROM CL_RELGSCLTE WHERE C3198=$P{codigo})
AND cl.C0902=c.IDPERSONA
AND C.C1499 = P.C1221
AND d.IDPERSONA  =c.IDPERSONA
AND d.principal = 'S' AND d.TIPODOC = t.TIPODOC]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
		<field name="estCivil" class="java.lang.String"/>
		<field name="profesion" class="java.lang.String"/>
		<field name="DESCRIPCION" class="java.lang.String"/>
		<field name="nrodocumento" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataNombres">
		<parameter name="codigo" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[DECLARE @valores VARCHAR(1000)
SELECT @valores= COALESCE(@valores + ', ', '') + C1000 FROM CL_CLIENTES WHERE C0902 IN (
SELECT C3199 FROM CL_RELGSCLTE WHERE C3198=$P{codigo})
select @valores as nombres]]>
		</queryString>
		<field name="nombres" class="java.lang.String"/>
	</subDataset>
	<parameter name="NRO_SOLICITUD" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[Select grupo from SL_SOLICITUDCREDITO where C5000 =$P{NRO_SOLICITUD}*100]]>
	</queryString>
	<field name="grupo" class="java.math.BigDecimal"/>
