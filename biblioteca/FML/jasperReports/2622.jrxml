	<queryString>
		<![CDATA[SELECT 
b.CODIGO_INTERNO AS 'CUENTA',
b.MONEDA, 
m.C6400 AS 'DESCRIPCION_MONEDA',
m.C6401 AS 'SIMBOLO_MONEDA', 
p.C6300 AS 'DESCRIPCION_RUBRO',
b.PROMEDIO_DIARIO_ACTUAL, 
CASE WHEN b3.SALDOSMESANTMO IS NULL THEN 0 ELSE b3.SALDOSMESANTMO END AS SALDOSMESANTMO , 
b.SALDOFINMESMO,
b.SUCURSAL
FROM CO_BALANCE_RESULTADOS b 
INNER JOIN MONEDAS m ON 
b.ID_BCE=1 
AND b.TIPO_BALANCE='D' 
AND b.MONEDA=m.C6399 
AND (b.SUCURSAL=$P{P_SUCURSAL} OR $P{P_SUCURSAL}=-1)
AND b.FECHA = (SELECT MAX(b2.FECHA) FROM CO_BALANCE_RESULTADOS b2 WHERE b2.ID_BCE=1 AND b2.TIPO_BALANCE='D' AND MONTH(b2.FECHA)=$P{P_MES} AND YEAR(b2.FECHA)= $P{P_ANO})
INNER JOIN PLANCTAS p ON 
b.CODIGO_INTERNO=p.C6301
AND (p.C6326=$P{P_RUBRO} OR $P{P_RUBRO}=-1)
LEFT JOIN CO_BALANCE_RESULTADOS b3 ON 
b3.ID_BCE=b.ID_BCE 
AND  b3.SUCURSAL=b.SUCURSAL 
AND b3.MONEDA=b.MONEDA 
AND b3.CODIGO_INTERNO=b.CODIGO_INTERNO 
AND b3.TIPO_BALANCE=b.TIPO_BALANCE
AND b3.FECHA = (SELECT MIN(b4.FECHA) FROM CO_BALANCE_RESULTADOS b4 WHERE b4.ID_BCE=1 AND b4.TIPO_BALANCE='D' AND MONTH(b4.FECHA)= $P{P_MES}  AND YEAR(b4.FECHA)= $P{P_ANO} )
WHERE NOT (b.PROMEDIO_DIARIO_ACTUAL=0 and (b3.SALDOSMESANTMO=0 OR b3.SALDOSMESANTMO IS NULL) and b.SALDOFINMESMO=0)
ORDER BY b.CODIGO_INTERNO ASC, B.SUCURSAL ASC]]>
	</queryString>
