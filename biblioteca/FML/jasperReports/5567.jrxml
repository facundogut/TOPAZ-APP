	<parameter name="JTS_OID" class="java.lang.String"/>
	<parameter name="TOPAZ_FECHA_SISTEMA" class="java.util.Date" isForPrompting="false"/>
	<parameter name="TOPAZ_INICIALES_USUARIO" class="java.lang.String" isForPrompting="false"/>
	<parameter name="TOPAZ_SUCURSAL_SISTEMA" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT DISTINCT s.OPERACION AS NROPRESTAMO,
s.C1632 AS TASAINT,
s.ordinal as DESGLOSE, 
s.C1601 AS MONTO,
s.C1728 AS ESTADO,
s.C1803 AS NROCLIENTE,
cl.NOMBRECLIENTE,
doc.TIPODOCUMENTO,
doc.NUMERODOCUMENTO,
s.SUCURSAL,
s.PRODUCTO,
CASE WHEN s.C1642 <> hp.CANTIDADCUOTAS THEN  hp.CANTIDADCUOTAS*hp.PERIODO ELSE s.C1642*s.C1644 END  AS PLAZO,
s.C1621 AS FECDESEM,
s.MONEDA,
--CASE WHEN s.C1632<>hp.TASAINTERES THEN hp.TASAINTERES ELSE s.C1632 END AS TASAINT,
s.JTS_OID AS JTS,
CASE WHEN s.C1642 <> hp.CANTIDADCUOTAS THEN hp.CANTIDADCUOTAS ELSE s.C1642 END  AS TOTALCUOTAS,
p.c6283 AS CODFLIA,
pa.NOMBRE1 AS Razon,
p.C6251 AS PRODUCTO_DESC,
m.C6400 AS MONEDA_DESC,
hp.CANTIDADCUOTAS AS CUOTAS,
s.C1600-s.C1601 AS INTERES,
CASE WHEN s.C1627 <> hp.FECHAVTOFINAL THEN hp.FECHAVTOFINAL ELSE s.C1627 END  AS VENCIMIENTO,
(CASE WHEN DATEDIFF(day,s.C1628,pa.FECHAPROCESO) = 0 THEN 0
WHEN DATEDIFF(day,s.C1628,pa.FECHAPROCESO) < 0 THEN 0
WHEN DATEDIFF(day,s.C1628,pa.FECHAPROCESO) > 0 THEN
(DATEDIFF(day,s.C1628,pa.FECHAPROCESO)) END ) AS dias_atraso
FROM SALDOS s WITH (NOLOCK)
INNER JOIN PRODUCTOS p WITH (NOLOCK) ON p.C6250=s.PRODUCTO AND p.TZ_LOCK=0
INNER JOIN BS_HISTORIA_PLAZO hp ON hp.SALDOS_JTS_OID=s.JTS_OID AND hp.TIPOMOV IN ('I','A') AND hp.TZ_LOCK=0
INNER JOIN CLI_CLIENTES cl WITH (NOLOCK) ON cl.CODIGOCLIENTE=s.C1803 AND cl.TZ_LOCK=0
INNER JOIN CLI_ClientePersona per WITH (NOLOCK) ON per.CODIGOCLIENTE=cl.CODIGOCLIENTE AND per.TITULARIDAD='T' AND per.TZ_LOCK=0
INNER JOIN CLI_DocumentosPFPJ doc WITH (NOLOCK) ON doc.NUMEROPERSONAFJ=per.NUMEROPERSONA AND doc.TZ_LOCK=0
INNER JOIN MONEDAS m WITH (NOLOCK) ON m.C6399=s.MONEDA
