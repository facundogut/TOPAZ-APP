	<queryString>
		<![CDATA[SELECT * FROM
(SELECT E.EMAIL AS EMAILOR, S.CUENTA AS CUENTAOR, V.CTA_CBU AS CBUOR,
S.SUCURSAL AS SUCOR,  SUC.NOMBRESUCURSAL AS NOMSUCOR, P.C6251 AS PRODOR, S.ORDINAL,
CASE 
WHEN D.TIPOPERSONA = 'F' THEN (SELECT F.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0)
WHEN D.TIPOPERSONA = 'J' THEN (SELECT F2.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0))
END PRIMERNOMBREOR,
CASE 
WHEN D.TIPOPERSONA = 'F' THEN (SELECT F.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0)
WHEN D.TIPOPERSONA = 'J' THEN (SELECT F2.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK=0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0))
END APELLIDOPATERNOOR,
CASE 
WHEN D.TIPOPERSONA = 'F' THEN (SELECT TOP 1 D2.NUM_DOC_FISICO FROM dbo.CLI_DocumentosPFPJ D2 WITH(nolock) WHERE CP.NUMEROPERSONA = D2.NUMEROPERSONAFJ AND D2.TZ_LOCK=0)
WHEN D.TIPOPERSONA = 'J' THEN (SELECT TOP 1 D3.NUM_DOC_FISICO FROM dbo.CLI_DocumentosPFPJ D3 WITH(nolock) WHERE D3.TZ_LOCK = 0 AND D3.NUMEROPERSONAFJ IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK=0))
END NUM_DOC_FISICOOR,
CASE
WHEN D.TIPOPERSONA = 'F' THEN
	'El que suscribe, ' + (SELECT F.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0) + ' ' + (SELECT F.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0) + ', D.N.I. N° ' 
	+ (SELECT TOP 1 D2.NUM_DOC_FISICO FROM dbo.CLI_DocumentosPFPJ D2 WITH(nolock) WHERE CP.NUMEROPERSONA = D2.NUMEROPERSONAFJ AND D2.TZ_LOCK = 0) + ' , titular de la ' + P.C6251 + ' N° ' +   CONVERT(VARCHAR(55),S.CUENTA) + ', habilitada en Sucursal ' + CONVERT(VARCHAR(55),S.SUCURSAL) + ' '+ SUC.NOMBRESUCURSAL + ' del Nuevo Banco del Chaco S.A. CBU Nº '
	+ CONVERT(VARCHAR(55),V.CTA_CBU) + ' solicita a ustedes, por la presente, debitar de la misma, la suma mensual de Pesos' 
WHEN D.TIPOPERSONA = 'J' THEN
	'El que suscribe, ' + (SELECT F2.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0)) + ' ' +
	(SELECT F2.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0)) + ', D.N.I. N° '  +
	(SELECT TOP 1 D3.NUM_DOC_FISICO FROM dbo.CLI_DocumentosPFPJ D3 WITH(nolock) WHERE D3.TZ_LOCK = 0 AND D3.NUMEROPERSONAFJ IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0)) + ' , titular de la ' + P.C6251 + ' N° ' +   CONVERT(VARCHAR(55),S.CUENTA) + ', habilitada en Sucursal ' +
	CONVERT(VARCHAR(55),S.SUCURSAL) + ' ' + SUC.NOMBRESUCURSAL + ' del Nuevo Banco del Chaco S.A. CBU Nº ' + CONVERT(VARCHAR(55),V.CTA_CBU) + ' solicita a ustedes, por la presente, debitar de la misma, la suma mensual de Pesos' 
END PARRAFO1
FROM SALDOS S WITH(nolock)
INNER JOIN PRODUCTOS P WITH(nolock) ON S.PRODUCTO = P.C6250 AND P.TZ_LOCK=0
INNER JOIN VTA_SALDOS V WITH(nolock) ON S.JTS_OID = V.JTS_OID_SALDO AND v.TZ_LOCK=0
INNER JOIN SUCURSALES SUC WITH(nolock) ON S.SUCURSAL = SUC.SUCURSAL AND SUC.TZ_LOCK=0
INNER JOIN GRL_ESTADOS_DE_CUENTA G WITH(nolock) ON S.JTS_OID = G.JTSOID AND g.TZ_LOCK=0
INNER JOIN CLI_ClientePersona CP WITH(nolock) ON S.C1803 = CP.CODIGOCLIENTE AND CP.TITULARIDAD = 'T' AND CP.TZ_LOCK=0
INNER JOIN CLI_DocumentosPFPJ D WITH(nolock) ON CP.NUMEROPERSONA = D.NUMEROPERSONAFJ AND D.TZ_LOCK=0
INNER JOIN CLI_EMAILS E WITH(nolock) ON G.FORMATO_MAIL = E.FORMATO AND G.TIPO_MAIL = E.TIPO 
					AND G.ORDINAL_MAIL = E.ORDINAL AND E.ID = CP.NUMEROPERSONA AND E.TZ_LOCK=0
WHERE s.JTS_OID = $P{Jts_oid_origen}
 ) O
LEFT JOIN 
(SELECT ' ' AS EMAILDES, S.CUENTA AS CUENTADES, V.CTA_CBU AS CBUDES,
S.SUCURSAL AS SUCDES,  SUC.NOMBRESUCURSAL AS NOMSUCDES, P.C6251 AS PRODDES, S.ORDINAL AS ORDINAL2,
CASE 
WHEN D.TIPOPERSONA = 'F' THEN (SELECT F.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0)
WHEN D.TIPOPERSONA = 'J' THEN (SELECT F2.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0))
END PRIMERNOMBREDES,
CASE 
WHEN D.TIPOPERSONA = 'F' THEN (SELECT F.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0)
WHEN D.TIPOPERSONA = 'J' THEN (SELECT F2.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0))
END APELLIDOPATERNODES,
CASE 
WHEN D.TIPOPERSONA = 'F' THEN (SELECT TOP 1 D2.NUM_DOC_FISICO FROM dbo.CLI_DocumentosPFPJ D2 WITH(nolock) WHERE CP.NUMEROPERSONA = D2.NUMEROPERSONAFJ AND D2.TZ_LOCK = 0)
WHEN D.TIPOPERSONA = 'J' THEN (SELECT TOP 1 D3.NUM_DOC_FISICO FROM dbo.CLI_DocumentosPFPJ D3 WITH(nolock) WHERE D3.TZ_LOCK = 0 AND D3.NUMEROPERSONAFJ IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0))
END NUM_DOC_FISICODES,
CASE WHEN D.TIPOPERSONA = 'F' THEN
	(P.C6251 + ' Nº ' + CONVERT(VARCHAR(55),S.CUENTA) + ' abierta en Sucursal ' + CONVERT(VARCHAR(55),S.SUCURSAL) + ' ' + SUC.NOMBRESUCURSAL  + ' del  NUEVO BANCO DEL CHACO S.A., CBU  N° ' + CONVERT(VARCHAR(55),V.CTA_CBU) + ' de titularidad de, ' + 
	 (SELECT F.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0) + ' ' + (SELECT F.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F WITH(nolock) WHERE CP.NUMEROPERSONA = F.NUMEROPERSONAFISICA AND F.TZ_LOCK = 0) + 
	 ', desde Fecha de inicio del débito: ')
WHEN D.TIPOPERSONA = 'J' THEN 
		(P.C6251 + ' Nº ' + CONVERT(VARCHAR(55),S.CUENTA) + ' abierta en Sucursal ' + CONVERT(VARCHAR(55),S.SUCURSAL) + ' ' + SUC.NOMBRESUCURSAL  + ' del  NUEVO BANCO DEL CHACO S.A., CBU  N° ' + CONVERT(VARCHAR(55),V.CTA_CBU) + ' de titularidad de, ' + 
	 (SELECT F2.APELLIDOPATERNO FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0)) +
	 ' ' + (SELECT F2.PRIMERNOMBRE FROM dbo.CLI_PERSONASFISICAS F2 WITH(nolock) WHERE F2.TZ_LOCK = 0 AND F2.NUMEROPERSONAFISICA IN (SELECT TOP 1 NUMEROPERSONAFISICA FROM CLI_INTEGRANTESPJ I WITH(nolock) WHERE I.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND I.TZ_LOCK = 0)) +
 		', desde Fecha de inicio del débito: ')
END AS PARRAFO2
FROM dbo.SALDOS S WITH(nolock)
INNER JOIN PRODUCTOS P WITH(nolock) ON S.PRODUCTO = P.C6250 AND P.TZ_LOCK=0
INNER JOIN VTA_SALDOS V WITH(nolock) ON S.JTS_OID = V.JTS_OID_SALDO AND v.TZ_LOCK=0
INNER JOIN SUCURSALES SUC WITH(nolock) ON S.SUCURSAL = SUC.SUCURSAL AND SUC.TZ_LOCK=0
INNER JOIN CLI_ClientePersona CP WITH(nolock) ON S.C1803 = CP.CODIGOCLIENTE AND CP.TITULARIDAD = 'T' AND CP.TZ_LOCK=0
INNER JOIN CLI_DocumentosPFPJ D WITH(nolock) ON CP.NUMEROPERSONA = D.NUMEROPERSONAFJ AND D.TZ_LOCK=0

WHERE S.JTS_OID = $P{Jts_oid_destino}
) D
ON (O.ORDINAL = D.ORDINAL2)]]>
	</queryString>
