	<queryString>
		<![CDATA[SELECT a.codigo_org, a.nombre_org, a.cuit_org, a.num_garantia, a.fecha_ini, a.fecha_vto, a.tipo, a.sucursal, SUM(a.monto_utilizado) as  monto_utilizado, a.nro_certificado, a.tipo_doc, a.nro_doc, a.descripcion, a.valor_actual, a.valor_garantia
FROM VW_STOCK_GARANTIAS a WITH(NOLOCK)
----JOIN VW_STOCK_GARANTIAS b ON a.num_garantia = b.num_garantia
WHERE 
 (1=$P{TODOS_FAMILIA} OR a.familia = $P{FAMILIA})  AND
 (1=$P{TODOS_PRODUCTO} OR a.producto = $P{PRODUCTO}) AND
--FILTRO RANGO FECHAS
(1=$P{FECHA_TODOS} OR $P{FECHA_DESDE} <= a.fecha_ini AND $P{FECHA_HASTA} >= a.fecha_ini) 
--FILTRO VIGENCIA
AND ( 1=$P{TODOS_VIG} OR 1=$P{VENCIDOS} AND a.fecha_vto <$P{TOPAZ_FECHA_SISTEMA}  OR 1=$P{VIGENTES} AND  a.fecha_vto >=$P{TOPAZ_FECHA_SISTEMA})
--FILTRO AFECTACION
AND (1=$P{TODOS_AFECTADAS} OR 1=$P{NO_AFECTADAS} AND a.monto_utilizado=0 OR  0=$P{NO_AFECTADAS} ) 
--FILTRO SUCURSAL
AND (1=$P{TODOS_SUCURSAL} OR a.sucursal=$P{SUCURSAL}) 
--FILTRO CLIENTE
AND (1=$P{TODOS_DOCUMENTO} OR	 a.nro_doc=$P{DOCUMENTO} AND a.tipo_doc=$P{T_DOCUMENTO}) 
--FILTRO INSTRUMENTACION
AND (1=$P{TODOS_GARANTIA} OR a.tipo=$P{TIPO_GARANTIA})
--FILTRO ORGANISMO
AND (1=$P{TODOS_ORGANISMO} OR a.codigo_org = $P{ORGANISMO_EMISOR})
GROUP BY a.codigo_org, a.nombre_org, a.cuit_org, a.num_garantia, a.fecha_ini, a.fecha_vto, a.tipo, a.sucursal, a.nro_certificado, a.tipo_doc, a.nro_doc, a.descripcion, a.valor_actual, a.valor_garantia
HAVING SUM(a.monto_utilizado) >0 AND 0=$P{NO_AFECTADAS} OR 1=$P{NO_AFECTADAS} OR 1=$P{TODOS_AFECTADAS}]]>
	</queryString>
