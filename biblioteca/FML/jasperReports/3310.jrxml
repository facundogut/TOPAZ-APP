	<queryString>
		<![CDATA[-- Auxiliar base del reporte

DECLARE @tablaTMP TABLE
(
FechaRealMov DATETIME,
FECHAPROCESO DATETIME,
FECHAVALOR DATETIME,
CUENTA NUMERIC(12,0),
SUCURSAL_CUENTA NUMERIC(5,0),
NombreSucursal VARCHAR(35),
DescipMoneda VARCHAR(20),
CONCEPTO VARCHAR(50),
DescProducto VARCHAR(50),
SignoSal char(1),
DEBITOCREDITO VARCHAR(7),
SignoMov char(1),
CAPITALREALIZADO NUMERIC(15,2),
Saldo_Contable NUMERIC(15,2),
Saldo NUMERIC(15,2),
Saldo24Horas NUMERIC(15,2),
Saldo48Horas NUMERIC(15,2),
ASIENTO NUMERIC(10,0),
NOMBRE_CUENTA VARCHAR(250),
COD_TRANSACCION NUMERIC(5,0),
COD_TRANSAC_DESCRIP VARCHAR(60),
JTSOID float(53), 
RegistroxJTS BIGINT

)

-- Auxiliar de Temporal de Movimientos Contables (para acumulador)

DECLARE @tablaTMPAux TABLE (
		JTSOID float(53),
		RegistroxJTS BIGINT,
		Saldo_Contable NUMERIC (15,2)
	)

BEGIN

INSERT INTO @tablaTMP
		SELECT  
				(CASE
						WHEN Mc.MARCAAJUSTE = 'A'
						THEN Mc.FECHAVALOR
		   				ELSE Mc.FECHAPROCESO
	   			END) AS FechaRealMov,
				mc.FECHAPROCESO, 
				mc.FECHAVALOR,
				mc.CUENTA,
				mc.SUCURSAL_CUENTA,
				r.NOMBRESUCURSAL  AS NombreSucursal,
				m.C6400 AS DescipMoneda, 
				mc.CONCEPTO,
		        p.C6251 AS DescProducto,
		        pl.C6305 AS SignoSal,
		        (CASE mc.DEBITOCREDITO  WHEN 'C' THEN 'CREDITO' WHEN 'D' THEN 'DEBITO'  END  ) AS DEBITOCREDITO,  
		        mc.DEBITOCREDITO AS  SignoMov,         
	            mc.CAPITALREALIZADO, 
	            0 AS Saldo_Contable,
		        s.C1604 AS Saldo, 		       
		        s.C1606 AS Saldo24Horas,
		        s.C1607 AS Saldo48Horas,
		        mc.ASIENTO,
		        VTA_SALDOS.NOMBRE_CUENTA, mc.COD_TRANSACCION, 
		        ct.DESCRIPCION  AS COD_TRANSAC_DESCRIP,
		        s.JTS_OID AS JTSOID,
		       /* NUMERADOR */
				DENSE_RANK() OVER (
						PARTITION BY S.JTS_OID
						ORDER BY
							mc.FECHAPROCESO,
							A.HORAFIN,
							mc.SUCURSAL_CUENTA,
							mc.MONEDA,
							mc.CUENTA,
							mc.PRODUCTO,
							mc.OPERACION_CUENTA,
							mc.ORDINAL_CUENTA,
							mc.ASIENTO,
							mc.JTS_OID ASC
				) AS RegistroxJTS     
		
		FROM MOVIMIENTOS_CONTABLES mc WITH(nolock)
		    INNER JOIN SALDOS s WITH(nolock) ON s.JTS_OID = mc.SALDO_JTS_OID 
		    INNER JOIN dbo.ASIENTOS AS A with (nolock) ON
					Mc.ASIENTO = A.ASIENTO
					AND Mc.FECHAPROCESO = A.FECHAPROCESO
					AND Mc.SUCURSAL = A.SUCURSAL
					AND (CASE
							WHEN mc.MARCAAJUSTE = 'A'
								THEN Mc.FECHAVALOR
							ELSE mc.FECHAPROCESO
						END) >=$P{FECHA_DESDE}

					AND A.ESTADO = 77  
		    INNER JOIN SUCURSALES r  WITH(nolock) ON r.SUCURSAL = mc.SUCURSAL_CUENTA AND r.TZ_LOCK=0
		    INNER JOIN monedas m WITH(nolock) ON m.C6399 = mc.MONEDA AND m.TZ_LOCK=0
		    INNER JOIN PRODUCTOS p WITH(nolock) ON p.C6250 = mc.PRODUCTO
		    INNER JOIN VTA_SALDOS WITH(nolock) ON s.JTS_OID = VTA_SALDOS.JTS_OID_SALDO AND VTA_SALDOS.TZ_LOCK=0
		    INNER JOIN dbo.PLANCTAS  AS pl with (nolock) ON
				p.TZ_LOCK = 0
				AND s.C1730 = pl.C6326
			left JOIN TTR_CODIGO_TRANSACCION_DEF  ct WITH(nolock) ON ct.CODIGO_TRANSACCION = mc.COD_TRANSACCION AND ct.TZ_LOCK=0
		
		WHERE mc.CUENTA = $P{NroCuenta}
		      AND s.C1785 = 2
		      AND mc.FECHAPROCESO >= $P{FECHA_DESDE}
		      AND mc.MONEDA = $P{MONEDA}
		ORDER BY
		FechaRealMov,
		Mc.FECHAPROCESO,
		A.HORAFIN,
		Mc.SUCURSAL_CUENTA,
		Mc.MONEDA,
		Mc.CUENTA,
		Mc.PRODUCTO,
		Mc.OPERACION_CUENTA,
		Mc.ORDINAL_CUENTA,
		Mc.ASIENTO
	
  
/* Acumula saldo según signo: Mismo signo => suma el monto, de lo contrario lo resta*/
		INSERT INTO @tablaTMPAux
		SELECT
			m.JTSOID AS JTSOID,
			m.RegistroxJTS AS RegistroxJTS,
			SUM(a.CAPITALREALIZADO *
				CASE
					WHEN a.SignoSal = a.SignoMov
						THEN 1
					ELSE -1
				END
			   ) AS Saldo_Contable
		FROM @tablaTMP m
		LEFT JOIN @tablaTMP a ON
			a.JTSOID = m.JTSOID
			AND	a.RegistroxJTS <= m.RegistroxJTS
		GROUP BY
			m.RegistroxJTS,
			m.JTSOID
		ORDER BY
			m.RegistroxJTS,
			m.JTSOID
		
		UPDATE m
		SET m.Saldo_Contable = a.Saldo_Contable
		FROM @tablaTMP m
		INNER JOIN @tablaTMPAux a ON
			a.JTSOID = m.JTSOID
			AND	a.RegistroxJTS = m.RegistroxJTS

END

SELECT *
FROM @tablaTMP
]]>
	</queryString>
