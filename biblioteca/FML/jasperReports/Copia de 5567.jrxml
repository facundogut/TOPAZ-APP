	<subDataset name="DATADETALLE">
		<parameter name="NUMEROCUOTA" class="java.lang.String"/>
		<parameter name="JTSOID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
(SELECT NROCUOTA FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS NCUOTA,
(SELECT FECHAVTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS FVENC,
(SELECT CAPITAL FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS CAPITAL,
(SELECT INTERES FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS INTERES,
(SELECT GASTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS GASTO,
(SELECT CAPITAL+INTERES+GASTO FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID AND NROCUOTA=vsp.NROCUOTA) AS TOTAL,
(SELECT TOP 1 FECHAVALOR FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND SALDOS_JTS_OID=p.SALDOS_JTS_OID ORDER BY FECHAVALOR DESC) AS FECHAVALOR,
(SELECT DATEDIFF(d,a.FECHAVALOR,b.FECHAVTO) FROM VBS_PAYS_DETAIL a, V_SALDOS_PLANPAGOS b WHERE a.CUOTA=vsp.NROCUOTA AND a.TIPO='P' AND a.SALDOS_JTS_OID=p.SALDOS_JTS_OID AND b.IDJTSOID=vsp.IDJTSOID AND b.NROCUOTA=vsp.NROCUOTA) AS DIAS,
(SELECT TOP 1 FECHAPROCESOMOV FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID ORDER BY FECHAPROCESOMOV DESC) AS FECHAPROCMOV,
(SELECT TOP 1 NROASIENTOMOV FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID ORDER BY NROASIENTOMOV DESC) AS ASIENTO,
(SELECT TOP 1 SUCURSALMVTO FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID ORDER BY SUCURSALMVTO DESC) AS SUC,
(SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CAPITALP,
(SELECT sum(INTERESPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS INTERESP,
(SELECT sum(MORAPAGADA) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS MORAP,
(SELECT sum(CAPITALPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CARGOSP,
((SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)+(SELECT sum(CAPITALPAGADO) FROM VBS_PAYS_DETAIL WHERE CUOTA = vsp.NROCUOTA AND TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)) AS TOTALP
FROM V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN VBS_PAYS_DETAIL p ON vsp.IDJTSOID=p.SALDOS_JTS_OID
WHERE vsp.IDJTSOID=$P{JTSOID} AND vsp.NROCUOTA=$P{NUMEROCUOTA}]]>
		</queryString>
		<field name="NCUOTA" class="java.math.BigDecimal"/>
		<field name="FVENC" class="java.sql.Timestamp"/>
		<field name="CAPITAL" class="java.math.BigDecimal"/>
		<field name="INTERES" class="java.math.BigDecimal"/>
		<field name="GASTO" class="java.math.BigDecimal"/>
		<field name="TOTAL" class="java.math.BigDecimal"/>
		<field name="FECHAVALOR" class="java.sql.Timestamp"/>
		<field name="DIAS" class="java.lang.Integer"/>
		<field name="FECHAPROCMOV" class="java.sql.Timestamp"/>
		<field name="ASIENTO" class="java.math.BigDecimal"/>
		<field name="SUC" class="java.math.BigDecimal"/>
		<field name="CAPITALP" class="java.math.BigDecimal"/>
		<field name="INTERESP" class="java.math.BigDecimal"/>
		<field name="MORAP" class="java.math.BigDecimal"/>
		<field name="CARGOSP" class="java.math.BigDecimal"/>
		<field name="TOTALP" class="java.math.BigDecimal"/>
		<group name="NCUOTA">
			<groupExpression><![CDATA[$F{NCUOTA}]]></groupExpression>
		</group>
	</subDataset>
	<subDataset name="DATAFAMILIA">
		<parameter name="CODFLIA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DESCRIPCION FROM CO_FAMILIAS
WHERE CODFAMILIA=$P{CODFLIA}]]>
		</queryString>
		<field name="DESCRIPCION" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATACIIU">
		<parameter name="NROPRESTAMO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT a.C1266 AS DESCRIPCION FROM SALDOS s, SL_SOLICITUDCREDITO c, TC_ACIVIDADESCIIU a
WHERE c.C5000=s.C1704 AND
c.CODACTCIIU=a.C1265 AND
s.CUENTA=$P{NROPRESTAMO}]]>
		</queryString>
		<field name="DESCRIPCION" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAGRUPO">
		<parameter name="NROPRESTAMO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT c.GRUPO FROM SALDOS s, SL_SOLICITUDCREDITO c
WHERE c.C5000=s.C1704 AND
s.CUENTA=$P{NROPRESTAMO}]]>
		</queryString>
		<field name="GRUPO" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="DATACLIENTE">
		<parameter name="CODCLI" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C1000 AS NOM FROM CL_CLIENTES
WHERE C0902=$P{CODCLI}]]>
		</queryString>
		<field name="NOM" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATASUC">
		<parameter name="NROSUC" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6020 AS NOMSUC FROM TC_SUCURSALES s
WHERE C6021=$P{NROSUC}]]>
		</queryString>
		<field name="NOMSUC" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAANALISTA">
		<parameter name="CODANALISTA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT TOP 1 NOMBRE FROM AU_RELFUNCIONARIOUSR
WHERE CODFUNCIONARIO=$P{CODANALISTA}]]>
		</queryString>
		<field name="NOMBRE" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAPROD">
		<parameter name="CODPROD" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6251 AS NOMPROD FROM CO_PRODUCTOS
WHERE C6250=$P{CODPROD}]]>
		</queryString>
		<field name="NOMPROD" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATAMONEDA">
		<parameter name="CODMONEDA" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6400 AS NOMMON FROM CO_MONEDAS
WHERE C6399=$P{CODMONEDA}]]>
		</queryString>
		<field name="NOMMON" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DATATOTAL">
		<parameter name="JTSOID" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
(SELECT sum(CAPITAL) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS CAPITALTOTAL,
(SELECT sum(INTERES) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS INTERESTOTAL,
(SELECT sum(GASTO) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS GASTOTOTAL,
(SELECT sum(CAPITAL+INTERES+GASTO) FROM V_SALDOS_PLANPAGOS WHERE IDJTSOID=vsp.IDJTSOID) AS TOTAL,
(SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CAPITALPTOTAL,
(SELECT sum(INTERESPAGADO) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS INTERESPTOTAL,
(SELECT sum(MORAPAGADA) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS MORAPTOTAL,
(SELECT sum(CAPITALPAGADO) FROM VBS_PAYS_DETAIL WHERE TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID) AS CARGOSPTOTAL,
((SELECT (sum(CAPITALPAGADO)+sum(CAPITALADELANTADO)+sum(MORAPAGADA)+sum(INTERESPAGADO)) FROM VBS_PAYS_DETAIL WHERE TIPO='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)+(SELECT sum(CAPITALPAGADO) FROM VBS_PAYS_DETAIL WHERE TIPO!='P' AND SALDOS_JTS_OID=p.SALDOS_JTS_OID)) AS TOTALP
FROM V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN VBS_PAYS_DETAIL p ON vsp.IDJTSOID=p.SALDOS_JTS_OID
WHERE vsp.IDJTSOID=$P{JTSOID}]]>
		</queryString>
		<field name="CAPITALTOTAL" class="java.lang.Float"/>
		<field name="INTERESTOTAL" class="java.lang.Float"/>
		<field name="GASTOTOTAL" class="java.lang.Float"/>
		<field name="TOTAL" class="java.lang.Float"/>
		<field name="CAPITALPTOTAL" class="java.lang.Float"/>
		<field name="INTERESPTOTAL" class="java.lang.Float"/>
		<field name="MORAPTOTAL" class="java.lang.Float"/>
		<field name="CARGOSPTOTAL" class="java.lang.Float"/>
		<field name="TOTALP" class="java.lang.Float"/>
	</subDataset>
	<parameter name="NPRESTAMO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT vsp.NROPRESTAMO, s.C1604*(-1) AS SALDO,s.C1601 AS MONTO,s.C1728 AS ESTADO,vsp.NROCLIENTE,s.SUCURSAL,vsp.ANALISTA,vsp.NROPRODUCTO,s.C1642*s.C1644 AS PLAZO,
vsp.FECDESEM, vsp.NROSOLICITUD,vsp.MONEDA,vsp.TASAINT,s.C4765*s.C1642 AS PLAZOGRACIA, s.JTS_OID AS JTS, s.C1644 AS TOTALCUOTAS, vsp.NROCUOTA AS CUOTA, p.C7996 AS CODFLIA, p.C8006 AS PREMIO, p.C8007 AS PORCENTAJE
FROM (V_SALDOS_PLANPAGOS vsp LEFT OUTER JOIN VBS_PAYS_DETAIL vpd ON vsp.IDJTSOID=vpd.SALDOS_JTS_OID AND vsp.NROCUOTA=vpd.CUOTA), CO_PRODUCTOS p, SALDOS s WHERE
(vsp.NroProducto = p.C6250) and
vsp.TipoProd = 5 and
((p.C7996 = 2 and
(p.ProDblCro<>0 or p.C8007 = 0)) or
(p.C7996 <> 2 )) and
(s.JTS_OID = vsp.IDJTSOID) and
(s.TZ_LOCK = 0 AND s.TZ_LOCK NOT BETWEEN 300000000000000  AND  399999999999999) AND
s.CUENTA=$P{NPRESTAMO}]]>
	</queryString>
	<field name="NROPRESTAMO" class="java.math.BigDecimal"/>
	<field name="SALDO" class="java.math.BigDecimal"/>
	<field name="MONTO" class="java.math.BigDecimal"/>
	<field name="ESTADO" class="java.lang.String"/>
	<field name="NROCLIENTE" class="java.math.BigDecimal"/>
	<field name="SUCURSAL" class="java.math.BigDecimal"/>
	<field name="ANALISTA" class="java.math.BigDecimal"/>
	<field name="NROPRODUCTO" class="java.math.BigDecimal"/>
	<field name="PLAZO" class="java.math.BigDecimal"/>
