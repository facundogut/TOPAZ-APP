	<parameter name="TOPAZ_SUCURSAL_SISTEMA" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="NROSOLICITUD" class="java.math.BigDecimal"/>
	<parameter name="GARANTIA" class="java.math.BigDecimal"/>
	<parameter name="CARGO1" class="java.math.BigDecimal"/>
	<parameter name="CARGO2" class="java.math.BigDecimal"/>
	<parameter name="CARGO3" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
  (SELECT CL.C1000
  FROM   CL_CLIENTES CL
  WHERE  CL.C0902 = SL.C5187
  AND    CL.TZ_LOCK = 0) AS INFORMACION_1,
  (SELECT (SELECT OP.DESCRIPCION FROM OPCIONES OP WHERE OP.NUMERODECAMPO = 1409 AND OP.OPCIONINTERNA = PF.C1409 AND OP.IDIOMA = 'E')
  FROM   CL_CLIENTES CL, CL_CLIENTPERSONA CP, CL_PERSONASFISICAS PF
  WHERE  CL.C0902 = SL.C5187
  AND    CP.C1430 = CL.C0902
  AND    PF.IDPERSONA = CP.IDPERSONA
  AND    PF.TZ_LOCK = 0
  AND    CP.TZ_LOCK = 0
  AND    CL.TZ_LOCK = 0) AS INFORMACION_2,
  (SELECT (SELECT PO.C1222 FROM TC_PROFESIONES PO WHERE PO.C1221 = PF.C1499 AND PO.TZ_LOCK = 0)
  FROM   CL_CLIENTES CL, CL_CLIENTPERSONA CP, CL_PERSONASFISICAS PF
  WHERE  CL.C0902 = SL.C5187
  AND    CP.C1430 = CL.C0902
  AND    PF.IDPERSONA = CP.IDPERSONA
  AND    PF.TZ_LOCK = 0
  AND    CP.TZ_LOCK = 0
  AND    CL.TZ_LOCK = 0) AS INFORMACION_3,
  (SELECT (SELECT (D3.C1216 + ', ' + DI.REFERENCIA)
          FROM   TC_DIRECCIONES DI, TC_DIR_DIM3 D3
          WHERE  DI.ID = CL.C0902 AND DI.FORMATO = 'P' AND DI.TIPODIRECCION = 'R'
          AND    D3.C1212 = DI.DIM1
          AND    D3.C1214 = DI.DIM2
          AND    D3.C1215 = DI.DIM3
          AND    DI.TZ_LOCK = 0
          AND    D3.TZ_LOCK = 0)
  FROM   CL_CLIENTES CL
  WHERE  CL.C0902 = SL.C5187
  AND    CL.TZ_LOCK = 0) AS INFORMACION_4,
  (SELECT (SELECT RD.NRODOCUMENTO FROM CL_RELPERDOC RD WHERE  RD.IDPERSONA = PF.IDPERSONA AND RD.PRINCIPAL = 'S' AND RD.ESTADO = 'A' AND RD.TZ_LOCK = 0)
  FROM   CL_CLIENTES CL, CL_CLIENTPERSONA CP, CL_PERSONASFISICAS PF
  WHERE  CL.C0902 = SL.C5187
  AND    CP.C1430 = CL.C0902
  AND    PF.IDPERSONA = CP.IDPERSONA
  AND    PF.TZ_LOCK = 0
  AND    CP.TZ_LOCK = 0
  AND    CL.TZ_LOCK = 0) AS INFORMACION_5,
  (CASE SL.MONEDA
     WHEN 1 THEN dbo.NumeroALetras(SL.C5036)
     WHEN 2 THEN dbo.NumeroALetras(SL.C5036*(SELECT MO.C6440 FROM CO_MONEDAS MO WHERE MO.C6399=3))
     WHEN 3 THEN dbo.NumeroALetras(SL.C5036*(SELECT MO.C6440 FROM CO_MONEDAS MO WHERE MO.C6399=3))
   END) AS INFORMACION_6,
  (CASE SL.MONEDA
     WHEN 1 THEN CAST(SL.C5036 AS VARCHAR(15))
     WHEN 2 THEN CAST((SL.C5036*(SELECT MO.C6440 FROM CO_MONEDAS MO WHERE MO.C6399=3)) AS VARCHAR(15))
     WHEN 3 THEN CAST((SL.C5036*(SELECT MO.C6440 FROM CO_MONEDAS MO WHERE MO.C6399=3)) AS VARCHAR(15))
   END) AS INFORMACION_7,
  (CASE SL.MONEDA
     WHEN 1 THEN dbo.NumeroALetras(SL.C5036/(SELECT MO.C6440 FROM CO_MONEDAS MO WHERE MO.C6399=3))
     WHEN 2 THEN dbo.NumeroALetras(SL.C5036)
     WHEN 3 THEN dbo.NumeroALetras(SL.C5036)
   END) AS INFORMACION_8,
  (CASE SL.MONEDA
     WHEN 1 THEN CAST((SL.C5036/(SELECT MO.C6440 FROM CO_MONEDAS MO WHERE MO.C6399=3)) AS VARCHAR(15))
     WHEN 2 THEN CAST((SL.C5036) AS VARCHAR(15))
     WHEN 3 THEN CAST((SL.C5036) AS VARCHAR(15))
   END) AS INFORMACION_9,
  (SUBSTRING(CAST(ROUND(SL.C5039, 2) AS VARCHAR(10)), 1, CHARINDEX('.', CAST(ROUND(SL.C5039, 2) AS VARCHAR(10)))+2)) AS INFORMACION_10,
  (CASE PR.C6259
     WHEN 'F'
     THEN (CAST(SL.C5186 AS VARCHAR(3)) + ' cuotas niveladas ' +
           (CASE SL.C5034
              WHEN 1 THEN 'diarias '
              WHEN 7 THEN 'semanal '
              WHEN 15 THEN 'quincenal '
              WHEN 30 THEN 'mensual '
              WHEN 60 THEN 'bi-mensual '
              WHEN 90 THEN 'trimestral '
              WHEN 120 THEN 'cuatrimestral '
              WHEN 180 THEN 'semestral '
              ELSE ' en 9 meses '
            END))
     WHEN 'H'
     THEN (CAST(SL.C5186 AS VARCHAR(3)) + ' cuotas consecutivas y decrecientes ' +
           (CASE SL.C5034
              WHEN 1 THEN 'diaria '
              WHEN 7 THEN 'semanal '
              WHEN 15 THEN 'quincenal '
              WHEN 30 THEN 'mensual '
              WHEN 60 THEN 'bi-mensual '
              WHEN 90 THEN 'trimestral '
              WHEN 120 THEN 'cuatrimestral '
              WHEN 180 THEN 'semestral '
              ELSE ' en 9 meses '
            END))
     WHEN 'C'
     THEN (CAST(SL.C5186 AS VARCHAR(3)) + ' cuotas irregulares ')
     ELSE
       '1 cuota al vencimiento'
  END) AS INFORMACION_11,
  (' '+char(10)) AS SALTO,
  (CASE PR.C6259
     WHEN 'F'
     THEN ((CASE SL.C5034
              WHEN 1 THEN 'diariamente '
              WHEN 7 THEN 'semanalmente '
              WHEN 15 THEN 'quincenalmente '
              WHEN 30 THEN 'mensualmente '
              WHEN 60 THEN 'bi-mensualmente '
              WHEN 90 THEN 'trimestralmente '
              WHEN 120 THEN 'cuatrimestralmente '
              WHEN 180 THEN 'semestralmente '
              ELSE ' en 9 meses '
            END) + 'las cuotas niveladas; ')
     WHEN 'H'
     THEN ((CASE SL.C5034
              WHEN 1 THEN 'diariamente '
              WHEN 7 THEN 'semanalmente '
              WHEN 15 THEN 'quincenalmente '
              WHEN 30 THEN 'mensualmente '
              WHEN 60 THEN 'bi-mensualmente '
              WHEN 90 THEN 'trimestralmente '
              WHEN 120 THEN 'cuatrimestralmente '
              WHEN 180 THEN 'semestralmente '
              ELSE ' en 9 meses '
            END) + 'las cuotas consecutivas y decrecientes;')
     WHEN 'C'
     THEN ('las cuotas irregulares; ')
     ELSE
       'la cuota al vencimiento'
  END) AS INFORMACION_12,
  (CONVERT(VARCHAR(12), SL.C5183,113)) AS INFORMACION_13,
  (CONVERT(VARCHAR(12), SL.C5054,113)) AS INFORMACION_14,
  (CAST(ROUND($P{CARGO1}, 2) AS VARCHAR(20))) AS INFORMACION_15, --Parametro Entrada
  (CAST(ROUND($P{CARGO2}, 2) AS VARCHAR(20))) AS INFORMACION_16, --Parametro Entrada
  (SELECT SU.C6020 FROM TC_SUCURSALES SU WHERE SU.C6021 = $P{TOPAZ_SUCURSAL_SISTEMA} AND SU.TZ_LOCK = 0) AS INFORMACION_17,  --Parametro Entrada
  (SELECT PS.HORA + ' de la ' + PS.TIEMPO  + ' del día ' + PS.DIA + ' de ' + PS.Mesletras + ' del ' + PS.anioletras
  FROM PARAM_CONTRATO_SUC PS WHERE PS.IdSucursal = $P{TOPAZ_SUCURSAL_SISTEMA} AND PS.TZ_LOCK = 0) AS INFORMACION_18,   --Parametro Entrada
  (SELECT PS.Municipio FROM PARAM_CONTRATO_SUC PS WHERE PS.IdSucursal = $P{TOPAZ_SUCURSAL_SISTEMA} AND PS.TZ_LOCK = 0) AS INFORMACION_19,   --Parametro Entrada
  (SELECT CONCAT(CASE
                   WHEN day(PA.FECHAPROCESO)= 1 THEN 'al primer día'
                   ELSE CONCAT('a los ',day(PA.FECHAPROCESO),' días')
                 END,
                 ' del mes de ',
                  datename(month,PA.FECHAPROCESO),
                  ' del año ',
                  year(PA.FECHAPROCESO))
  FROM PARAMETROS PA) AS INFORMACION_20,
  (CASE $P{CARGO3}
     WHEN 1 THEN 'PRENDA COMERCIAL, PRENDA AGRARIA Y FIANZA SOLIDARIA '
     WHEN 2 THEN 'PRENDA AGRARIA '
     WHEN 3 THEN 'PRENDA COMERCIAL '
     WHEN 4 THEN 'PRENDA COMERCIAL Y FIANZA SOLIDARIA '
     WHEN 5 THEN 'PRENDA AGRARIA Y FIANZA SOLIDARIA '
     ELSE 'FIANZA SOLIDARIA '
  END) AS INFORMACION_21,
     (SELECT TOP 1 PC.Nombre_Notario
     FROM   PARAM_CONTRATOS_POR_SUC_2 PC
     WHERE  PC.Sucursal = $P{TOPAZ_SUCURSAL_SISTEMA}
        AND    PC.TZ_LOCK = 0) AS INFORMACION_22
FROM   SL_SOLICITUDCREDITO SL, CO_PRODUCTOS PR
WHERE  SL.C5000 = $P{NROSOLICITUD}*100   --Parametro Entrada
AND    SL.C5008 = PR.C6250
AND    SL.TZ_LOCK = 0
AND    PR.TZ_LOCK = 0]]>
	</queryString>
