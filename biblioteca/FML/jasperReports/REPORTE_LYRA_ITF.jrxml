	<queryString>
		<![CDATA[select 
	c.shopkey,
	c.shopname,
	c.convenio,
	isnull(rechazos, 0) rechazos,
	isnull(importeRechazado ,0) importeRechazado,
	isnull(aceptado ,0) aceptado,
	isnull(importeAceptado ,0) importeAceptado,
	(SELECT FECHAPROCESO FROM PARAMETROS (nolock)) fechaProc,
	isnull((SELECT sum(cant_reg) FROM itf_lyra_rec_reporte(nolock)), 0) tot_reg,
	isnull((SELECT sum(importe) FROM itf_lyra_rec_reporte(nolock)), 0)  tot_importes,
    isnull((SELECT sum(importe) FROM itf_lyra_rec_reporte(nolock) WHERE status = 'R'), 0)  tot_importeRechazado,
   	isnull((SELECT sum(importe) FROM itf_lyra_rec_reporte(nolock) WHERE status = 'C'), 0)  tot_importeAceptado,
	isnull((SELECT sum(cant_reg)  FROM itf_lyra_rec_reporte(nolock) WHERE status = 'C' ), 0) tot_cantAceptados,
 	isnull((SELECT sum(cant_reg)  FROM itf_lyra_rec_reporte(nolock) WHERE status = 'R' ), 0)  tot_cantRechazados
 	
	from itf_lyra_rec_reporte (nolock) c LEFT JOIN 
	(SELECT  shopkey, sum(cant_reg) rechazos ,sum(importe) importeRechazado FROM itf_lyra_rec_reporte(nolock) a WHERE status = 'R' GROUP BY shopkey) a ON a.shopkey = c.shopkey LEFT JOIN 
	(SELECT  shopkey, sum(cant_reg) aceptado ,sum(importe) importeAceptado FROM itf_lyra_rec_reporte(nolock) a WHERE status = 'C' GROUP BY shopkey) b ON c.shopkey = b.shopkey
	GROUP BY c.shopkey, c.shopname, convenio, rechazos, importeRechazado,aceptado,importeAceptado
	]]>
	</queryString>
