	<queryString>
		<![CDATA[SELECT
	HP.TIPOMOV AS TipoMov,
	OT.DESCRIPCION AS DescrTipo,
	HP.FECHAPROCESOMOV AS FechaProceso,
	M.FECHAVALOR AS FechaValor,
	HP.SUCURSALMOV AS SucursalMov,
	U.NOMBRESUCURSAL AS Sucursal,
	HP.NROASIENTOMOV AS Asiento,
	A.OPERACION AS OperacionMov,
	A.DESCRIPCION AS DescrOperacion,
	CAN.DSC_CANAL AS Canal,
	HP.SALDOS_JTS_OID AS JTS_OID,
	S.SUCURSAL AS SucursalP,	
	S.C1803 AS Cliente,
	C.NOMBRECLIENTE AS NombreCliente,
	D.TIPODOCUMENTO AS TipoDoc,
	D.NUMERODOCUMENTO AS NumDoc,
	M.MONEDA AS Moneda,		
	S.CUENTA AS CuentaP,
	S.PRODUCTO AS ProductoP, 
	S.OPERACION AS OperacionP,
	S.ORDINAL AS OrdinalP,
	CASE 
		--(365 / (F.VTO - F.VALOR)) * (1+TASA) ^ ((F.VTO - F.VALOR)/365) - 1
		WHEN P.C6253 = 'E' --TASA EFECTIVA
			THEN ((P.C6255 / CONVERT(NUMERIC(4),(S.C1642))) *
					(POWER((1+(S.C1632/100)),(CONVERT(NUMERIC(4), S.C1642)/P.C6255)) - 1)) * 100
		--(365 / (F.VTO - F.VALOR)) * (1+TASA) ^ ((F.VTO - F.VALOR)/30) - 1
		WHEN P.C6253 = 'M' --TASA EFECTIVA MENSUAL
			THEN ((P.C6255 / CONVERT(NUMERIC(4), S.C1642)) * 
					(POWER((1+(S.C1632/100)),(CONVERT(NUMERIC(4), S.C1642)/30)) - 1)) * 100
		--TASA * (365/365)
		WHEN P.C6253 = 'N' --TASA NOMINAL
			THEN S.C1632
	END AS TNA,
	PD.CUOTA AS Cuota,
	CASE
			WHEN M.DEBITOCREDITO = 'C' THEN 'Crédito'
			WHEN M.DEBITOCREDITO = 'D' THEN 'Débito'
	END AS 'Débito / Crédito',
	M.RUBROCONTABLE AS Rubro,
	REPLACE(
        REPLACE(
            REPLACE(
                CONVERT(VARCHAR, CONVERT(MONEY, M.CAPITALREALIZADO), 1), 
                '.', '|'), 
            ',', '.'), 
        '|', ',') AS Importe
FROM BS_HISTORIA_PLAZO AS HP WITH (NOLOCK)
INNER JOIN BS_PAYS_DETAIL AS PD WITH (NOLOCK) ON
	PD.NROASIENTOMOV = HP.NROASIENTOMOV
	AND PD.SUCURSALMVTO = HP.SUCURSALMOV
	AND PD.FECHAPROCESOMOV = HP.FECHAPROCESOMOV
INNER JOIN ASIENTOS AS A WITH (NOLOCK) ON
	A.ASIENTO = HP.NROASIENTOMOV
	AND A.SUCURSAL = HP.SUCURSALMOV
	AND A.FECHAPROCESO = HP.FECHAPROCESOMOV
	AND A.ESTADO = 77
INNER JOIN MOVIMIENTOS_CONTABLES M WITH (NOLOCK) ON
	M.ASIENTO = HP.NROASIENTOMOV
	AND M.SUCURSAL = HP.SUCURSALMOV
	AND M.FECHAPROCESO = HP.FECHAPROCESOMOV
	AND M.FECHAVALOR BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
INNER JOIN SALDOS AS S WITH (NOLOCK) ON
	S.JTS_OID = HP.SALDOS_JTS_OID
	AND S.C1785 = 5
	AND ((S.TZ_LOCK < 100000000000000 OR S.TZ_LOCK >= 200000000000000)
		AND (S.TZ_LOCK < 300000000000000 OR S.TZ_LOCK >= 400000000000000)  
	)
INNER JOIN SUCURSALES AS U WITH (NOLOCK) ON
	U.SUCURSAL = HP.SUCURSALMOV
	AND ((U.TZ_LOCK < 100000000000000 OR U.TZ_LOCK >= 200000000000000)
		AND (U.TZ_LOCK < 300000000000000 OR U.TZ_LOCK >= 400000000000000)  
	)
INNER JOIN PRODUCTOS AS P WITH (nolock) ON
	P.C6250 = S.PRODUCTO
	AND ((P.TZ_LOCK < 100000000000000 OR P.TZ_LOCK >= 200000000000000)
		AND (P.TZ_LOCK < 300000000000000 OR P.TZ_LOCK >= 400000000000000)  
	)
INNER JOIN CLI_CLIENTES AS C WITH (NOLOCK) ON
	C.CODIGOCLIENTE = S.C1803
	AND ((C.TZ_LOCK < 100000000000000 OR C.TZ_LOCK >= 200000000000000)
		AND (C.TZ_LOCK < 300000000000000 OR C.TZ_LOCK >= 400000000000000)  
	)
INNER JOIN CLI_ClientePersona CP WITH (NOLOCK) ON
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE
	AND CP.TITULARIDAD = 'T'
	AND ((CP.TZ_LOCK < 100000000000000 OR CP.TZ_LOCK >= 200000000000000)
		AND (CP.TZ_LOCK < 300000000000000 OR CP.TZ_LOCK >= 400000000000000)  
	)
LEFT JOIN OPCIONES AS OT WITH (NOLOCK) ON
	OT.NUMERODECAMPO = 4702
	AND OT.IDIOMA = 'E'
	AND OT.OPCIONINTERNA = HP.TIPOMOV
LEFT JOIN CLI_CANALES AS CAN WITH (NOLOCK) ON
	CAN.COD_CANAL = S.C1800
	AND ((CAN.TZ_LOCK < 100000000000000 OR CAN. TZ_LOCK >= 200000000000000)
		AND (CAN.TZ_LOCK < 300000000000000 OR CAN.TZ_LOCK >= 400000000000000)  
	)
LEFT JOIN CLI_DOCUMENTOSPFPJ D WITH (NOLOCK) ON
	D.NUMEROPERSONAFJ = CP.NUMEROPERSONA
	AND D.TIPODOCUMENTO = (	SELECT TOP 1 TIPODOCUMENTO
								FROM CLI_DOCUMENTOSPFPJ
								WHERE
									NUMEROPERSONAFJ = D.NUMEROPERSONAFJ
									AND ((TZ_LOCK < 100000000000000 OR TZ_LOCK >= 200000000000000)
										AND (TZ_LOCK < 300000000000000 OR TZ_LOCK >= 400000000000000)  
									)
							)
	AND D.NUMERODOCUMENTO = (	SELECT TOP 1 NUMERODOCUMENTO
								FROM CLI_DOCUMENTOSPFPJ
								WHERE
									NUMEROPERSONAFJ = D.NUMEROPERSONAFJ
									AND TIPODOCUMENTO = D.TIPODOCUMENTO
									AND ((TZ_LOCK < 100000000000000 OR TZ_LOCK >= 200000000000000)
										AND (TZ_LOCK < 300000000000000 OR TZ_LOCK >= 400000000000000)  
									)
							)
	AND ((D.TZ_LOCK < 100000000000000 OR D.TZ_LOCK >= 200000000000000)
		AND (D.TZ_LOCK < 300000000000000 OR D.TZ_LOCK >= 400000000000000)  
	)
WHERE
	HP.TIPOMOV IN ('A','P','O','X','R','U')
	AND (HP.SUCURSALMOV = $P{SUCURSAL} OR $P{SUCURSAL} = 0)
	AND ((HP.TZ_LOCK < 100000000000000 OR HP.TZ_LOCK >= 200000000000000)
		AND (HP.TZ_LOCK < 300000000000000 OR HP.TZ_LOCK >= 400000000000000)  
	)]]>
	</queryString>
