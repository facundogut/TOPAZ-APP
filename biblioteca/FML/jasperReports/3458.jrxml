	<queryString>
		<![CDATA[SELECT 
	CASE
		WHEN S.TIPOLIBRETA = 'C' THEN 'COMUN'
		WHEN S.TIPOLIBRETA = 'D' THEN 'DIFERIDO'
	END AS TIPOCHEQUES,
	C.SERIE,
	C.NROCHEQUE,
	C.IMPORTE,
	B.NRO_AVISO,
	B.FECHA_RECHAZO,
	C.CUENTA,
	(SELECT FECHAPROCESO FROM PARAMETROS) AS FECHAPROCESO,
	CLI.NOMBRECLIENTE,
	V.NOMBRE,
	(SELECT CONCAT(CONCAT(D.CALLE, ' '), D.NUMERO) 
		FROM CLI_DIRECCIONES D 
		WHERE D.ID = C.SUCURSAL 
		AND D.FORMATO = 'S' 
		AND D.TIPODIRECCION = 'R') 
	AS DIRSUCURSAL,
	(SELECT D.CPA_NUEVO 
		FROM CLI_DIRECCIONES D 
		WHERE D.ID = C.SUCURSAL 
		AND D.FORMATO = 'S' 
		AND D.TIPODIRECCION = 'R') 
	AS CPASUCURSAL,
	(SELECT P.DESCRIPCION 
		FROM CLI_PROVINCIAS P 
		WHERE P.CODIGOPAIS = 
		(SELECT D.PAIS 
			FROM CLI_DIRECCIONES D 
			WHERE D.ID = C.SUCURSAL 
			AND D.FORMATO = 'S' 
			AND D.TIPODIRECCION = 'R') 
		AND P.DIM1 = 
		(SELECT D.PROVINCIA 
			FROM CLI_DIRECCIONES D 
			WHERE D.ID = C.SUCURSAL 
			AND D.FORMATO = 'S' 
			AND D.TIPODIRECCION = 'R')
		) 
	AS PROVSUCURSAL,
	(SELECT L.DESCRIPCION_DIM3 
		FROM CLI_LOCALIDADES L 
		WHERE L.CODIGOPAIS = 
		(SELECT D.PAIS 
			FROM CLI_DIRECCIONES D 
			WHERE D.ID = C.SUCURSAL 
			AND D.FORMATO = 'S' 
			AND D.TIPODIRECCION = 'R') 
		AND L.DIM1 = 
		(SELECT D.PROVINCIA 
			FROM CLI_DIRECCIONES D 
			WHERE D.ID = C.SUCURSAL 
			AND D.FORMATO = 'S' 
			AND D.TIPODIRECCION = 'R') 
		AND L.DIM2 = 
		(SELECT D.DEPARTAMENTO 
			FROM CLI_DIRECCIONES D 
			WHERE D.ID = C.SUCURSAL 
			AND D.FORMATO = 'S' 
			AND D.TIPODIRECCION = 'R') 
		AND L.DIM3 = 
		(SELECT D.LOCALIDAD 
			FROM CLI_DIRECCIONES D 
			WHERE D.ID = C.SUCURSAL 
			AND D.FORMATO = 'S' 
			AND D.TIPODIRECCION = 'R')
		) 
	AS LOCALIDADSUCURSAL,
	CASE 
		WHEN V.TIPOPERSONA = 'F' THEN
	   		(SELECT CONCAT(CONCAT(D.CALLE, ' '), D.NUMERO) 
	   		FROM CLI_DIRECCIONES D 
	   		WHERE D.ID = V.NUMEROPERSONA 
	   		AND D.FORMATO = 'PF' 
	   		AND D.TIPODIRECCION = 'PR')
		WHEN V.TIPOPERSONA = 'J' THEN
	   		(SELECT CONCAT(CONCAT(D.CALLE, ' '), D.NUMERO) 
	   		FROM CLI_DIRECCIONES D 
	   		WHERE D.ID = V.NUMEROPERSONA 
	   		AND D.FORMATO = 'PJ' 
	   		AND D.TIPODIRECCION = 'L')
	END AS DIRCLIENTE,
	CASE 
	   	WHEN V.TIPOPERSONA = 'F' THEN
	   		(SELECT D.CPA_NUEVO 
	   		FROM CLI_DIRECCIONES D 
	   		WHERE D.ID = V.NUMEROPERSONA 
	   		AND D.FORMATO = 'PF' 
	   		AND D.TIPODIRECCION = 'PR')
	   	WHEN V.TIPOPERSONA = 'J' THEN
	   		(SELECT D.CPA_NUEVO 
	   		FROM CLI_DIRECCIONES D 
	   		WHERE D.ID = V.NUMEROPERSONA 
	   		AND D.FORMATO = 'PJ' 
	   		AND D.TIPODIRECCION = 'L')
	END AS CPACLIENTE,
	CASE 
	   	WHEN V.TIPOPERSONA = 'F' THEN
		   	(SELECT P.DESCRIPCION 
		   	FROM CLI_PROVINCIAS P 
		   	WHERE P.CODIGOPAIS = 
		   		(SELECT D.PAIS 
		   		FROM CLI_DIRECCIONES D 
		   		WHERE D.ID = V.NUMEROPERSONA 
		   		AND D.FORMATO = 'PF' 
		   		AND D.TIPODIRECCION = 'PR') 
		   	AND P.DIM1 = 
		   		(SELECT D.PROVINCIA 
		   		FROM CLI_DIRECCIONES D 
		   		WHERE D.ID = V.NUMEROPERSONA 
		   		AND D.FORMATO = 'PF' 
		   		AND D.TIPODIRECCION = 'PR')
		   	)
	   	WHEN V.TIPOPERSONA = 'J' THEN
	   		(SELECT P.DESCRIPCION 
	   		FROM CLI_PROVINCIAS P 
	   		WHERE P.CODIGOPAIS = 
	   			(SELECT D.PAIS 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PJ' 
	   			AND D.TIPODIRECCION = 'L') 
	   		AND P.DIM1 = 
	   			(SELECT D.PROVINCIA 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PJ' 
	   			AND D.TIPODIRECCION = 'L')
	   		)
	END AS PROVCLIENTE,
	CASE 
		WHEN V.TIPOPERSONA = 'F' THEN
	   		(SELECT L.DESCRIPCION_DIM3 
	   		FROM CLI_LOCALIDADES L 
	   		WHERE L.CODIGOPAIS = 
	   			(SELECT D.PAIS 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PF' 
	   			AND D.TIPODIRECCION = 'PR') 
	   		AND L.DIM1 = 
	   			(SELECT D.PROVINCIA 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PF' 
	   			AND D.TIPODIRECCION = 'PR') 
	   		AND L.DIM2 = 
	   			(SELECT D.DEPARTAMENTO 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PF' 
	   			AND D.TIPODIRECCION = 'PR') 
	   		AND L.DIM3 = 
	   			(SELECT D.LOCALIDAD 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PF' 
	   			AND D.TIPODIRECCION = 'PR')
	   		) 
	   	WHEN V.TIPOPERSONA = 'J' THEN
	   		(SELECT L.DESCRIPCION_DIM3 
	   		FROM CLI_LOCALIDADES L 
	   		WHERE L.CODIGOPAIS = 
	   			(SELECT D.PAIS 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PJ' 
	   			AND D.TIPODIRECCION = 'L') 
	   		AND L.DIM1 = 
	   			(SELECT D.PROVINCIA 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PJ' 
	   			AND D.TIPODIRECCION = 'L') 
	   		AND L.DIM2 = 
	   			(SELECT D.DEPARTAMENTO 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PJ' 
	   			AND D.TIPODIRECCION = 'L') 
	   		AND L.DIM3 = 
	   			(SELECT D.LOCALIDAD 
	   			FROM CLI_DIRECCIONES D 
	   			WHERE D.ID = V.NUMEROPERSONA 
	   			AND D.FORMATO = 'PJ' 
	   			AND D.TIPODIRECCION = 'L')
	   		) 
	END AS LOCALIDADCLIENTE,
	CASE 
		WHEN T.TIPO_RECHAZO = 2
	   		THEN 'MOTIVOS FROMALES'
	   	WHEN T.TIPO_RECHAZO = 3
	   		THEN 'SIN FONDOS'
	END AS DESCRECHAZO,
	CASE 
		WHEN T.TIPO_RECHAZO = 2
	   		THEN 'B'
	   	WHEN T.TIPO_RECHAZO = 3
	   		THEN 'A'
	END AS DESCRECHAZO ,
	R.DESCRIPCION,
	CONCAT('AVISO DE ', R.DESCRIPCION_CORTA) AS DESCRIPCION_CORTA 
FROM 
	CLE_CHEQUES_CLEARING_DEVUELTOS C
INNER JOIN CHE_BCO_RECHAZADOS AS B ON 
	C.SUCURSAL = B.SUCURSAL 
	AND C.PRODUCTO = B.PRODUCTO
	AND C.CUENTA = B.CUENTA 
	AND C.MONEDA = B.MONEDA 
	AND C.NROCHEQUE = B.NRO_CHEQUE 
	AND C.SUCURSAL = B.SUCURSAL
	AND C.SERIE = B.SERIE_CHEQUE
	AND B.TZ_LOCK = 0
INNER JOIN CHE_CHEQUERAS AS S ON 
	B.SUCURSAL = S.SUCURSAL 
	AND B.PRODUCTO = S.PRODUCTO 
	AND B.MONEDA = S.MONEDA 
	AND B.CUENTA = S.CUENTA 
	AND B.NRO_CHEQUE BETWEEN S.CHEQUEDESDE AND S.CHEQUEHASTA
	AND S.TZ_LOCK = 0
INNER JOIN CLI_CLIENTES AS CLI ON 
	C.CLIENTE = CLI.CODIGOCLIENTE
	AND CLI.TZ_LOCK = 0
INNER JOIN VW_CLIENTES_PERSONAS AS V 
	ON CLI.CODIGOCLIENTE = V.CODIGOCLIENTE 
	AND V.TITULARIDAD = 'T'
INNER JOIN CLE_TIPO_CAUSAL AS T ON 
	C.CONCDEVOLUCION = T.CODIGO_DE_CAUSAL
	AND T.TZ_LOCK = 0
INNER JOIN CHE_MOTIVOS_RECHAZO AS R ON 
	R.CODIGO_BCRA = T.CODIGO_BCRA
	AND R.TZ_LOCK = 0
WHERE 
	C.FECHACHEQUE = $P{FECHARECHAZO} 
	AND C.SUCURSAL = $P{SUCURSAL} 
	AND C.CUENTA = $P{CUENTA}
	AND C.NROCHEQUE IN (SELECT CONVERT(INT, value) FROM STRING_SPLIT ($P{CHEQUES}, ','))
	AND C.TZ_LOCK = 0]]>
	</queryString>
