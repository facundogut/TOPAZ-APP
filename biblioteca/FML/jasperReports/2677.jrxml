	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="NBCH Data Adapter "/>
	<parameter name="NRO_CONVENIO" class="java.lang.Integer">
		<parameterDescription><![CDATA[Número de Convenio]]></parameterDescription>
	</parameter>
	<parameter name="TOPAZ_SUCURSAL_SISTEMA" class="java.lang.Integer" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT TOP 1
		--CONVENIO--
		(SELECT CASE CONCAT('',a.Cuit) WHEN '0' THEN REPLICATE('_',11) WHEN '' THEN REPLICATE('_',11) ELSE CONCAT('',a.Cuit) END) AS Cuit,
		(SELECT CASE CONCAT('',a.Plazo) WHEN '0' THEN REPLICATE('_',5) WHEN ''  THEN REPLICATE('_',5) ELSE CONCAT('',a.Plazo) END) AS Plazo, 
		(SELECT CASE CONCAT('',a.Plazo) WHEN '0' THEN REPLICATE('_',10) ELSE dbo.Fn_NumberToWords(a.Plazo) END) AS W_Plazo,
		a.FecAlta, 
		a.Id_ConvRec, 
		(SELECT CASE CONCAT('',a.FecAlta) WHEN '' THEN REPLICATE('_',5) ELSE LEFT(CONVERT(VARCHAR,a.FecAlta,106),2) END) AS DIA,  
		(SELECT CASE CONCAT('', a.FecAlta) WHEN '' THEN REPLICATE('_',5) ELSE (SELECT dbo.Fn_NumberToWords(RIGHT(CONVERT(VARCHAR,FecAlta,106),4))) END) AS AÑO,
		
		-- CUENTA REC -- 
		(SELECT CASE CONCAT('',b.CUENTA) WHEN '' THEN REPLICATE('_',12)  ELSE CONCAT('',b.CUENTA) END)AS CUENTA_REC, 
		(SELECT CASE CONCAT('',e.C6251) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',e.C6251) END )AS TIPO_CUENTA_REC,
		(SELECT CASE CONCAT('',b.SUCURSAL) WHEN ''  THEN REPLICATE('_',5) ELSE CONCAT('',b.SUCURSAL) END) AS SUCURSAL_REC, 
		(SELECT CASE CONCAT('',d.NOMBRESUCURSAL) WHEN ''  THEN REPLICATE('_',40)  ELSE CONCAT('',d.NOMBRESUCURSAL) END) AS LOC_SUC_REC, 
		
		-- CUENTA COM -- 
		(SELECT CASE CONCAT('',f.CUENTA) WHEN '' THEN REPLICATE('_',12) ELSE CONCAT('',f.CUENTA) END) AS CUENTA_COM, 
		(SELECT CASE CONCAT('',i.C6251) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',i.C6251) END)AS TIPO_CUENTA_COM, 
		(SELECT CASE CONCAT('',f.SUCURSAL) WHEN '' THEN REPLICATE('_',5) ELSE CONCAT('',f.SUCURSAL) END)  AS SUCURSAL_COM,
		(SELECT CASE CONCAT('',h.NOMBRESUCURSAL) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',h.NOMBRESUCURSAL) END) AS LOC_SUC_COM,
		
		--CUNETA CAR -- 
		(SELECT CASE CONCAT('',j.CUENTA) WHEN '' THEN REPLICATE('_',12) ELSE CONCAT('',j.CUENTA) END) AS CUENTA_CAR, 
		(SELECT CASE CONCAT('',m.C6251) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',m.C6251) END) AS TIPO_CUENTA_CAR, 
		(SELECT CASE CONCAT('',j.SUCURSAL) WHEN '' THEN REPLICATE('_',5) ELSE CONCAT('',j.SUCURSAL) END) AS SUCURSAL_CAR, 
		(SELECT CASE CONCAT('',l.NOMBRESUCURSAL) WHEN '' THEN REPLICATE('_',5) ELSE CONCAT('',l.NOMBRESUCURSAL) END)  AS LOC_SUC_CAR,
		
		-- DIRECCION FISCAL --
		(SELECT CASE CONCAT('',tf.NUMERO) WHEN '' THEN REPLICATE('_',15) ELSE CONCAT('',tf.NUMERO) END) AS TEL_DF,
		(SELECT  CASE CONCAT('',n.CALLE) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',n.CALLE)  END) AS CALLE_DF, 
		(SELECT CASE CONCAT('',n.NUMERO) WHEN '' THEN REPLICATE('_',8) ELSE CONCAT('',n.NUMERO) END) AS NRO_DF, 
		(SELECT CASE CONCAT('apto ',n.APARTAMENTO) WHEN 'apto ' THEN '' ELSE CONCAT('apto ',n.APARTAMENTO) END)AS APTO_DF, 
	   	(SELECT CASE CONCAT('piso ',n.PISO) WHEN 'piso ' THEN '' ELSE CONCAT('piso ',n.PISO)  END) AS PISO_DF,
		(SELECT CASE CONCAT('',p.DESCRIPCION_DIM3) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',p.DESCRIPCION_DIM3) END) AS LOCALIDAD_DF,
		(SELECT CASE CONCAT('', o.DESCRIPCION) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('', o.DESCRIPCION) END) AS PROVINCIA_DF,
		
		--DIRECCION REAL --
		(SELECT CASE CONCAT('',tr.NUMERO) WHEN '' THEN REPLICATE('_',15) ELSE CONCAT('',tr.NUMERO) END ) AS TEL_DR,
		(SELECT CASE CONCAT('',r.CALLE) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',r.CALLE) END) AS CALLE_DR,
		(SELECT CASE CONCAT('',r.NUMERO) WHEN '' THEN REPLICATE('_',8) ELSE CONCAT('',r.NUMERO) END) AS NRO_DR,
		(SELECT CASE CONCAT('apto ',r.APARTAMENTO) WHEN 'apto ' THEN '' ELSE  CONCAT('apto ',r.APARTAMENTO) END) AS APTO_DR,
		(SELECT CASE CONCAT('piso ', r.PISO) WHEN 'piso ' THEN '' ELSE CONCAT('piso ', r.PISO) END) AS PISO_DR,
		(SELECT CASE CONCAT('',t.DESCRIPCION_DIM3) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',t.DESCRIPCION_DIM3) END) AS LOCALIDAD_DR,
		(SELECT CASE CONCAT('',s.DESCRIPCION) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',s.DESCRIPCION) END)  AS PROVINCIA_DR,
		
		-- SUCURSAL -- 
		(SELECT CASE CONCAT('',v.NOMBREDEP) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('',v.NOMBREDEP) END) AS DEP_CONTRATO,
		
		--CLIENTE  --
		(SELECT CASE CONCAT('', w.NOMBRECLIENTE) WHEN '' THEN REPLICATE('_',35) ELSE CONCAT('', w.NOMBRECLIENTE) END) AS NOMBRECLIENTE,
		(SELECT CASE CONCAT('',w.TIPO) WHEN '' THEN REPLICATE('_',15) WHEN 'J' THEN 'la Empresa' WHEN 'I' THEN 'la Institución' WHEN 'F' THEN 'la Persona' END) AS REPRESENTACION
		
		
		FROM CONV_CONVENIOS_REC a 
		
	    --CUENTA REC --
	    LEFT JOIN SALDOS b ON a.CuentaRec = b.JTS_OID AND b.TZ_LOCK = 0
	    LEFT JOIN CLI_DIRECCIONES c ON b.SUCURSAL = c.ID AND c.FORMATO = 'S' AND c.TIPODIRECCION = 'R' AND c.TZ_LOCK = 0
	    LEFT JOIN SUCURSALES d ON d.SUCURSAL = b.SUCURSAL AND d.TZ_LOCK = 0
	    LEFT JOIN PRODUCTOS e ON b.PRODUCTO = e.C6250 AND e.TZ_LOCK = 0
	    
	    --CUENTA COM --
	    LEFT JOIN SALDOS f ON a.CuentaCom = f.JTS_OID AND f.TZ_LOCK = 0
	    LEFT JOIN CLI_DIRECCIONES g ON f.SUCURSAL = g.ID AND c.FORMATO = 'S' AND c.TIPODIRECCION = 'R' AND g.TZ_LOCK = 0
	    LEFT JOIN SUCURSALES h ON h.SUCURSAL = f.SUCURSAL AND h.TZ_LOCK = 0
	    LEFT JOIN PRODUCTOS i ON f.PRODUCTO = i.C6250 AND i.TZ_LOCK = 0
	    
	    --CUENTA CARG -- 
	    LEFT JOIN SALDOS j ON a.CuentaCargos = j.JTS_OID AND j.TZ_LOCK = 0
	    LEFT JOIN CLI_DIRECCIONES k ON j.SUCURSAL = k.ID AND c.FORMATO = 'S' AND c.TIPODIRECCION = 'R' AND k.TZ_LOCK = 0
	    LEFT JOIN SUCURSALES l ON l.SUCURSAL = j.SUCURSAL AND l.TZ_LOCK = 0
	    LEFT JOIN PRODUCTOS m ON m.C6250 = j.PRODUCTO AND m.TZ_LOCK = 0
	    
	   --DIRECCION FISCAL--
	    LEFT JOIN CLI_ClientePersona x ON a.Cliente = x.CODIGOCLIENTE AND x.TITULARIDAD = 'T' AND x.TZ_LOCK = 0
		LEFT JOIN CLI_DIRECCIONES n ON x.NUMEROPERSONA = n.ID AND n.TIPODIRECCION IN ('F') AND n.FORMATO IN ('PF', 'PJ') AND n.ORDINAL_DIR = 1 AND n.TZ_LOCK = 0
		LEFT JOIN CLI_TELEFONOS tf ON tf.ID = x.NUMEROPERSONA AND tf.FORMATO IN ('PF','PJ') AND tf.ORDINAL = 1 AND tf.TIPO = 'LE' AND tf.TZ_LOCK = 0
		LEFT JOIN CLI_PROVINCIAS o ON o.CODIGOPAIS =n.PAIS AND o.DIM1 = n.PROVINCIA AND o.TZ_LOCK = 0
		LEFT JOIN CLI_LOCALIDADES p ON p.DIM1 = n.PROVINCIA AND p.DIM2 = n.DEPARTAMENTO AND p.DIM3 = n.LOCALIDAD AND p.TZ_LOCK = 0
	    
	    --DIRECCION REAL--
	    LEFT JOIN CLI_ClientePersona z ON a.Cliente = z.CODIGOCLIENTE AND z.TITULARIDAD = 'T' AND z.TZ_LOCK = 0
		LEFT JOIN  CLI_DIRECCIONES r ON z.NUMEROPERSONA = r.ID AND a.Dir_Formato = r.FORMATO AND r.TIPODIRECCION  IN ('R', 'L') AND r.TZ_LOCK = 0
		LEFT JOIN CLI_TELEFONOS tr ON z.NUMEROPERSONA = tr.ID AND tr.FORMATO IN ('PF','PJ') AND tr.ORDINAL = 1 AND tr.TIPO IN ('LE','PE') AND tr.TZ_LOCK = 0
	    LEFT JOIN  CLI_PROVINCIAS s ON s.CODIGOPAIS = r.PAIS AND s.DIM1 = r.PROVINCIA AND s.TZ_LOCK = 0 AND s.TZ_LOCK = 0
		LEFT JOIN CLI_LOCALIDADES t ON t.DIM1 = r.PROVINCIA AND t.DIM2 = r.DEPARTAMENTO AND t.DIM3 = r.LOCALIDAD AND t.TZ_LOCK = 0
	    
	    --SUCURSAL --
	    LEFT JOIN CLI_DIRECCIONES u ON u.ID =  $P{TOPAZ_SUCURSAL_SISTEMA} AND u.FORMATO = 'S' AND u.TIPODIRECCION = 'R' AND u.TZ_LOCK = 0
	    LEFT JOIN CLI_DEPARTAMENTOS v ON v.CODIGOPAIS = u.PAIS AND v.DEPARTAMENTO = u.DEPARTAMENTO AND v.PROVINCIA = u.PROVINCIA AND v.TZ_LOCK = 0
	    
	    --CLIENTE ---
	    LEFT JOIN CLI_CLIENTES w ON w.CODIGOCLIENTE = a.Cliente  AND w.TZ_LOCK = 0
	    WHERE a.Id_ConvRec =  $P{NRO_CONVENIO}  AND a.TZ_LOCK = 0]]>
	</queryString>
