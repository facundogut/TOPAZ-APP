	<parameter name="MES" class="java.lang.String"/>
	<parameter name="ANIO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT ext.EXPEDIENTE, cli.NOMBRECLIENTE, pd.CUOTA, pd.VENCIMIENTOCUOTA, pd.CAPITALPAGADO + pd.CAPITALADELANTADO + pd.INTERESPAGADO + pd.MORAPAGADA  AS MONTOTOTAL, pd.CAPITALPAGADO + pd.CAPITALADELANTADO AS CAPITAL, pd.INTERESPAGADO, pd.MORAPAGADA, pd.FECHAVALOR AS FECHAPAGO
FROM BS_PAYS_DETAIL pd
JOIN BS_HISTORIA_PLAZO hp ON pd.HP_JTS_OID = hp.JTS_OID
JOIN ASIENTOS a ON hp.NROASIENTOMOV = a.ASIENTO AND hp.FECHAPROCESOMOV = a.FECHAPROCESO AND hp.SUCURSALMOV = a.SUCURSAL AND a.ESTADO = 77 AND a.OPERACION <> 6827
JOIN SALDOS sp ON hp.SALDOS_JTS_OID = sp.JTS_OID AND sp.TZ_LOCK = 0
JOIN PRODUCTOS p ON sp.PRODUCTO = p.C6250 AND p.C6800 IN ('CF','CR') AND pd.TZ_LOCK = 0
JOIN CRE_SALDOS ext ON sp.JTS_OID = ext.SALDOS_JTS_OID AND ext.TZ_LOCK = 0
JOIN CLI_CLIENTES cli ON sp.C1803 = cli.CODIGOCLIENTE AND cli.TZ_LOCK = 0
WHERE (pd.CAPITALPAGADO + pd.CAPITALADELANTADO > 0 OR pd.INTERESPAGADO > 0 OR pd.MORAPAGADA > 0) AND
FORMAT(pd.FECHAVALOR,'MM') = RIGHT('00' + Ltrim(Rtrim($P{MES})),2) AND FORMAT(pd.FECHAVALOR,'yyyy') = $P{ANIO}]]>
	</queryString>
	<field name="EXPEDIENTE" class="java.lang.String"/>
	<field name="NOMBRECLIENTE" class="java.lang.String"/>
	<field name="CUOTA" class="java.math.BigDecimal"/>
	<field name="VENCIMIENTOCUOTA" class="java.sql.Timestamp"/>
