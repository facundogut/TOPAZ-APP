	<parameter name="NUMPER" class="java.lang.Integer">
		<parameterDescription><![CDATA[Número de Persona]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
 ISNULL(P.PRIMERNOMBRE,'') AS PRIMERNOMBRE,
 ISNULL(P.SEGUNDONOMBRE, '') AS SEGUNDONOMBRE,
 ISNULL(P.APELLIDOPATERNO, '') AS APELLIDOPATERNO,
 ISNULL(D.TIPO_DOC_FISICO, '') AS TIPO_DOC_FISICO, 
 ISNULL(D.NUM_DOC_FISICO, 0) AS NUM_DOC_FISICO,
 ISNULL(D.TIPODOCUMENTO, '') AS TIPODOCUMENTO, 
 ISNULL(D.NUMERODOCUMENTO, 0) AS NUMERODOCUMENTO,
 ISNULL(P.FECHANACIMIENTO, '') AS FECHANACIMIENTO,
 ISNULL(P.NACIONALIDAD, '') AS NACIONALIDAD,
 ISNULL(P.FECHAALTA, '') AS FECHAALTA
FROM CLI_PERSONASFISICAS AS P, CLI_DOCUMENTOSPFPJ AS D 
WHERE (P.NUMEROPERSONAFISICA = D.NUMEROPERSONAFJ) 
AND P.NUMEROPERSONAFISICA=$P{NUMPER}
AND P.TZ_LOCK=0
AND D.TZ_LOCK=0]]>
	</queryString>
