	<queryString>
		<![CDATA[SELECT 	
	count(*) cant_det, 
	cab.totalregistros total_registros,
	(cab.totalregistros - count(*)) rechazados,
	sum(det.importe) importe_grabado,
	cab.totalimporte total_importe,
	(cab.totalimporte - sum(det.importe)) importe_rechazado,	
	convert(VARCHAR(10), cab.fecha_cobranza, 103) fecha_cobranza
	FROM REC_CAB_RECAUDOS_CANAL (nolock) cab 
	JOIN REC_DET_RECAUDOS_CANAL (nolock) det ON cab.id = det.id_Cabezal 
		WHERE cab.id =  $P{ID_CAB} GROUP BY cab.totalregistros, cab.fecha_cobranza, cab.totalimporte]]>
	</queryString>
