	<parameter name="FechaReporte" class="java.util.Date"/>
	<queryString>
		<![CDATA[DECLARE @FechaCorte  AS SmallDatetime  =$P{FechaReporte}
DECLARE @FechaInicio AS SmallDatetime =  CONVERT( SMALLDATETIME, ('01' +'/'+ convert(nvarchar,MONTH(@FechaCorte)) +'/' + CONVERT(NVARCHAR, YEAR(@FechaCorte))))

SELECT Detalle.Usuario
       ,Detalle.Id_Region
       ,UPPER(Detalle.Nombre_Region) AS Nombre_Region
       ,Detalle.CodSucursal
       ,Detalle.Sucursal
       ,Detalle.Oficial_Credito
       ,Detalle.Cedula_Oficial
       ,Detalle.CodCargo
       ,'Des_Cargo' = Case When Detalle.CodCargo = 119 THEN 'SENIOR 1'
						   When Detalle.CodCargo = 120 THEN 'SENIOR 2'
						   When Detalle.CodCargo = 121 THEN 'ENTRENAMIENTO'
						   When Detalle.CodCargo = 122 THEN 'JUNIOR 1'
						   When Detalle.CodCargo = 123 THEN 'JUNIOR 2'
						   When Detalle.CodCargo = 125 THEN 'GRUPO SOLIDARIO'
                      End
      ,Detalle.Salario_Base
      ,(Detalle.Incentivo_Base * Detalle.Porc_Incentivo) AS Monto_Incentivo
      ,Detalle.Incentivo_Base AS SalBase_Incentivo
      ,Detalle.Porc_SalBasico
	  ,Detalle.Porc_Incentivo
      /* A - Saldo de Cartera */
      ,Sum(Detalle.Saldo_Cartera) AS Saldo_Cartera
      ,'A_Incentivo_Cartera' = (Select (ir.INCENTIVO/100.00) * (ip.Peso)  * (Detalle.Incentivo_Base) * (Detalle.Porc_Incentivo)
								   From dbo.INC_RANGOS ir Inner Join dbo.INC_PESO ip ON ir.CodVariable = ip.CodVariable And ir.CodRegion =ip.CodRegion And ir.CodCargo = ip.CodCargo
														  Inner JOIN dbo.INC_VARIABLES_INDICADORES iv ON ir.CodVariable = iv.CodVariable
								  Where ir.CodVariable = 'A'
								    And ip.Tz_lock     = 0
								    And ir.Tz_lock     = 0
								    And iv.Tz_lock     = 0
								    And ir.CodRegion   = Detalle.Id_Region
							        And ir.CodCargo    = Detalle.CodCargo
							        And (Sum(Detalle.Saldo_Cartera) >= ir.MINIMO
							        And Sum(Detalle.Saldo_Cartera)  <= ir.MAXIMO) )

      /* B - Incentivo Numero de Clientes Activos */
      ,COUNT(DISTINCT CodCliente) AS Numero_Clientes_Activos
      ,'B_Incentivo_Numero_Clientes_Activos' = (Select (ir.INCENTIVO/100.00) * (ip.Peso)  * (Detalle.Incentivo_Base) * (Detalle.Porc_Incentivo)
												  From dbo.INC_RANGOS ir Inner Join dbo.INC_PESO ip ON ir.CodVariable = ip.CodVariable And ir.CodRegion =ip.CodRegion And ir.CodCargo = ip.CodCargo
														  Inner JOIN dbo.INC_VARIABLES_INDICADORES iv ON ir.CodVariable = iv.CodVariable
												  Where ir.CodVariable = 'B'
													And ip.Tz_lock     = 0
													And ir.Tz_lock     = 0
													And iv.Tz_lock     = 0
													And ir.CodRegion   = Detalle.Id_Region
													And ir.CodCargo    = Detalle.CodCargo
													And Count(DISTINCT CodCliente) >= ir.MINIMO
													And Count(DISTINCT CodCliente) <= ir.MAXIMO)
     ,Sum(Total_Colocaciones) AS Total_Colocaciones

      /* C - Colocaciones de Creditos Nuevos */
     ,Sum(Nuevas_Colocaciones) AS Colocaciones_Creditos_Nuevos

     ,'C_Incentivo_Colocaciones_Creditos_Nuevos' = (Select (ir.INCENTIVO/100.00) * (ip.Peso)  * (Detalle.Incentivo_Base) * (Detalle.Porc_Incentivo)
												  From dbo.INC_RANGOS ir Inner Join dbo.INC_PESO ip ON ir.CodVariable = ip.CodVariable And ir.CodRegion =ip.CodRegion And ir.CodCargo = ip.CodCargo
														  Inner JOIN dbo.INC_VARIABLES_INDICADORES iv ON ir.CodVariable = iv.CodVariable
												  Where ir.CodVariable = 'C'
													And ip.Tz_lock     = 0
													And ir.Tz_lock     = 0
													And iv.Tz_lock     = 0
													And ir.CodRegion   = Detalle.Id_Region
													And ir.CodCargo    = Detalle.CodCargo
													And Sum(Nuevas_Colocaciones)  >= ir.MINIMO
													And Sum(Nuevas_Colocaciones)  <= ir.MAXIMO)

      ,Sum(Represtamos) AS Colocaciones_Creditos_Represtamos
      /* D - Colocaciones de Créditos Represtamos */
      ,'D_Incentivo_Colocaciones_Creditos_Represtamos' =   (Select (ir.INCENTIVO/100.00) * (ip.Peso)  * (Detalle.Incentivo_Base) * (Detalle.Porc_Incentivo)
															  From dbo.INC_RANGOS ir Inner Join dbo.INC_PESO ip ON ir.CodVariable = ip.CodVariable And ir.CodRegion =ip.CodRegion And ir.CodCargo = ip.CodCargo
																	  Inner JOIN dbo.INC_VARIABLES_INDICADORES iv ON ir.CodVariable = iv.CodVariable
															  Where ir.CodVariable = 'D'
																And ip.Tz_lock     = 0
																And ir.Tz_lock     = 0
																And iv.Tz_lock     = 0
																And ir.CodRegion   = Detalle.Id_Region
																And ir.CodCargo    = Detalle.CodCargo
																And Sum(Represtamos) >= ir.MINIMO
		 														And Sum(Represtamos) <= ir.MAXIMO)


      /* E -  Estructura de Cartera, No se tiene definido actualmente los parametros
      *  que permitan analizar este incentivo, todos los oficiales ganan el 100%   */

      ,100.00 AS Estructura_Cartera
      ,'E_Incentivo_Estructura_Cartera' = Case When Detalle.CodCargo = 125 Then 0.00 Else
      (Select (ir.INCENTIVO/100.00) * (ip.Peso)  * (Detalle.Incentivo_Base) * (Detalle.Porc_Incentivo)
		From dbo.INC_RANGOS ir Inner Join dbo.INC_PESO ip ON ir.CodVariable = ip.CodVariable And ir.CodRegion =ip.CodRegion And ir.CodCargo = ip.CodCargo
				               Inner JOIN dbo.INC_VARIABLES_INDICADORES iv ON ir.CodVariable = iv.CodVariable
		Where ir.CodVariable = 'E'
		And ip.Tz_lock     = 0
		And ir.Tz_lock     = 0
		And iv.Tz_lock     = 0
		And ir.CodRegion   = Detalle.Id_Region
		And ir.CodCargo    = Detalle.CodCargo
		And 100 >= ir.MINIMO
		And 100 <= ir.MAXIMO)
        End
      /* F - Indice de Cartera en Riesgo */
      ,(Sum(Detalle.CER1) / Sum(Detalle.Saldo_MO)) * 100 AS Porcentaje_CER1

      ,'Porc_Mora_Deducible' =  (Select ((ir.INCENTIVO/100.00) * IIF((ip.Peso)=0,1,(ip.Peso))) * 100
									From dbo.INC_RANGOS ir Inner Join dbo.INC_PESO ip ON ir.CodVariable = ip.CodVariable And ir.CodRegion =ip.CodRegion And ir.CodCargo = ip.CodCargo
														   Inner JOIN dbo.INC_VARIABLES_INDICADORES iv ON ir.CodVariable = iv.CodVariable
									Where ir.CodVariable = 'F'
										And ip.Tz_lock     = 0
										And ir.Tz_lock     = 0
										And iv.Tz_lock     = 0
										And ir.CodRegion   = Detalle.Id_Region
										And ir.CodCargo    = Detalle.CodCargo
										And ((Sum(Detalle.CER1) / Sum(Detalle.Saldo_MO))* 100.00) >= ir.MINIMO
		 								And ((Sum(Detalle.CER1) / Sum(Detalle.Saldo_MO))* 100.00) <= ir.MAXIMO)
        ,Detalle.Usr_Gerente_Regional
FROM (
SELECT u.Iniciales AS Usuario
       ,egp.Fecha_Saldo        AS Fecha_Saldo
	   ,egp.Fecha_Desembolso   AS Fecha_Desembolso
	   ,egp.Codigo_Cliente     AS CodCliente
       ,egp.Nro_Prestamo       AS No_Prestamo
       ,trs.CodRegion          AS Id_Region
       ,Region                 AS Nombre_Region
       ,egp.Sucursal_Prestamo  AS CodSucursal
       ,ts.C6020               AS Sucursal
       ,e.Nombre               AS Oficial_Credito
       ,cr.NroDocumento        AS Cedula_Oficial
       ,e.CodCargo             AS CodCargo
       ,c.Descripcion          AS Des_Cargo
       ,e.SalarioMensual       AS Salario_Base
       ,egp.Codigo_Moneda      AS Moneda
       ,Importe_Desembolso  = Importe_Desembolso_MO
       ,Saldo_Cartera       = Saldo_MO
       ,'Total_Colocaciones' = Case When Fecha_Desembolso Between @FechaInicio And @FechaCorte Then 1 Else 0 End

       ,'Nuevas_Colocaciones'= Case When (Select Count(Cli.Cliente)
	 				                       From (Select ca.customerid Cliente
											   From [dbo].cuaccount ca
												  Where ca.customerid = egp.Codigo_Cliente
												    And ca.parentaccountid Is Null
													And MONTH(egp.Fecha_Desembolso) = MONTH(@FechaCorte)
													And YEAR(egp.Fecha_Desembolso)  = YEAR(@FechaCorte)
						      Union All
										  Select s2.C1803 Cliente
											From Saldos s2
										   Where s2.C1803 = egp.Codigo_Cliente
										     And s2.C9314 = 5
											 And MONTH(egp.Fecha_Desembolso) = MONTH(@FechaCorte)
											 And YEAR(egp.Fecha_Desembolso) = YEAR(@FechaCorte)
											 And Not Exists (Select sre.SALDOSJTSOIDEXT FROM SL_RASTROS_EXTORNOS sre Where sre.SALDOSJTSOIDEXT = s2.JTS_OID)
											 And s2.Fecha_Migracion <> '30/04/2014') Cli) = 1 Then 1 Else 0 End
    ,'Represtamos'= Case When (Select Count(Cli.Cliente)
					          From(Select ca.customerid Cliente
						             From [dbo].cuaccount ca
						            Where ca.Customerid = egp.Codigo_Cliente
						              And ca.parentaccountid Is Null
						              And MONTH(egp.Fecha_Desembolso) = MONTH(@FechaCorte)
						              And YEAR(egp.Fecha_Desembolso)  = YEAR(@FechaCorte)
						  Union All
								  Select s2.C1803 Cliente
									From Saldos s2
								   Where s2.C1803 = egp.Codigo_Cliente
									 And S2.C9314 = 5
									 And MONTH(egp.Fecha_Desembolso) = MONTH(@FechaCorte)
									 And YEAR(egp.Fecha_Desembolso)  = YEAR(@FechaCorte)
									 And Not Exists(Select sre.SaldosJTSOIDEXT
													  From Sl_Rastros_Extornos sre
													 Where sre.SaldosJTSOIDEXT = s2.JTS_OID)
                    								   And s2.Fecha_Migracion <> '30/04/2014') Cli) > 1 Then 1 Else 0 END
    ,egp.Saldo_MO
    ,CER1  = Case When egp.Dias_Atraso > 0  Then egp.Saldo_MO Else 0 End
	,ip.Porc_SalBasico
	,ip.Porc_Incentivo
	,ip.Incentivo_Base
	,'Usr_Gerente_Regional' = (Select ru.UsuarioTopaz
	                             From Au_RelFuncionariousr ru
                                 Inner Join Rh_Cargos rc ON ru.Codcargo = rc.Codcargo
                                 Inner Join Au_RelFuncionarioAgencia tr ON ru.UsuarioTopaz = tr.UsuarioTopaz
                                 And tr.CodAgencia   = egp.Sucursal_Prestamo
							   Where rc.CodCargo = 21
								 And rc.TZ_LOCK  = 0
								 And ru.TZ_LOCK  = 0
								 AND tr.TZ_LOCK  = 0)
FROM [dbo].[FN_EGP_FUNDESER_UNIVERSO] (@FechaCorte) egp
      INNER JOIN RH_FUNCIONARIO rf            ON egp.Codigo_Analista  = rf.CodFuncionario
	  INNER JOIN CL_RELPERDOC cr              ON cr.IdPersona         = rf.IdPersona
      INNER JOIN VINC_NOMINA e       ON (REPLACE(e.Cedula,'-' ,'')) collate Modern_Spanish_CI_AS IN (ISNULL(cr.NRODOCUMENTO,''))
      INNER JOIN VINC_NOMINA_CARGOS c         ON e.CodCargo           = c.CodCargo
      INNER JOIN TC_SUCURSALES ts             ON ts.C6021             = egp.Sucursal_Prestamo
      INNER JOIN TC_REL_REGION_SUCURSAL trs   ON trs.CodSucursal      = ts.C6021
      INNER JOIN TC_REGIONES tr               ON tr.CodRegion         = trs.CodRegion
      INNER JOIN INC_PARAMETROS_CARGOS ip     ON e.CodCargo           = ip.CodCargo And ip.CodRegion =tr.CodRegion AND egp.SUCURSAL_PRESTAMO = ip.CodSucursal
      INNER JOIN USUARIOS u                   ON Convert(varchar,rf.CodFuncionario)  = u.Clave
WHERE e.CodCargo          In (119,120,121,122,123,125)
  And rf.FUNCESTADO = 'A'
  And rf.TZ_LOCK    =  0
 -- AND egp.Sucursal_Prestamo  = @Sucursal
  And egp.Fecha_Saldo        = @FechaCorte
 --AND u.Iniciales            = @Usuario
  And Not Exists(Select sre.SALDOSJTSOIDEXT From SL_Rastros_Extornos sre WITH(NOLOCK) Where sre.SaldosJTSOIDEXT = egp.JTS_OID)
)Detalle
GROUP BY  Detalle.Usuario
         ,Detalle.Id_Region
         ,Detalle.Nombre_Region
         ,Detalle.CodSucursal
		 ,Detalle.Sucursal
		 ,Detalle.Oficial_Credito
		 ,Detalle.Cedula_Oficial
		 ,Detalle.CodCargo
		 ,Detalle.Des_Cargo
		 ,Detalle.Salario_Base
		 ,Detalle.Porc_SalBasico
	     ,Detalle.Porc_Incentivo
	     ,Detalle.Incentivo_Base
	     ,Detalle.Usr_Gerente_Regional
ORDER BY Detalle.Id_Region,Detalle.CodSucursal,Detalle.CodCargo]]>
	</queryString>
	<field name="Usuario" class="java.lang.String"/>
	<field name="Id_Region" class="java.math.BigDecimal"/>
	<field name="Nombre_Region" class="java.lang.String"/>
	<field name="CodSucursal" class="java.math.BigDecimal"/>
	<field name="Sucursal" class="java.lang.String"/>
	<field name="Oficial_Credito" class="java.lang.String"/>
	<field name="Cedula_Oficial" class="java.lang.String"/>
	<field name="CodCargo" class="java.lang.Integer"/>
	<field name="Des_Cargo" class="java.lang.String"/>
	<field name="Salario_Base" class="java.math.BigDecimal"/>
	<field name="Monto_Incentivo" class="java.math.BigDecimal"/>
	<field name="SalBase_Incentivo" class="java.math.BigDecimal"/>
	<field name="Porc_SalBasico" class="java.math.BigDecimal"/>
	<field name="Porc_Incentivo" class="java.math.BigDecimal"/>
	<field name="Saldo_Cartera" class="java.math.BigDecimal"/>
	<field name="A_Incentivo_Cartera" class="java.math.BigDecimal"/>
	<field name="Numero_Clientes_Activos" class="java.lang.Integer"/>
	<field name="B_Incentivo_Numero_Clientes_Activos" class="java.math.BigDecimal"/>
	<field name="Total_Colocaciones" class="java.lang.Integer"/>
	<field name="Colocaciones_Creditos_Nuevos" class="java.lang.Integer"/>
	<field name="C_Incentivo_Colocaciones_Creditos_Nuevos" class="java.math.BigDecimal"/>
	<field name="Colocaciones_Creditos_Represtamos" class="java.lang.Integer"/>
	<field name="D_Incentivo_Colocaciones_Creditos_Represtamos" class="java.math.BigDecimal"/>
	<field name="Estructura_Cartera" class="java.math.BigDecimal"/>
	<field name="E_Incentivo_Estructura_Cartera" class="java.math.BigDecimal"/>
	<field name="Porcentaje_CER1" class="java.math.BigDecimal"/>
	<field name="Porc_Mora_Deducible" class="java.math.BigDecimal"/>
	<field name="Usr_Gerente_Regional" class="java.lang.String"/>
	<variable name="Cartera_Riesgo" class="java.lang.Number">
		<variableExpression><![CDATA[($F{Porc_Mora_Deducible}==1?($F{A_Incentivo_Cartera}+ $F{B_Incentivo_Numero_Clientes_Activos} + $F{C_Incentivo_Colocaciones_Creditos_Nuevos}+ $F{D_Incentivo_Colocaciones_Creditos_Represtamos} + $F{E_Incentivo_Estructura_Cartera})*($F{Porc_Mora_Deducible}*-1):
    ($F{Porc_Mora_Deducible}==0.70?($F{A_Incentivo_Cartera}+ $F{B_Incentivo_Numero_Clientes_Activos} + $F{C_Incentivo_Colocaciones_Creditos_Nuevos}+ $F{D_Incentivo_Colocaciones_Creditos_Represtamos} + $F{E_Incentivo_Estructura_Cartera})*($F{Porc_Mora_Deducible}*-1):
        ($F{Porc_Mora_Deducible}==0.35?($F{A_Incentivo_Cartera}+ $F{B_Incentivo_Numero_Clientes_Activos} + $F{C_Incentivo_Colocaciones_Creditos_Nuevos}+ $F{D_Incentivo_Colocaciones_Creditos_Represtamos} + $F{E_Incentivo_Estructura_Cartera})*($F{Porc_Mora_Deducible}*-1):0.00)))]]></variableExpression>
	</variable>
	<variable name="Total_Incentivos" class="java.lang.Number">
		<variableExpression><![CDATA[$F{A_Incentivo_Cartera}+$F{B_Incentivo_Numero_Clientes_Activos}+$F{C_Incentivo_Colocaciones_Creditos_Nuevos}+$F{D_Incentivo_Colocaciones_Creditos_Represtamos}+ $F{E_Incentivo_Estructura_Cartera}+$V{Cartera_Riesgo}]]></variableExpression>
		<initialValueExpression><![CDATA[]]></initialValueExpression>
	</variable>
	<variable name="Total_Salario_Incentivo" class="java.lang.Number" resetType="None">
		<variableExpression><![CDATA[]]></variableExpression>
		<initialValueExpression><![CDATA[]]></initialValueExpression>
	</variable>
	<variable name="SalBase40" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{Salario_Base}*0.40]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
	<variable name="SalBase60" class="java.lang.Number">
		<variableExpression><![CDATA[$F{Salario_Base}*0.60]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
	<variable name="Salario_Base_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{Salario_Base}]]></variableExpression>
	</variable>
	<variable name="Saldo_Cartera_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{Saldo_Cartera}]]></variableExpression>
	</variable>
	<variable name="Numero_Clientes_Activos_1" class="java.lang.Integer" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{Numero_Clientes_Activos}]]></variableExpression>
	</variable>
	<variable name="Colocaciones_Creditos_Nuevos_1" class="java.lang.Integer" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{Colocaciones_Creditos_Nuevos}]]></variableExpression>
	</variable>
	<variable name="Colocaciones_Creditos_Represtamos_1" class="java.lang.Integer" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{Colocaciones_Creditos_Represtamos}]]></variableExpression>
	</variable>
	<variable name="Porcentaje_CER1_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{Porcentaje_CER1}]]></variableExpression>
	</variable>
	<variable name="A_Incentivo_Cartera_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{A_Incentivo_Cartera}]]></variableExpression>
	</variable>
	<variable name="B_Incentivo_Numero_Clientes_Activos_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{B_Incentivo_Numero_Clientes_Activos}]]></variableExpression>
	</variable>
	<variable name="C_Incentivo_Colocaciones_Creditos_Nuevos_1" class="java.math.BigDecimal" resetType="Group" resetGroup="Sucursal" calculation="Sum">
		<variableExpression><![CDATA[$F{C_Incentivo_Colocaciones_Creditos_Nuevos}]]></variableExpression>
