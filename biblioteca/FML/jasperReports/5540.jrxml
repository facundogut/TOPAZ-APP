	<subDataset name="DataDireccion">
		<parameter name="NCLI" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT  d2.C1207 AS Departamento, d3.C1216 AS Municipio, d4.NOMDIM4 AS Barrio,d.REFERENCIA AS Direccion
FROM  TC_DIRECCIONES d, TC_DIR_DIM2 d2, TC_DIR_DIM3 d3, TC_DIR_DIM4 d4
WHERE d.ID=$P{NCLI} AND d.FORMATO='C' AND d.TIPODIRECCION='L'
AND d2.C1205=d.DIM1 AND d2.C1206=d.DIM2
AND d3.C1212=d.DIM1 AND d3.C1214=d.DIM2 AND d3.C1215=d.DIM3
AND d4.CODDIM1=d.DIM1 AND d4.CODDIM2=d.DIM2 AND d4.CODDIM3=d.DIM3 AND d4.CODDIM4=d.DIM4]]>
		</queryString>
		<field name="Departamento" class="java.lang.String"/>
		<field name="Municipio" class="java.lang.String"/>
		<field name="Barrio" class="java.lang.String"/>
		<field name="Direccion" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataDocumento">
		<parameter name="NCLI" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT TOP 1
	CASE c.C1040
		WHEN 'S' THEN
			(SELECT rel.NRODOCUMENTO FROM CL_RELGSCLTE rg, CL_RELPERDOC rel
			WHERE c.C0902=rg.C3198 AND rg.C3210 = 'AC' AND rg.C3201='S'
			AND rel.IDPERSONA=rg.C3199 AND rel.PRINCIPAL='S')
		ELSE
			(SELECT rel.NRODOCUMENTO FROM CL_RELPERDOC rel
			WHERE c.C0902=rel.IDPERSONA)
	END	AS DOCUMENTO
FROM CL_CLIENTES c WHERE c.C0902=$P{NCLI}]]>
		</queryString>
		<field name="DOCUMENTO" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataPrestamo">
		<parameter name="NSOL" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C5002 AS NPRESTAMO FROM SL_SOLICITUDCREDITO WHERE C5000=$P{NSOL}]]>
		</queryString>
		<field name="NPRESTAMO" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="DataSucursal">
		<parameter name="NSUC" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6020 AS NOMSUC FROM TC_SUCURSALES
WHERE C6021=$P{NSUC}]]>
		</queryString>
		<field name="NOMSUC" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataRUC">
		<queryString language="SQL">
			<![CDATA[SELECT TC_VARCHAR2 AS RUC FROM TC_PARAMETROS WHERE TC_CODIGO=29]]>
		</queryString>
		<field name="RUC" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataTexto">
		<parameter name="MONTO" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT ((dbo.NumeroALetras ($P{MONTO}))+'-----------------') AS TEXT]]>
		</queryString>
		<field name="TEXT" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataMoneda">
		<parameter name="NROSOL" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT C6401 AS SIMBOLOMONEDA  FROM CO_MONEDAS
WHERE C6399 = (SELECT MONEDA FROM SL_SOLICITUDCREDITO WHERE C5000=$P{NROSOL})]]>
		</queryString>
		<field name="SIMBOLOMONEDA" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DataNombre">
		<parameter name="NCLI" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT c.C1000 AS Nombre
FROM CL_CLIENTES c
WHERE  c.C0902=$P{NCLI}]]>
		</queryString>
		<field name="Nombre" class="java.lang.String"/>
	</subDataset>
