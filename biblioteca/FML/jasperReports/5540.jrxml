	<queryString>
		<![CDATA[SELECT
	CASE s.GRUPO
		WHEN 0 THEN (SELECT C1000 FROM CL_CLIENTES WHERE C0902=s.C5187)
		ELSE (SELECT C1000 FROM CL_CLIENTES WHERE C0902=sol.C5082)
	END AS Nombre, s.C5036, s.C5187 AS NCLIENTE, s.MODOCOBROCARGOS AS MCCARGO, LugarDesemb AS SUCPAGO, C5023 AS MONTOSOLICITADO,
	CASE s.GRUPO
		WHEN 0 THEN s.C5023
		ELSE sol.C5088
	END AS MONTOSOLICITADOPERSONA
FROM SL_SOLICITUDCREDITO s FULL OUTER JOIN SL_SOLICITUDCREDITOPERSONA sol ON (s.C5000=sol.C5080 AND sol.C5084=802)
WHERE s.C5000=$P{NROSOLICITUD}]]>
	</queryString>
