	<subDataset name="ADICIONALES" uuid="87c7670f-ffef-4cb6-8a73-ac7c03772941">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Data Adapter NBCH"/>
		<parameter name="ID_TARJETA" class="java.lang.String"/>
		<parameter name="JTS_OID" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT
P.C6251 AS TIPO_PRODUCTO,
R.NRO_CUENTA,
O.DESCRIPCION AS ESTADO,
CASE WHEN T.ESTADO = 'X' THEN 'S' ELSE 'N' END AS BAJA
--,
--T.OBSERVACIONES
FROM TJD_SOLICITUD_CUENTAS_LINK AS R
INNER JOIN VTA_SALDOS AS V ON R.NUMERO_CUENTA_PBF = V.CTA_REDLINK 
AND V.CTA_REDLINK != '' AND V.CTA_REDLINK != '0' AND V.TZ_LOCK = 0
INNER JOIN SALDOS AS S ON V.JTS_OID_SALDO = S.JTS_OID AND S.TZ_LOCK = 0
INNER JOIN TJD_SOLICITUD_LINK AS T ON R.NRO_TARJETA = T.NRO_TARJETA_BASE AND T.TZ_LOCK = 0
LEFT JOIN PRODUCTOS AS P ON S.PRODUCTO = P.C6250 AND P.TZ_LOCK = 0
LEFT JOIN OPCIONES AS O ON O.OPCIONINTERNA = T.ESTADO AND O.NUMERODECAMPO = 34812 AND O.IDIOMA = 'E'
WHERE R.TZ_LOCK = 0 AND T.NRO_TARJETA_BASE = $P{ID_TARJETA} AND S.JTS_OID <> $P{JTS_OID}]]>
		</queryString>
		<field name="TIPO_PRODUCTO" class="java.lang.String"/>
		<field name="NRO_CUENTA" class="java.math.BigDecimal"/>
		<field name="ESTADO" class="java.lang.String"/>
		<field name="BAJA" class="java.lang.String"/>
	</subDataset>
	<parameter name="TOPAZ_SUCURSAL_SISTEMA" class="java.lang.Integer" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<parameter name="JTS_OID" class="java.math.BigDecimal">
		<parameterDescription><![CDATA[JTS OID:]]></parameterDescription>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA[java.lang.System.getProperty("Topaz.reportesJR")]]></defaultValueExpression>
	</parameter>
	<parameter name="CTA_REDLINK" class="java.lang.String">
		<parameterDescription><![CDATA[Cuenta Redlink:]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT
S.C1803 AS CLIENTE,
SU.NOMBRESUCURSAL AS NOM_SUCURSAL_SISTEMA,
R.NRO_TARJETA,
T.FECHA_SOLICITUD,
R.SUCURSAL,
R.NRO_CUENTA,
P.C6251 AS TIPO_PRODUCTO,
LT.LIMITE AS LIMITE_DEBITO,
concat(T.NOMBRE, ' ', T.APELLIDO) AS NOMBRE_TARJETA,
DO.NUMERODOCUMENTO,
PF.FECHANACIMIENTO,
TJDIR.CALLE,
TJDIR.NUMERO AS NUMERO_DIR,
TJDIR.DESC_LOCALIDAD AS LOCALIDAD,
TJDIR.CODIGO_POSTAL AS CPA_NUEVO,
TJDIR.TELEFONO AS TEL_FIJO,
NULL AS CELULAR,
T.NRO_TARJETA_BASE,
S.JTS_OID,
OC.DESCRIPCION AS TIPO_COMISION
FROM SALDOS AS S WITH (nolock)

INNER JOIN TJD_SOLICITUD_CUENTAS_LINK AS R WITH (nolock) ON $P{CTA_REDLINK} = R.NUMERO_CUENTA_PBF AND R.TZ_LOCK = 0 AND $P{CTA_REDLINK} != '0' AND $P{CTA_REDLINK} != ''
INNER JOIN TJD_SOLICITUD_LINK T WITH (nolock) ON R.ID_SOLICITUD = T.ID_SOLICITUD AND T.TIPO_SOLICITUD IN ('120005','120006') AND T.TZ_LOCK = 0
LEFT JOIN TJD_LIMITES_LINK AS LT WITH (nolock) ON T.LIMITE_DEBITO = LT.CODIGO AND LT.TZ_LOCK = 0
LEFT JOIN TJD_COBRO_COMISIONES AS C WITH (nolock) ON R.ID_SOLICITUD = C.ID_SOLICITUD AND C.TZ_LOCK = 0
AND C.JTS_OID = (SELECT TOP 1 JTS_OID FROM TJD_COBRO_COMISIONES WITH (nolock) WHERE ID_SOLICITUD = C.ID_SOLICITUD)
LEFT JOIN OPCIONES AS OC WITH (nolock) ON C.TIPO_COMISION = OC.OPCIONINTERNA AND OC.NUMERODECAMPO = 34960 AND OC.IDIOMA = 'E'
LEFT JOIN SUCURSALES SU WITH (nolock) ON $P{TOPAZ_SUCURSAL_SISTEMA} = SU.SUCURSAL AND SU.TZ_LOCK = 0
LEFT JOIN PRODUCTOS AS P WITH (nolock) ON S.PRODUCTO = P.C6250 AND P.TZ_LOCK = 0
LEFT JOIN CLI_DOCUMENTOSPFPJ AS DO WITH (nolock) ON T.NRO_PERSONA = DO.NUMEROPERSONAFJ AND DO.TZ_LOCK = 0
LEFT JOIN CLI_PERSONASFISICAS AS PF WITH (nolock) ON T.NRO_PERSONA = PF.NUMEROPERSONAFISICA AND PF.TZ_LOCK = 0
LEFT JOIN TJD_SOLICITUD_DIRECCIONES_LINK TJDIR WITH (nolock) ON T.ID_SOLICITUD = TJDIR.ID AND T.TIPO_DOMICILIO = TJDIR.TIPO_DIRECCION 
WHERE S.TZ_LOCK = 0 
AND S.JTS_OID = $P{JTS_OID}]]>
	</queryString>
