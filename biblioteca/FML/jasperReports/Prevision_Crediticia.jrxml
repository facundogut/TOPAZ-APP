	<queryString>
		<![CDATA[SELECT
				c.C0902 AS Nro_Cliente,
				c.C1000 AS Nombre_Cliente,
				c.CalifAlin as Calificacion_Alineada,
				s.MONEDA,
				s.C1730 AS Rubro_Contable,
				s.criterio AS Tipo_Credito,
				s.Producto,
				s.CUENTA AS Nro_Pmo,
				p.SALDO_CAPITAL,
				p.SALDO_INTERES_ACTIVO,
				p.MONTO_TOTAL_GTIA_CAPITAL,
				p.MONTO_TOTAL_GTIA_INTERES,
				p.RESERVA_BASICA_CAPITAL,
				p.RESERVA_BASICA_INTERES,
				(CASE WHEN p.RESERVA_X_DESCUBIERTO_CAPITAL+p.RESERVA_X_DESCUBIERTO_INTERES>p.RESERVA_MINIMA_X_CAPITAL+p.RESERVA_MINIMA_X_INTERES THEN p.RESERVA_X_DESCUBIERTO_CAPITAL ELSE 0 END) AS RES_CONT_X_DESCUBIERTO_CAPITAL,
				(CASE WHEN p.RESERVA_X_DESCUBIERTO_CAPITAL+p.RESERVA_X_DESCUBIERTO_INTERES>p.RESERVA_MINIMA_X_CAPITAL+p.RESERVA_MINIMA_X_INTERES THEN p.RESERVA_X_DESCUBIERTO_INTERES ELSE 0 END) AS RES_CONT_X_DESCUBIERTO_INTERES,
				(CASE WHEN p.RESERVA_X_DESCUBIERTO_CAPITAL+p.RESERVA_X_DESCUBIERTO_INTERES<p.RESERVA_MINIMA_X_CAPITAL+p.RESERVA_MINIMA_X_INTERES THEN p.RESERVA_MINIMA_X_CAPITAL ELSE 0 END) AS RES_CONT_MINIMA_CAPITAL,
				(CASE WHEN p.RESERVA_X_DESCUBIERTO_CAPITAL+p.RESERVA_X_DESCUBIERTO_INTERES<p.RESERVA_MINIMA_X_CAPITAL+p.RESERVA_MINIMA_X_INTERES THEN p.RESERVA_MINIMA_X_INTERES ELSE 0 END) AS RES_CONT_MINIMA_INTERES,
				p.FECHA_CONTABILIZADO
				FROM SALDOS s, CL_CLIENTES c, HISTORICO_CALCULO_PREVISIONES p
				WHERE s.C1803=c.C0902
				AND s.jts_oid=p.SALDOS_JTS_OID
				AND p.FECHA_PROCESO=(SELECT FECHAPROCESO FROM PARAMETROS)
				ORDER BY c.C0902, s.CRITERIO, s.c1730]]>
	</queryString>
