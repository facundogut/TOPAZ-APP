	<queryString>
		<![CDATA[SELECT DISTINCT
-- CAMPOS CABEZAL
	S.SUCURSAL,
	SUC.NOMBRESUCURSAL,
	A.FECHAPROCESO AS FECHAASIENTO,
--
	PS.CERTIFICADO_DPF AS CERTIFICADODPF, 
	P.C6250 AS CODPROD,
	P.C6251 AS DESCPROD,
	CASE WHEN (P.C6250) = (SELECT NUMERICO FROM PARAMETROSGENERALES WHERE CODIGO = 707) 
		THEN 'Valor Adquisitivo Act. por CER Precancelable- Ley 25.827 UVA'
	ELSE
		CASE WHEN (M.C6399) = (SELECT NUMERICO FROM PARAMETROSGENERALES WHERE CODIGO = 18) 
			THEN 'Valor Adquisitivo Actualizables por "CER" Ley 25.827 ("UVA")'
				WHEN (M.C6399) = (SELECT NUMERICO FROM PARAMETROSGENERALES WHERE CODIGO = 709)
			THEN  'VIVIENDA actualizables por "ICC" - Ley 27.271 ("UVI")'
				ELSE (SELECT C6251 FROM PRODUCTOS PPrd WHERE PPrd.C6250 = P.C6250)
		END 
	END AS TIPO,
	S.CUENTA,
	VTA.NOMBRE_CUENTA AS NOMBRECUENTA,
	S.C1620 AS FECHAALTA,
	S.C1627 AS FECHAVTO, 
	DATEDIFF(day, S.C1621, S.C1627) AS PLAZO,
	S.C1728 AS ESTADO,
	S.C1601 AS CAPITAL,
	PP.C2310 AS INTERESPENDIENTE
FROM PRODUCTOS P, SALDOS S, PLANPAGOS PP, PZO_SALDOS PS, MONEDAS M, 
	SUCURSALES SUC, BS_HISTORIA_PLAZO HIS, ASIENTOS A, VTA_SALDOS VTA
WHERE S.JTS_OID = PP.SALDO_JTS_OID
	AND S.JTS_OID = PS.JTS_OID_SALDO
	AND S.PRODUCTO = P.C6250
	AND S.C1785 = 4
	AND P.C6252 = 4
	AND P.C6259 = 'P'
	AND S.MONEDA = M.C6399
	AND S.SUCURSAL = SUC.SUCURSAL
	AND S.JTS_OID = HIS.SALDOS_JTS_OID
	AND HIS.SUCURSALMOV = A.SUCURSAL
	AND HIS.FECHAPROCESOMOV = A.FECHAPROCESO
	AND HIS.NROASIENTOMOV = A.ASIENTO
	AND S.C1665 = VTA.JTS_OID_SALDO
	AND S.TZ_LOCK = 0
	AND PP.TZ_LOCK = 0
	AND P.TZ_LOCK = 0
	AND PS.TZ_LOCK = 0
	AND M.TZ_LOCK = 0
	AND SUC.TZ_LOCK = 0
	AND HIS.TZ_LOCK = 0
	AND VTA.TZ_LOCK = 0
	AND (S.SUCURSAL = $P{SUCURSAL} OR $P{SUCURSAL} = -1)
	AND (A.FECHAPROCESO BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA})
	ORDER BY S.SUCURSAL]]>
	</queryString>
