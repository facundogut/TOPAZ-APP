    <rowlimit>0</rowlimit>
    <sql> 
DECLARE @FECHA_PARAMETRO AS DATETIME
SET @FECHA_PARAMETRO = CONVERT(DATETIME,?,103);
 
DECLARE @CANT_DIAS_MES AS INT
SET @CANT_DIAS_MES = DAY(EOMONTH(convert(datetime,@FECHA_PARAMETRO,103)))   
 
DECLARE @FECHA_PARAMETRO_MES_ANTERIOR AS DATETIME
SET @FECHA_PARAMETRO_MES_ANTERIOR = DATEADD(month, -1, @FECHA_PARAMETRO)
 
 
SELECT
	@FECHA_PARAMETRO AS FECHA, 
	CASE
		WHEN A.CATEGORIA = 1 THEN
			CASE WHEN A.UBICACION = 'E'
				THEN 831002001
				ELSE 831001001
			END
		WHEN A.CATEGORIA = 2 THEN
			CASE WHEN A.UBICACION = 'E'
				THEN 832002001
				ELSE 832001001
			END
	END AS PARTIDA,
	A.MONEDA AS MONEDA, 
	A.NRO_ATM AS ATM, 
	A.SUCURSAL_UBICACION AS SUCURSAL, 
	A.CATEGORIA AS GRUPO_CAT, 
	A.UBICACION AS UBICACION,
	isnull(AC.COEF_SUCURSAL,0) AS PONDERADOR,
	isnull(AH.RETIROS, 0) AS DISPENSADO,
	@CANT_DIAS_MES AS DIAS,
	((isnull(AH.RETIROS, 0))/@CANT_DIAS_MES) AS PROMEDIO,
	(((isnull(AH.RETIROS, 0))/@CANT_DIAS_MES)*(isnull(AC.COEF_SUCURSAL,0))) AS FRANQUICIA
FROM 
TABLA_ATM A WITH(NOLOCK)
LEFT JOIN SALDOSATM_HISTORICO AH WITH(NOLOCK)
   			ON AH.NRO_ATM = A.NRO_ATM 
   			AND AH.TZ_LOCK = 0 
LEFT JOIN CAJ_ATM_CATEGORIAS AC WITH(NOLOCK)
			ON AC.CATEGORIA = A.CATEGORIA
			AND AC.TZ_LOCK = 0
WHERE A.TZ_LOCK = 0 
AND A.ESTADO = 'H'
AND AH.FECHAPROCESO = @FECHA_PARAMETRO	
 
UNION
 
SELECT
	@FECHA_PARAMETRO AS FECHA, 
	CASE
		WHEN A.CATEGORIA = 3 THEN
			CASE WHEN A.UBICACION = 'E'
				THEN 833002001
				ELSE 833001001
			END
		WHEN (A.CATEGORIA = 4 OR A.CATEGORIA = 5 OR A.CATEGORIA = 6)  THEN
			CASE WHEN A.UBICACION = 'E'
				THEN 840002001
				ELSE 840001001
			END
	END AS PARTIDA,
	A.MONEDA AS MONEDA, 
	A.NRO_ATM AS ATM, 
	A.SUCURSAL_UBICACION AS SUCURSAL, 
	A.CATEGORIA AS GRUPO_CAT, 
	A.UBICACION AS UBICACION,
	isnull(AC.COEF_SUCURSAL,0) AS PONDERADOR,
	isnull(AH.RETIROS, 0) AS DISPENSADO,
	@CANT_DIAS_MES AS DIAS,
	((isnull(AH.RETIROS, 0))/@CANT_DIAS_MES) AS PROMEDIO,
	(((isnull(AH.RETIROS, 0))/@CANT_DIAS_MES)*(isnull(AC.COEF_SUCURSAL,0))) AS FRANQUICIA
FROM 
TABLA_ATM A WITH(NOLOCK)
LEFT JOIN SALDOSATM_HISTORICO AH WITH(NOLOCK)
   			ON AH.NRO_ATM = A.NRO_ATM 
   			AND AH.TZ_LOCK = 0 
LEFT JOIN CAJ_ATM_CATEGORIAS AC WITH(NOLOCK)
			ON AC.CATEGORIA = A.CATEGORIA
			AND AC.TZ_LOCK = 0
WHERE A.TZ_LOCK = 0 
AND A.ESTADO = 'H'
AND AH.FECHAPROCESO = @FECHA_PARAMETRO_MES_ANTERIOR </sql>
