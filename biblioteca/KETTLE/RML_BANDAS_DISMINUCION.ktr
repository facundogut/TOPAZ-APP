    <rowlimit>0</rowlimit>
    <sql>DECLARE @FECHA_PARAMETRO AS DATETIME;
SET @FECHA_PARAMETRO = CONVERT(DATETIME,?,103);
DECLARE @CANT_DIAS_MES AS INT
SET @CANT_DIAS_MES = DAY(EOMONTH(convert(datetime,@FECHA_PARAMETRO,103)));  
SELECT 
	@FECHA_PARAMETRO AS FECHA,
	MD.Suc_Producto AS SUCURSAL,
	MD.Operacion AS PRESTAMO,
	MD.Desglose AS DESGLOSE,
	MD.Cod_Moneda AS MONEDA,
	MD.Familia AS LINEA,
	MD.Fecha_Info AS FECHA_ASIENTO,
	CASE 
	    WHEN MD.Fecha_Alta_Oper IS NULL OR MD.Fecha_Vto IS NULL THEN 0
	    ELSE DATEDIFF(DAY, MD.Fecha_Alta_Oper, MD.Fecha_Vto)
	END AS PLAZO,
	MC.Cliente AS CUENTA,
	MC.Deuda_Total / @CANT_DIAS_MES AS PROMEDIO,
	MC.CUIT_CUIL_CDI AS CUIT,
	MC.Denominacion AS DENOMINACION,
	CASE 
                    WHEN MD.Cod_Moneda &lt;&gt; 1 THEN 
                        MD.Saldo_Capital * (
                            SELECT TOP 1 H.TIPO_CAMBIO_OFICIAL
                            FROM HISTORICOTIPOSCAMBIO H 
                            WHERE H.FECHA_COTIZACION &lt;= MD.Fecha_Info
                            ORDER BY H.FECHA_COTIZACION DESC
                        )
                    ELSE MD.Saldo_Capital
     END AS SALDO,
	CASE
		WHEN D.TIPOPERSONA = 'J'
			THEN 809000001
		WHEN D.TIPOPERSONA = 'F'
			THEN 808000001
	END AS PARTIDA,
	MC.Tamano_Emp AS TAMANIO
FROM MEMO_DETALLE MD WITH(NOLOCK)
INNER JOIN MEMO_CABECERA MC 
			ON MD.Fecha_Info=MC.Fecha_Info
            AND MD.Cliente=MC.Cliente
LEFT JOIN CLI_ClientePersona CP WITH(NOLOCK)
			ON CP.CODIGOCLIENTE = MD.Cliente
			AND CP.TITULARIDAD = 'T'
			AND CP.TZ_LOCK = 0
LEFT JOIN CLI_DocumentosPFPJ D WITH(NOLOCK)
			ON D.NUMEROPERSONAFJ = CP.NUMEROPERSONA 
			AND D.TZ_LOCK = 0
WHERE MD.Fecha_Info=@FECHA_PARAMETRO 
AND MC.Fecha_Info=@FECHA_PARAMETRO</sql>
