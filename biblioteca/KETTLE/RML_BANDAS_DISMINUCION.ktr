  <step>
    <name>AHORA 12</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazConect</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE @FECHA_PARAMETRO AS DATETIME;
SET @FECHA_PARAMETRO = CONVERT(DATETIME,?,103);
 
SELECT 
	@FECHA_PARAMETRO AS FECHA_INFORMACION,
	CASE
		WHEN MU.FECHA_ALTA_USU &lt;= ('20200930')
			THEN 817000001
		WHEN MU.FECHA_ALTA_USU BETWEEN '20201001' AND '20220131'
			THEN 836000001
		WHEN MU.FECHA_ALTA_USU BETWEEN '20220201' AND '20240524'
			THEN 854000001
	END AS PARTIDA,
	MA.DESCRIPCION AS ADMINISTRADORA,
	AA.NRO_USUARIO AS NRO_SUARIO,
	AA.SALDO_PROMEDIO AS SALDO,
	D.TIPODOCUMENTO AS TIPO_DOC,
	D.NUMERODOCUMENTO AS NURO_DOC,
	CASE 
		WHEN D.TIPOPERSONA = 'J' 
		THEN (SELECT PJ.RAZONSOCIAL FROM CLI_PERSONASJURIDICAS PJ WHERE PJ.NUMEROPERSONAJURIDICA = D.NUMEROPERSONAFJ AND PJ.TZ_LOCK = 0)
		ELSE (SELECT CONCAT(PF.PRIMERNOMBRE,' ',PF.APELLIDOPATERNO) FROM CLI_PERSONASFISICAS PF WHERE PF.NUMEROPERSONAFISICA = D.NUMEROPERSONAFJ AND PF.TZ_LOCK = 0)
	END AS NOMBRE_APELLIDO
FROM TJC_ADINTAR_AHORA12 AA WITH(NOLOCK)
LEFT JOIN TJC_MAESTRO_USUARIO MU WITH(NOLOCK)
			ON MU.USUARIO = AA.NRO_USUARIO
			AND MU.TZ_LOCK = 0
LEFT JOIN TJC_MAESTRO_ADMINISTRADORAS MA WITH(NOLOCK)
			ON MA.COD_ADMINISTRADORA = AA.ADMINISTRADORA
			AND MA.TZ_LOCK = 0
INNER JOIN CLI_ClientePersona CP WITH(NOLOCK)
			ON CP.CODIGOCLIENTE = MU.CLIENTE
			AND CP.TZ_LOCK = 0
INNER JOIN CLI_DocumentosPFPJ D WITH(NOLOCK)
			ON D.NUMEROPERSONAFJ = CP.NUMEROPERSONA
			AND D.TZ_LOCK = 0
WHERE AA.TZ_LOCK = 0 
AND AA.FECHA_INFORMAR = @FECHA_PARAMETRO</sql>
