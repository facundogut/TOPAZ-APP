      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>//Script here

function esFechaValida(fechaStr) {
    // Verificar el formato correcto usando una expresión regular que permita ceros opcionales
    const regexFecha = /^(\d{1,2})\/(\d{1,2})\/(\d{4})$/;
    if (!regexFecha.test(fechaStr)) {
        return false;
    }

    // Descomponer la fecha en día, mes y año
    const [ , diaStr, mesStr, anioStr ] = fechaStr.match(regexFecha);
    const dia = parseInt(diaStr, 10);
    const mes = parseInt(mesStr, 10) - 1; // Los meses en JavaScript son 0-11
    const anio = parseInt(anioStr, 10);

    // Crear un objeto de fecha
    const fecha = new Date(anio, mes, dia);

    // Verificar que la fecha es válida
    if (fecha.getFullYear() === anio &amp;&amp; 
        fecha.getMonth() === mes &amp;&amp; 
        fecha.getDate() === dia) {
        return true;
    } else {
        return false;
    }
}
</jsScript_script>
