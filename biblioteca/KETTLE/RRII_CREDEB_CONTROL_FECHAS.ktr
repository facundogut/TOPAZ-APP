	var dia = parseInt(diaStr, 10);
    var mes = parseInt(mesStr, 10) - 1; // Los meses en JavaScript son 0-11
    var anio = parseInt(anioStr, 10);
	var fechaParseada = new Date(anio,mes,dia);
	var esAnteriorAFechaActual = fechaParseada.getTime() &lt;= FECHA_EJECUCION.getTime();
