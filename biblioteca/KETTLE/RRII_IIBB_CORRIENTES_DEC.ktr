    <sql>--Fecha parametro
DECLARE @fecha_parametro DATE = CONVERT(DATETIME,?,103);

--Registro decenal IIBB CORRIENTES
SELECT DISTINCT

	DOC.NUMERODOCUMENTO AS NRO_DOCUMENTO,

	V.CTA_CBU AS CBU,
	
	YEAR(M.FECHACONTABLE) AS ANIO_INFORMADO,
	
	MONTH(M.FECHACONTABLE) AS MES_INFORMADO,
	
	CASE WHEN DAY(M.FECHACONTABLE) &lt;= 10 THEN
		1
	WHEN DAY(M.FECHACONTABLE) &lt;= 20 THEN
		2
	ELSE
		3
	END AS DECENA,
	
	F.MONEDA AS MONEDA,
	
	CASE WHEN AE.ASIENTO_ORIGINAL IS NULL THEN
		'+' 
	ELSE
		'-'
	END AS TIPO_OPERACION,
	
	CASE WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0) THEN
		FL.MONTO_IMPONIBLE 
	ELSE
		FL.MONTO_IMPONIBLE * ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	END AS BASE_IMPONIBLE,
	
	CASE WHEN AE.ASIENTO_ORIGINAL IS NULL THEN
		'+' 
	ELSE
		'-'
	END AS TIPO_OPERACION_1,
	
	CASE WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0) THEN
		FL.MONTO_FACTURABLE 
	ELSE
		FL.MONTO_FACTURABLE * ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	END AS RETENCION,
	
	M.FECHACONTABLE AS FECHA_COBRO,

	CP.CODIGOCLIENTE, 	

	ISNULL(pf.IIBB_CORRIENTES_ALI,pj.IIBB_CORRIENTES_ALI) AS IIBB_CORRIENTES_ALI

FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK)

INNER JOIN SALDOS S WITH(NOLOCK) ON 
	S.SUCURSAL = GDC.SALDO_ORIG_SUCURSAL
	AND S.PRODUCTO = GDC.SALDO_ORIG_PRODUCTO
	AND S.CUENTA = GDC.SALDO_ORIG_CUENTA
	AND S.MONEDA = GDC.SALDO_ORIG_MONEDA
	AND S.OPERACION = GDC.SALDO_ORIG_OPERACION
	AND S.ORDINAL = GDC.SALDO_ORIG_ORDINAL
	AND S.TZ_LOCK=0
	
INNER JOIN CI_FACTURA F WITH(NOLOCK) ON
	F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.ASIENTO = GDC.MOV_ASIENTO
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID
	AND F.TZ_LOCK=0
	
INNER JOIN CI_FACTURA_LINEA AS FL WITH (nolock) ON	
	FL.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO	
	AND FL.ASIENTO = F.ASIENTO	
	AND FL.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO	
	AND 
	(
		(FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 9
			AND TZ_LOCK=0)) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 9
			AND TZ_LOCK=0))
	)
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO = FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO = FL.ID_IMPUESTO)
	AND FL.TZ_LOCK=0 
		
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
	AND M.SUCURSAL = F.SUCURSAL_ASIENTO
	AND M.ASIENTO = F.ASIENTO
	AND M.DEBITOCREDITO = 'C'
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
			
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO=FL.ASIENTO AND
	A.FECHAPROCESO=FL.FECHA_PROCESO_ASIENTO AND
	A.SUCURSAL=FL.SUCURSAL_ASIENTO AND
	A.ESTADO=77

INNER JOIN VTA_SALDOS AS V WITH (nolock) ON
	V.JTS_OID_SALDO = S.JTS_OID 
	AND V.TZ_LOCK=0
	
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803
	AND C.TZ_LOCK=0
	
INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE AND 
	CP.TZ_LOCK=0
----=============================================================================================	

INNER JOIN CON_BITACORA_IMPUESTOS CBI WITH (NOLOCK) ON
	CBI.ID_CLIENTE=C.CODIGOCLIENTE AND
	CBI.TIPO_CARGO_IMPOSITIVO = 9 AND
	CBI.FECHA_INICIO = (SELECT MAX(FECHA_INICIO) FROM CON_BITACORA_IMPUESTOS 
						WHERE FECHA_INICIO &lt;= M.FECHACONTABLE AND
							CBI.ID_CLIENTE = ID_CLIENTE AND
							CBI.TIPO_CARGO_IMPOSITIVO = TIPO_CARGO_IMPOSITIVO AND 
							TZ_LOCK=0) AND
	CBI.JTS_NOVEDAD = (SELECT MAX(JTS_NOVEDAD) FROM CON_BITACORA_IMPUESTOS 
						WHERE CBI.ID_CLIENTE = ID_CLIENTE AND
							CBI.TIPO_CARGO_IMPOSITIVO = TIPO_CARGO_IMPOSITIVO AND
							CBI.FECHA_INICIO = FECHA_INICIO AND
							TZ_LOCK=0) AND
	CBI.TZ_LOCK=0

LEFT JOIN CLI_PERSONASFISICAS AS PF WITH(NOLOCK) ON  
	PF.NUMEROPERSONAFISICA = CBI.ID_PERSONA AND
	PF.TZ_LOCK=0

LEFT JOIN CLI_PERSONASJURIDICAS AS PJ WITH(NOLOCK) ON  
	PJ.NUMEROPERSONAJURIDICA = CP.NUMEROPERSONA AND
	PJ.TZ_LOCK=0
----=============================================================================================	
LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON
	DOC.TZ_LOCK=0
	AND DOC.NUMEROPERSONAFJ = CBI.ID_PERSONA
	AND DOC.TIPODOCUMENTO = (
						SELECT TOP 1 TIPODOCUMENTO
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							TZ_LOCK=0 AND	
							NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							TZ_LOCK=0 AND 
							NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ AND 
							TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
							)
							
							
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MI WITH (nolock) ON	
	MI.ASIENTO = F.ASIENTO	
	AND MI.SUCURSAL = F.SUCURSAL_ASIENTO	
	AND MI.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
	AND MI.TZ_LOCK=0
	
LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO

WHERE
	GDC.MOV_SIGNO='C' AND 
	AE.ASIENTO_ORIGINAL IS NULL AND
	YEAR(M.FECHAPROCESO) = YEAR(CONVERT(DATETIME,@fecha_parametro,103))
	AND MONTH(M.FECHAPROCESO) = MONTH(CONVERT(DATETIME,@fecha_parametro,103))
	AND (((DATEPART(DAY,M.FECHAPROCESO)-1)/10 + 1) = ((DATEPART(DAY,@fecha_parametro)-1)/10 + 1))

UNION ALL
-- DEVOLUCIONES IIBB CORRIENTES
SELECT DISTINCT 
	DOC.NUMERODOCUMENTO AS NRO_DOCUMENTO,
	
	V.CTA_CBU AS CBU,
	
	YEAR(RI.FECHA_DEVOLUCION) AS ANIO_INFORMADO,
	
	MONTH(RI.FECHA_DEVOLUCION) AS MES_INFORMADO,
	
	CASE WHEN DAY(RI.FECHA_DEVOLUCION)&lt;=10 THEN
		1
	WHEN DAY(RI.FECHA_DEVOLUCION)&lt;=20 THEN
		2
	ELSE
		3
	END AS DECENA,
	
	F.MONEDA AS MONEDA,
	
   	'-' AS TIPO_OPERACION,
	
	CASE WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0) THEN
		RI.BASE_IMPONIBLE 
	ELSE
		RI.BASE_IMPONIBLE * ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	
	END AS BASE_IMPONIBLE,
	
	'-' AS TIPO_OPERACION_1,
	
	CASE WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0) THEN
		FL.MONTO_FACTURABLE 
	ELSE 
		FL.MONTO_FACTURABLE * ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	END AS RETENCION,
--=================================================================================
	RI.FECHA_DEVOLUCION AS FECHA_COBRO,
	S.C1803 AS CODIGOCLIENTE,
	--M.FECHACONTABLE AS FECHA_COBRO,
	ISNULL(pf.IIBB_CORRIENTES_ALI,pj.IIBB_CORRIENTES_ALI) AS IIBB_CORRIENTES_ALI
--=================================================================================
FROM 
	GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK) 
	
INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND F.ASIENTO = GDC.MOV_ASIENTO 
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)) 
	
INNER JOIN CI_FACTURA_LINEA FL WITH(NOLOCK) ON 
	FL.ORDINAL_MOVIMIENTO = F.ORDINAL_MOVIMIENTO
	AND FL.ASIENTO = F.ASIENTO
	AND FL.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO
	AND FL.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO
	AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 15
			AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) 
				AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)
			))) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 15
			AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) 
				AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)
			))) 
	)
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  )	
	
INNER JOIN RRII_IMP_DEVOLUCIONES RI WITH(NOLOCK) ON
	RI.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO
	AND RI.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO
	AND RI.ASIENTO= F.ASIENTO

INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ESTADO = 77 AND
	A.ASIENTO = F.ASIENTO AND
	A.SUCURSAL = F.SUCURSAL_ASIENTO AND
	A.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
	
INNER JOIN VTA_SALDOS AS V WITH (nolock) ON
	V.JTS_OID_SALDO = S.JTS_OID
	AND ((V.TZ_LOCK &lt; 300000000000000 OR V.TZ_LOCK >= 400000000000000) 
		AND (V.TZ_LOCK &lt; 100000000000000 OR V.TZ_LOCK >= 200000000000000)
	)
	
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803
	AND ((C.TZ_LOCK &lt; 300000000000000 OR C.TZ_LOCK >= 400000000000000) 
		AND (C.TZ_LOCK &lt; 100000000000000 OR C.TZ_LOCK >= 200000000000000)
	)
INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND ((CP.TZ_LOCK &lt; 300000000000000 OR CP.TZ_LOCK >= 400000000000000) 
		AND (CP.TZ_LOCK &lt; 100000000000000 OR CP.TZ_LOCK >= 200000000000000)
	)

INNER JOIN CON_BITACORA_IMPUESTOS CBI WITH (NOLOCK) ON
	CBI.ID_CLIENTE = CP.CODIGOCLIENTE AND 
	CBI.TIPO_CARGO_IMPOSITIVO = 15 AND
	CBI.FECHA_INICIO = (SELECT MAX(FECHA_INICIO) FROM CON_BITACORA_IMPUESTOS 
						WHERE FECHA_INICIO &lt;= RI.FECHA_PROCESO_ASIENTO AND
							CBI.ID_CLIENTE = ID_CLIENTE AND
							CBI.TIPO_CARGO_IMPOSITIVO = TIPO_CARGO_IMPOSITIVO) AND

	CBI.JTS_NOVEDAD = (SELECT MAX(JTS_NOVEDAD) FROM CON_BITACORA_IMPUESTOS 
						WHERE CBI.ID_CLIENTE = ID_CLIENTE AND
							CBI.TIPO_CARGO_IMPOSITIVO = TIPO_CARGO_IMPOSITIVO AND
							CBI.FECHA_INICIO = FECHA_INICIO)

LEFT JOIN CLI_PERSONASFISICAS AS PF WITH(NOLOCK) ON  
	PF.NUMEROPERSONAFISICA = CBI.ID_PERSONA
	AND ((pf.TZ_LOCK &lt; 300000000000000 OR pf.TZ_LOCK >= 400000000000000) 
		AND (pf.TZ_LOCK &lt; 100000000000000 OR pf.TZ_LOCK >= 200000000000000))

LEFT JOIN CLI_PERSONASJURIDICAS AS PJ  WITH(NOLOCK) ON  
	PJ.NUMEROPERSONAJURIDICA = CBI.ID_PERSONA 
	AND ((pj.TZ_LOCK &lt; 300000000000000 OR pj.TZ_LOCK >= 400000000000000) 
		AND (pj.TZ_LOCK &lt; 100000000000000 OR pj.TZ_LOCK >= 200000000000000))

LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON
	((DOC.TZ_LOCK &lt; 300000000000000 OR DOC.TZ_LOCK >= 400000000000000) 
		AND (DOC.TZ_LOCK &lt; 100000000000000 OR DOC.TZ_LOCK >= 200000000000000)
	)
	AND DOC.NUMEROPERSONAFJ = CBI.ID_PERSONA
	AND DOC.TIPODOCUMENTO = (
						SELECT TOP 1 TIPODOCUMENTO
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000) 
								AND (TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000))
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000) 
								AND (TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000))
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
						)
						
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MI WITH (nolock) ON	
	MI.ASIENTO = F.ASIENTO	
	AND MI.SUCURSAL = F.SUCURSAL_ASIENTO	
	AND MI.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
	AND ((MI.TZ_LOCK &lt; 300000000000000 OR MI.TZ_LOCK >= 400000000000000) 
		AND (MI.TZ_LOCK &lt; 100000000000000 OR MI.TZ_LOCK >= 200000000000000)
	)
	
LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO
--===============================================================================================================================
--INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
--	M.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
--	AND M.SUCURSAL = F.SUCURSAL_ASIENTO
--	AND M.ASIENTO = F.ASIENTO
--	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
--	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
--		WHERE M.FECHAPROCESO=FECHAPROCESO
--			AND M.SUCURSAL=SUCURSAL
--			AND M.ASIENTO=ASIENTO
--			AND M.DEBITOCREDITO=DEBITOCREDITO)
--===============================================================================================================================
	
WHERE 
	((GDC.tz_lock &lt; 300000000000000 OR GDC.tz_lock >= 400000000000000) 
	AND (GDC.tz_lock &lt; 100000000000000 OR GDC.tz_lock >= 200000000000000))
	AND FL.OPERACION = 2089
	AND AE.ASIENTO_ORIGINAL IS NULL 
	AND GDC.MOV_SIGNO='D'
	AND YEAR(RI.FECHA_PROCESO_ASIENTO) = YEAR(CONVERT(DATETIME,@fecha_parametro,103))
	AND MONTH(RI.FECHA_PROCESO_ASIENTO) = MONTH(CONVERT(DATETIME,@fecha_parametro,103))
	AND (((DATEPART(DAY,RI.FECHA_PROCESO_ASIENTO)-1)/10 + 1) = ((DATEPART(DAY,@fecha_parametro)-1)/10 + 1))</sql>
