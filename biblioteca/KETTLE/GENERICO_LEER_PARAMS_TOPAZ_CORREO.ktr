        <jsScript_script>trans_Status = CONTINUE_TRANSFORMATION; //Estado default (Permite arrojar error por el javascript)

//Validez Parametria
if (TIPO_EMAIL != 'P' &amp;&amp; TIPO_EMAIL != 'C' &amp;&amp; TIPO_EMAIL != null)
	abortar("Se encontro un tipo de EMAIL invalido en la parametria. Tabla EMAIL_LIST campo TIPO. Valores Aceptados P y C, valor encontrado " + TIPO_EMAIL + " .Revise configuracion en EMAIL_CONF con ID:" + ID_CONF);

//Control destinatarios
if (TIPO_EMAIL == 'P')
{
	if (DEST == '')
		DEST = EMAIL;
	else
		abortar("Solo se puede tener un correo principal al cual enviar como destinatario, revise parametria en EMAIL_LIST. Revise configuracion en EMAIL_CONF con ID:" + ID_CONF);
}

//Build CC (Con copia)
if (TIPO_EMAIL == 'C' &amp;&amp; FIN == false)
{
	CC += EMAIL + ' ';
}
</jsScript_script>
