    <connection>TopazDS</connection>
    <sql>select 
TRIM(ecs.SERVER_HOST) as 'HOST', 
ecs.SERVER_PORT as 'PORT', 
TRIM(ecs.AUTENTICACION) as 'AUTENTICACION', 
TRIM(ecs.AUTH_USER) as 'USER', 
TRIM(ecs.AUTH_PASS) as 'PASS', 
TRIM(el.TIPO) as 'TIPO_EMAIL', 
TRIM(el.EMAIL) as 'EMAIL', 
ec.ID as 'ID_CONF' 
from EMAIL_CONF ec 
inner join EMAIL_CONF_DNS ecd on ec.ID_DNS = ecd.ID 
inner join EMAIL_CONF_PROC ecp on ec.ID_PROCESO = ecp.ID 
inner join EMAIL_CONF_SERV ecs on ec.ID_SERV = ecs.ID 
left join EMAIL_LIST el on ec.ID = el.ID_CONF 
where ecd.DNS = ? and ecp.CLAVE_PROCESO = '${PROCESO}' and ec.TIPO = '${TIPO_ENVIO}' ; </sql>
