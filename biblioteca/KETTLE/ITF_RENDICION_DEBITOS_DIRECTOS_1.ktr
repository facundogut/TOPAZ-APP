if (!validoFecha(FECHA_DESDE)) abortar('Formato de fecha desde incorrecto.');
if (!validoFecha(FECHA_HASTA)) abortar('Formato de fecha hasta incorrecto.');
if (FECHA_DESDE>FECHA_HASTA) abortar('Fecha desde no puede ser mayor a fecha hasta.');
</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>//Script here

function abortar (MSG)
{
	writeToLog("e", "Error: " + MSG);
	setVariable("msg_err", MSG, "r");
	trans_Status = ERROR_TRANSFORMATION;
}

function isNumeric(n) {
  return !isNaN(parseFloat(n)) &amp;&amp; isFinite(n);
}

function validoFecha(Fecha) {
	var fechaOK1 = 1;
	if (!isNumeric(Fecha) || Fecha.length != 8) return false;
	var year = parseInt(Fecha.substr(0, 4), 10);
	var month = parseInt(Fecha.substr(4, 2), 10);
	var day = parseInt(Fecha.substr(6, 2), 10);

	if(month>12 || month&lt;1) fechaOK1 = 0;
	if(day>31 || day&lt;1)fechaOK1 = 0;
	if(month== 2 &amp;&amp; day>29)fechaOK1 = 0;
	if((month==1||month==3||month==5||month==7||month==8||month==10||month==12) &amp;&amp; day>31)fechaOK1 = 0;
