    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT 	 (SELECT count(1) FROM SNP_PRESTACIONES_EMPRESAS p WITH (NOLOCK)
 			INNER JOIN SNP_MSG_ORDENES o WITH (NOLOCK) on o.CUIT_EO = p.CUIT_EO 
 				AND o.PRESTACION = p.PRESTACION 
 				AND p.TZ_LOCK = 0
 				AND o.TZ_LOCK = 0
          WHERE p.CUIT_EO = ?) AS EXISTE
		,(SELECT CONVERT(nvarchar(8), FECHAPROCESO, 112)  FROM PARAMETROS WITH (NOLOCK)) AS PERIODO_ACTUAL
		, (SELECT count(1) FROM SNP_PRESTACIONES_EMPRESAS p WITH (NOLOCK)
 			INNER JOIN SNP_MSG_ORDENES o WITH (NOLOCK) on o.CUIT_EO = p.CUIT_EO 
 				AND o.PRESTACION = p.PRESTACION 
 				AND p.TZ_LOCK = 0
 				AND o.TZ_LOCK = 0
 		    WHERE o.FECHA_COMPENSACION BETWEEN ? AND ?) AS EXISTE_PERIODO
         ,(CASE WHEN ISNULL(?,'') = '' THEN 1 ELSE (SELECT count(1) FROM SNP_PRESTACIONES_EMPRESAS p WHERE p.CUIT_EO = ? AND p.PRESTACION = ? AND p.TZ_LOCK = 0) END) AS EXISTE_PRESTACION</sql>
    <outer_join>N</outer_join>
