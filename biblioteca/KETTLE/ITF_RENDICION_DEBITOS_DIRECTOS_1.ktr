if(EXISTE_PERIODO == 0) abortar('No existen débitos directos para el período ingresado.');
if(PERIODO_ACTUAL &lt; FECHA_HASTA) abortar('El período debe de ser menor o igual a la fecha actual.');
if(EXISTE_PRESTACION == 0) abortar('Prestación no corresponde al Ente.');</jsScript_script>
