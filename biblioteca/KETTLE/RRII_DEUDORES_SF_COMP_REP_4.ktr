    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT CONVERT(VARCHAR,TIPO_ID) + ';' +
CONVERT(VARCHAR,NRO_ID) + ';' +
CONVERT(VARCHAR,TIPO_ASIST_CRED) + ';' +
CONVERT(VARCHAR,OPERACION) + ';' +
CONVERT(VARCHAR,COD_POSTAL_SUC) + ';' +
CONVERT(VARCHAR,COD_MONEDA) + ';' +
CONVERT(VARCHAR,FECHA_ORIGEN,23) + ';' +
CASE WHEN ISNULL(MONTO_ORIGINAL, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,MONTO_ORIGINAL),',',''),'.','') END + ';' +
CASE WHEN ISNULL(SALDO_DEUDA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,SALDO_DEUDA),',',''),'.','') END + ';' +
CASE WHEN ISNULL(PLAZO_ORIGINAL, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,PLAZO_ORIGINAL),',',''),'.','') END + ';' +
CONVERT(VARCHAR,TIPO_TASA_INT) + ';' +
CASE WHEN DESC_TIPO_TASA_INT IS NULL THEN '' ELSE CONVERT(VARCHAR,DESC_TIPO_TASA_INT) END + ';' +
CONVERT(VARCHAR,FREC_ACTUALIZACION_TASA) + ';' +
REPLACE(CONVERT(VARCHAR,CONVERT(NUMERIC(5,2) , TNA)), '.', ',') + ';' +
CASE WHEN CFT_TNA IS NULL THEN '' ELSE REPLACE(CONVERT(VARCHAR,CONVERT(NUMERIC(5,2),CFT_TNA)), '.', ',') END + ';' +
CONVERT(VARCHAR,SIST_AMORTIZACION) + ';' +
CONVERT(VARCHAR,DESC_SIST_AMORTIZACION) + ';' +
CONVERT(VARCHAR,FREC_AMORTIZACION) + ';' +
CONVERT(VARCHAR,DESC_FREC_AMORTIZACION) + ';' +
CASE WHEN ISNULL(DEUDA_VENCIDA_GTIAA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,DEUDA_VENCIDA_GTIAA),',',''),'.','') END + ';' +
CASE WHEN ISNULL(DEUDA_TOTAL_GTIAA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,DEUDA_TOTAL_GTIAA),',',''),'.','') END + ';' +
CASE WHEN ISNULL(DEUDA_VENCIDA_GTIAB, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,DEUDA_VENCIDA_GTIAB),',',''),'.','') END + ';' +
CASE WHEN ISNULL(DEUDA_TOTAL_GTIAB, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,DEUDA_TOTAL_GTIAB),',',''),'.','') END + ';' +
CASE WHEN ISNULL(INT_VENCIDA_GTIAB, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,INT_VENCIDA_GTIAB),',',''),'.','') END + ';' +
CASE WHEN ISNULL(INT_TOTAL_GTIAB, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,INT_TOTAL_GTIAB),',',''),'.','') END + ';' +
CASE WHEN ISNULL(DEUDA_VENCIDA_SGTIA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,DEUDA_VENCIDA_SGTIA),',',''),'.','') END + ';' +
CASE WHEN ISNULL(DEUDA_TOTAL_SGTIA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,DEUDA_TOTAL_SGTIA),',',''),'.','') END + ';' +
CASE WHEN ISNULL(INT_VENCIDA_SGTIA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,INT_VENCIDA_SGTIA),',',''),'.','') END + ';' +
CASE WHEN ISNULL(INT_TOTAL_SGTIA, 0) = 0 THEN '' ELSE 
REPLACE(REPLACE(CONVERT(VARCHAR,INT_TOTAL_SGTIA),',',''),'.','') END + ';' +
CASE WHEN FECHA_1VTO_IMPAGO IS NULL THEN '' ELSE CONVERT(VARCHAR,FECHA_1VTO_IMPAGO,23) END + ';' +
CASE WHEN FECHA_1INT_DEVENGAMIENTO IS NULL THEN '' ELSE CONVERT(VARCHAR,FECHA_1INT_DEVENGAMIENTO,23) END + ';' +
CASE WHEN TNA_ORIGINAL IS NULL THEN '' ELSE REPLACE(CONVERT(VARCHAR,CONVERT(NUMERIC(5,2),TNA_ORIGINAL)),'.',',') END + ';' +
CASE WHEN FECHA_ULT_REP_TI IS NULL THEN '' ELSE CONVERT(VARCHAR,FECHA_ULT_REP_TI,23) END
AS LINEA_COMP

FROM DEUDORES_4306_COMP WITH(NOLOCK)
WHERE 
    FECHA_PROCESO=CONVERT(DATETIME,?,103);</sql>
