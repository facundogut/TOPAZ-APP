    <entry>
      <name>Actualiza Movimientos Vencidos</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>UPDATE SBCMPI
SET 
	ESTADO = 'X', 
	RESULTADO_ULTIMA_EJECUCION = 'Solicitud vencida'
WHERE 
	ESTADO NOT IN ('X', 'T') AND
	FECHA_VENCIMIENTO &lt;= '${FECHATOP}'
;</sql>
