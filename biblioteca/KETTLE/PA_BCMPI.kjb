      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>IF ('${REF_EXT_ID_GICM}' &lt;&gt; 'null')
BEGIN
	DELETE FROM dbo.GRL_ACREDITACIONES_MASIVAS
	WHERE 
		CANAL = '${CANAL}' AND
		REFERENCIA_EXTERNA = '${REF_EXT_ID_GICM}' AND
		ESTADO = 'I'
END
ELSE BEGIN
	DELETE FROM dbo.GRL_ACREDITACIONES_MASIVAS 
	FROM dbo.GRL_ACREDITACIONES_MASIVAS GAM
	WHERE 
		CANAL = '${CANAL}' AND
		FCHA_PROCESAR = (SELECT FECHAPROCESO FROM PARAMETROS) AND
		ESTADO = 'I'
END

UPDATE dbo.SBCMPI
SET
	ESTADO = 'I'
WHERE 
	ESTADO = 'B'</sql>
