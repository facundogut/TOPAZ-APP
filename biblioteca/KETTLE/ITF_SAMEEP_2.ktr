  <step>
    <name>get data reporte</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE
	@codEnte VARCHAR (12) = ?, --'406',  PRUEBA EN desa
	@fechaRen VARCHAR(8) = ?; --'20221026';


SELECT trim(Campo)  AS Resultado,
sucur
FROM (
--CABECERA CAJA
    SELECT DISTINCT CONCAT('C',
    RIGHT('000' + CAST(DENSE_RANK() OVER (PARTITION BY detcaja.SUCURSAL_COBRANZA ORDER BY cabcaja.ID) AS VARCHAR(3)), 3),
    RIGHT('0000' + CAST(detcaja.SUCURSAL_COBRANZA AS VARCHAR(4)), 4),
    CONVERT(VARCHAR(6), cabcaja.FECHACARGA, 12),
    RIGHT('0000000000' + CAST(count(*) AS VARCHAR(10)), 10),
    RIGHT('000000000000' +REPLACE(CAST(sum(detcaja.IMPORTE) AS VARCHAR(12)), '.', ''), 12),
    REPLICATE('0', 25)
    ) AS Campo,
    detcaja.SUCURSAL_COBRANZA AS sucur
    FROM CONV_REL_ENTECONV e 
    	JOIN CONV_CONVENIOS_REC c ON e.ID_CONVREC = c.ID_CONVREC OR e.ID_CONVREC = c.ID_CONVPADRE
    	JOIN REC_RENDICION r ON c.ID_CONVREC = r.CONVENIO-- OR c.ID_CONVREC = r.CONVENIO_PADRE
    	JOIN REC_LIQUIDACION liq ON r.ID_RENDICION = liq.ID_RENDICION
    	JOIN REC_CAB_RECAUDOS_CAJA cabcaja ON cabcaja.ID_LIQUIDACION = liq.ID_LIQUIDACION
       	JOIN REC_DET_RECAUDOS_CAJA detcaja ON detcaja.ID_CABEZAL = cabcaja.ID
    	
    WHERE 
    	e.cod_ente=@codEnte
        AND r.fecha=CAST(@fechaRen AS DATE)
      --  AND detcaja.importe > 0
        AND detcaja.tz_lock=0
        AND e.tz_lock=0
        AND c.tz_lock=0
        AND r.tz_lock=0
        AND cabcaja.tz_lock=0 
        AND liq.tz_lock=0  
        AND liq.estado = 'R'
        
GROUP BY detcaja.sucursal_cobranza, cabcaja.FECHACARGA,  cabcaja.ID
    UNION ALL
    
--TALON CAJA
    SELECT DISTINCT    CONCAT('T',
    RIGHT('000' + CAST(DENSE_RANK() OVER (PARTITION BY detcaja.SUCURSAL_COBRANZA ORDER BY cabcaja.ID) AS VARCHAR(3)), 3),
    RIGHT(REPLICATE('0', 40) + LEFT(trim(CAST(detcaja.codigo_barras AS VARCHAR(120))),40), 40),
    RIGHT('0000000000' +REPLACE(CAST(detcaja.IMPORTE AS VARCHAR(10)), '.', ''), 10),
    RIGHT('000000' + CAST(detcaja.Asiento_cobranza AS varchar(10)), 6), (SELECT
                                                                          CASE
                                                                            WHEN LEN(detcaja.codigo_barras) = 0 THEN 5
                                                                            ELSE 0
                                                                          END)
    ) AS Campo,
    detcaja.SUCURSAL_COBRANZA AS sucur
   FROM CONV_REL_ENTECONV e 
    	JOIN CONV_CONVENIOS_REC c ON e.ID_CONVREC = c.ID_CONVREC OR e.ID_CONVREC = c.ID_CONVPADRE
    	JOIN REC_RENDICION r ON c.ID_CONVREC = r.CONVENIO --OR c.ID_CONVREC = r.CONVENIO_PADRE
    	JOIN REC_LIQUIDACION liq ON r.ID_RENDICION = liq.ID_RENDICION
    	JOIN REC_CAB_RECAUDOS_CAJA cabcaja ON cabcaja.ID_LIQUIDACION = liq.ID_LIQUIDACION
       	JOIN REC_DET_RECAUDOS_CAJA detcaja ON detcaja.ID_CABEZAL = cabcaja.ID
       	
     WHERE  e.cod_ente=@codEnte
        AND r.fecha=CAST(@fechaRen AS DATE)
     --  AND detcaja.importe > 0
        AND detcaja.tz_lock=0
        AND e.tz_lock=0
        AND c.tz_lock=0
        AND r.tz_lock=0
        AND cabcaja.tz_lock=0 
        AND liq.tz_lock=0    
        AND liq.estado = 'R'  

        UNION ALL
        
        
--CABEcERA CANAL
    SELECT DISTINCT CONCAT('C',
    RIGHT('000' + CAST((DENSE_RANK() OVER (PARTITION BY SUC.sucursal ORDER BY cabcanal.ID)
    ) AS VARCHAR(3)), 3),
     CASE WHEN SUC.sucursal IS NULL THEN '0000' ELSE RIGHT(CONCAT(replicate('0',4), SUC.sucursal), 4) END,
    CONVERT(VARCHAR(6), cabcanal.FECHA_COBRANZA, 12),
    RIGHT('0000000000' + CAST(cabcanal.TOTALREGISTROS AS VARCHAR(10)), 10),
    RIGHT('000000000000' +REPLACE(CAST(cabcanal.TOTALIMPORTE AS VARCHAR(12)), '.', ''), 12),
    REPLICATE('0', 25)
    )AS Campo,
    SUC.sucursal AS sucur
    FROM CONV_REL_ENTECONV e 
    	JOIN CONV_CONVENIOS_REC c ON e.ID_CONVREC = c.ID_CONVREC OR e.ID_CONVREC = c.ID_CONVPADRE
    	JOIN SUC_FICTICIA_CONV_CANALES suc ON suc.canal = c.canal
    	JOIN REC_RENDICION r ON c.ID_CONVREC = r.CONVENIO --OR c.ID_CONVREC = r.CONVENIO_PADRE NO ES NECESARIO
    	JOIN REC_LIQUIDACION liq ON r.ID_RENDICION = liq.ID_RENDICION
    	JOIN REC_CAB_RECAUDOS_CANAL cabcanal ON cabcanal.ID_LIQUIDACION = liq.ID_LIQUIDACION
    --	JOIN REC_DET_RECAUDOS_CANAL detcanal ON cabcanal.ID = detcanal.ID_CABEZAL
    
    
    WHERE e.cod_ente=@codEnte
        AND r.fecha = CAST(@fechaRen AS DATE)  
    --    AND detcanal.importe>0
    --    AND detcanal.tz_lock=0
        AND e.tz_lock=0
        AND c.tz_lock=0
        AND r.tz_lock=0
        AND cabcanal.tz_lock=0 
        AND liq.tz_lock=0  
        AND liq.estado = 'R'
        
    UNION ALL
    
        
--talon canal
       SELECT DISTINCT    CONCAT('T',    
       RIGHT('000' + CAST((DENSE_RANK() OVER (PARTITION BY SUC.sucursal ORDER BY cabcanal.ID)
    ) AS VARCHAR(3)), 3),	
    RIGHT(REPLICATE('0', 40) + LEFT(trim(CAST(detcanal.codigo_barras AS VARCHAR(120))),40), 40),
    RIGHT('0000000000' +REPLACE(CAST(detcanal.IMPORTE AS VARCHAR(17)), '.', ''), 10),
    RIGHT('000000' + CAST(detcanal.Id_linea AS varchar(15)), 6), (SELECT
                                                                          CASE
                                                                            WHEN LEN(detcanal.codigo_barras) = 0 THEN 5
                                                                            ELSE 0
                                                                          END)
    )AS Campo,
    SUC.sucursal AS sucur
    
   	FROM CONV_REL_ENTECONV e 
    	JOIN CONV_CONVENIOS_REC c ON e.ID_CONVREC = c.ID_CONVREC OR e.ID_CONVREC = c.ID_CONVPADRE
    	JOIN SUC_FICTICIA_CONV_CANALES suc ON suc.canal = c.canal
    	JOIN REC_RENDICION r ON c.ID_CONVREC = r.CONVENIO --OR c.ID_CONVREC = r.CONVENIO_PADRE
    	JOIN REC_LIQUIDACION liq ON r.ID_RENDICION = liq.ID_RENDICION
    	JOIN REC_CAB_RECAUDOS_CANAL cabcanal ON cabcanal.ID_LIQUIDACION = liq.ID_LIQUIDACION
    	JOIN REC_DET_RECAUDOS_CANAL detcanal ON cabcanal.ID = detcanal.ID_CABEZAL
    WHERE
       	 e.cod_ente=@codEnte
        AND r.fecha=CAST(@fechaRen AS DATE)
       -- AND detcanal.importe>0
        AND detcanal.tz_lock=0
        AND e.tz_lock=0
        AND c.tz_lock=0
        AND r.tz_lock=0
        AND cabcanal.tz_lock=0 
        AND liq.tz_lock=0      
        AND liq.estado = 'R'
  )      
AS resultado
ORDER BY resultado.sucur,substring(campo,2,3),substring(campo,1,1)
</sql>
