    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE 
	@codEnte VARCHAR(12) = '${P0_VALUE}',
	@fecha VARCHAR(12) = '${P1_VALUE}',
	@conv INT = (SELECT TOP 1 ID_CONVREC 
							FROM CONV_REL_ENTECONV (nolock) rel 
							JOIN REC_rendicion (nolock)  ren ON ren.convenio_padre = rel.id_convrec 
							WHERE rel.COD_ENTE = '${P0_VALUE}'
							AND ren.FECHA = CAST('${P1_VALUE}' AS DATE) );
 
DECLARE	
	
	@rutaSalida VARCHAR(500)= '${Top.Output}CONVENIOS/SAMEEP/',	
	@archivoRen VARCHAR(12) = 
	
	(SELECT TOP 1
		CONCAT('T',RIGHT(concat('000',CASE WHEN ren.CONVENIO_PADRE = 0 THEN ren.CONVENIO ELSE  ren.CONVENIO_PADRE END),3),RIGHT(@fecha,4)) 
		FROM CONV_REL_ENTECONV (nolock) relEnte 
			JOIN REC_RENDICION (nolock) ren ON relEnte.id_convrec = ren.convenio OR  relEnte.id_convrec = ren.convenio_padre
			WHERE  
			 	relEnte.COD_ENTE = @codEnte AND			   
				ren.ESTADO = 'R' AND
				ren.tz_lock = 0 AND 
				ren.FECHA = @fecha);



	
SELECT
	@conv convenio,
	@archivoRen AS archivoRen,
	@rutaSalida AS rutaSalida
	

</sql>
