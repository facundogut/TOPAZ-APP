    <quoteString>N</quoteString>
    <sql>INSERT INTO DEUDORES_4302
SELECT 
---4302
4302 AS CODIGO_DISENIO,
---MC.Deuda_Total,
   CASE 
		WHEN MC.CUENTA_ORDEN = 'S' THEN 4
        ELSE 2
    END AS CONCEPTO,
	
MC.Categoria_Cliente AS CATEGORIA,
CASE WHEN LEN(MC.Clasificacion) = 0 OR MC.Clasificacion IS NULL THEN '0' ELSE
(SELECT TOP 1 RPI.NUM1 
     FROM RRI_PARAMETROS_DEF RPD WITH(NOLOCK)
     JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON RPI.CODIGO=RPD.CODIGO
     WHERE RPD.CODIGO=205 AND (RPI.ID4=MC.Clasificacion OR CONVERT(VARCHAR,CONVERT(NUMERIC(15),RPI.ID1))=(CASE WHEN LEN(MC.Clasificacion) = 0 THEN '0' ELSE ISNULL(MC.Clasificacion,'0') END)))
END AS SITUACION,
	
ROUND(SUM(MC.Deuda_GtiasPrefA)/1000,0) AS GAR_PREF_A_CAP_INT,

ROUND(SUM(CASE 
        WHEN MC.Deuda_Total = MC.Prevision THEN 0 
        ELSE MC.Deuda_Total - MC.Prevision
    END)/1000,0) AS  GAR_PREF_B_CAP_INT_NOPREV,
    
ROUND(SUM(CASE 
	 WHEN  (MC.Clasificacion='1' OR MC.Clasificacion='21') THEN 0
        ELSE MC.Deuda_GtiasPrefB
END)/1000,0) AS GAR_PREF_B_INT_PREV,

ROUND(SUM(MC.Deuda_Sin_Gar)/1000,0) AS SIN_GAR_CAP_INT_NOPREV,

----CEILING(SUM(CASE WHEN  (MC.Clasificacion='1' OR MC.Clasificacion='21') THEN 0 ELSE MC.Deuda_Sin_Gar END)/1000) 
0 AS SIN_GAR_INT_PREV,

SUM(CASE WHEN (MC.Clasificacion='1' OR MC.Clasificacion='21') AND (MC.Deuda_GtiasPrefA > 0 OR (MC.Deuda_Total - MC.Prevision) > 0) THEN 1
WHEN (MC.Clasificacion&lt;&gt;'1' AND MC.Clasificacion&lt;&gt;'21') AND (MC.Deuda_GtiasPrefA > 0 OR (MC.Deuda_Total - MC.Prevision) > 0 OR MC.Deuda_Sin_Gar > 0
OR MC.Deuda_GtiasPrefB > 0) THEN 1
ELSE 0 END) AS CANT_DEUDORES,

CONVERT(DATETIME,?,103) AS FECHA_PROCESO
FROM MEMO_CABECERA MC WITH(NOLOCK)
WHERE MC.Fecha_Info= CONVERT(DATETIME,?,103)
AND MC.TZ_LOCK=0
AND MC.Deuda_Total &lt; (SELECT IMPORTE FROM PARAMETROSGENERALES WITH(NOLOCK) WHERE CODIGO=784) 
AND MC.CUIT_CUIL_CDI NOT IN (SELECT NRO_ID FROM DEUDORES_4305 WITH(NOLOCK) WHERE FECHA_PROCESO = CONVERT(DATETIME,?,103))
AND MC.Deuda_Total > 0
GROUP BY 
        CASE 
		WHEN MC.CUENTA_ORDEN = 'S' THEN 4
        ELSE 2
    END,
    CASE 
	 WHEN (MC.Clasificacion&lt;&gt;'1' AND MC.Clasificacion&lt;&gt;'21')
     THEN 5
        ELSE ISNULL(TRY_CAST(MC.Clasificacion AS NUMERIC(2)), 0) 
END,
    MC.Categoria_Cliente, MC.Clasificacion</sql>
