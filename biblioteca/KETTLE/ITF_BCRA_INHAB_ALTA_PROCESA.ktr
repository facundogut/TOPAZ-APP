  <step>
    <name>Lee Documentos</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <sql>SELECT
	CAST(NUMERODOCUMENTO AS NUMERIC(12,0)) AS "TOPAZ_CUIT",
	NUMEROPERSONAFJ AS "TOPAZ_NUMERO_PERSONA",
	CASE (TIPOPERSONA)
		WHEN 'F' THEN CONCAT(PF.APELLIDOPATERNO, PF.APELLIDOMATERNO, PF.PRIMERNOMBRE, PF.SEGUNDONOMBRE)
		WHEN 'J' THEN PJ.RAZONSOCIAL
	END AS "TOPAZ_NOMBRE_PERSONA"
FROM CLI_DOCUMENTOSPFPJ CDP WITH (NOLOCK)
LEFT JOIN CLI_PERSONASFISICAS PF ON CDP.TIPOPERSONA = 'F' AND PF.NUMEROPERSONAFISICA = CDP.NUMEROPERSONAFJ AND PF.TZ_LOCK LIKE '[04]%'
LEFT JOIN CLI_PERSONASJURIDICAS PJ ON CDP.TIPOPERSONA = 'J' AND PJ.NUMEROPERSONAJURIDICA = CDP.NUMEROPERSONAFJ AND PJ.TZ_LOCK LIKE '[04]%'
WHERE 
	ISNUMERIC(NUMERODOCUMENTO) = 1 AND
	CDP.TZ_LOCK LIKE '[04]%' AND 
	COALESCE(PF.ESTADO, PJ.ESTADO) = 0
ORDER BY TOPAZ_CUIT ASC</sql>
    <limit>0</limit>
    <lookup/>
