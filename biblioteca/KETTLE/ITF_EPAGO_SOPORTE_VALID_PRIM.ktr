        <jsScript_script>//Script here

currentRow = row;

CODIGO_ERROR = 0;
ERROR = null;

// Lote no existe en BD Stage
if ( CANTIDAD_REGISTROS_STAGE == null || parseInt(CANTIDAD_REGISTROS_STAGE, 10) == 0 )
	setError(200, 'Lote no encontrado en BD Stage');

else if ( EXISTE_LOTE_TOPAZ != null )
	setError(201, 'Lote ya procesado');

// Cantidad de Registros distinta a parametro
else if ( parseInt(CANTIDAD_REGISTROS_STAGE, 10) != parseInt(CANTIDAD_REGISTROS, 10) )
	setError(202, 'Cantidad de Registros informada por parametro distinta a cantidad de registros encontrados');

// Sumatoria de Importes calculada distinta a parametro
else if ( getFieldValue("SUMA_IMPORTES_STAGE").compareTo( getFieldValue("IMPORTE_TOTAL") ) != 0 )
	setError(203, 'Sumatoria de Importes informada por parametro distinta a calculada');

// Solo puede ser una la jurisdicción que ordena
//else if ( CANTIDAD_JURISDICCIONES != 1 )
