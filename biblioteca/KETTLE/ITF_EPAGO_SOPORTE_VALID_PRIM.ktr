const setError = function(codigo, descripcion) {
	CODIGO_ERROR = parseInt(codigo, 10);
	//CODIGO_ERROR = 0;
	ERROR = String(descripcion);
	setVariable('COD_RETORNO', CODIGO_ERROR, 'r');
	setVariable('DESC_MENSAJE', ERROR, 'r');
	writeToLog('e', 'Se encontró un error en las validaciones primarias');
	writeToLog('e', 'Código de error: ' + CODIGO_ERROR);
	writeToLog('e', 'Mensaje de error: ' + ERROR);
}

const getFieldIndex = function (fieldName) {
	return getInputRowMeta().indexOfValue(fieldName);
}

const getFieldValue = function (fieldName) {
	return currentRow[ getFieldIndex(fieldName) ];
}</jsScript_script>
