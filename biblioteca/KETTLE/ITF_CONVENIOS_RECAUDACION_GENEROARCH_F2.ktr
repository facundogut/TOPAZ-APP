  <step>
    <name>traigo regs ordenados</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TOPAZCONNECTION</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE 
	@convPadre VARCHAR (12) = '${P0_VALUE}',
	@fechaRen VARCHAR(8) = '${P1_VALUE}';


SELECT * FROM (
--CAB

SELECT DISTINCT
  		CONCAT(
		'C',
		RIGHT(CONCAT(REPLICATE('0',4),det.sucursal_cobranza),4), --sucursal
		RIGHT(@fechaRen,6), --fecha ren
		RIGHT(CONCAT(REPLICATE('0',10),count(1)),10) , --cant talones
		RIGHT(CONCAT(REPLICATE('0',15),floor(sum(det.importe)*100)),15), --tot importes
		REPLICATE('0',49)) REGISTRO, 
		ren.fecha, ren.archivo, det.sucursal_cobranza sucursal
		
FROM REC_LIQUIDACION (NOLOCK) liq 
	JOIN REC_RENDICION (NOLOCK) ren ON liq.ID_RENDICION = ren.ID_RENDICION
	JOIN CONV_CONVENIOS_REC (nolock) conv ON conv.id_convrec = ren.convenio
	JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION 	
	JOIN REC_DET_RECAUDOS_CAJA (NOLOCK) det ON cab.ID = det.ID_CABEZAL

			WHERE  
		  		(conv.Id_ConvPadre = @convPadre OR conv.Id_ConvRec = @convPadre) AND
		  		ren.FECHA = CAST(@fechaRen AS DATETIME) AND
				cab.tz_lock = 0 AND 
				det.tz_lock = 0 AND 
				liq.tz_lock = 0 AND 
				ren.tz_lock = 0  AND
				conv.FORMATO_RENDICION = 	
			(
			SELECT opcioninterna 
			FROM OPCIONES 
			WHERE 
			NUMERODECAMPO = 45146 AND 
			IDIOMA = 'E' AND 
			upper(DESCRIPCION) LIKE '%FORMATO ESTANDAR 2%'
			)
GROUP BY  det.sucursal_cobranza, ren.fecha, ren.archivo			
			
UNION ALL 

SELECT DISTINCT
	concat(
	'C',
	RIGHT(CONCAT(REPLICATE('0',4),suc.SUCURSAL),4), 
   	RIGHT(@fechaRen,6),
   	RIGHT(CONCAT(REPLICATE('0',10),count(1)),10),			
	RIGHT(CONCAT(REPLICATE('0',15),floor(sum(det.importe)*100)),15),   
	REPLICATE('0',49) ) REGISTRO,
	ren.fecha, ren.archivo, suc.sucursal
	
	FROM REC_LIQUIDACION (NOLOCK) liq 
		JOIN REC_RENDICION (NOLOCK) ren ON liq.ID_RENDICION = ren.ID_RENDICION
	    JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON conv.id_convrec = ren.convenio
	    JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
	    JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
	    JOIN REC_DET_RECAUDOS_CANAL (nolock) det ON cab.id = det.id_cabezal
	WHERE 
		 (conv.Id_ConvPadre = @convPadre OR conv.Id_ConvRec = @convPadre) AND
   		ren.FECHA = CAST(@fechaRen AS DATETIME) AND
		cab.tz_lock = 0 AND 
		liq.tz_lock = 0 AND 
		ren.tz_lock = 0 AND
				conv.FORMATO_RENDICION = 	
			(
			SELECT opcioninterna 
			FROM OPCIONES 
			WHERE 
			NUMERODECAMPO = 45146 AND 
			IDIOMA = 'E' AND 
			upper(DESCRIPCION) LIKE '%FORMATO ESTANDAR 2%'
			)
GROUP BY  suc.SUCURSAL,ren.fecha, ren.archivo

UNION ALL
---DET 

SELECT
	CONCAT(
	'T', 
	LEFT(CONCAT(det.CODIGO_BARRAS,REPLICATE(' ',65)),65),
	CONVERT(VARCHAR(6), det.FECHA_COBRANZA ,12),
	RIGHT(CONCAT(REPLICATE('0', 12),floor(det.IMPORTE *100)),12),
	'0'),
	ren.fecha, ren.archivo, det.sucursal_cobranza sucursal
	FROM 
	REC_DET_RECAUDOS_CAJA (NOLOCK) AS det 
	JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) AS cab ON det.ID_CABEZAL= cab.ID
	JOIN REC_LIQUIDACION (NOLOCK) AS liq ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
	JOIN REC_RENDICION (NOLOCK) AS ren ON ren.ID_RENDICION = liq.ID_RENDICION
	JOIN CONV_CONVENIOS_REC (nolock) conv ON conv.id_convrec = ren.convenio
	WHERE 
 		(conv.Id_ConvPadre = @convPadre OR conv.Id_ConvRec = @convPadre) AND
   		ren.FECHA = CAST(@fechaRen AS DATETIME) AND 
		cab.tz_lock = 0 AND 
		det.tz_lock = 0 AND 
		liq.ESTADO = 'R' AND
		conv.FORMATO_RENDICION = 	
			(
			SELECT opcioninterna 
			FROM OPCIONES 
			WHERE 
			NUMERODECAMPO = 45146 AND 
			IDIOMA = 'E' AND 
			upper(DESCRIPCION) LIKE '%FORMATO ESTANDAR 2%'
			)

UNION ALL

SELECT
   
	CONCAT(
	'T',
	LEFT(CONCAT(det.CODIGO_BARRAS,REPLICATE(' ',65)),65),
	CONVERT(VARCHAR(6), det.FECHA_COBRANZA ,12),
	RIGHT(CONCAT(REPLICATE('0', 12),floor(det.IMPORTE *100)),12),
	'0'),
		ren.fecha, ren.archivo, suc.sucursal
	FROM
	REC_DET_RECAUDOS_CANAL (NOLOCK) AS det 
	JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) AS cab ON det.ID_CABEZAL= cab.ID
	JOIN REC_LIQUIDACION (NOLOCK) AS liq ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
	JOIN REC_RENDICION (NOLOCK) AS ren ON ren.ID_RENDICION = liq.ID_RENDICION
	JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON conv.id_convrec = ren.convenio 
	JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
	WHERE 
  		(conv.Id_ConvPadre = @convPadre OR conv.Id_ConvRec = @convPadre) AND
	 	ren.FECHA = CAST(@fechaRen AS DATETIME) AND 
		det.TZ_LOCK=0 AND 
		liq.ESTADO = 'R' AND  
		cab.tz_lock = 0 AND 
		det.tz_lock = 0 AND 
		conv.tz_lock = 0 AND
		conv.FORMATO_RENDICION = 	
			(
			SELECT opcioninterna 
			FROM OPCIONES 
			WHERE 
			NUMERODECAMPO = 45146 AND 
			IDIOMA = 'E' AND 
			upper(DESCRIPCION) LIKE '%FORMATO ESTANDAR 2%'
			)


--END DET
) a
ORDER BY  a.archivo, a.sucursal, a.fecha, LEFT(a.registro, 1)</sql>
