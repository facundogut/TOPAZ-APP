    --CANTIDADCHEQUES
  FROM CHE_CHEQUERAS WITH(NOLOCK)
  GROUP BY SUCURSAL, CUENTA, MONEDA, OPERACION, FECHAENTREGACLIENTE
),
TOTAL AS (
  SELECT
    IMPUESTO,
    FECHA_PROCESO_ASIENTO,
