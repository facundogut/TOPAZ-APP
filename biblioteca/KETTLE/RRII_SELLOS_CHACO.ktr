DECLARE @fecha_parametro DATE = CONVERT(DATETIME,?,103);

WITH FACTURA_LINEA_ACUMULADA AS (
	SELECT
		SUM(FL.MONTO_IMPONIBLE) AS SUMA_MONTO_IMPONIBLE,
		SUM(FL.MONTO_FACTURABLE) AS SUMA_MONTO_FACTURABLE,
		FL.ASIENTO,
		FL.SUCURSAL_ASIENTO,
		FL.FECHA_PROCESO_ASIENTO,
		FL.ID_CARGO,
		FL.ID_IMPUESTO,
		FL.OPERACION
	FROM CI_FACTURA_LINEA AS FL WITH(nolock)
	WHERE
		YEAR(FL.FECHA_PROCESO_ASIENTO) = YEAR(@fecha_parametro)
		AND MONTH(FL.FECHA_PROCESO_ASIENTO) = MONTH(@fecha_parametro)
		AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 10
				AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)))) 	
			OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 10
				AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))))
		)
		AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
			AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)
		)
	GROUP BY
		FL.ASIENTO,
		FL.SUCURSAL_ASIENTO,
		FL.FECHA_PROCESO_ASIENTO,
		FL.ID_CARGO,
		FL.ID_IMPUESTO,
		FL.OPERACION
),

IDSELLO AS (

  SELECT 
    
        FL.ASIENTO,
     	FL.SUCURSAL_ASIENTO,
		FL.FECHA_PROCESO_ASIENTO,
		FL.ID_CARGO,
		FL.ID_IMPUESTO,
		FL.OPERACION,
        --FL.TR_REAL,
      --ROW_NUMBER() OVER (ORDER BY FL.ASIENTO) AS correlativo
       ROW_NUMBER() OVER(
      PARTITION BY FL.ASIENTO
      ORDER BY FL.ASIENTO
    ) AS correlativo
  
FROM FACTURA_LINEA_ACUMULADA AS FL	
	
WHERE
  YEAR(FL.FECHA_PROCESO_ASIENTO) = YEAR(@fecha_parametro)
  AND MONTH(FL.FECHA_PROCESO_ASIENTO) = MONTH(@fecha_parametro)

GROUP BY 

		FL.ASIENTO,
		FL.SUCURSAL_ASIENTO,
		FL.FECHA_PROCESO_ASIENTO,
		FL.ID_CARGO,
		FL.ID_IMPUESTO,
	   --	FL.TR_REAL,
		FL.OPERACION


),
CHE_CHEQUERA_TOTAL AS (
  SELECT 
    --TOP 1 
    SUCURSAL,
    CUENTA,
    MONEDA,
    OPERACION,
    FECHAENTREGACLIENTE,
    SUM(CANTIDADCHEQUES) AS TOTAL_CHEQUES,
    MAX(FECHAENTREGACLIENTE) AS FECHA
    --CANTIDADCHEQUES
  FROM CHE_CHEQUERAS WITH(NOLOCK)
  GROUP BY SUCURSAL, CUENTA, MONEDA, OPERACION, FECHAENTREGACLIENTE
)

--SELLOS CHACO CHEQUES 
SELECT DISTINCT
	
   'CHEQUES' AS REF,
	CONCAT (YEAR(@fecha_parametro),
		CASE WHEN MONTH(@fecha_parametro) &lt;10 
			THEN CONCAT('0',MONTH(@fecha_parametro)) 
		ELSE 
			MONTH(@fecha_parametro) END) AS PERIODO,
	
	FL.OPERACION,
	
	FL.ID_CARGO,
	
	FL.ASIENTO,
	
	FL.FECHA_PROCESO_ASIENTO,
	
	FL.SUCURSAL_ASIENTO,
	
	--FL.tr_real,
	
	(SELECT NOMBRE2 FROM PARAMETROS WITH(NOLOCK)) AS CUIT_AG_RET,
  
    CASE 
      WHEN RP.CHAR2 IN ('CV','CD','CM','CH','KS') THEN
           CONCAT(
             RP.CHAR2,
             FL.SUCURSAL_ASIENTO,
             S.CUENTA,
             CAST(I.correlativo AS VARCHAR(10)) 
           )
      WHEN S.MONEDA = 2 THEN
           CONCAT(
             'PRD',
             FL.SUCURSAL_ASIENTO,
             (SELECT OPERACION_CUENTA
              FROM MOVIMIENTOS_CONTABLES MC
              WHERE MC.ASIENTO = FL.ASIENTO 
                AND MC.FECHAPROCESO = FL.FECHA_PROCESO_ASIENTO
                AND MC.SUCURSAL = FL.SUCURSAL_ASIENTO
                AND MC.TRTIPO = 62),
             CAST(I.correlativo AS VARCHAR(10))
           )
      ELSE
           CONCAT(
             RP.CHAR2,
             FL.SUCURSAL_ASIENTO,
             (SELECT OPERACION_CUENTA
              FROM MOVIMIENTOS_CONTABLES MC
              WHERE MC.ASIENTO = FL.ASIENTO 
                AND MC.FECHAPROCESO = FL.FECHA_PROCESO_ASIENTO
                AND MC.SUCURSAL = FL.SUCURSAL_ASIENTO
                AND MC.TRTIPO = 62),
             CAST(I.correlativo AS VARCHAR(10))
			 )
           
    END AS ID_SELLO,
    M.FECHACONTABLE AS FECHACONTABLE,
	CONVERT(varchar,M.FECHACONTABLE,112) AS FECHA_SELLO,
	
	CS.COD_NATURA AS COD_NAT,
	
	CASE WHEN RPP.ID4 IS NOT NULL THEN
		RPP.NUM1
	ELSE
		4
	END AS TIPO_DOCUMENTO,
	
	DOC.NUMERODOCUMENTO AS NUMERO_DOC,
	
	LEFT(CONCAT(C.NOMBRECLIENTE, REPLICATE(' ',25)), 25) AS NOMBRE,
	
	--CC.CANTIDADCHEQUES AS MONTO_IMPONIBLE,
    ISNULL(CQ.TOTAL_CHEQUES, 0) AS MONTO_IMPONIBLE,
   --ISNULL(CQ.CANTIDADCHEQUES, 0) AS MONTO_IMPONIBLE,

	CASE WHEN (FL.ID_CARGO IN (SELECT ID1 FROM RRI_PARAMETROS_INF WITH(NOLOCK) WHERE CODIGO=506)) THEN
		1
	ELSE
		0
	END AS TIPO_EXENCION,
	
	CS.CANT_FOJAS AS CANT_FOJAS,
	
	CASE WHEN (FL.ID_CARGO IN (SELECT ID1 FROM RRI_PARAMETROS_INF WITH(NOLOCK) WHERE CODIGO=506)) THEN
		'LEY DE LA PROVINCIA DEL CHACO Nº 5.551/2005'
	ELSE
		REPLICATE(' ',50)
	END AS NORMATIVA,
	
	REPLICATE(' ',50) AS OBSERVACIONES,
	
	--	CASE WHEN S.MONEDA=2 THEN
	--	(FF.TOTAL_MONTO * 
	--   		ISNULL((SELECT TOP 1 TC_VENTA FROM CON_COTIZACIONES_BNA WHERE Fecha_Cotizacion &lt; M.FECHACONTABLE ORDER BY FECHA_COTIZACION DESC),0) - 
	--			((CS.CANT_FOJAS * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=600))))
	--ELSE
	--	FF.TOTAL_MONTO - (CS.CANT_FOJAS * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=600))	
 --  END AS IMPUESTO_2,
   
 --  	CASE WHEN S.MONEDA=2 THEN
	--	FF.TOTAL_MONTO * ISNULL((SELECT TOP 1 TC_VENTA FROM CON_COTIZACIONES_BNA WHERE Fecha_Cotizacion &lt; M.FECHACONTABLE ORDER BY FECHA_COTIZACION DESC),0)
	--ELSE
	--	FF.TOTAL_MONTO
	--END AS TOTAL_2,
   --SUM(	FF.IMPUESTO )AS IMPUESTO_2,
	--SUM(FF.TOTAL )AS TOTAL_2,

	CASE WHEN S.MONEDA=2 THEN
		(FL.SUMA_MONTO_FACTURABLE * 
	   		ISNULL((SELECT TOP 1 TC_VENTA FROM CON_COTIZACIONES_BNA WHERE Fecha_Cotizacion &lt; M.FECHACONTABLE ORDER BY FECHA_COTIZACION DESC),0) - 
				((CS.CANT_FOJAS * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=600))))
	ELSE
		FL.SUMA_MONTO_FACTURABLE - (CS.CANT_FOJAS * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=600))	
   END AS IMPUESTO,
	
	
	CASE WHEN CS.T_EXENCION = 1 THEN 
		(CS.CANT_FOJAS * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=600)) * 0.5 -- + (CS.CANTIDAD_UF * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=601))) * 0.5 
   	ELSE
   		(CS.CANT_FOJAS * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=600)) -- + (CS.CANTIDAD_UF * (SELECT IMPORTE FROM PARAMETROSGENERALES WHERE CODIGO=601))
   	END  AS IMPORTE_FOJAS,

	CASE WHEN S.MONEDA=2 THEN
		FL.SUMA_MONTO_FACTURABLE * ISNULL((SELECT TOP 1 TC_VENTA FROM CON_COTIZACIONES_BNA WHERE Fecha_Cotizacion &lt; M.FECHACONTABLE ORDER BY FECHA_COTIZACION DESC),0)
	ELSE
		FL.SUMA_MONTO_FACTURABLE
	END AS TOTAL

FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK)

INNER JOIN SALDOS S WITH(NOLOCK) ON 
	S.SUCURSAL = GDC.SALDO_ORIG_SUCURSAL
	AND S.PRODUCTO = GDC.SALDO_ORIG_PRODUCTO
	AND S.CUENTA = GDC.SALDO_ORIG_CUENTA
	AND S.MONEDA = GDC.SALDO_ORIG_MONEDA
	AND S.OPERACION = GDC.SALDO_ORIG_OPERACION
	AND S.ORDINAL = GDC.SALDO_ORIG_ORDINAL
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000))
	
INNER JOIN FACTURA_LINEA_ACUMULADA FL WITH(NOLOCK) ON
	FL.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND FL.ASIENTO = GDC.MOV_ASIENTO
	AND FL.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
		
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO = FL.FECHA_PROCESO_ASIENTO AND
	M.SUCURSAL = FL.SUCURSAL_ASIENTO
	AND M.ASIENTO = FL.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
			
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO = M.ASIENTO AND
	A.ESTADO = 77 AND
	A.SUCURSAL = M.SUCURSAL AND
	A.FECHAPROCESO = M.FECHAPROCESO

INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803
	AND ((C.TZ_LOCK &lt; 300000000000000 OR C.TZ_LOCK >= 400000000000000) 
		AND (C.TZ_LOCK &lt; 100000000000000 OR C.TZ_LOCK >= 200000000000000)
	)
	
INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND CP.TITULARIDAD='T'
	AND ((CP.TZ_LOCK &lt; 300000000000000 OR CP.TZ_LOCK >= 400000000000000) 
		AND (CP.TZ_LOCK &lt; 100000000000000 OR CP.TZ_LOCK >= 200000000000000)
	)
	
INNER JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON
	((DOC.TZ_LOCK &lt; 300000000000000 OR DOC.TZ_LOCK >= 400000000000000) 
		AND (DOC.TZ_LOCK &lt; 100000000000000 OR DOC.TZ_LOCK >= 200000000000000)
	)
	AND DOC.NUMEROPERSONAFJ = CP.NUMEROPERSONA	
	AND DOC.TIPODOCUMENTO = (
						SELECT TOP 1 TIPODOCUMENTO
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000) 
								AND (TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000))
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000) 
								AND (TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000))
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
							)
LEFT JOIN IDSELLO AS I WITH (NOLOCK) ON 

	I.SUCURSAL_ASIENTO = FL.SUCURSAL_ASIENTO
	AND I.ASIENTO = FL.ASIENTO
	AND I.FECHA_PROCESO_ASIENTO = FL.FECHA_PROCESO_ASIENTO
  
LEFT JOIN RRI_PARAMETROS_INF RPP WITH(NOLOCK) ON 
	RPP.CODIGO = 507 AND
	DOC.TIPODOCUMENTO = RPP.ID4


LEFT JOIN RRI_PARAMETROS_INF RP WITH(NOLOCK) ON

    RP.CODIGO = 504 

    AND (

    	RP.ID1 = FL.ID_CARGO 

    	OR (

    		NOT EXISTS (SELECT * FROM RRI_PARAMETROS_INF WHERE CODIGO = RP.CODIGO AND ID1 = FL.ID_CARGO)

    		AND RP.ID2 = FL.OPERACION

    	) OR (

    		NOT EXISTS (SELECT * FROM RRI_PARAMETROS_INF WHERE CODIGO = RP.CODIGO 

    			AND (ID1 = FL.ID_CARGO OR ID2 = FL.OPERACION))

    		AND RP.ID1 = 0

    		AND RP.ID2 = 0

    	)

    )

LEFT JOIN CON_SELLOS CS WITH (NOLOCK) ON 
	CS.COD_PROD = CASE WHEN S.MONEDA=2 THEN
					'PRD'
				  ELSE
				  	RP.CHAR2
				  END AND
	CS.TZ_LOCK = 0			
	
LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = FL.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = FL.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = FL.FECHA_PROCESO_ASIENTO
	
 LEFT JOIN CHE_CHEQUERA_TOTAL CQ
    ON CQ.SUCURSAL             = FL.SUCURSAL_ASIENTO
   AND CQ.CUENTA              = S.CUENTA
   AND CQ.MONEDA              = S.MONEDA
   AND CQ.OPERACION           = S.OPERACION
   AND CQ.FECHAENTREGACLIENTE = FL.FECHA_PROCESO_ASIENTO
   
	
WHERE
	YEAR(M.FECHACONTABLE) = YEAR(@fecha_parametro) AND
	MONTH(M.FECHACONTABLE) = MONTH(@fecha_parametro)  AND
	AE.ASIENTO_ORIGINAL IS NULL AND
	(FL.OPERACION= 3598 OR FL.ID_CARGO=280)
   --AND M.ASIENTO=58000010 --IN (21000020,21000079)--58000010--23000059--58000010
</sql>
