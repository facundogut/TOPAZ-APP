	LEFT JOIN TOTAL T ON 
	T.FECHA_PROCESO_ASIENTO=RSC.FECHA_PROCESO_ASIENTO
		
   WHERE 
    YEAR(RSC.FECHACONTABLE) = YEAR(@fecha_parametro) AND
	MONTH(RSC.FECHACONTABLE) = MONTH(@fecha_parametro) </sql>
