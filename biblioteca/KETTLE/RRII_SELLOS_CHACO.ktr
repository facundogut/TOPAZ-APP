    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>function prepararNumeros(numero,cantCeros) {
    // Convertir el número a string para obtener la parte entera
    var numeroStr = numero.toString();
	var cerosToAdd = cantCeros;

    // Dividir el número en parte entera y decimal
    var partes = numeroStr.split(".");

    // Obtener la parte entera y la parte decimal
    var parteEntera = partes[0];
    var parteDecimal = partes.length > 1 ? partes[1] : "00";

    var numeroSalida = parteEntera + parteDecimal;

    // Calcular cuántos ceros agregar para alcanzar una longitud de 10 caracteres
    cerosToAdd = cerosToAdd - numeroSalida.length;

    // Completar con ceros a la izquierda
    for (var i = 0; i &lt; cerosToAdd; i++) {
        numeroSalida = "0" + numeroSalida;
    }

    return numeroSalida;
}

function padIzquierda(value, length) {
    var str = String(value || ""); 
    while (str.length &lt; length) {
        str = '0' + str;
    }
    return str;
}

function padLeftWithZeros(value, length) {
    var str = String(value || ""); 
    if (str[0] === '-') {
        str = '-' + padIzquierda(str.substring(1), length - 1); 
    } else {
        str = padIzquierda(str, length);
    }
    return str;
}

	var MontoImpTxt = prepararNumeros(MONTO_IMPONIBLE,12);
	var ImpuestoTxt = prepararNumeros(IMPUESTO,12);
	var TotalTxt = prepararNumeros(TOTAL,12);
	var ImporteFojasTxt = prepararNumeros(IMPORTE_FOJAS,8);

	var MONTO_IMPONIBLE_STR = ( MONTO_IMPONIBLE_TXT!= null) ? padLeftWithZeros(MONTO_IMPONIBLE_TXT, 12) : "000000000000";
    var IMPUESTO_STR = ( IMPUESTO_TXT!= null) ? padLeftWithZeros(IMPUESTO_TXT, 12) : "000000000000";

    var IMPORTE_FOJAS_STR = ( IMPORTE_FOJAS_TXT!= null) ? padLeftWithZeros(IMPORTE_FOJAS_TXT, 8) : "00000000";
    var TOTAL_STR = ( TOTAL_TXT!= null) ? padLeftWithZeros(TOTAL_TXT, 12) : "000000000000";

</jsScript_script>
