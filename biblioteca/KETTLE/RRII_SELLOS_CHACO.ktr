),
CHE_CHEQUERA_TOTAL AS (
  SELECT 
    --TOP 1 
    SUCURSAL,
    CUENTA,
    MONEDA,
    OPERACION,
    FECHAENTREGACLIENTE,
    SUM(CANTIDADCHEQUES) AS TOTAL_CHEQUES,
