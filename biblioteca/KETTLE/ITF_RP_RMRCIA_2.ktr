    <connection>NBCH</connection>
    <sql>DECLARE @fechaRen VARCHAR(12) = '${P0_VALUE}';
DECLARE @convPadre VARCHAR(5) = (SELECT NUMERICO_1 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 476);
DECLARE @convHijo VARCHAR(5) = (SELECT NUMERICO_2 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 476);--IDENTIFICA A LOT


IF (isdate(@fechaRen) = 1)
BEGIN

--cab
SELECT linea REG FROM (
SELECT 
	cab.id,
	concat(
		'0',
		CASE WHEN relEnte.cod_ente IS NULL OR relEnte.cod_ente NOT IN ('FA0','855') THEN 'LOT' ELSE relEnte.cod_ente END,
		CONVERT(VARCHAR(8),cab.FECHACARGA,112),
		REPLICATE('0', 86)
	) linea
	FROM CONV_REL_ENTECONV (nolock) relEnte
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN CONV_CONVENIOS_REC conv  (NOLOCK) ON conv.Id_ConvRec = ren.convenio --OR relEnte.ID_CONVREC = conv.Id_ConvPadre		
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
			WHERE  
			 	(ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO IN (@convPadre , @convHijo)) AND
				cab.tz_lock = 0 AND 
				ren.ESTADO = 'R' AND 
				ren.Tz_Lock = 0 AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) 
				
UNION ALL 

SELECT
	cab.id,
	concat(
		'0',
		CASE WHEN relEnte.cod_ente IS NULL OR relEnte.cod_ente NOT IN ('FA0','855') THEN 'LOT' ELSE relEnte.cod_ente END,
		CONVERT(VARCHAR(8),cab.FECHACARGA,112),
		REPLICATE('0', 86)
	) linea
	FROM CONV_REL_ENTECONV (nolock) relEnte 
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
		JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON  conv.Id_ConvRec = ren.convenio --OR conv.Id_ConvPadre = ren.convenio_padre
		JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
			WHERE  
			    (ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO IN (@convPadre , @convHijo)) AND
				ren.ESTADO = 'R' AND  
				cab.tz_lock = 0 AND 
				conv.tz_lock = 0 AND 
				ren.Tz_Lock = 0 AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) 
-------
UNION ALL 
--det

SELECT
	cab.id,
	concat(
	   CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN LEFT(concat(det.codigo_barras_rendido,replicate(' ',97)),97)
			ELSE concat('10', substring(det.codigo_barras,8,14),replicate(' ', 12)) END,
		CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN '' 
			ELSE RIGHT(CONCAT('000000000000',CAST(CAST(det.importe*100 AS BIGINT) AS VARCHAR(12))),12) END ,
		CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN '' 
			ELSE CONVERT(VARCHAR(8),det.fecha_cobranza,112) END,
		CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN '' 
			ELSE LEFT(CONCAT(trim(det.codigo_barras),replicate(' ',50)),50) END
	) linea		
	FROM CONV_REL_ENTECONV (nolock) relEnte
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN CONV_CONVENIOS_REC conv  (NOLOCK) ON conv.Id_ConvRec = ren.convenio --OR relEnte.ID_CONVREC = conv.Id_ConvPadre		
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
		JOIN REC_DET_RECAUDOS_CAJA (NOLOCK) det ON det.id_cabezal = cab.id
			WHERE  
			 	(ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO IN (@convPadre , @convHijo)) AND
				cab.tz_lock = 0 AND 
				ren.ESTADO = 'R' AND 
				ren.Tz_Lock = 0 AND 
				ren.FECHA = CAST(@fechaRen AS DATETIME) 

UNION ALL 
SELECT
	cab.id,
   	concat(
	   CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN LEFT(concat(det.codigo_barras_rendido,replicate(' ',98)),98)
			ELSE concat('10', substring(det.codigo_barras,8,14),replicate(' ', 12)) END,
		CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN '' 
			ELSE RIGHT(CONCAT('000000000000',CAST(CAST(det.importe*100 AS BIGINT) AS VARCHAR(12))),12) END ,
		CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN '' 
			ELSE CONVERT(VARCHAR(8),det.fecha_cobranza,112) END,
		CASE WHEN det.codigo_barras_rendido &lt;&gt; ' ' THEN '' 
			ELSE LEFT(CONCAT(trim(det.codigo_barras),replicate(' ',50)),50) END
	) linea			
   	FROM CONV_REL_ENTECONV (nolock) relEnte 
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
		JOIN REC_DET_RECAUDOS_CANAL (NOLOCK) det ON det.id_cabezal = cab.id
		JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON  conv.Id_ConvRec = ren.convenio --OR conv.Id_ConvPadre = ren.convenio_padre
		JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
			WHERE  
			  	(ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO IN (@convPadre , @convHijo)) AND
				ren.ESTADO = 'R' AND  
				cab.tz_lock = 0 AND 
				det.tz_lock = 0 AND 
				conv.tz_lock = 0 AND 
				ren.Tz_Lock = 0 AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) 		

--pie
UNION ALL 
SELECT 
	cab.id,
	concat(
		'2',
		RIGHT(concat('000000',ren.totalregistros + 2),6),
		RIGHT(concat(replicate('0',16),CAST(ren.totalimporte*100 AS BIGINT)),16),
		REPLICATE('0', 75)
	) linea
	FROM CONV_REL_ENTECONV (nolock) relEnte
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN CONV_CONVENIOS_REC conv  (NOLOCK) ON conv.Id_ConvRec = ren.convenio --OR relEnte.ID_CONVREC = conv.Id_ConvPadre		
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
			WHERE  
			 	(ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO IN (@convPadre , @convHijo)) AND
				cab.tz_lock = 0 AND 
				ren.ESTADO = 'R' AND 
				ren.Tz_Lock = 0 AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) 
				
UNION ALL 

SELECT
	cab.id,
	concat(
		'2',
		RIGHT(concat('000000',ren.totalregistros + 2),6),
		RIGHT(concat(replicate('0',16),CAST(ren.totalimporte*100 AS BIGINT)),16),
		REPLICATE('0', 75)
	) linea
	FROM CONV_REL_ENTECONV (nolock) relEnte 
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
		JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON  conv.Id_ConvRec = ren.convenio --OR conv.Id_ConvPadre = ren.convenio_padre
		JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
			WHERE  
			    (ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO IN (@convPadre , @convHijo)) AND
				ren.ESTADO = 'R' AND  
				cab.tz_lock = 0 AND 
				conv.tz_lock = 0 AND 
				ren.Tz_Lock = 0 AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) 

) xd ORDER BY id, substring(linea, 1,1)
---------

END</sql>
