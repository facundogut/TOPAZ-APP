        <class_source>import org.apache.commons.codec.digest.DigestUtils;
import java.io.FileInputStream;
import java.io.IOException;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
    Object[] r = getRow(); // Obtenemos la fila actual
    if (r == null) {
        setOutputDone(); // No hay más filas, terminamos
        return false;
    }
	// Creo una fila para que tenga espacio para el nuevo campo de salida
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

    // Suponiendo que el campo "filename" contiene el nombre y ruta del archivo
	String filename =get(Fields.In, "filename").getString(r);//get(Fields.In, "filename").getString(r);
    String md5Hex = null;

    FileInputStream inputStream = null;
    try {
        inputStream = new FileInputStream(filename);
        md5Hex = DigestUtils.md5Hex(inputStream); // Calculamos el MD5 del archivo
    } catch (IOException e) {
        logError("Error processing file: " + filename, e);
        throw new KettleException("Error processing file: " + filename, e);
    } finally {
        if (inputStream != null) {
            try {
                inputStream.close(); // Cerramos el archivo
            } catch (IOException e) {
                logError("Error closing file: " + filename, e);
            }
        }
    }

    // Ahora añadimos el MD5 al row actual o lo devolvemos en un campo específico
    //get(Fields.Out, "md5").setValue(r, md5Hex); // Asignar el MD5 a la fila actual
	get(Fields.Out, "md5").setValue(outputRow, md5Hex);

    //putRow(data.outputRowMeta, r); // Enviar la fila modificada a la siguiente etapa
	putRow(data.outputRowMeta, outputRow);

    return true; // Continuar procesando
}</class_source>
