    <sql>
SELECT * FROM (

SELECT 
RIGHT(concat(replicate('0', 10), P.NUMEROPERSONA), 10) AS ID_CLIENTE,
LEFT(CASE WHEN P.TIPOPERSONA = 'F' THEN (PF.PRIMERNOMBRE+' '+PF.SEGUNDONOMBRE + replicate(' ', 30)) ELSE P.NOMBRE+ replicate(' ', 30) END,30) COLLATE SQL_Latin1_General_CP1253_CI_AI AS NOMBRE,
LEFT(CASE WHEN P.TIPOPERSONA = 'F' THEN (PF.APELLIDOPATERNO+' '+PF.APELLIDOMATERNO+ replicate(' ', 30)) ELSE replicate(' ', 30) END,30) COLLATE SQL_Latin1_General_CP1253_CI_AI AS APELLIDO,
LEFT((CASE WHEN P.TIPOPERSONA = 'F' THEN 0 ELSE 1 END),1) AS  TIPO_PERSONA,
RIGHT(concat(replicate(' ', 4), P.TipoDocIdent), 4) AS TIPO_DOCUMENTO,
RIGHT(concat(replicate('0', 11), P.NroDocIdent), 11) AS NRO_ID,
REPLACE(LEFT(concat((D.CALLE+' '+CONVERT(VARCHAR(8),D.NUMERO)+' '+CONVERT(varchar(8),D.PISO)+' '+D.APARTAMENTO+' '+D.BARRIO+' '+D.MANZANA+' '+D.PARCELA+' '+D.CASA+' '+D.MONOBLOCK+' '+D.TIRA+' '+D.CHACRA+replicate(' ',36)), replicate(' ', 36)),36) COLLATE SQL_Latin1_General_CP1253_CI_AI, '"','' ) AS DOMICILIO,
LEFT(CASE WHEN ll.DESCRIPCION_DIM3 IS NOT NULL THEN (ll.DESCRIPCION_DIM3+ replicate(' ', 30)) ELSE replicate(' ', 30) END,30) COLLATE SQL_Latin1_General_CP1253_CI_AI AS LOCALIDAD,
LEFT((CASE WHEN D.CPA_VIEJO IS NOT NULL THEN RIGHT(concat(replicate('0', 5), D.CPA_VIEJO), 5) ELSE replicate('0', 5) END), 5) AS CODIGO_POSTAL, 
(CASE
   WHEN CP.DIM1='A' THEN concat('ST', replicate(' ', 3))
   WHEN CP.DIM1='B' THEN concat('BA', replicate(' ', 3))
   WHEN CP.DIM1='C' THEN concat('CF', replicate(' ', 3))
   WHEN CP.DIM1='D' THEN concat('SL', replicate(' ', 3)) 
   WHEN CP.DIM1='E' THEN concat('ER', replicate(' ', 3))
   WHEN CP.DIM1='F' THEN concat('LR', replicate(' ', 3))
   WHEN CP.DIM1='G' THEN concat('SE', replicate(' ', 3))
   WHEN CP.DIM1='H' THEN concat('CH', replicate(' ', 3))
   WHEN CP.DIM1='J' THEN concat('SJ', replicate(' ', 3))
   WHEN CP.DIM1='K' THEN concat('CT', replicate(' ', 3))
   WHEN CP.DIM1='L' THEN concat('LP', replicate(' ', 3))
   WHEN CP.DIM1='M' THEN concat('MZ', replicate(' ', 3))
   WHEN CP.DIM1='N' THEN concat('MS', replicate(' ', 3))
   WHEN CP.DIM1='P' THEN concat('FM', replicate(' ', 3))
   WHEN CP.DIM1='Q' THEN concat('NQ', replicate(' ', 3))
   WHEN CP.DIM1='R' THEN concat('RN', replicate(' ', 3))
   WHEN CP.DIM1='S' THEN concat('SF', replicate(' ', 3))
   WHEN CP.DIM1='T' THEN concat('TC', replicate(' ', 3))
   WHEN CP.DIM1='U' THEN concat('CB', replicate(' ', 3))
   WHEN CP.DIM1='V' THEN concat('TF', replicate(' ', 3))
   WHEN CP.DIM1='W' THEN concat('CR', replicate(' ', 3))
   WHEN CP.DIM1='X' THEN concat('CD', replicate(' ', 3))
   WHEN CP.DIM1='Y' THEN concat('JJ', replicate(' ', 3))
   WHEN CP.DIM1='Z' THEN concat('SC', replicate(' ', 3))
   WHEN CP.DIM1 IS NULL THEN replicate(' ', 5)
END) AS PROVINCIA,
LEFT(concat((CASE WHEN P.TIPOPERSONA = 'F' THEN (SELECT ISNULL(APELLIDOPATERNO, '')+' '+ ISNULL(PRIMERNOMBRE, '') FROM CLI_PERSONASFISICAS WITH (NOLOCK) WHERE NUMEROPERSONAFISICA = VI.PERSONA_VINCULADA) END), replicate(' ', 30)), 30) COLLATE SQL_Latin1_General_CP1253_CI_AI AS CONYUGE,


LEFT( concat( (CASE WHEN P.TIPOPERSONA = 'F' THEN (SELECT TOP 1 a.* FROM (SELECT NUMERO FROM CLI_TELEFONOS WITH (NOLOCK) WHERE ID = P.NUMEROPERSONA AND TIPO = 'PE' AND TZ_LOCK = 0 AND FORMATO = 'PF') AS a) ELSE (SELECT TOP 1 b.* FROM (SELECT NUMERO FROM CLI_TELEFONOS WITH (NOLOCK) WHERE ID = P.NUMEROPERSONA AND TIPO = 'LE' AND TZ_LOCK = 0 AND FORMATO = 'PJ') AS b )END), replicate(' ', 15)),15) AS TELEFONO,

LEFT(concat( (CASE WHEN P.TIPOPERSONA = 'F' THEN (SELECT TOP 1 c.* FROM (SELECT EMAIL FROM CLI_EMAILS WITH (NOLOCK) WHERE ID = P.NUMEROPERSONA AND FORMATO = 'PF' AND TIPO = 'PE' AND TZ_LOCK = 0) AS c) ELSE (SELECT TOP 1 d.* FROM (SELECT EMAIL FROM CLI_EMAILS WITH (NOLOCK) WHERE ID = P.NUMEROPERSONA AND TZ_LOCK = 0 AND FORMATO = 'PJ' AND TIPO = 'LE') AS d) END), replicate(' ', 50)),50) AS EMAIL,

LEFT(concat( (
CASE
	WHEN P.TIPOPERSONA = 'F' THEN (SELECT DESCRIPCION FROM OPCIONES WITH (NOLOCK) WHERE IDIOMA = 'E' AND OPCIONINTERNA = PF.IVA AND NUMERODECAMPO = 2789)
 	WHEN P.TIPOPERSONA = 'J' THEN (SELECT DESCRIPCION FROM OPCIONES WITH (NOLOCK) WHERE IDIOMA = 'E' AND OPCIONINTERNA = PJ.IVA AND NUMERODECAMPO = 4608)
END), replicate(' ', 30)),30) COLLATE SQL_Latin1_General_CP1253_CI_AI AS CONDICIONIVA, 


RIGHT( concat(replicate('0', 5), (CASE WHEN P.TIPOPERSONA = 'F' THEN PF.SUC_ALTA WHEN P.TIPOPERSONA = 'J' THEN PJ.SUC_ALTA END)) ,5) AS SUCURSAL,

LEFT(concat(
(CASE 
	WHEN P.TIPOPERSONA = 'F' THEN (SELECT DESCRIPCION FROM OPCIONES WITH (NOLOCK) WHERE NUMERODECAMPO = 33366 AND IDIOMA='E' AND OPCIONINTERNA = PF.ESTADO)
	WHEN P.TIPOPERSONA = 'J' THEN (SELECT DESCRIPCION FROM OPCIONES WITH (NOLOCK) WHERE NUMERODECAMPO = 33367 AND IDIOMA='E' AND OPCIONINTERNA = PJ.ESTADO)
END)
, replicate(' ', 40)
)
, 40) AS NOTA, 

(CASE WHEN P.TIPOPERSONA = 'J' THEN '0' ELSE (CASE WHEN PF.EMPLEADO_BC_BANCO = 2 THEN '1' ELSE '0' END) END) AS MARCA_EMPLEADO,

(SELECT 

CASE 

WHEN P.TIPOPERSONA = 'F' THEN (

SELECT TOP 1 
CASE 
WHEN PF.ESTADO	  =  1  AND BTPF.TIPO_TRAZA IS NULL THEN 'B' 
WHEN PF.ESTADO	  =  1  AND BTPF.TIPO_TRAZA ='M' THEN 'B' 
WHEN PF.ESTADO	  =  1  AND BTPF.TIPO_TRAZA ='A' THEN 'B'
WHEN BTPF.ESTADO  =  0  AND BTPF.TIPO_TRAZA ='A' THEN 'A' 
WHEN BTPF.ESTADO  =  1  AND BTPF.TIPO_TRAZA ='M' THEN 'B' 
WHEN BTPF.ESTADO  =  1  AND BTPF.TIPO_TRAZA ='A' THEN 'B' 
WHEN BTPF.ESTADO  =  0  AND BTPF.TIPO_TRAZA ='A' THEN 'A' 
WHEN PF.ESTADO	  =  0  AND BTPF.TIPO_TRAZA IS NULL AND (BTDIR.TZ_LOCK =0 OR BTTEL.TZ_LOCK= 0 OR BTEM.TZ_LOCK= 0 OR BTCON.TZ_LOCK= 0) THEN 'M' 
WHEN PF.ESTADO	=  0  AND (BTPF.TIPO_TRAZA IS NULL OR BTPF.TIPO_TRAZA = 'M') AND 
	(BTDIR.TIPO_TRAZA ='A' OR BTDIR.TIPO_TRAZA ='M' OR BTDIR.TIPO_TRAZA ='A' OR BTDIR.TIPO_TRAZA ='M' OR BTTEL.TIPO_TRAZA ='A' OR BTTEL.TIPO_TRAZA ='M' OR
	BTEM.TIPO_TRAZA ='A' OR BTEM.TIPO_TRAZA ='M' OR BTCON.TIPO_TRAZA ='A' OR BTCON.TIPO_TRAZA ='M') THEN 'M' 
ELSE '' END AS T_TRAZA
FROM CLI_PERSONASFISICAS PF
LEFT JOIN BITACORA_PERSONAS_FISICAS  BTPF 	(NOLOCK) ON BTPF.NUMEROPERSONAFISICA  = PF.NUMEROPERSONAFISICA AND BTPF.FECHA = CAST( ? as date) AND PF.FECHAALTA &lt;= CAST( ? as date)
LEFT JOIN BITACORA_DIRECCIONES       BTDIR (NOLOCK) ON BTDIR.ID  = PF.NUMEROPERSONAFISICA	AND BTDIR.FORMATO = 'PF' AND BTDIR.FECHA = CAST( ? as date)
LEFT JOIN BITACORA_TELEFONOS 		 BTTEL (NOLOCK) ON BTTEL.ID = PF.NUMEROPERSONAFISICA AND BTTEL.FORMATO = 'PF' AND BTTEL.FECHA = CAST( ? as date)
LEFT JOIN BITACORA_CORREOELECTRONICO BTEM 	(NOLOCK) ON BTEM.ID  = PF.NUMEROPERSONAFISICA	AND BTEM.FORMATO  = 'PF' AND BTEM.FECHA =  CAST( ? as date)
LEFT JOIN BITACORA_CONYUGUES BTCON (NOLOCK) 		 ON BTCON.NUMEROPERSONAFISICA  = PF.NUMEROPERSONAFISICA AND BTCON.FECHA = CAST( ? as date)

WHERE PF.NUMEROPERSONAFISICA = P.NUMEROPERSONA AND PF.FECHAALTA &lt;= CAST( ? as date)
AND (PF.TZ_LOCK=0 OR BTPF.TZ_LOCK=0 OR BTDIR.TZ_LOCK=0 OR BTTEL.TZ_LOCK=0 OR BTEM.TZ_LOCK=0)
AND (BTPF.FECHA = CAST( ? as date) 
	OR BTDIR.FECHA = CAST( ? as date)
	OR BTTEL.FECHA = CAST( ? as date)
	OR BTEM.FECHA = CAST( ? as date)
	)

AND NOT EXISTS (SELECT TOP 1 1 FROM BITACORA_PERSONAS_FISICAS 
					WHERE NUMEROPERSONAFISICA = PF.NUMEROPERSONAFISICA AND FECHA=PF.FECHAALTA AND TIPO_TRAZA = 'A' AND 1 = (SELECT TOP 1 1 FROM BITACORA_PERSONAS_FISICAS WHERE NUMEROPERSONAFISICA = PF.NUMEROPERSONAFISICA AND FECHA=PF.FECHAALTA AND PF.ESTADO = 1 AND TIPO_TRAZA ='M')) 	

 ORDER BY BTPF.TIPO_TRAZA ASC, BTPF.HORA DESC)

ELSE (
SELECT TOP 1 
CASE
WHEN PJ.ESTADO	  =  1  AND BTPJ.TIPO_TRAZA IS NULL THEN 'B'
WHEN PJ.ESTADO	  =  1  AND BTPJ.TIPO_TRAZA ='M' THEN 'B'
WHEN PJ.ESTADO	  =  1  AND BTPJ.TIPO_TRAZA ='A' THEN 'B' 
WHEN PJ.ESTADO	  =  0  AND BTPJ.TIPO_TRAZA IS NULL AND BTDIR.TZ_LOCK IS NULL AND BTTEL.TZ_LOCK IS NULL AND BTEM.TZ_LOCK IS NULL THEN ' ' 
WHEN PJ.ESTADO	  =  0  AND BTPJ.TIPO_TRAZA IS NULL AND (BTDIR.TZ_LOCK =0 OR BTTEL.TZ_LOCK= 0 OR BTEM.TZ_LOCK= 0) THEN 'M' 
WHEN BTPJ.ESTADO	  =  0  AND BTPJ.TIPO_TRAZA ='A' THEN 'A'  
WHEN BTPJ.ESTADO	  =  1  AND BTPJ.TIPO_TRAZA ='M' THEN 'B'
WHEN BTPJ.ESTADO	  =  1  AND BTPJ.TIPO_TRAZA ='A' THEN 'B'
WHEN PJ.ESTADO	=  0  AND (BTPJ.TIPO_TRAZA IS NULL OR BTPJ.TIPO_TRAZA = 'M') AND 
	(BTDIR.TIPO_TRAZA ='A' OR BTDIR.TIPO_TRAZA ='M' OR BTDIR.TIPO_TRAZA ='A' OR BTDIR.TIPO_TRAZA ='M' OR BTTEL.TIPO_TRAZA ='A' OR BTTEL.TIPO_TRAZA ='M' OR
	BTEM.TIPO_TRAZA ='A' OR BTEM.TIPO_TRAZA ='M') THEN 'M' 
ELSE '' END AS T_TRAZA
FROM CLI_PERSONASJURIDICAS PJ
LEFT JOIN BITACORA_PERSONAS_JURIDICAS BTPJ (NOLOCK) ON BTPJ.NUMEROPERSONAJURIDICA  = PJ.NUMEROPERSONAJURIDICA AND BTPJ.FECHA = CAST( ? as date) AND PJ.FECHAALTA &lt;= CAST( ? as date)
LEFT JOIN BITACORA_DIRECCIONES       BTDIR (NOLOCK) ON BTDIR.ID = BTPJ.NUMEROPERSONAJURIDICA	AND BTDIR.FORMATO = 'PJ' AND BTDIR.FECHA = CAST( ? as date)
LEFT JOIN BITACORA_TELEFONOS 		  BTTEL (NOLOCK) ON BTTEL.ID = BTPJ.NUMEROPERSONAJURIDICA   AND BTTEL.FORMATO = 'PJ' AND BTTEL.FECHA = CAST( ? as date)
LEFT JOIN BITACORA_CORREOELECTRONICO BTEM 	(NOLOCK) ON BTEM.ID  = BTPJ.NUMEROPERSONAJURIDICA	AND BTEM.FORMATO  = 'PJ' AND BTEM.FECHA = CAST( ? as date)

WHERE PJ.NUMEROPERSONAJURIDICA = P.NUMEROPERSONA AND PJ.FECHAALTA &lt;= CAST( ? as date)
AND (PJ.TZ_LOCK=0 OR BTPJ.TZ_LOCK=0 OR BTDIR.TZ_LOCK=0 OR BTTEL.TZ_LOCK=0 OR BTEM.TZ_LOCK=0)
AND (BTPJ.FECHA = CAST( ? as date) 
	OR BTDIR.FECHA = CAST( ? as date)
	OR BTTEL.FECHA = CAST( ? as date)
	OR BTEM.FECHA = CAST( ? as date)
	)

AND NOT EXISTS (SELECT TOP 1 1 FROM BITACORA_PERSONAS_JURIDICAS 
					WHERE NUMEROPERSONAJURIDICA = PJ.NUMEROPERSONAJURIDICA AND FECHA=PJ.FECHAALTA AND TIPO_TRAZA = 'A' AND 1 = (SELECT TOP 1 1 FROM BITACORA_PERSONAS_FISICAS WHERE NUMEROPERSONAFISICA = PJ.NUMEROPERSONAJURIDICA AND FECHA=PJ.FECHAALTA AND PJ.ESTADO = 1 AND TIPO_TRAZA ='M')) 	

ORDER BY BTPJ.TIPO_TRAZA ASC, BTPJ.HORA DESC)

END ) AS ESTADO
 
FROM VW_PERSONAS_FYJ P
LEFT JOIN CLI_PERSONASJURIDICAS PJ ON (P.NUMEROPERSONA = PJ.NUMEROPERSONAJURIDICA)
LEFT JOIN CLI_PERSONASFISICAS PF ON (PF.NUMEROPERSONAFISICA = P.NUMEROPERSONA)
LEFT JOIN CLI_DIRECCIONES D ON (P.NUMEROPERSONA = D.ID) AND (D.FORMATO IN ('PF','PJ') AND D.ORDINAL_DIR = 1 AND D.TZ_LOCK = 0 ) 
LEFT JOIN CLI_LOCALIDADES ll ON (ll.DIM1 = D.PROVINCIA AND ll.DIM2=D.DEPARTAMENTO AND ll.DIM3 = D.LOCALIDAD AND ll.TZ_LOCK = 0)
LEFT JOIN CLI_PROVINCIAS CP ON CP.DIM1=D.PROVINCIA
LEFT JOIN CLI_VINCULACIONES VI ON (P.NUMEROPERSONA = VI.PERSONA_VINCULANTE AND VI.ROL = 71 AND VI.TZ_LOCK = 0 AND VI.FECHA_FIN IS NULL)

) P </sql>
