      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>trans_Status = CONTINUE_TRANSFORMATION; //Estado default (Permite arrojar error por el javascript)

//Validaciones para todos los registros
if (Reg_Comp == null)
{
	abortar("Se detecto una linea vacia en el archivo.", "NODATA", NROREG);
}
else if (Reg_Comp.length != 132)
{
	abortar("Largo de Registro Invalido, deberia ser de 132 caracteres. El Largo del registro es: " + Reg_Comp.length, Reg_Comp, NROREG);
}
else if (MEMORY == 'T') //Validaciones por tipo de Registro
{
	abortar("Estructura de archivo invalida. Despues de un Pie de Archivo NO se esperan mas Registros", Reg_Comp, NROREG);
}
else if (TIPOREG == 'T')
{
	var REGINF = Reg_Comp.slice(1,7);
	
	if (isNaN(REGINF))
	{
		abortar("Campo cantidad de Registros en el Pie de Archivo NO Numerico. Campo Cantidad de Registros: " + REGINF, Reg_Comp, NROREG);
	}
	else if (CANTREGIND != Number(REGINF))
	{
		abortar("Archivo Rechazado. La Cantidad de Registros individuales informada en el Pie NO coincide con la calculada. Cantidad de Registros individuales: " + CANTREGIND + " Cantidad de Registros individuales Informada en el Pie: " + Number(REGINF), "NODATA", "NODATA");
	}
	else
	{
		setVariable("CANTREGL", NROREG, "r");
		setVariable("CANTREGIND", CANTREGIND, "r");
	}
}

MEMORY = TIPOREG;
CANTREGIND ++;

</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>//Variables
var MEMORY = ''; //Variable que memoriza el tipo de registro leido anteriormente
var CANTREGIND = 0; //Variable para contar los registros individuales

//Funciones
function abortar (MSG, REG, NRO) //Funcion que setea variables de error y aborta
{
	writeToLog("e", "Error: " + MSG);
	writeToLog("e", "Registro: " + REG);
	writeToLog("e", "Numero Registro: " + NRO);
	setVariable("ERROR", MSG, "r");
	setVariable("REGISTRO", REG, "r");
	setVariable("NROREG", NRO, "r");
	trans_Status = ERROR_TRANSFORMATION;
}

</jsScript_script>
      </jsScript>
