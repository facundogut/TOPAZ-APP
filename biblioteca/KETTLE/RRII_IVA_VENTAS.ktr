    <rowlimit>0</rowlimit>
    <sql>DECLARE @FechaP1 AS DATETIME;
SET @FechaP1 = CONVERT(DATETIME,?,103);

DECLARE @FechaP2 AS DATETIME;
SET @FechaP2 = CONVERT(DATETIME,?,103);

DECLARE @PERIOD AS VARCHAR;
SET @PERIOD = ?;


--ASIENTOS SIN EXTORNAR IVA VENTAS
SELECT DISTINCT

	ISNULL((SELECT MAX(NUMERADOR_CBTE) 
	FROM RRII_IVA_VENTAS_COMPROBANTES WHERE 
	PERIODO =@PERIOD),0) AS NUMERADOR,
 	
	S.PRODUCTO AS MODULO,
	
	S.CUENTA AS CUENTA,
	
	M.OPERACION AS OPERACION,
	
	DOC.TIPODOCUMENTO AS TIPODOCUMENTO,
	
	FL.MONTO_FACTURABLE AS MONTO_FACTURABLE,
	
	FL.MONTO_IMPONIBLE AS MONTO_IMPONIBLE,
	
	M.FECHACONTABLE AS FECHA_COMPROBANTE,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHA_COMPROBANTE,	
	
	'099' AS TIPO_COMPROBANTE,	
		
	CASE 
	    WHEN R.CHAR1 = 'G' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
	    
	    WHEN R.CHAR1 = 'P' AND P.C6800='L' AND R.ID5='L' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
	    
	    ELSE CONCAT(REPLICATE('0', 4), '7')
	
	END AS PUNTO_VENTA,
	
	F.ASIENTO AS ASIENTO,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHACONTABLE,
	
	F.SUCURSAL_ASIENTO AS SUCURSAL_ASIENTO,	  
	
	dbo.AGREGOCEROS(RD.CODIGO_DOC_RRII, 2) AS CODIGO_DOCUMENTO_COMPRADOR,	

	RIGHT(CONCAT(dbo.AGREGOCEROS(0, 20), 

	DOC.NUMERODOCUMENTO), 20) AS NUMERO_DOCUMENTO,	

	LEFT(CONCAT(C.NOMBRECLIENTE, REPLICATE(' ',30)), 30) AS NOMBRE_CLIENTE,

CASE		
	WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
	THEN
		(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE)		
	ELSE
		(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE) * ISNULL(MI.TIPO_CAMBIO_MOV, 0)
END AS MONTO_TOTAL,

	dbo.AGREGOCEROS(0, 15) AS TOTAL_NO_INTEGRA,	

	dbo.AGREGOCEROS(0, 15) AS PERCEPCION,	

	dbo.AGREGOCEROS(0, 15) AS TOTAL_EXENTAS,	

	dbo.AGREGOCEROS(0, 15) AS A_CUENTA_IMPUESTOS,	

	dbo.AGREGOCEROS(0, 15) AS INGRESOS_BRUTOS,	

	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_MUNICIPALES,	

	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_INTERNOS,	

	RM.COD_MONEDA_RRII AS CODIGO_MONEDA,	
	CASE 
		WHEN S.MONEDA = (SELECT MONNAC FROM PARAMETROS WITH(NOLOCK)) THEN
			1 
		ELSE
			ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	END AS TIPO_CAMBIO,


	1 AS CANT_ALICUOTAS_IVA,	

	0 AS CODIGO_OPERACION,	

	dbo.AGREGOCEROS(0, 15) AS OTROS_TRIBUTOS,	

	CONVERT(DATETIME,M.FECHAVALOR,103) AS FECHA_VENCIMIENTO
 
FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK)

INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA	
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	
	AND GDC.MOV_SIGNO = 'C'
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )	
	
INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.ASIENTO = GDC.MOV_ASIENTO
	AND F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND F.ORDINAL_MOVIMIENTO = GDC.MOV_ORDINAL
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)) 

INNER JOIN CI_FACTURA_LINEA FL WITH(NOLOCK) ON
	FL.ASIENTO = GDC.MOV_ASIENTO
	AND FL.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND FL.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND FL.ORDINAL_MOVIMIENTO = F.ORDINAL_MOVIMIENTO	
	
	--AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 1 AND TZ_LOCK = 0)) 	
	--	OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 1 AND TZ_LOCK = 0)))
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  ) 
LEFT JOIN CI_IMPUESTOS IMP WITH (NOLOCK) 
    ON FL.ID_IMPUESTO = IMP.ID_IMPUESTO
    AND IMP.TIPO_IMPUESTO = 1
    AND IMP.TZ_LOCK = 0
    
LEFT JOIN CI_CARGOS CARG WITH (NOLOCK)
    ON FL.ID_CARGO = CARG.ID_CARGO
    AND CARG.TIPO_CARGO_IMPOSITIVO = 1
    AND CARG.TZ_LOCK = 0

INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO=F.FECHA_PROCESO_ASIENTO
	AND M.SUCURSAL=F.SUCURSAL_ASIENTO
	AND M.ASIENTO=F.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
	
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO=M.ASIENTO AND
	A.ESTADO=77 AND
	A.SUCURSAL=F.SUCURSAL_ASIENTO AND
	A.FECHAPROCESO=M.FECHAPROCESO

INNER JOIN PRODUCTOS P WITH (NOLOCK) ON
	P.C6250 = GDC.SALDO_ORIG_PRODUCTO AND
	( (P.tz_lock &lt; 300000000000000 OR P.tz_lock >= 400000000000000) 
	AND (P.tz_lock &lt; 100000000000000 OR P.tz_lock >= 200000000000000)  )


LEFT JOIN RRI_PARAMETROS_INF R WITH(NOLOCK) ON 
	R.CODIGO = 511 AND 
	R.ID1 = GDC.MOV_OPERACION AND
	R.ID5 = CASE WHEN R.CHAR1='P' AND P.C6800='L' THEN 'L' 
			WHEN R.CHAR1='G' THEN 'X'
			ELSE 'P' END AND
	R.TZ_LOCK = 0
	
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803	
	AND ( (C.tz_lock &lt; 300000000000000 OR C.tz_lock >= 400000000000000) 
	AND (C.tz_lock &lt; 100000000000000 OR C.tz_lock >= 200000000000000)  )	

INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND CP.TITULARIDAD = 'T'	
	AND ( (CP.tz_lock &lt; 300000000000000 OR CP.tz_lock >= 400000000000000) 
	AND (CP.tz_lock &lt; 100000000000000 OR CP.tz_lock >= 200000000000000)  )	

LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON	
	( (DOC.tz_lock &lt; 300000000000000 OR DOC.tz_lock >= 400000000000000) 
	AND (DOC.tz_lock &lt; 100000000000000 OR DOC.tz_lock >= 200000000000000)  )	
	AND DOC.NUMEROPERSONAFJ = CP.NUMEROPERSONA	
	AND DOC.TIPODOCUMENTO = (						
						SELECT TOP 1 TIPODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							( (tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) 
							AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)  )	
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)	
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							( (tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) 
							AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)  )	
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
						)
LEFT JOIN RRII_IVA_DOCUMENTOS AS RD WITH (nolock) ON	
	RD.TIPO_DOCUMENTO = DOC.TIPODOCUMENTO
	
LEFT JOIN RRII_IVA_MONEDAS AS RM WITH (nolock) ON	
	RM.CODIGO_MONEDA = F.MONEDA
	
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MI WITH (nolock) ON	
	MI.ASIENTO = F.ASIENTO	
	AND MI.SUCURSAL = F.SUCURSAL_ASIENTO	
	AND MI.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO	
	AND ( (MI.tz_lock &lt; 300000000000000 OR MI.tz_lock >= 400000000000000) 
	AND (MI.tz_lock &lt; 100000000000000 OR MI.tz_lock >= 200000000000000)  )
	
 --Me quiero quedar con los asientos que no han sido extornados, por eso el siguiente LEFT JOIN
LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO
	
WHERE
	((GDC.tz_lock &lt; 300000000000000 OR GDC.tz_lock >= 400000000000000) 
	AND (GDC.tz_lock &lt; 100000000000000 OR GDC.tz_lock >= 200000000000000))
	AND M.FECHACONTABLE BETWEEN @FechaP1 AND @FechaP2 	
	AND IMP.ID_IMPUESTO IS NOT NULL OR CARG.ID_CARGO IS NOT NULL

UNION
--IVA VENTAS ASIENTOS EXTORNADOS
SELECT
	ISNULL((SELECT MAX(NUMERADOR_CBTE) 
	FROM RRII_IVA_VENTAS_COMPROBANTES WHERE 
	PERIODO =@PERIOD),0) AS NUMERADOR,
	
	S.PRODUCTO AS MODULO,
	
	S.CUENTA AS CUENTA,
	
	M.OPERACION AS OPERACION,
	
	DOC.TIPODOCUMENTO AS TIPODOCUMENTO,
	
	FL.MONTO_FACTURABLE AS MONTO_FACTURABLE,
	
	FL.MONTO_IMPONIBLE AS MONTO_IMPONIBLE,
	
	M.FECHACONTABLE AS FECHA_COMPROBANTE,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHA_COMPROBANTE,	
	
	'099' AS TIPO_COMPROBANTE,	
	
	CASE 
    WHEN R.CHAR1 = 'G' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
    
    WHEN R.CHAR1 = 'P' AND P.C6800='L' AND R.ID5='L' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
    
    ELSE CONCAT(REPLICATE('0', 4), '7')
	
	END AS PUNTO_VENTA,
	
	F.ASIENTO AS ASIENTO,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHACONTABLE,
	
	F.SUCURSAL_ASIENTO AS SUCURSAL_ASIENTO,	  
	
	dbo.AGREGOCEROS(RD.CODIGO_DOC_RRII, 2) AS CODIGO_DOCUMENTO_COMPRADOR,	
	
	RIGHT(CONCAT(dbo.AGREGOCEROS(0, 20),DOC.NUMERODOCUMENTO), 20) AS NUMERO_DOCUMENTO,	
	
	LEFT(CONCAT(C.NOMBRECLIENTE, REPLICATE(' ',30)), 30) AS NOMBRE_CLIENTE,
	
	CASE WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
	THEN
		(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE)
	ELSE
		(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE) * ISNULL(MIE.TIPO_CAMBIO_MOV, 0)			
	END AS MONTO_TOTAL,
	
	dbo.AGREGOCEROS(0, 15) AS TOTAL_NO_INTEGRA,	
	
	dbo.AGREGOCEROS(0, 15) AS PERCEPCION,	
	
	dbo.AGREGOCEROS(0, 15) AS TOTAL_EXENTAS,	
	
	dbo.AGREGOCEROS(0, 15) AS A_CUENTA_IMPUESTOS,	
	
	dbo.AGREGOCEROS(0, 15) AS INGRESOS_BRUTOS,	
	
	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_MUNICIPALES,	
	
	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_INTERNOS,	
	
	RM.COD_MONEDA_RRII AS CODIGO_MONEDA,
	CASE 
		WHEN S.MONEDA = (SELECT MONNAC FROM PARAMETROS WITH(NOLOCK)) THEN
			1 
		ELSE
			ISNULL(MIE.TIPO_CAMBIO_MOV, 0)
	END AS TIPO_CAMBIO,
	
	1 AS CANT_ALICUOTAS_IVA,	
	
	0 AS CODIGO_OPERACION,	
	
	dbo.AGREGOCEROS(0, 15) AS OTROS_TRIBUTOS,	
	
	CONVERT(DATETIME,M.FECHAVALOR,103) AS FECHA_VENCIMIENTO
	
FROM CON_ASIENTOS_EXTORNADOS AE WITH(NOLOCK) 
	
INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.SUCURSAL_ASIENTO = AE.SUCURSAL_ORIGINAL
	AND F.ASIENTO = AE.ASIENTO_ORIGINAL
	AND F.FECHA_PROCESO_ASIENTO = AE.FECHAPROCESO_ORIGINAL
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)  )
		
INNER JOIN CI_FACTURA_LINEA FL WITH(NOLOCK) ON 
	FL.ORDINAL_MOVIMIENTO = F.ORDINAL_MOVIMIENTO
	AND FL.ASIENTO = F.ASIENTO
	AND FL.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO
	AND FL.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO
	AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 1 AND TZ_LOCK = 0)) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 1 AND TZ_LOCK = 0)))
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  )	
	
INNER JOIN GRL_DETALLE_CONTABILIDAD GDC WITH (NOLOCK)
	ON GDC.MOV_ASIENTO = FL.ASIENTO
	AND GDC.MOV_FECHA_PROCESO = FL.FECHA_PROCESO_ASIENTO
	AND GDC.MOV_SUCURSAL = FL.SUCURSAL_ASIENTO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND GDC.MOV_TRREAL = FL.TR_REAL 
	AND GDC.MOV_SIGNO = 'D'
	AND ( (GDC.tz_lock &lt; 300000000000000 OR GDC.tz_lock >= 400000000000000) 
	AND (GDC.tz_lock &lt; 100000000000000 OR GDC.tz_lock >= 200000000000000)  )	

INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )	

--MOVIMIENTOS CONTABLES SE USA PARA FILTRO DE FECHA CONTABLE 
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO=F.FECHA_PROCESO_ASIENTO
	AND M.SUCURSAL=F.SUCURSAL_ASIENTO
	AND M.ASIENTO=F.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
	
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO = M.ASIENTO
	AND	A.ESTADO = 77 
	AND A.SUCURSAL = F.SUCURSAL_ASIENTO 
	AND A.FECHAPROCESO = M.FECHAPROCESO
	
INNER JOIN PRODUCTOS P WITH (NOLOCK) ON
	P.C6250 = GDC.SALDO_ORIG_PRODUCTO AND
	( (P.tz_lock &lt; 300000000000000 OR P.tz_lock >= 400000000000000) 
	AND (P.tz_lock &lt; 100000000000000 OR P.tz_lock >= 200000000000000)  )

LEFT JOIN RRI_PARAMETROS_INF R WITH(NOLOCK) ON 
	R.CODIGO = 511 AND 
	R.ID1 = GDC.MOV_OPERACION AND
	R.ID5 = CASE WHEN R.CHAR1='P' AND P.C6800='L' THEN 'L' 
			WHEN R.CHAR1='G' THEN 'X'
			ELSE 'P' END AND
	R.TZ_LOCK = 0
	
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803	
	AND ( (C.tz_lock &lt; 300000000000000 OR C.tz_lock >= 400000000000000) 
	AND (C.tz_lock &lt; 100000000000000 OR C.tz_lock >= 200000000000000)  )	

INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND CP.TITULARIDAD = 'T'	
	AND ( (CP.tz_lock &lt; 300000000000000 OR CP.tz_lock >= 400000000000000) 
	AND (CP.tz_lock &lt; 100000000000000 OR CP.tz_lock >= 200000000000000)  )	

LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON	
	( (DOC.tz_lock &lt; 300000000000000 OR DOC.tz_lock >= 400000000000000) 
	AND (DOC.tz_lock &lt; 100000000000000 OR DOC.tz_lock >= 200000000000000)  )	
	AND DOC.NUMEROPERSONAFJ = CP.NUMEROPERSONA	
	AND DOC.TIPODOCUMENTO = (						
						SELECT TOP 1 TIPODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)
						WHERE						
							( (tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) 
							AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)  )						
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)	
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							( (tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) 
							AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)  )						
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
						)

LEFT JOIN RRII_IVA_DOCUMENTOS AS RD WITH (nolock) ON	
	RD.TIPO_DOCUMENTO = DOC.TIPODOCUMENTO

LEFT JOIN RRII_IVA_MONEDAS AS RM WITH (nolock) ON	
	RM.CODIGO_MONEDA = F.MONEDA
	
/*para asientos extornados en moneda dolar o ndice, hay que tomar la cotizacin del asiento extornado original*/
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MIE WITH (nolock) ON	
	MIE.ASIENTO = AE.ASIENTO_ORIGINAL	
	AND MIE.SUCURSAL = AE.SUCURSAL_ORIGINAL	
	AND MIE.FECHAPROCESO = AE.FECHAPROCESO_ORIGINAL
	AND ( (MIE.tz_lock &lt; 300000000000000 OR MIE.tz_lock >= 400000000000000) 
	AND (MIE.tz_lock &lt; 100000000000000 OR MIE.tz_lock >= 200000000000000)  )

WHERE 
	AE.ASIENTO_ORIGINAL IS NOT NULL
	AND M.FECHACONTABLE BETWEEN @FechaP1 AND @FechaP2 

UNION ALL

--ASIENTOS POR LINEA DE CAJA SIN EXTORNAR
SELECT
	ISNULL((SELECT MAX(NUMERADOR_CBTE) 
	FROM RRII_IVA_VENTAS_COMPROBANTES WHERE 
	PERIODO =@PERIOD),0) AS NUMERADOR,
	
	S.PRODUCTO AS MODULO,

	S.CUENTA AS CUENTA,

	M.OPERACION AS OPERACION,

	F.FIRSTID AS TIPODOCUMENTO,

	FL.MONTO_FACTURABLE AS MONTO_FACTURABLE,

	FL.MONTO_IMPONIBLE AS MONTO_IMPONIBLE,

	M.FECHACONTABLE AS FECHA_COMPROBANTE,

	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHA_COMPROBANTE,	

	'099' AS TIPO_COMPROBANTE,	

	CASE 
    WHEN R.CHAR1 = 'G' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
    
    WHEN R.CHAR1 = 'P' AND P.C6800='L' AND R.ID5='L' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
    
    ELSE CONCAT(REPLICATE('0', 4), '7')
	
	END AS PUNTO_VENTA,

	F.ASIENTO AS ASIENTO,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHACONTABLE,
	
	F.SUCURSAL_ASIENTO AS SUCURSAL_ASIENTO,	  

	dbo.AGREGOCEROS(RD.CODIGO_DOC_RRII, 2) AS CODIGO_DOCUMENTO_COMPRADOR,

	RIGHT(CONCAT(dbo.AGREGOCEROS(0, 20), 

	F.SECONDID), 20) AS NUMERO_DOCUMENTO,

	LEFT(CONCAT(F.THIRDID, REPLICATE(' ',30)), 30) AS NOMBRE_CLIENTE,

	CASE		
		WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
		THEN
			(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE)		
		ELSE
			(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE) * ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	END AS MONTO_TOTAL,

	dbo.AGREGOCEROS(0, 15) AS TOTAL_NO_INTEGRA,	

	dbo.AGREGOCEROS(0, 15) AS PERCEPCION,	

	dbo.AGREGOCEROS(0, 15) AS TOTAL_EXENTAS,	

	dbo.AGREGOCEROS(0, 15) AS A_CUENTA_IMPUESTOS,	

	dbo.AGREGOCEROS(0, 15) AS INGRESOS_BRUTOS,	

	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_MUNICIPALES,	

	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_INTERNOS,	

	RM.COD_MONEDA_RRII AS CODIGO_MONEDA,

	CASE 
		WHEN S.MONEDA = (SELECT MONNAC FROM PARAMETROS WITH(NOLOCK)) THEN
			1 
		ELSE
			ISNULL(MI.TIPO_CAMBIO_MOV, 0)
	END AS TIPO_CAMBIO,	

	1 AS CANT_ALICUOTAS_IVA,	

	0 AS CODIGO_OPERACION,	

	dbo.AGREGOCEROS(0, 15) AS OTROS_TRIBUTOS,	

	CONVERT(DATETIME,M.FECHAVALOR,103) AS FECHA_VENCIMIENTO

FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK)

INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND GDC.MOV_SIGNO = 'C'
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND F.ASIENTO = GDC.MOV_ASIENTO 
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO 
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA_LINEA FL WITH(NOLOCK) ON 
	FL.ORDINAL_MOVIMIENTO = F.ORDINAL_MOVIMIENTO
	AND FL.ASIENTO = GDC.MOV_ASIENTO
	AND FL.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND FL.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 1 AND TZ_LOCK = 0)) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 1 AND TZ_LOCK = 0)))
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  )

INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO=F.FECHA_PROCESO_ASIENTO
	AND M.SUCURSAL=F.SUCURSAL_ASIENTO
	AND M.ASIENTO=F.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
				
LEFT JOIN RRII_IVA_DOCUMENTOS AS RD WITH (nolock) ON	
	RD.TIPO_DOCUMENTO = F.FIRSTID
	
LEFT JOIN RRII_IVA_MONEDAS AS RM WITH (nolock) ON	
	RM.CODIGO_MONEDA = F.MONEDA
			
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO=M.ASIENTO AND
	A.ESTADO=77 AND
	A.SUCURSAL= F.SUCURSAL_ASIENTO AND
	A.FECHAPROCESO=M.FECHAPROCESO

INNER JOIN PRODUCTOS P WITH (NOLOCK) ON
	P.C6250 = GDC.SALDO_ORIG_PRODUCTO AND
	( (P.tz_lock &lt; 300000000000000 OR P.tz_lock >= 400000000000000) 
	AND (P.tz_lock &lt; 100000000000000 OR P.tz_lock >= 200000000000000)  )


LEFT JOIN RRI_PARAMETROS_INF R WITH(NOLOCK) ON 
	R.CODIGO = 511 AND 
	R.ID1 = GDC.MOV_OPERACION AND
	R.ID5 = CASE WHEN R.CHAR1='P' AND P.C6800='L' THEN 'L' 
			WHEN R.CHAR1='G' THEN 'X'
			ELSE 'P' END AND
	R.TZ_LOCK = 0
	
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MI WITH (nolock) ON	
	MI.ASIENTO = F.ASIENTO	
	AND MI.SUCURSAL = F.SUCURSAL_ASIENTO	
	AND MI.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO	
	AND ( (MI.tz_lock &lt; 300000000000000 OR MI.tz_lock >= 400000000000000) 
	AND (MI.tz_lock &lt; 100000000000000 OR MI.tz_lock >= 200000000000000)  )

-- Me quiero quedar con los asientos que no han sido extornados, por eso el siguiente LEFT JOIN
LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO

WHERE 
	F.FIRSTID IN ('CUIT','CUIL')
	AND M.FECHACONTABLE BETWEEN @FechaP1 AND @FechaP2 
--
UNION ALL

--ASIENTOS POR LINEA DE CAJA IVA VENTAS EXTORNADOS
SELECT
	ISNULL((SELECT MAX(NUMERADOR_CBTE) 
	FROM RRII_IVA_VENTAS_COMPROBANTES WHERE 
	PERIODO =@PERIOD),0) AS NUMERADOR,
	
	S.PRODUCTO AS MODULO,
	
	S.CUENTA AS CUENTA,
	
	M.OPERACION AS OPERACION,
	
	F.FIRSTID AS TIPODOCUMENTO,
	
	FL.MONTO_FACTURABLE AS MONTO_FACTURABLE,
	
	FL.MONTO_IMPONIBLE AS MONTO_IMPONIBLE,
	
	M.FECHACONTABLE AS FECHA_COMPROBANTE,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHA_COMPROBANTE,	
	
	'099' AS TIPO_COMPROBANTE,	
	
	CASE 
    WHEN R.CHAR1 = 'G' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
    
    WHEN R.CHAR1 = 'P' AND P.C6800='L' AND R.ID5='L' THEN CONCAT(REPLICATE('0',4),R.CHAR2)
    
    ELSE CONCAT(REPLICATE('0', 4), '7')
	
	END AS PUNTO_VENTA,
	
	F.ASIENTO AS ASIENTO,
	
	CONVERT(DATETIME,M.FECHACONTABLE,103) AS FECHACONTABLE,

	F.SUCURSAL_ASIENTO AS SUCURSAL_ASIENTO,	  
	
	dbo.AGREGOCEROS(RD.CODIGO_DOC_RRII, 2) AS CODIGO_DOCUMENTO_COMPRADOR,
	
	RIGHT(CONCAT(dbo.AGREGOCEROS(0, 20),F.SECONDID), 20) AS NUMERO_DOCUMENTO,
	
	LEFT(CONCAT(F.THIRDID, REPLICATE(' ',30)), 30) AS NOMBRE_CLIENTE,
	
	CASE		
		WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
		THEN
			(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE)		
			ELSE 
				(FL.MONTO_IMPONIBLE + FL.MONTO_FACTURABLE) * ISNULL(MIE.TIPO_CAMBIO_MOV, 0)
	END AS MONTO_TOTAL,
	
	dbo.AGREGOCEROS(0, 15) AS TOTAL_NO_INTEGRA,	
	
	dbo.AGREGOCEROS(0, 15) AS PERCEPCION,	
	
	dbo.AGREGOCEROS(0, 15) AS TOTAL_EXENTAS,	
	
	dbo.AGREGOCEROS(0, 15) AS A_CUENTA_IMPUESTOS,	
	
	dbo.AGREGOCEROS(0, 15) AS INGRESOS_BRUTOS,	
	
	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_MUNICIPALES,	
	
	dbo.AGREGOCEROS(0, 15) AS IMPUESTOS_INTERNOS,	
	
	RM.COD_MONEDA_RRII AS CODIGO_MONEDA,

	CASE 
		WHEN S.MONEDA = (SELECT MONNAC FROM PARAMETROS WITH(NOLOCK)) THEN
			1 
		ELSE
			ISNULL(MIE.TIPO_CAMBIO_MOV, 0)
	END AS TIPO_CAMBIO,	
	
	1 AS CANT_ALICUOTAS_IVA,	
	
	0 AS CODIGO_OPERACION,	
	
	dbo.AGREGOCEROS(0, 15) AS OTROS_TRIBUTOS,	
	
	CONVERT(DATETIME,M.FECHAVALOR,103) AS FECHA_VENCIMIENTO

FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK)

INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND GDC.MOV_SIGNO = 'D'
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND F.ASIENTO = GDC.MOV_ASIENTO 
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO 
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA_LINEA FL WITH(NOLOCK) ON 
	FL.ORDINAL_MOVIMIENTO = F.ORDINAL_MOVIMIENTO
	AND FL.ASIENTO = GDC.MOV_ASIENTO
	AND FL.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND FL.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 1 AND TZ_LOCK = 0)) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 1 AND TZ_LOCK = 0)))
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  )

INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO=F.FECHA_PROCESO_ASIENTO
	AND M.SUCURSAL=F.SUCURSAL_ASIENTO
	AND M.ASIENTO=F.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
				
LEFT JOIN RRII_IVA_DOCUMENTOS AS RD WITH (nolock) ON	
	RD.TIPO_DOCUMENTO = F.FIRSTID
	
LEFT JOIN RRII_IVA_MONEDAS AS RM WITH (nolock) ON	
	RM.CODIGO_MONEDA = F.MONEDA
			
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO=M.ASIENTO AND
	A.ESTADO=77 AND
	A.SUCURSAL=F.SUCURSAL_ASIENTO AND
	A.FECHAPROCESO=M.FECHAPROCESO

INNER JOIN PRODUCTOS P WITH (NOLOCK) ON
	P.C6250 = GDC.SALDO_ORIG_PRODUCTO AND
	( (P.tz_lock &lt; 300000000000000 OR P.tz_lock >= 400000000000000) 
	AND (P.tz_lock &lt; 100000000000000 OR P.tz_lock >= 200000000000000)  )

LEFT JOIN RRI_PARAMETROS_INF R WITH(NOLOCK) ON 
	R.CODIGO = 511 AND 
	R.ID1 = GDC.MOV_OPERACION AND
	R.ID5 = CASE WHEN R.CHAR1='P' AND P.C6800='L' THEN 'L' 
			WHEN R.CHAR1='G' THEN 'X'
			ELSE 'P' END AND
	R.TZ_LOCK = 0
	
INNER JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO

--Para tipo de cambio de asientos extornados 
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MIE WITH (nolock) ON	
	MIE.ASIENTO = AE.ASIENTO_ORIGINAL	
	AND MIE.SUCURSAL = AE.SUCURSAL_ORIGINAL	
	AND MIE.FECHAPROCESO = AE.FECHAPROCESO_ORIGINAL
	AND ( (MIE.tz_lock &lt; 300000000000000 OR MIE.tz_lock >= 400000000000000) 
	AND (MIE.tz_lock &lt; 100000000000000 OR MIE.tz_lock >= 200000000000000)  )

WHERE 
	F.FIRSTID IN ('CUIT','CUIL')
	AND AE.ASIENTO_ORIGINAL IS NOT NULL
	AND M.FECHACONTABLE BETWEEN @FechaP1 AND @FechaP2 </sql>
