    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
	VS.JTS_OID_SALDO AS "JTS_OID_SALDO",
	S.C1803 AS "CODIGO_CLIENTE",
	S.MONEDA AS "MONEDA",
	S.PRODUCTO AS "PRODUCTO",
	S.CUENTA AS "CUENTA",
	S.SUCURSAL AS "SUCURSAL"
FROM VTA_SALDOS VS (NOLOCK)
INNER JOIN SALDOS S (NOLOCK) ON S.JTS_OID = VS.JTS_OID_SALDO AND S.TZ_LOCK LIKE '[04]%'
WHERE VS.CTA_REDLINK = ? AND VS.TZ_LOCK LIKE '[04]%'</sql>
