        <jsScript_script>//Script here

COD_ERROR = 0;
ERROR = null;


if ( !patronNumerico.test(TIPO_RECAUDACION_STAGE) )
	setError(54, 'Tipo de Recaudación no numérico');

else if ( !patronNumerico.test(MONEDA_STAGE) )
	setError(56, 'Moneda no numérica');

else if ( !patronNumerico.test(TIPO_MOVIMIENTO_STAGE) )
	setError(59, 'Tipo de Movimiento no numérico');

// Tipo de Cuenta destino valida
else if ( TOP_TIPO_CTA_DESTINO == null )
	setError(60, 'Tipo de cuenta inválido');

// Producto válido
else if ( !PRODUCTO_STAGE.equals('RC') )
	setError(61, 'Producto inválido');

// Tipo de recaudacion valida
else if ( TIPO_RECAUDACION_STAGE != 999999 )
	setError(62, 'Tipo de recaudación inválida');

// Forma de recaudacion válida
else if ( !FORMA_RECAUDACION_STAGE.equals('SC') )
	setError(63, 'Forma de recaudación distinta de "SC"');

// Valida moneda = pesos
else if ( Number(MONEDA_STAGE) != 1 )
	setError(64, 'Moneda distinta de 1 (Pesos)');	

// Valida tipo de movimiento = débito
else if ( Number(TIPO_MOVIMIENTO_STAGE) != 1 )
	setError(65, 'Tipo de Movimiento distinto de 1 (Debito)');

// Sucursal válida
else if ( EXISTE_SUCURSAL == null )
	setError(70, 'Sucursal Inexistente');

// Numero de cuenta existente
else if ( JTS_OID == null )
	setError(71, 'No se encontró la cuenta');

// Cuenta no corresponde a persona
else if ( parseInt(CLIENTE_CORRECTO, 10) == 0 )
	setError(72, 'Titular no asociado a cuenta indicada');

// Cuenta habilitada
else if ( CUENTA_CANCELADA.equals('1') )
	setError(73, 'La cuenta se encuentra cancelada');
</jsScript_script>
