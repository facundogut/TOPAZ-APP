    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
	S.JTS_OID,
	S.C1651 AS CUENTA_CANCELADA,
	CASE
		WHEN EXISTS (
			SELECT 1
			FROM CLI_ClientePersona CCP 
			INNER JOIN CLI_DocumentosPFPJ CDP ON CDP.NUMEROPERSONAFJ = CCP.NUMEROPERSONA AND CDP.TZ_LOCK = 0
			INNER JOIN (
				SELECT
					'F' AS "TIPO_PERSONA",
					NUMEROPERSONAFISICA AS "NUMERO_PERSONA"
				FROM CLI_PERSONASFISICAS
				WHERE ESTADO = 0
				UNION ALL
				SELECT 
					'J' AS "TIPO_PERSONA",
					NUMEROPERSONAJURIDICA AS "NUMERO_PERSONA"
				FROM CLI_PERSONASJURIDICAS
				WHERE ESTADO = 0
			) P ON P.NUMERO_PERSONA = CDP.NUMEROPERSONAFJ AND P.TIPO_PERSONA = CDP.TIPOPERSONA
			WHERE 
				CCP.CODIGOCLIENTE = S.C1803 AND 
				CDP.NUMERODOCUMENTO = TRY_CONVERT(BIGINT, ?) AND 
				CCP.TZ_LOCK = 0
		) THEN 1
		ELSE 0
	END AS "CLIENTE_CORRECTO"
FROM SALDOS S
WHERE
	S.MONEDA = 1 AND
	S.SUCURSAL = ? AND
	S.C1785 = ? AND -- TIPO DE CUENTA
	S.CUENTA = ?</sql>
