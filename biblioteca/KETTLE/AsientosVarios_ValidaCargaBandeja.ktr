    <connection>TopazDS</connection>
    <sql>with REGISTROS as 
(
select 
avr.FECHA_IMPORTACION, 
avr.MD5_ARCH, 
count(avr.MD5_ARCH) as 'CANT_REG' 
from ASIENTOS_VARIOS_REGISTROS avr (nolock) 
where avr.TZ_LOCK = 0 
group by avr.FECHA_IMPORTACION, avr.MD5_ARCH 
)
select 
ava.FECHA_IMPORTACION, 
ava.MD5_ARCH, 
ava.TOTAL_DEBITOS, 
ava.TOTAL_CREDITOS, 
ava.ESTADO, 
ava.CANAL_BANDEJA, 
ava.NOMBRE_ARCH, 
reg.CANT_REG 
from ASIENTOS_VARIOS_ARCHIVOS ava (nolock) 
left join REGISTROS reg on reg.FECHA_IMPORTACION = ava.FECHA_IMPORTACION and reg.MD5_ARCH = ava.MD5_ARCH 
where ava.TZ_LOCK = 0 and 
ava.FECHA_IMPORTACION = '${FECHATOP}' and 
ava.MD5_ARCH = '${MD5}' </sql>
