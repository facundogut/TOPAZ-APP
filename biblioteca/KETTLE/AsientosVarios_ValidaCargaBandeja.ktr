        <jsScript_script>if (TOTAL_DEBITOS == 0 &amp;&amp; TOTAL_CREDITOS == 0)
{
	abortar('El archivo ' + NOMBRE_ARCH + ' indica un total de importes tanto de Credito como de Debito iguales a 0.');
}

if (ESTADO != 'C')
{
	if (ESTADO == 'I')
	{
		abortar('El archivo ' + NOMBRE_ARCH + ' se encuentra Ingresado, debe solicitar que se confirme el procesamiento del mismo. La confirmacion se realiza mediante operacion 200.');
	}
	else if (ESTADO == 'R')
	{
		abortar('El archivo ' + NOMBRE_ARCH + ' se encuentra Rechazado y no puede ser procesado.');
	}
	else if (ESTADO == 'E')
	{
		abortar('El archivo ' + NOMBRE_ARCH + ' al momento esta siendo procesado por otra instancia del proceso.');
	}
	else if (ESTADO == 'P')
	{
		abortar('El archivo ' + NOMBRE_ARCH + ' fue procesado anteriormente.');
	}
	else
	{
		abortar('El archivo debe estar Confirmado para ser procesado. Actualmente su estado es: ' + ESTADO + '.');
	}
}

if (CANT_REG == 0)
{
	abortar('El archivo ' + NOMBRE_ARCH + ' indica que no contiene ningun registro para procesar.');
}

