    <sql>DECLARE @cantReg BIGINT = (SELECT count(*) + 2 FROM (
SELECT DISTINCT concat(
		LEFT(concat(t.id_tarjeta,replicate(' ',19)),19)
        , RIGHT(concat(replicate('0',8),replace(replace(convert(VARCHAR(15),(case when c.MONTO_CALCULADO>999999.99 then 999999.99 else c.MONTO_CALCULADO end)),'.',''),',','')),8)
        , RIGHT(concat(replicate('0',8),replace(replace(convert(VARCHAR(15),(case when c.CAPACIDAD_PAGO>999999.99 then 999999.99 else c.CAPACIDAD_PAGO end)),'.',''),',','')),8)
        , CASE WHEN s.C1785=3 THEN '11' ELSE '01' END 
        , RIGHT(concat(replicate(' ',19),(SELECT TOP 1 CTA_REDLINK  FROM VTA_SALDOS with (nolock) WHERE JTS_OID_SALDO=s.JTS_OID AND TZ_LOCK=0)),19)
        , RIGHT(concat(replicate('0',3),CASE WHEN s.moneda=1 THEN '032' WHEN s.moneda=2 THEN '840' ELSE '000' END),3)
        ) AS LINEA1
		, 2 AS orden1
FROM CRE_AUX_SOL_CALCULO_ADELANTO c with (nolock) 
JOIN topesproducto topes with (nolock) ON  c.PRODUCTO=topes.codproducto 
JOIN CRE_VINCULACIONES_CONVENIOS v with (nolock) ON c.CONVENIO=v.ID_CONVENIO 
JOIN tjd_tarjetas tjd with (nolock) ON tjd.ID_TARJETA IN ( 
														SELECT TOP 1 tsd2.ID_TARJETA
														FROM tjd_tarjetas tsd2 WITH (nolock)
														WHERE ESTADO = '1' AND tsd2.TZ_LOCK = 0 AND tsd2.ID_TARJETA IN (
																									SELECT t2.ID_TARJETA 
																									FROM TJD_REL_TARJETA_CUENTA t2 WITH (nolock)
																									WHERE t2.SALDO_JTS_OID=v.JTS_CV
																									AND T2.ESTADO NOT IN ('9','X')
																									AND t2.TZ_LOCK = 0
																									) 
														ORDER BY tsd2.titularidad DESC,tsd2.ID_TARJETA)
JOIN TJD_REL_TARJETA_CUENTA t with (nolock) ON t.SALDO_JTS_OID=v.JTS_CV AND t.ID_TARJETA = tjd.ID_TARJETA
JOIN saldos s with (nolock) ON t.SALDO_JTS_OID=s.JTS_OID and s.c1803=c.CLIENTE
WHERE TOPES.linea_scoring='L1'
										AND v.TZ_LOCK=0
										AND t.TZ_LOCK=0
										AND s.TZ_LOCK=0
										AND c.TZ_LOCK=0
										AND tjd.TZ_LOCK=0
										and c.MONTO_CALCULADO>0
										AND tjd.estado='1'
										AND T.ESTADO NOT IN ('9','X')
										AND C.CANAL='PR'
										AND topes.tz_lock=0
										AND topes.moneda=s.moneda
) a)


SELECT concat(
				'HRCLIENTES'
				,'0311'
				,LEFT(CONCAT(CONVERT(VARCHAR(6),FORMAT((SELECT TOP 1 fecharegistro FROM ITF_TLF_POS with (nolock) ORDER BY FECHAREGISTRO DESC), 'yyMMdd')),REPLICATE('0',6)),6)
				,replicate(' ',39)
				) AS LINEA1
				, 1 AS orden1
UNION ALL
SELECT DISTINCT concat(
		LEFT(concat(t.id_tarjeta,replicate(' ',19)),19)
        , RIGHT(concat(replicate('0',8),replace(replace(convert(VARCHAR(15),(case when c.MONTO_CALCULADO>999999.99 then 999999.99 else c.MONTO_CALCULADO end)),'.',''),',','')),8)
        , RIGHT(concat(replicate('0',8),replace(replace(convert(VARCHAR(15),(case when c.CAPACIDAD_PAGO>999999.99 then 999999.99 else c.CAPACIDAD_PAGO end)),'.',''),',','')),8)
        , CASE WHEN s.C1785=3 THEN '11' ELSE '01' END 
        , RIGHT(concat(replicate(' ',19),(SELECT TOP 1 CTA_REDLINK  FROM VTA_SALDOS with (nolock) WHERE JTS_OID_SALDO=s.JTS_OID AND TZ_LOCK=0)),19)
        , RIGHT(concat(replicate('0',3),CASE WHEN s.moneda=1 THEN '032' WHEN s.moneda=2 THEN '840' ELSE '000' END),3)
        ) AS LINEA1
		, 2 AS orden1
FROM CRE_AUX_SOL_CALCULO_ADELANTO c with (nolock) 
JOIN topesproducto topes with (nolock) ON  c.PRODUCTO=topes.codproducto 
JOIN CRE_VINCULACIONES_CONVENIOS v with (nolock) ON c.CONVENIO=v.ID_CONVENIO 
JOIN tjd_tarjetas tjd with (nolock) ON tjd.ID_TARJETA IN ( 
														SELECT TOP 1 tsd2.ID_TARJETA
														FROM tjd_tarjetas tsd2 WITH (nolock)
														WHERE ESTADO = '1' AND tsd2.TZ_LOCK = 0 AND tsd2.ID_TARJETA IN (
																									SELECT t2.ID_TARJETA 
																									FROM TJD_REL_TARJETA_CUENTA t2 WITH (nolock)
																									WHERE t2.SALDO_JTS_OID=v.JTS_CV
																									AND T2.ESTADO NOT IN ('9','X')
																									AND t2.TZ_LOCK = 0
																									) 
														ORDER BY tsd2.titularidad DESC,tsd2.ID_TARJETA)
JOIN TJD_REL_TARJETA_CUENTA t with (nolock) ON t.SALDO_JTS_OID=v.JTS_CV AND t.ID_TARJETA = tjd.ID_TARJETA
JOIN saldos s with (nolock) ON t.SALDO_JTS_OID=s.JTS_OID and s.c1803=c.CLIENTE
WHERE TOPES.linea_scoring='L1'
										AND v.TZ_LOCK=0
										AND t.TZ_LOCK=0
										AND s.TZ_LOCK=0
										AND c.TZ_LOCK=0
										AND tjd.TZ_LOCK=0
										and c.MONTO_CALCULADO>0
										AND tjd.estado='1'
										AND T.ESTADO NOT IN ('9','X')
										AND C.CANAL='PR'
										AND topes.tz_lock=0
										AND topes.moneda=s.moneda
UNION ALL
SELECT  concat(
		'TRCLIENTES'
        , RIGHT(CONCAT(REPLICATE('0',8),@cantReg),8)
        , REPLICATE(' ',41)
        ) AS LINEA1
		, 3 AS orden1
UNION ALL
SELECT CAST(@cantReg AS VARCHAR(20)) LINEA1, 4 AS orden
order by orden1
</sql>
