    <rowlimit>0</rowlimit>
    <sql>DECLARE @fecha_parametro AS DATETIME;
SET @fecha_parametro=CONVERT(DATETIME,?,103);
DECLARE @rectificativa AS VARCHAR;
SET @rectificativa=?;
WITH DIFERENCIAS AS (
SELECT  
	CASE WHEN SUBSTRING(CAST(CUENTA AS VARCHAR(2)), 1, 2) IN ('71','72') THEN 
		'O' 
	WHEN CUENTA IN (SELECT ID1 FROM RRI_PARAMETROS_INF WHERE CODIGO IN (866) AND TZ_LOCK=0) THEN 
		'O'  
	ELSE 'B' 
	END AS CUENTA,
	SUM(IMPORTE) AS IMPORTE,
	RECTIFICATIVA,
	FECHA
FROM RRII_BALANCE_SALDOS 
WHERE FECHA=@fecha_parametro AND 
	RECTIFICATIVA=@rectificativa
GROUP BY FECHA, CUENTA,RECTIFICATIVA
), 
RUBROS_AJUSTE AS (
SELECT 
	CASE WHEN CUENTA ='O' THEN (SELECT ID1 FROM RRI_PARAMETROS_INF WHERE CODIGO=866)
	WHEN CUENTA='B' AND SUM(IMPORTE)&lt;0 THEN (SELECT ID1  FROM RRI_PARAMETROS_INF WHERE CODIGO=867 AND ID4='P')
	ELSE
		(SELECT ID1  FROM RRI_PARAMETROS_INF WHERE CODIGO=867 AND ID4='G')
	END AS CUENTA,
	-1*SUM(IMPORTE) AS IMPORTE,
	RECTIFICATIVA,
	FECHA

FROM DIFERENCIAS GROUP BY CUENTA,RECTIFICATIVA,FECHA
)
SELECT *
FROM RUBROS_AJUSTE 
UNION 
SELECT CUENTA,SUM(IMPORTE),RECTIFICATIVA,FECHA
FROM RRII_BALANCE_SALDOS
WHERE FECHA=@fecha_parametro AND 
	RECTIFICATIVA=@rectificativa
GROUP BY CUENTA,RECTIFICATIVA,FECHA
ORDER BY CUENTA ASC</sql>
