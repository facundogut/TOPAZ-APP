    <sql>DECLARE @fecha_parametro AS DATETIME;
SET @fecha_parametro =CONVERT(DATETIME,?,103);

DECLARE @INI_MES        date = DATEFROMPARTS(YEAR(@fecha_parametro), MONTH(@fecha_parametro), 1);
DECLARE @ULTIMO_HABIL   date = dbo.ULTIMODIAHABIL(@fecha_parametro);
DECLARE @FIN_MES        date = @ULTIMO_HABIL;

-- Precalculo de tipo de cambio vigente por moneda
WITH HTC AS (
    SELECT MONEDA, TIPO_CAMBIO_OFICIAL
    FROM HISTORICOTIPOSCAMBIO WITH(NOLOCK)
    WHERE FECHA_COTIZACION = @fecha_parametro
      AND CODIGO_TIPO_CAMBIO = 1
      AND TZ_LOCK=0
),
-- Tabla de Titulares
Titulares AS (
    SELECT
        S.JTS_OID,
        A.CODIGOCLIENTE,
        B.TIPOPERSONA,
        COALESCE(NULLIF(B.TIPO_DOC_FISICO,''), B.TIPODOCUMENTO) AS TIPODOC,
        R.NUM1 AS TIPODOCUMENTO,
        B.NUMERODOCUMENTO,
        CASE 
            WHEN B.TIPOPERSONA = 'J' THEN PJ.RAZONSOCIAL
            ELSE CONCAT(PF.PRIMERNOMBRE, ' ', PF.APELLIDOPATERNO)
        END AS NOMBREPERSONA,
        ROW_NUMBER() OVER (PARTITION BY A.CODIGOCLIENTE, S.JTS_OID ORDER BY A.TITULARIDAD DESC) AS NRO_PERSONA
    FROM SALDOS S WITH(NOLOCK)
    INNER JOIN CLI_ClientePersona A WITH(NOLOCK)
        ON A.CODIGOCLIENTE = S.C1803
       AND (A.tz_lock &lt; 300000000000000 OR A.tz_lock >= 400000000000000)
       AND (A.tz_lock &lt; 100000000000000 OR A.tz_lock >= 200000000000000)
    INNER JOIN CLI_DocumentosPFPJ B WITH (NOLOCK)
        ON B.NUMEROPERSONAFJ = A.NUMEROPERSONA
       AND (B.tz_lock &lt; 300000000000000 OR B.tz_lock >= 400000000000000)
       AND (B.tz_lock &lt; 100000000000000 OR B.tz_lock >= 200000000000000)
    LEFT JOIN CLI_PERSONASFISICAS PF WITH (NOLOCK)
        ON PF.NUMEROPERSONAFISICA = A.NUMEROPERSONA
       AND (PF.tz_lock &lt; 300000000000000 OR PF.tz_lock >= 400000000000000)
       AND (PF.tz_lock &lt; 100000000000000 OR PF.tz_lock >= 200000000000000)
    LEFT JOIN CLI_PERSONASJURIDICAS PJ WITH (NOLOCK)
        ON PJ.NUMEROPERSONAJURIDICA = A.NUMEROPERSONA
       AND (PJ.tz_lock &lt; 300000000000000 OR PJ.tz_lock >= 400000000000000)
       AND (PJ.tz_lock &lt; 100000000000000 OR PJ.tz_lock >= 200000000000000)
    LEFT JOIN RRI_PARAMETROS_INF R WITH(NOLOCK)
        ON R.CODIGO = 11
       AND R.ID4 = COALESCE(NULLIF(B.TIPO_DOC_FISICO,''), B.TIPODOCUMENTO)
       AND R.TZ_LOCK = 0
    WHERE
        (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000)
        AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)
        AND S.C1785 IN (2,3,4)
),
-- Tabla de TOTAL DE DEBITOS
TABLA_SUMA_DEBITOS AS (
    SELECT 
        S.JTS_OID,
        SUM(
            CASE 
                WHEN S.MONEDA IN (999,988) 
                    THEN M.CAPITALREALIZADO * H1.TIPO_CAMBIO_OFICIAL 
                ELSE M.CAPITALREALIZADO 
            END
        ) AS SUMA_DEBITOS
    FROM SALDOS S WITH(NOLOCK)
    INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) 
        ON M.SALDO_JTS_OID = S.JTS_OID
       AND M.DEBITOCREDITO = 'D'
       AND M.FECHAPROCESO BETWEEN @INI_MES and @FIN_MES
    INNER JOIN ASIENTOS A WITH(NOLOCK) 
        ON A.ASIENTO = M.ASIENTO 
       AND A.FECHAPROCESO = M.FECHAPROCESO 
       AND A.SUCURSAL = M.SUCURSAL 
       AND A.ESTADO = 77
    LEFT JOIN HTC H1 
        ON H1.MONEDA = S.MONEDA
    WHERE 
        (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000)
        AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)
        AND (S.C1785 IN (2,3) OR (S.C1785=4 AND DATEDIFF(DAY,S.C1620,S.C1627) > 0))
    GROUP BY S.JTS_OID
),
-- Tabla de TOTAL DE CREDITOS
TABLA_SUMA_CREDITOS AS (
SELECT SUM(CASE WHEN S.MONEDA IN (999,988) THEN M.CAPITALREALIZADO*H1.TIPO_CAMBIO_OFICIAL ELSE M.CAPITALREALIZADO END) AS SUMA_CREDITOS,
	S.JTS_OID
FROM SALDOS S WITH(NOLOCK) 
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON
	M.SALDO_JTS_OID=S.JTS_OID AND
	M.DEBITOCREDITO='C' AND
	M.FECHAPROCESO BETWEEN @INI_MES and @FIN_MES
INNER JOIN ASIENTOS A WITH(NOLOCK) ON 
	A.ASIENTO=M.ASIENTO AND 
	A.FECHAPROCESO=M.FECHAPROCESO AND 
	A.SUCURSAL=M.SUCURSAL AND 
	A.ESTADO=77
LEFT JOIN HTC H1 
        ON H1.MONEDA = S.MONEDA
WHERE 
	((S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) AND 
		(S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)) AND
	((S.C1785=2) OR (S.C1785=3) OR (S.C1785=4 AND DATEDIFF(DAY,S.C1620,S.C1627)>0))
GROUP BY S.JTS_OID),
--DPF UVA UVI
TABLA_DPF AS(
SELECT 
	S.CUENTA AS CUENTA,
	S.C1803 AS CLIENTE,
	S.C1604 * H.TIPO_CAMBIO_OFICIAL AS SALDO_APERTURA_MN,
	S.C1620 AS FECHA_APERTURA_DPF,
	S.JTS_OID,
	S.MONEDA
FROM SALDOS S WITH(NOLOCK) 
OUTER APPLY (
        SELECT TOP 1 H.TIPO_CAMBIO_OFICIAL
        FROM HISTORICOTIPOSCAMBIO H WITH(NOLOCK)
        WHERE H.MONEDA = S.MONEDA
          AND H.TZ_LOCK = 0
          AND H.FECHA_COTIZACION &lt;= S.C1620
          AND H.CODIGO_TIPO_CAMBIO = 1
        ORDER BY H.FECHA_COTIZACION DESC
) H
WHERE 	((S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) AND 
		(S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)) AND
		S.C1785=4 AND S.C1604 &lt;&gt; 0 AND S.MONEDA IN (999,988)
)
-- CONSULTA FINAL
SELECT DISTINCT
    S.C1803 AS CLIENTE,
    T1.TIPOPERSONA AS TPERSO,
    -- Datos del Titular 1
    T1.TIPOPERSONA AS TIPOPERSONA1,
    CASE WHEN T1.TIPOPERSONA IN ('J','F') THEN ISNULL(T1.TIPODOCUMENTO,8) ELSE NULL END AS TIPODOCUMENTO1,
    T1.NUMERODOCUMENTO AS NUMERODOCUMENTO1,
    T1.NOMBREPERSONA AS NOMBREPERSONA1,
    -- Datos del Titular 2
    T2.TIPOPERSONA AS TIPOPERSONA2,
    CASE WHEN T2.TIPOPERSONA IN ('J','F') THEN ISNULL(T2.TIPODOCUMENTO,8) ELSE NULL END AS TIPODOCUMENTO2,
    T2.NUMERODOCUMENTO AS NUMERODOCUMENTO2,
    T2.NOMBREPERSONA AS NOMBREPERSONA2,
    -- Datos del Titular 3
    T3.TIPOPERSONA AS TIPOPERSONA3,
    CASE WHEN T3.TIPOPERSONA IN ('J','F') THEN ISNULL(T3.TIPODOCUMENTO,8) ELSE NULL END  AS TIPODOCUMENTO3,
    T3.NUMERODOCUMENTO AS NUMERODOCUMENTO3,
    T3.NOMBREPERSONA AS NOMBREPERSONA3,
    -- Datos del Titular 4
    T4.TIPOPERSONA AS TIPOPERSONA4,
    CASE WHEN T4.TIPOPERSONA IN ('J','F') THEN ISNULL(T4.TIPODOCUMENTO,8) ELSE NULL END  AS TIPODOCUMENTO4,
    T4.NUMERODOCUMENTO AS NUMERODOCUMENTO4,
    T4.NOMBREPERSONA AS NOMBREPERSONA4,
    -- Datos del Titular 5
    T5.TIPOPERSONA AS TIPOPERSONA5,
    CASE WHEN T5.TIPOPERSONA IN ('J','F') THEN ISNULL(T5.TIPODOCUMENTO,8) ELSE NULL END  AS TIPODOCUMENTO5,
    T5.NUMERODOCUMENTO AS NUMERODOCUMENTO5,
    T5.NOMBREPERSONA AS NOMBREPERSONA5,
    S.PRODUCTO,
    S.JTS_OID AS JTS_OID,
    FORMAT(@fecha_parametro,'yyyyMMdd') as FechaParametroEntrada,
    S.SUCURSAL as NroSucursal,
    R.ID3 AS TIPOOPERACION,
    CASE WHEN S.C1785&lt;&gt;4 THEN S.CUENTA ELSE S.OPERACION END AS NROCUENTA,
    FORMAT(S.C1620, 'yyyyMMdd') AS FECHAAPERTURACUENTA,
    CASE WHEN S.C1785 IN(2,3) THEN '000000000000000' ELSE S.OPERACION END NROCERTIFICADOPLAZOFIJO,

    -- ORIGENIMPOSICION (mantengo la lógica original con subselects internas)
    CASE WHEN S.C1785=4 THEN
        CASE WHEN S.MONEDA NOT IN (988, 999) AND S.C1659 NOT IN ('M','C') THEN
            ISNULL(
                (SELECT SO.C1785 
                 FROM SALDOS SO WITH (NOLOCK) 
                 WHERE SO.JTS_OID = S.C1665
                   AND SO.C1688 = 0
                   AND ((SO.tz_lock &lt; 300000000000000 OR SO.tz_lock >= 400000000000000)
                        AND (SO.tz_lock &lt; 100000000000000 OR SO.tz_lock >= 200000000000000))
                ), 6)
        WHEN S.C1659 IN ('M','C') THEN 4
        WHEN S.MONEDA IN (998, 999) THEN 
            (SELECT S3.C1785 
             FROM SALDOS S3 WITH (NOLOCK) 
             WHERE ((S3.tz_lock &lt; 300000000000000 OR S3.tz_lock >= 400000000000000) 
                    AND (S3.tz_lock &lt; 100000000000000 OR S3.tz_lock >= 200000000000000))
               AND S3.JTS_OID = (
                    SELECT TOP 1 CV.JTS_OID_CTA_FUENTE 
                    FROM CV_TRANSFERENCIA CV WITH (NOLOCK) 
                    INNER JOIN SALDOS SX WITH (NOLOCK) 
                        ON CV.JTS_OID_CTA_DESTINO = SX.C1665
                    WHERE SX.C1688 = 0
                      AND ((SX.tz_lock &lt; 300000000000000 OR SX.tz_lock >= 400000000000000)
                           AND (SX.tz_lock &lt; 100000000000000 OR SX.tz_lock >= 200000000000000))
                      AND CV.JTS_OID_CTA_DESTINO = S.C1665
                      AND ((CV.tz_lock &lt; 300000000000000 OR CV.tz_lock >= 400000000000000)
                           AND (CV.tz_lock &lt; 100000000000000 OR CV.tz_lock >= 200000000000000))
               )
            )
        ELSE 6
        END
    ELSE 6 END AS ORIGENIMPOSICION,

    CASE WHEN S.C1688 > 0 THEN S.OPERACION ELSE 0 END PLAZOFIJORENOVADO,
    FORMAT(S.C1620, 'yyyyMMdd') AS FECHACONSTITUCION,
    CASE WHEN S.C1785=4 THEN DATEDIFF(DAY,S.C1620,S.C1627) ELSE '00000' END AS PLAZO,
    FORMAT(S.C1627, 'yyyyMMdd') AS FECHAVENCIMIENTOPLAZOFIJO,

    -- CAPITAL
    CASE WHEN S.MONEDA IN (999,988) THEN
        CASE WHEN S.C1785=4 THEN S.C1601 * H2.TIPO_CAMBIO_OFICIAL
             ELSE GSD.SALDO_AJUSTADO * H1.TIPO_CAMBIO_OFICIAL
        END
    ELSE
        GSD.SALDO_AJUSTADO
    END AS CAPITALMONEDAORIGEN,

    -- CAPITALP
    CASE WHEN S.MONEDA IN (999,988) THEN
        CASE WHEN S.C1785=4 THEN S.C1601 * H1.TIPO_CAMBIO_OFICIAL
             ELSE GSD.SALDO_AJUSTADO * H1.TIPO_CAMBIO_OFICIAL
        END
    ELSE
        CASE WHEN S.C1785=4 THEN S.C1601 * H1.TIPO_CAMBIO_OFICIAL
             ELSE GSD.SALDO_AJUSTADO * H1.TIPO_CAMBIO_OFICIAL
        END
    END AS CAPITALPESOS,

    -- INTDEV
    CASE WHEN S.MONEDA IN (999,988) THEN 
        CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0) * H1.TIPO_CAMBIO_OFICIAL
             ELSE COALESCE(HD_V.INTERESESPAGO,0) * H1.TIPO_CAMBIO_OFICIAL
        END
    ELSE 
        CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0)
             ELSE COALESCE(HD_V.INTERESESPAGO,0)
        END
    END AS INTERESDEVENGADO,

    -- INTDEVP
    CASE WHEN S.MONEDA IN (999,988) THEN 
        CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0) * H1.TIPO_CAMBIO_OFICIAL
             ELSE COALESCE(HD_V.INTERESESPAGO,0) * H1.TIPO_CAMBIO_OFICIAL
        END
    ELSE 
        CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0) * H1.TIPO_CAMBIO_OFICIAL
             ELSE COALESCE(HD_V.INTERESESPAGO,0) * H1.TIPO_CAMBIO_OFICIAL
        END
    END AS INTERESDEVENGADOPESOS,

    0 AS COEF,

    -- UVA-UVI
    CASE WHEN S.MONEDA IN (999,988) THEN
        CASE WHEN S.C1785=4 THEN ISNULL((S.C1601 * H1.TIPO_CAMBIO_OFICIAL) - (S.C1601 * H2.TIPO_CAMBIO_OFICIAL),0)
             ELSE ISNULL((S.C1604 * H1.TIPO_CAMBIO_OFICIAL) - (S.C1604 * H2.TIPO_CAMBIO_OFICIAL),0)
        END
    ELSE 0 END AS AJUSTEUVAUVI,

    -- MONTO
    (
    CASE WHEN S.MONEDA IN (999,988) THEN
            CASE WHEN S.C1785=4 THEN S.C1601 * H2.TIPO_CAMBIO_OFICIAL
                 ELSE GSD.SALDO_AJUSTADO * H1.TIPO_CAMBIO_OFICIAL
            END
        ELSE
            GSD.SALDO_AJUSTADO
    END
    +
    CASE WHEN S.MONEDA IN (999,988) THEN 
            CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0) * H1.TIPO_CAMBIO_OFICIAL
                 ELSE COALESCE(HD_V.INTERESESPAGO,0) * H1.TIPO_CAMBIO_OFICIAL
            END
        ELSE 
            CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0)
                 ELSE COALESCE(HD_V.INTERESESPAGO,0)
            END
    END
    ) AS MONTO,

    -- MONTOPESOS
    (
    CASE WHEN S.MONEDA IN (999,988) THEN
            CASE WHEN S.C1785=4 THEN S.C1601 * H1.TIPO_CAMBIO_OFICIAL
                 ELSE GSD.SALDO_AJUSTADO * H1.TIPO_CAMBIO_OFICIAL
            END
        ELSE
            CASE WHEN S.C1785=4 THEN S.C1601 * H1.TIPO_CAMBIO_OFICIAL
                 ELSE GSD.SALDO_AJUSTADO * H1.TIPO_CAMBIO_OFICIAL
            END
    END
      +
    CASE WHEN S.MONEDA IN (999,988) THEN 
            CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0) * H1.TIPO_CAMBIO_OFICIAL
                 ELSE COALESCE(HD_V.INTERESESPAGO,0) * H1.TIPO_CAMBIO_OFICIAL
            END
        ELSE 
            CASE WHEN S.C1785=4 THEN COALESCE(HD.INTERES_DEVENGADO,0) * H1.TIPO_CAMBIO_OFICIAL
                 ELSE COALESCE(HD_V.INTERESESPAGO,0) * H1.TIPO_CAMBIO_OFICIAL
            END
    END
    ) AS MONTOPESOS,

    -- TASA INTERES PACTADA / TIPO TASA / TVAR
    CASE WHEN S.C1785 IN (2,3) THEN 0 ELSE S.C1632 END AS TASAINTERESPACTADA,
    CASE WHEN S.MONEDA IN (999,988) THEN 3 WHEN S.C1690 = 'F' THEN 0 WHEN S.C1690 = 'V' THEN 1 ELSE 0 END TIPOTASA,
    CASE WHEN S.C1785 IN (2,3) THEN 0 ELSE 1 END AS TASAVARIABLE,

    -- TREFE / TASANOMINALANUAL (mantengo la expresión original)
    CASE WHEN S.C1785 IN (2,3) THEN 0
    ELSE
        CASE PRD.C6259 WHEN 'P' THEN
            CAST(
                (
                    ( POWER( ( ((S.C1632 / 100) * S.C1642) / PRD.C6255) + 1, PRD.C6255 / S.C1642) - 1 )
                    * 100
                ) AS DECIMAL(11,2)
            )
        ELSE
            CAST(
                (
                    ( POWER( ( ((S.C1632 / 100) * DATEDIFF(DAY,S.C1620,S.C1627)) / PRD.C6255) + 1, PRD.C6255 / DATEDIFF(DAY,S.C1620,S.C1627)) - 1 )
                    * 100
                ) AS DECIMAL(11,2)
            )
        END
    END AS TASANOMINALANUAL,

    ISNULL(TSD.SUMA_DEBITOS,0) AS DEBITOSTOTALES,
    ISNULL(TSC.SUMA_CREDITOS,0) AS CREDITOSTOTALES,

    GSD.PROMEDIO_DIARIO * H1.TIPO_CAMBIO_OFICIAL AS PROMEDIOMENSUAL,

    CASE WHEN S.MONEDA IN (1,999,9888) THEN 1 ELSE S.MONEDA END AS MONEDAORIGEN,

    CASE WHEN COALESCE(HD_V.INTERESESPAGO,0) &lt;&gt; 0 THEN 1 ELSE 0 END AS RETRIB,
    STR(LEFT(GSD.RUBROCONTABLE,6)) AS CUENTACONTABLE,
    GSD.RUBROCONTABLE AS CUENTACONTABLEUSOINTERNO

FROM SALDOS S WITH (NOLOCK)

INNER JOIN GRL_SALDOS_DIARIOS GSD WITH (NOLOCK)
    ON S.JTS_OID = GSD.SALDOS_JTS_OID
   AND GSD.FECHA = @fecha_parametro
   AND ((GSD.tz_lock &lt; 300000000000000 OR GSD.tz_lock >= 400000000000000)
        AND (GSD.tz_lock &lt; 100000000000000 OR GSD.tz_lock >= 200000000000000))

INNER JOIN MONEDAS M WITH (NOLOCK)
    ON S.MONEDA = M.C6399
   AND ((M.tz_lock &lt; 300000000000000 OR M.tz_lock >= 400000000000000)
        AND (M.tz_lock &lt; 100000000000000 OR M.tz_lock >= 200000000000000))

INNER JOIN PRODUCTOS PRD WITH (NOLOCK)
    ON S.PRODUCTO = PRD.C6250
   AND PRD.C6255 != 0
   AND ((PRD.tz_lock &lt; 300000000000000 OR PRD.tz_lock >= 400000000000000)
        AND (PRD.tz_lock &lt; 100000000000000 OR PRD.tz_lock >= 200000000000000))

INNER JOIN RRI_PARAMETROS_INF R WITH (NOLOCK)
    ON R.CODIGO = 5
   AND R.ID1 = S.PRODUCTO
   AND R.ID2 = S.MONEDA
   AND R.TZ_LOCK = 0

LEFT JOIN TABLA_SUMA_DEBITOS TSD WITH (NOLOCK)
    ON TSD.JTS_OID = S.JTS_OID

LEFT JOIN TABLA_SUMA_CREDITOS TSC WITH (NOLOCK)
    ON TSC.JTS_OID = S.JTS_OID

LEFT JOIN SALDOS S2 WITH (NOLOCK)
    ON S2.SUCURSAL = S.SUCURSAL
   AND S2.CUENTA = S.CUENTA
   AND S2.C1803 = S.C1803
   AND S2.PRODUCTO = 88
   AND S2.MONEDA = 1

-- Tipo de cambio para FECHA PARAMETRO / FECHA de GSD: reemplaza el LEFT JOIN + SELECT MAX(...) por OUTER APPLY (TOP 1)
OUTER APPLY (
    SELECT TOP 1 H.TIPO_CAMBIO_OFICIAL
    FROM HISTORICOTIPOSCAMBIO H WITH (NOLOCK)
    WHERE H.MONEDA = S.MONEDA
      AND H.CODIGO_TIPO_CAMBIO = 1
      AND H.TZ_LOCK = 0
      AND H.FECHA_COTIZACION &lt;= GSD.FECHA
    ORDER BY H.FECHA_COTIZACION DESC
) H1

-- Tipo de cambio para FECHA CONSTITUCIÓN (S.C1620)
OUTER APPLY (
    SELECT TOP 1 H.TIPO_CAMBIO_OFICIAL
    FROM HISTORICOTIPOSCAMBIO H WITH (NOLOCK)
    WHERE H.MONEDA = S.MONEDA
      AND H.CODIGO_TIPO_CAMBIO = 1
      AND H.TZ_LOCK = 0
      AND H.FECHA_COTIZACION &lt;= S.C1620
    ORDER BY H.FECHA_COTIZACION DESC
) H2

LEFT JOIN TABLA_DPF TD WITH (NOLOCK)
    ON TD.JTS_OID = S.JTS_OID

-- Histórico devengamiento (último registro &lt;= GSD.FECHA) — usamos OUTER APPLY para evitar subselect correlacionado
OUTER APPLY (
    SELECT TOP 1 HD.INTERES_DEVENGADO
    FROM HISTORICO_DEVENGAMIENTO HD WITH (NOLOCK)
    WHERE HD.SALDO_JTS_OID = S.JTS_OID
      AND HD.TZ_LOCK = 0
      AND HD.FECHA &lt;= GSD.FECHA
    ORDER BY HD.FECHA DESC
) HD

-- Histórico devengado vista (último registro &lt;= GSD.FECHA)
OUTER APPLY (
    SELECT TOP 1 HV.INTERESESPAGO
    FROM HISTORICO_DEVENGADO_VISTA HV WITH (NOLOCK)
    WHERE HV.SALDOS_JTS_OID = S.JTS_OID
      AND HV.FECHA &lt;= GSD.FECHA
    ORDER BY HV.FECHA DESC
) HD_V

LEFT JOIN Titulares T1
    ON T1.CODIGOCLIENTE = S.C1803 AND T1.NRO_PERSONA = 1 AND T1.JTS_OID = S.JTS_OID
LEFT JOIN Titulares T2
    ON T2.CODIGOCLIENTE = S.C1803 AND T2.NRO_PERSONA = 2 AND T2.JTS_OID = S.JTS_OID
LEFT JOIN Titulares T3
    ON T3.CODIGOCLIENTE = S.C1803 AND T3.NRO_PERSONA = 3 AND T3.JTS_OID = S.JTS_OID
LEFT JOIN Titulares T4
    ON T4.CODIGOCLIENTE = S.C1803 AND T4.NRO_PERSONA = 4 AND T4.JTS_OID = S.JTS_OID
LEFT JOIN Titulares T5
    ON T5.CODIGOCLIENTE = S.C1803 AND T5.NRO_PERSONA = 5 AND T5.JTS_OID = S.JTS_OID

WHERE 
    ((S.C1785=2 AND GSD.SALDO_AJUSTADO>=0)
     OR (S.C1785=3 AND GSD.SALDO_AJUSTADO>=0)
     OR (S.C1785=4 AND GSD.SALDO_AJUSTADO&lt;&gt;0 AND DATEDIFF(DAY,S.C1620,S.C1627)>0))
  AND ((S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000)
       AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000))
</sql>
