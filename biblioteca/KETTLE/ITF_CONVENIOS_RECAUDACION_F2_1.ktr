    <sql>SELECT 
'C' AS TPO_REGISTRO,
dbo.NCADENAS((SELECT NUMSUCURSAL FROM PARAMETROS),4,'0','P') AS SUCURSAL,
CONVERT(VARCHAR, RR.FECHA ,12) AS FECHA,
dbo.NCADENAS(RC.TOTALREGISTROS,10,'0','P') AS CANT_TALONES,
dbo.ncadenas(floor(RC.TOTALIMPORTE *100),15,'0','P') AS IMP_COBRADO,
dbo.ncadenas('0',37,'0','P') AS RESTO,
CONCAT('${Top.Output}CONVENIOS/B',dbo.ncadenas(RC.CONVENIO,3,'0','P'),substring((CONVERT(CHAR(8),(SELECT FECHAPROCESO FROM PARAMETROS),112)),5,4)) AS NOM_ARCHIVO,
(Select count(*) from REC_CAB_RECAUDOS_CAJA RC where RC.ID_LIQUIDACION = ?) as HayDatos,
RC.ID as IDCabezal
FROM REC_CAB_RECAUDOS_CAJA RC INNER JOIN  REC_LIQUIDACION RL ON RC.ID_LIQUIDACION=Rl.ID_LIQUIDACION AND RL.TZ_LOCK = 0
JOIN REC_RENDICION RR ON RR.ID_RENDICION = RL.ID_RENDICION AND RR.TZ_LOCK = 0
WHERE RC.TZ_LOCK=0 AND RC.ID_LIQUIDACION = ? </sql>
