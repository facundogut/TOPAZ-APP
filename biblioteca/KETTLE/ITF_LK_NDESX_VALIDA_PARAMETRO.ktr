        <jsScript_script>// Valida que el registro tenga registros
trans_Status = CONTINUE_TRANSFORMATION;
var error = ''
var tipo_arch = rtrim(LINEA.substr(0,25))
var es_cabecera = 'TRANSFERENCIAS POR DEST.'

// Contar registros
contador++

// Si el archivo no se lee correctamente
if (LINEA == null || LINEA == '') {
	error = 'Archivo sin registros';
} else {

	if ( contador == 1 &amp;&amp; tipo_arch != es_cabecera ) error = 'Archivo sin registros tipo cabecera';
	if ( tipo_arch == es_cabecera ) cabecera++;
	if ( cabecera > 1) error = 'Archivo con más de una cabecera'

	// Fecha de cabecera
	if (contador == 1 &amp;&amp; error == '' &amp;&amp; cabecera == 1) {
		var FECHAPROCESO = parseDate(getVariable('FECHATOP_F1', ''), 'yyyyMMdd')
		var fecha_cab = parseDate(LINEA.substr(25,6), 'yyMMdd');
    
		if (!fecha_cab) {
			error = 'Fecha cabecera inválida.'
		} else if ( dateDiff(fecha_cab,FECHAPROCESO, "d") &lt; 0 ) {
			error = 'Fecha cabecera mayor a fecha proceso';
		}	
	}
}

if (error != '') {
	// Kaboom!
	setVariable('msg_err', error, 'r');
    writeToLog('e', error);
	trans_Status = ERROR_TRANSFORMATION;
}</jsScript_script>
