      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>const filePathString = getVariable('Top.Input', '') + getVariable('RUTA', '') + getVariable('NA', '');

writeToLog('b', filePathString);
const filePath = java.nio.file.Paths.get(filePathString);
const fileInputStream = new java.io.FileInputStream(filePath);
const inputStreamReader = new java.io.InputStreamReader(fileInputStream, "Cp1252");
//const reader = java.nio.file.Files.newBufferedReader(filePath, java.nio.charset.StandardCharsets.UTF_8);
const reader = new java.io.BufferedReader(inputStreamReader);

const headerLength = 397;
const detailLength = 570;
var globalCount = 0;
var lineCount = 0;
var error = '';

const readFile = function ( currentRow ) {

  lineCount++;
  var stringRead = readString(headerLength);

  if (stringRead == null || stringRead == '')
    _step_.setErrors( 1 );

  var outputRow = currentRow.slice();

  outputRow[ getOutputRowMeta().indexOfValue('LINEA') ] = ( java.lang.String.valueOf(stringRead) )

  putRow( outputRow );

  while ( (stringRead = readString(detailLength)) ) {
	lineCount++;
    outputRow = currentRow.slice()
    outputRow[ getOutputRowMeta().indexOfValue('LINEA') ] = ( java.lang.String.valueOf(stringRead) )
    putRow( outputRow );
  }

}

const readString = function (length) {
  var charchar;
  var charArray = [];
  for (var i = 0; i &lt; length; i++) {
    globalCount++
    try {
      charchar = reader.read();
    } catch (ex) {
	  error = 'Error inesperado en lectura del archivo. Columna: ' + globalCount;
      writeToLog('e', error + ' - Excepción: ' + ex.message);
	  setVariable('msg_err', error, 'r');
      _step_.setErrors( 1 );
      continue
    }
    if (charchar == -1 &amp;&amp; i == 0) {
      writeToLog('b', 'Lectura del archivo OK');
      return ''
    }
    else if (charchar == -1) {
	  error = 'Error en lectura del archivo: verificar estructura de archivo. Registro N°: '+ lineCount;
      writeToLog('e', error);
	  setVariable('msg_err', error, 'r');
      _step_.setErrors( 1 );
      return null
    }
    else {
      charArray.push( String.fromCharCode(charchar) );
    }
  }
  return charArray.join('');
}</jsScript_script>
