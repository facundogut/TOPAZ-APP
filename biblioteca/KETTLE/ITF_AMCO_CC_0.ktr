  <step>
    <name>Validaciones</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT COUNT(*) AS NumeroDeFilas, CONCAT('${Top.Output}AMCO/INFORME CUENTA CORRIENTE#MENSUAL-',(SELECT CONVERT(VARCHAR(8), FECHAPROCESO, 112) FROM PARAMETROS),'.CSV') AS RUTA
FROM (
    
SELECT DISTINCT         'BCONBC' AS CATALOGO, 
        '"CUENTA CORRIENTE"' AS '"TAREA"', 
        '"Cuenta Corriente"' AS '"REPORTE"',
        '""' AS '"CONTENEDOR"',
        '"INFORME CUENTA CORRIENTE"' AS '"EMISION"',
        CONCAT('"', CONVERT(varchar(10), (SELECT FECHAPROCESO FROM PARAMETROS), 103), '"') AS '"FECHA"',
        CONCAT('INDEXTEXT01T LIKE ','''',LEFT(CAB.CBU, 8),'-',RIGHT(CAB.CBU, 14),'''','"') AS '"QUERY"',
        '"RESUMEN.PDF"' AS '"NOMBRE_ARCHIVO"',
        CONCAT('"', (
            CASE 
                WHEN UPPER(DOCS.TIPOPERSONA) = 'F' THEN
                    SUBSTRING(DOCS.NUMERODOCUMENTO, 3, LEN(DOCS.NUMERODOCUMENTO) - 3) 
                ELSE
                    DOCS.NUMERODOCUMENTO 
            END
        ), '"') AS '"PASSWORD"',
        CONCAT('"', SR.EMAIL, '"') AS '"PARA"',
        '""' AS '"CC"',
        '""' AS '"CCO"',
        '"ENVIO DE NUEVO RESUMEN"' AS '"ASUNTO"',
        '"RESUMEN_CUENTACORRIENTE.HTML"' AS '"MENSAJE"',
        '""' AS '"ADJUNTOS"',
        '"POOL1"' AS '"POOL"',
        CONCAT('"', cli.[Nombre de Persona], '"') AS '"NOMBRE"',
	   CONCAT('${Top.Output}AMCO/INFORME CUENTA CORRIENTE#MENSUAL-',(SELECT CONVERT(VARCHAR(8), FECHAPROCESO, 112) FROM PARAMETROS),'.CSV') AS RUTA
FROM GRL_CAB_ENVIO_ESTCTA CAB
JOIN GRL_ESTADOS_DE_CUENTA est ON CAB.JTSOID=est.JTSOID
INNER JOIN VW_CLI_PERSONAS CLI ON cab.CODIGOCLIENTE=CLI.[CODIGO de cliente]
INNER JOIN SUSCRIPCIONES_RESUMEN SR WITH (NOLOCK) ON SR.TZ_LOCK LIKE '[04]%' AND SR.ESTADO = 'S' AND SR.JTS_OID_SALDO = CAB.JTSOID
INNER JOIN VW_CLI_DOCUMENTOSPFPJ DOCS ON CLI.[Numero de Persona] = DOCS.NUMEROPERSONAFJ
INNER JOIN cli_clientes clientes ON clientes.CODIGOCLIENTE=CLI.[Codigo de Cliente]
INNER JOIN cli_sectores sec ON try_CAST(clientes.SUBDIVISION1 AS NUMERIC)=try_convert(numeric,sec.SECTOR_contable)
INNER JOIN SALDOS sal ON CAB.JTSOID=sal.JTS_OID
WHERE DOCS.TZ_LOCK=0
AND UPPER(CLI.Titularidad)='T'
AND try_convert(numeric,sec.SECTOR) NOT IN (3,4,5)
AND clientes.TZ_LOCK=0
AND sec.TZ_LOCK=0
AND sal.C1785=2
AND sal.TZ_LOCK=0
AND CAB.JTSOID>0
AND est.TZ_LOCK=0
AND est.CORREO_TRADICIONAL='S'
and sal.C1651 != 1
and sal.C1728 &lt;&gt; 'I'
and SAL.JTS_OID NOT IN (SELECT SALDO_JTS_OID FROM GRL_BLOQUEOS WHERE COD_BLOQUEO=24)
and cab.CBU &lt;&gt; ''

) AS ConteoFilas;
</sql>
