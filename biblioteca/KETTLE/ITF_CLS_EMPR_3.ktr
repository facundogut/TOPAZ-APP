    <sql>SELECT 
	(SELECT count(*) from SNP_PRESTACIONES_EMPRESAS) AS totEmp,
	(select count(1) from SNP_PRESTACIONES_EMPRESAS_AUX) as totRecibido,
	count(*) AS totCambios,
	(SELECT NUMERICO_1 FROM dbo.ITF_MASTER_PARAMETROS  WHERE CODIGO = 166)  AS porcentajeCambio
		FROM ITF_COELSA_EMPRESAS_HOMO_HISTORIAL WHERE ID_TICKET = ? AND TIPO_ACCION IN ('A','M','B') ;</sql>
