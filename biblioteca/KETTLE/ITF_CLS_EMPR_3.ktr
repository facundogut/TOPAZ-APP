    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var mostrar_warning = '';
var cod_res = 2;
var consolidar = true;
var porcentaje = parseFloat('0.' + porcentajeCambio);



if(opcionEjecucion == 'N'){ //ejecucion Normal
	//if(totCambios &lt; totEmp*porcentaje){
	if(totRecibido &lt; totEmp*porcentaje){
		consolidar = false;
		mostrar_warning = 'ADVERTENCIA: la cantidad de registros recibidos es menor al '+ porcentajeCambio + '%. No se han realizado acciones.';
		cod_res = 2;
	}
	else{
		mostrar_warning = 'El porcentaje registros recibidos excede '+ porcentajeCambio + '%. Se han realizado '+totCambios+' acciones.';
		cod_res = 1;
	}
}
else{ //ejecucion Forzada
	//if(totCambios &lt; totEmp*porcentaje){
	if(totRecibido &lt; totEmp*porcentaje){
		mostrar_warning = 'Se han realizado '+totCambios+' acciones. El porcentaje de registros recibidos es menor al '+ porcentajeCambio + '%.';
		cod_res = 1;
	}
	else{
		mostrar_warning = 'Se han realizado '+totCambios+' acciones. El porcentaje de registros recibidos excede al '+ porcentajeCambio + '%.';
		cod_res = 1;	
	}
}</jsScript_script>
