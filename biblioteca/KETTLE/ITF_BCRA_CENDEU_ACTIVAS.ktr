    <quoteString>N</quoteString>
    <sql>INSERT INTO CRE_BCRA_CENDEU WITH (TABLOCK)
SELECT Fecha_Proceso,COD_ENTIDAD,FECHA,B.[Codigo De Cliente],TPO_IDENTIFICACION,NRO_IDENTIFICACION,ACTIVIDAD,SITUACION_BCRA,PRESTAMOS,PARTICIPACIONES,GARANTIAS,OTROS_CONCEPTOS,
(PRESTAMOS +  PARTICIPACIONES + GARANTIAS + OTROS_CONCEPTOS) AS DEUDA_TOTAL,GARANTIA_A,GARANTIA_B,SIN_GARANTIAS,CONTRA_GARANTIA_A,
CONTRA_GARANTIA_B,SIN_CONTRA_GARANTIA,PREVISIONES,DEUDA_CUBIERTA,PROCESO_JUDICIAL,REFINANCIACIONES,RECATEGORIZACION,SIT_JURIDICA,IRR_DISP_TECNICA,
DIAS_ATRASO,SITUACION_NBCH,0
FROM ITF_BCRA_CENDEU A WITH (nolock)
JOIN VW_CLI_PERSONAS B ON A.NRO_IDENTIFICACION = B.[Numero de Documento] 
WHERE A.Fecha_Proceso = (SELECT FECHAPROCESO FROM PARAMETROS WITH (nolock)) AND  B.Titularidad = 'T';</sql>
