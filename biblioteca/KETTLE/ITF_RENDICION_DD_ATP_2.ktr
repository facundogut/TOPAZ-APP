    <connection>DESA</connection>
    <sql>DECLARE @p_CUIT VARCHAR(20) = '${P0_VALUE}', @p_fechaDesde VARCHAR(8) = '${P1_VALUE}', @p_fechaHasta VARCHAR(8) = '${P2_VALUE}' ;


SELECT DATO, 
  --FECHA_COMPENSACION,
  ID FROM 
(

SELECT CONCAT('C', --AS TIPOREGISTRO,
  '0050', --AS SUCURSAL,
  SUBSTRING(CONVERT(VARCHAR,a.FECHA_RENDIDA,112),3,6), --AS FECHARENDICION,
  --SUBSTRING(CONVERT(VARCHAR,a.FECHA_COMPENSACION,112),3,6), -- AS FECHARENDICION,
  RIGHT(CONCAT(REPLICATE('0',10), COUNT(1)), 10), --AS CANTIDADLOTES,
  RIGHT(CONCAT(REPLICATE('0',15),REPLACE(CONVERT(VARCHAR,SUM((case when a.FECHA_VENCIMIENTO &lt; a.FECHA_COMPENSACION and a.FECHA_COMPENSACION &lt; a.SEGUNDO_VTO then a.IMPORTE
         else a.SEGUNDO_IMPORTE end))),'.','')),15), --AS IMPORTECOBRADO,
  REPLICATE('0',49)) AS DATO, --RESTO,
  --a.FECHA_COMPENSACION,
  1 AS ID
--select a.*
FROM SNP_MSG_ORDENES a WITH (NOLOCK)
  INNER join SNP_PRESTACIONES_EMPRESAS p WITH (NOLOCK) on p.CUIT_EO = a.CUIT_EO and p.PRESTACION = a.PRESTACION
  INNER JOIN SNP_SOPORTE_DEBITO_DIRECTO b WITH (NOLOCK) ON b.CUIT = a.CUIT_EO AND b.CONVENIO = a.CONVENIO AND b.LOTE_ID = a.ID_ARCHIVO
WHERE a.CUIT_EO = @p_CUIT
  AND a.FECHA_COMPENSACION between @p_fechaDesde and @p_fechaHasta
  AND a.ESTADO = 'PR'
  AND a.TZ_LOCK = 0
  AND p.TZ_LOCK = 0
GROUP BY SUBSTRING(CONVERT(VARCHAR,a.FECHA_RENDIDA,112),3,6)
--ORDER BY 3 ASC
UNION ALL

SELECT CONCAT('T', --AS TIPOREGISTRO,
  405, --AS CODIGOFORMULARIO,
  --a.CUIT_TC,-- AS CUITCLIENTE,
  CONVERT(NUMERIC,RIGHT(a.CLIENTE_PAGADOR,11)), --AS CUITCLIENTE
  --b.PERIODO,-- AS ANIOMES,
  LEFT(a.REFERENCIA,4), -- AS ANIOMES,
  SUBSTRING(a.REFERENCIA,6,2), --AS CUOTA,
  RIGHT(CONCAT(REPLICATE('0',11),REPLACE(CONVERT(VARCHAR, (case when a.FECHA_VENCIMIENTO &lt; a.FECHA_COMPENSACION and a.FECHA_COMPENSACION &lt; a.SEGUNDO_VTO then a.IMPORTE
         else a.SEGUNDO_IMPORTE end)),'.','')),11), --  AS IMPORTE,
  CONVERT(VARCHAR,(case when a.FECHA_VENCIMIENTO &lt; a.FECHA_COMPENSACION and a.FECHA_COMPENSACION &lt; a.SEGUNDO_VTO then a.FECHA_VENCIMIENTO 
         else a.SEGUNDO_VTO end),112),-- AS FECVENCIMIENTO,
  RIGHT(a.REFERENCIA,6),-- AS NROPRESENTACION,
  0, -- AS PARIDAD,
  REPLICATE(' ',19),-- AS Relleno,
  SUBSTRING(CONVERT(VARCHAR,a.FECHA_COMPENSACION,112),3,6), -- AS FECHACOBRO,
  RIGHT(CONCAT(REPLICATE('0',12),REPLACE(CONVERT(VARCHAR, (case when a.FECHA_VENCIMIENTO &lt; a.FECHA_COMPENSACION and a.FECHA_COMPENSACION &lt; a.SEGUNDO_VTO then a.IMPORTE
         else a.SEGUNDO_IMPORTE end)),'.','')),12), -- AS IMPORTECOBRADO,
  0) AS DATO, -- AS TIPOINGRESO,
  --a.FECHA_COMPENSACION,
  2 AS ID
FROM SNP_MSG_ORDENES a WITH (NOLOCK)
  INNER join SNP_PRESTACIONES_EMPRESAS p WITH (NOLOCK) on p.CUIT_EO = a.CUIT_EO and p.PRESTACION = a.PRESTACION
  INNER JOIN SNP_SOPORTE_DEBITO_DIRECTO b WITH (NOLOCK) ON b.CUIT = a.CUIT_EO AND b.CONVENIO = a.CONVENIO AND b.LOTE_ID = a.ID_ARCHIVO
WHERE a.CUIT_EO = @p_CUIT
  AND a.FECHA_COMPENSACION between @p_fechaDesde and @p_fechaHasta
  AND a.ESTADO = 'PR'
  AND a.TZ_LOCK = 0
  AND p.TZ_LOCK = 0
) xd 
ORDER BY --xd.FECHA_COMPENSACION, 
xd.ID</sql>
