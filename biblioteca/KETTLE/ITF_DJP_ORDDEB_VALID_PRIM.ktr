    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Transforma</jsScript_name>
        <jsScript_script>//Script here

var COD_ERROR = 0;
var ERROR = null;
var PERIODO_DT = parseDate(PARAM_PERIODO, 'yyyy-MM')
var PERIODO_INT = parseInt(date2str(PERIODO_DT, 'yyyyMM'), 10);

// VALIDACIONES

if (validaTipo(PARAM_CONVENIO, 'numero')!=null) {
	COD_ERROR = 5;
	ERROR = 'Parametro CONVENIO inválido o inexistente';
}
else if (validaTipo(PARAM_CUIT, 'numero')!=null) {
	COD_ERROR = 5;
	ERROR = 'Parametro CUIT inválido o inexistente';
}
else if (validaTipo(PARAM_LOTEID)!=null) {
	COD_ERROR = 5;
	ERROR = 'Parametro LOTEID inválido o inexistente';
}

else if (!PERIODO_DT) {
	COD_ERROR = 5;
	ERROR = 'Parametro PERIODO inválido o inexistente';
	PERIODO_INT = 0;
}

else if (validaTipo(PARAM_REGISTROS, 'numero')) {
	COD_ERROR = 5;
	ERROR = 'Parametro REGISTROS inválido o inexistente';
}

else if (validaTipo(PARAM_SUMAVENCI1, 'numero')) {
	COD_ERROR = 5;
	ERROR = 'Parametro SUMAVENCI1 inválido o inexistente';
}

else if (validaTipo(PARAM_SUMAVENCI2, 'numero')) {
	COD_ERROR = 5;
	ERROR = 'Parametro SUMAVENCI2 inválido o inexistente';
}
else if (CANT_REG_STAGE == 0) {
	COD_ERROR = 20;
	ERROR = 'Lote sin ordenes individuales';
} else if (EXISTE_CABEZAL == 1) {
	COD_ERROR = 21;
	ERROR = 'Lote ya procesado';
} else if (CONVENIO_PERIODO_CORRECTO == 1) {
	COD_ERROR = 27;
	ERROR = 'Un lote ya fué procesado segun convenio y periodo recibidos';
} else if (CANT_REG_STAGE != CANT_REG_PARAM) {
	COD_ERROR = 22;
	ERROR = 'Cantidad de ordenes incorrecta';
}  else if (SUM_1VENC_STAGE != SUM_1VENC_PARAM) {
	COD_ERROR = 23;
	ERROR = 'Sumatoria de 1º vencimiento incorrecta';
} else if (SUM_2VENC_STAGE != SUM_2VENC_PARAM) {
	COD_ERROR = 24;
	ERROR = 'Sumatoria de 2º vencimiento incorrecta';
}

// Set variable en caso de error
if (COD_ERROR != 0) {
	setVariable('Msg_Error', ERROR, 'r');
	setVariable('Cod_Error', COD_ERROR, 'r');
}
</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Inicio</jsScript_name>
        <jsScript_script>//Script here

var SUM_1VENC_PARAM = Number(getVariable('SUMAVENCI1', '0'));

var SUM_2VENC_PARAM = Number(getVariable('SUMAVENCI2', '0'))

var CANT_REG_PARAM = Number(getVariable('REGISTROS', '0'));

var validaTipo = function(field, tipo) {
	
	if (field == '' || field == null) {
		return 'error';
	}
	else if (tipo == 'numero') {
		if (isNaN(field)) return 'error';
		else if (Number(field)==0) return 'error';
	}
	
	return null;
};


const parseDate = function (date, format) {
  var parsedDate;
  try {
    parsedDate = str2date( date, format);
  } catch (ex) {
    return null;
  }
 
  var reversedDate = date2str( parsedDate, format);
 
  if ( reversedDate == date) {
    return parsedDate;
  } else {
    return null;
  }
}</jsScript_script>
      </jsScript>
