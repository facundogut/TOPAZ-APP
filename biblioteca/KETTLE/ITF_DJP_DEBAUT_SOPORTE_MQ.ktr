    <connection>StageDB</connection>
    <sql>SELECT
	odal.id AS 'ID',
	odal.producto AS 'PRODUCTO',
	odal.tipoCuentaDestino AS 'TIPO_CUENTA_DESTINO',
	odal.sucursalCuenta AS 'SUCURSAL_CUENTA',
	odal.numeroCuenta AS 'NUMERO_CUENTA',
	odal.formaRecaudacion AS 'FORMA_RECAUDACION',
	odal.tipoRecaudacion AS 'TIPO_RECAUDACION',
	odal.polizaJurisdiccion AS 'POLIZA_JURISDICCION',
	odal.periodo AS 'PERIODO',
	odal.moneda AS 'MONEDA',
	odal.cuil AS 'CUIL',
	odal.importe AS 'IMPORTE',
	odal.informacionExterna AS 'INFORMACION_EXTERNA',
	odal.tipoMovimiento AS 'TIPO_MOVIMIENTO',
	odal.idLote AS 'ID_LOTE',
	ROW_NUMBER() OVER (ORDER BY odal.ID ASC) AS 'RN'
FROM OrdenDebitoAutomaticoLote odal 
WHERE idLote = '${ID_LOTE}' AND (codigoError = 0 OR codigoError IS NULL)
ORDER BY odal.ID ASC</sql>
    <limit>0</limit>
