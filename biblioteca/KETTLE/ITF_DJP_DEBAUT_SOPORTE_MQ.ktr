  <step>
    <name>Obtiene Saldo JTS OID</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE
	@CCU BIGINT = ?,
	@FECHA_PROCESO DATETIME = ?,
	@CUENTA_SUCURSAL VARCHAR(5) = ?,
	@CUENTA_PRODUCTO_TIPO VARCHAR(2) = ?,
	@CUENTA_NUMERO VARCHAR(20) = ?
SELECT TOP 1
	S.JTS_OID,
	CASE
		WHEN S.C1679 = 0 THEN 0
		WHEN S.C1679 &lt;&gt; 0 AND EXISTS (
			SELECT 1
			FROM GRL_BLOQUEOS GB WITH (NOLOCK)
			INNER JOIN GRL_COD_BLOQUEOS GCB WITH (NOLOCK) ON GCB.COD_BLOQUEO = GB.COD_BLOQUEO AND GCB.ACCIONES_DEBITO IN (2,3)
			WHERE 
				GB.SALDO_JTS_OID = S.JTS_OID AND
				ESTADO = 1 AND
				( COALESCE(MANEJA_VENCIMIENTO,0) = 0 OR (MANEJA_VENCIMIENTO = 1 AND FECHA_VENCIMIENTO > @FECHA_PROCESO)) AND
				FECHA_VIGENCIA &lt;= @FECHA_PROCESO
		) THEN 1
	END AS "BLOQUEO_CUENTA",
	SSD.CLIENTE_ADHERIDO AS TIENE_STOP_DEBIT
FROM SALDOS S
LEFT JOIN SNP_STOP_DEBIT SSD ON 
	SSD.CLIENTE_ADHERIDO = S.C1803 AND 
	(SSD.SALDO_JTS_OID = 0 OR SSD.SALDO_JTS_OID = S.JTS_OID) AND 
	(SSD.ID_CONVENIO = 0 OR SSD.ID_CONVENIO = @CCU) AND 
	SSD.DEBITO_DIRECTO = 'N' AND 
	SSD.ESTADO = 'AC' AND 
	SSD.FECHA_DESDE &lt;= @FECHA_PROCESO AND 
	@FECHA_PROCESO &lt; SSD.FECHA_HASTA AND 
	SSD.TZ_LOCK = 0
WHERE
	S.MONEDA = 1 AND
	S.TZ_LOCK = 0 AND
	S.SUCURSAL = @CUENTA_SUCURSAL AND
	S.C1785 = @CUENTA_PRODUCTO_TIPO AND -- TIPO DE CUENTA
	S.CUENTA = @CUENTA_NUMERO</sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
