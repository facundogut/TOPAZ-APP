    <rowlimit>0</rowlimit>
    <sql>

DECLARE 
	@convPadre VARCHAR (12) = ?,
	@fechaRen VARCHAR(8) = ?; --caja:666 canal: 442200 desa


---CAB
SELECT DISTINCT
	det.sucursal_cobranza AS sucursal,
  		CONCAT(
		'C',
		RIGHT(CONCAT(REPLICATE('0',4),det.sucursal_cobranza),4), --sucursal
		RIGHT(@fechaRen,6), --fecha ren
		RIGHT(CONCAT(REPLICATE('0',10),xd.tot_registros),10) , --cant talones
		RIGHT(CONCAT(REPLICATE('0',12),floor(xd.tot_importe*100)),12), --tot importes
		REPLICATE('0',35)) header
		
FROM REC_LIQUIDACION (NOLOCK) liq 
	JOIN REC_RENDICION (NOLOCK) ren ON liq.ID_RENDICION = ren.ID_RENDICION
	JOIN CONV_CONVENIOS_REC (nolock) conv ON conv.id_convrec = ren.convenio OR conv.id_convrec = ren.convenio_padre
	JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION 	
	JOIN REC_DET_RECAUDOS_CAJA (NOLOCK) det ON cab.ID = det.ID_CABEZAL
 	JOIN 
	(
   		SELECT 
			det.sucursal_cobranza AS sucursal,
			sum(det.IMPORTE) AS tot_importe,
			count(1) AS tot_registros			
			FROM REC_DET_RECAUDOS_CAJA (NOLOCK) AS det 
				JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) AS cab ON det.ID_CABEZAL= cab.ID
				JOIN REC_LIQUIDACION (NOLOCK) AS liq ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
				JOIN REC_RENDICION (nolock) AS ren ON ren.ID_RENDICION = liq.ID_RENDICION
			WHERE det.TZ_LOCK=0 AND liq.ESTADO = 'R' AND ren.FECHA = CAST(@fechaRen AS DATETIME) AND (ren.convenio = @convPadre OR ren.convenio_padre = @convPadre)
		
			GROUP BY det.sucursal_cobranza
	) xd ON det.SUCURSAL_COBRANZA = xd.sucursal

			WHERE  
				conv.id_convrec = @convPadre AND 
				ren.FECHA = CAST(@fechaRen AS DATETIME) AND
				cab.tz_lock = 0 AND 
				det.tz_lock = 0 AND 
				liq.tz_lock = 0 AND 
				ren.tz_lock = 0  AND
				conv.FORMATO_RENDICION = 	
			(
			SELECT opcioninterna 
			FROM OPCIONES 
			WHERE 
			NUMERODECAMPO = 45146 AND 
			IDIOMA = 'E' AND 
			upper(DESCRIPCION) LIKE '%FORMATO ESTANDAR 1%'
			)
				
			
UNION ALL 

SELECT DISTINCT
	suc.SUCURSAL AS sucursal,
	concat(
	'C',
	RIGHT(CONCAT(REPLICATE('0',4),suc.SUCURSAL),4), 
   	RIGHT(@fechaRen,6),
   	RIGHT(CONCAT(REPLICATE('0',10),cab.TOTALREGISTROS),10),			
	RIGHT(CONCAT(REPLICATE('0',12),floor(cab.TOTALIMPORTE*100)),12),   
	REPLICATE('0',35) ) header
	
	FROM REC_LIQUIDACION (NOLOCK) liq 
		JOIN REC_RENDICION (NOLOCK) ren ON liq.ID_RENDICION = ren.ID_RENDICION
	    JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON conv.id_convrec = ren.convenio OR conv.id_convrec = ren.convenio_padre
	    JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
	    JOIN rec_cab_recaudos_canal (NOLOCK) cab ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
	WHERE 
		conv.id_convrec = @convPadre AND 
   		ren.FECHA = CAST(@fechaRen AS DATETIME) AND
		cab.tz_lock = 0 AND 
		liq.tz_lock = 0 AND 
