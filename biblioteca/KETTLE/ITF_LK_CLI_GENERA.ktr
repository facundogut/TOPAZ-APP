        <jsScript_script>REGISTRO = REGISTRO;

count++;</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Start</jsScript_name>
        <jsScript_script>LoadScriptFromTab('Util')

count = 1;

fechaTlf = getVariable("FECHA_TLF", "");

REGISTRO = (
	rfix("HRCLIENTES", 10) +
	lfix('0311', 4) +
	lfix(fechaTlf, 6) +
	rfix("  ", 2) +	
	rfix("  ", 37)
);

CABECERA = row.slice();

CABECERA[ rowMeta.indexOfValue('REGISTRO') ] = java.lang.String.valueOf(REGISTRO);

_step_.putRow( rowMeta, CABECERA);

</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>2</jsScript_type>
        <jsScript_name>End</jsScript_name>
        <jsScript_script>count++;

setVariable("CANT_L1", count, 'r');

REGISTRO = (
	rfix("TRCLIENTES", 10) +
	lfix(count, 8 ) +
	rfix("  ", 41)
);

PIE = row.slice();

PIE[ rowMeta.indexOfValue('REGISTRO') ] = java.lang.String.valueOf(REGISTRO);

_step_.putRow( rowMeta, PIE);</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>-1</jsScript_type>
        <jsScript_name>Util</jsScript_name>
        <jsScript_script>function lfix (value, length, filler) {
	if (filler == null)
		filler = '0';

	value = String(value);
	value = value.substring(0, length);
	return lpad(value, filler, length);
}

function rfix (value, length, filler ) {
	if (filler == null)
		filler = " ";	
	
	value = String(value);
	value = value.slice(-length);
	return rpad(value, filler, length);
}

function getCampo(campo) {
	return row[rowMeta.indexOfValue(String(campo))];
}

function getImporte(campo, decimales) {
	value = getCampo(campo);
	return value.scaleByPowerOfTen(decimales).toBigInteger();
}</jsScript_script>
      </jsScript>
    </jsScripts>
