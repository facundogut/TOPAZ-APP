    <rowlimit>0</rowlimit>
    <sql>declare @periodo varchar(max)=?;
WITH Resultados AS (
SELECT cab.jtsoid AS jtsoid
FROM GRL_CAB_ENVIO_ESTCTA cab
JOIN GRL_ESTADOS_DE_CUENTA est ON CAB.JTSOID=est.JTSOID
join saldos sal on sal.jts_oid=est.JTSOID
INNER JOIN GRL_DET_ENVIO_ESTCTA det ON cab.jtsoid = det.jtsoid AND cab.PERIODO = det.periodo AND cab.idcab = det.idcab
INNER JOIN VW_CLI_PERSONAS cli ON cab.CODIGOCLIENTE= cli.[CODIGO de cliente] AND cli.titularIDAD = 'T'
INNER JOIN monedas mon ON cab.MONEDA=mon.c6400
WHERE cab.TIPOPRODUCTO = 2
	AND est.CORREO_TRADICIONAL = 'S'
   AND det.TIPOMOV = 'I'
   and cab.periodo=@periodo
and sal.tz_lock=0
and sal.C1651 != 1
and sal.C1728 &lt;&gt; 'I'
and SAL.JTS_OID NOT IN (SELECT SALDO_JTS_OID FROM GRL_BLOQUEOS WHERE COD_BLOQUEO=24)
AND EXISTS(
	SELECT 1 
	FROM SUSCRIPCIONES_RESUMEN SR (NOLOCK)
	WHERE 
		SR.JTS_OID_SALDO = sal.JTS_OID AND
		SR.ESTADO = 'S' AND 
		SR.TZ_LOCK LIKE '[04]%'
	)
)
SELECT COUNT(*) AS CantidadFilasIndicador2,concat('${Top.Output}AMCO/RESUMENESCC','.TXT') as 'RutaArchivo'
FROM Resultados
</sql>
