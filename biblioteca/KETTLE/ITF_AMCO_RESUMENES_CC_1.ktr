    <sql>BEGIN
declare @periodo varchar(max)= ?;

; WITH COBROS_CREDEB AS (
	SELECT
		CE.JTSOID,
		--ISNULL(B.SEGMENTO, 0) AS SEGMEMNTO,
		--ISNULL(CT.TASA, CI.TASA) AS TASA,
		--CF.TASA_CREDITO_FISCAL,
		--CD.MONEDA,
		--FL.MONTO_FACTURABLE,
		CASE
			WHEN C6403 = 'I'
				THEN (FL.MONTO_FACTURABLE * HTC.TIPO_CAMBIO_VENTA)
			WHEN C6403 = 'N'
				THEN FL.MONTO_FACTURABLE
			ELSE
				(FL.MONTO_FACTURABLE * COT.TC_VENTA)
		END AS MONTO_CONVERTIDO_MN,
		(CF.TASA_CREDITO_FISCAL *
		CASE
			WHEN C6403 = 'I'
				THEN (FL.MONTO_FACTURABLE * HTC.TIPO_CAMBIO_VENTA)
			WHEN C6403 = 'N'
				THEN FL.MONTO_FACTURABLE
			ELSE
				(FL.MONTO_FACTURABLE * COT.TC_VENTA)
		END) AS CREDEB_COMP
	FROM GRL_CAB_ENVIO_ESTCTA AS CE WITH (nolock)
	INNER JOIN GRL_DET_ENVIO_ESTCTA AS CD WITH (nolock) ON
		CD.LEGAL = CE.LEGAL
		AND CD.PERIODO = CE.PERIODO
		AND CD.IDCAB = CE.IDCAB
		AND CD.JTSOID = CE.JTSOID
		AND CD.TIPOMOV = 'M'
		AND CD.IDDET = (
			SELECT TOP 1 IDDET
			FROM GRL_DET_ENVIO_ESTCTA
			WHERE LEGAL = CD.LEGAL
				AND PERIODO = CD.PERIODO
				AND IDCAB = CD.IDCAB
				AND JTSOID = CD.JTSOID
				AND TIPOMOV = CD.TIPOMOV
				AND SUCURSALORIGEN = CD.SUCURSALORIGEN
				AND ASIENTO = CD.ASIENTO
				AND FECHAVALOR = CD.FECHAVALOR
				/*AND CODMOVIMIENTO IN (
					SELECT DISTINCT CODIGO_TRANSACCION
					FROM CI_CARGOS
					WHERE TIPO_CARGO_IMPOSITIVO = 6
						AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
							AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
						)
					UNION
					SELECT DISTINCT CODIGO_TRANSACCION 
					FROM CI_IMPUESTOS
					WHERE TIPO_IMPUESTO = 6
						AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
							AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
						)
				)*/
		)
	INNER JOIN CI_FACTURA_LINEA AS FL WITH (nolock) ON
		FL.ASIENTO = CD.ASIENTO
		AND FL.SUCURSAL_ASIENTO = CD.SUCURSALORIGEN
		AND FL.FECHA_PROCESO_ASIENTO = CD.FECHAVALOR
		AND (FL.ID_CARGO IN (
				SELECT ID_CARGO
				FROM CI_CARGOS
				WHERE TIPO_CARGO_IMPOSITIVO = 6
					AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
						AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
					)
			)
			OR FL.ID_IMPUESTO IN (
				SELECT ID_IMPUESTO
				FROM CI_IMPUESTOS
				WHERE TIPO_IMPUESTO = 6
					AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
						AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
					)
			)
		)
		AND ((FL.TZ_LOCK &lt; 100000000000000 OR FL.TZ_LOCK >= 200000000000000)
			AND (FL.TZ_LOCK &lt; 300000000000000 OR FL.TZ_LOCK >= 400000000000000)
		)
	INNER JOIN MONEDAS AS M WITH (nolock) ON
		M.C6399 = CD.MONEDA
	LEFT JOIN CON_COTIZACIONES_BNA AS COT WITH (nolock) ON
		COT.Moneda = CD.MONEDA
		AND COT.Fecha_Cotizacion = /*dbo.diahabil(*/CD.FECHA_REAL_MOV/*, 'D')*/
		AND ((COT.TZ_LOCK &lt; 100000000000000 OR COT.TZ_LOCK >= 200000000000000)
			AND (COT.TZ_LOCK &lt; 300000000000000 OR COT.TZ_LOCK >= 400000000000000)
		)
	LEFT JOIN HISTORICOTIPOSCAMBIO AS HTC WITH (nolock) ON
		HTC.MONEDA = CD.MONEDA
		AND HTC.FECHA_COTIZACION = /*dbo.diahabil(*/CD.FECHA_REAL_MOV/*, 'D')*/
		AND HTC.CODIGO_TIPO_CAMBIO = (
			SELECT TOP 1 CODIGO_TIPO_CAMBIO
			FROM HISTORICOTIPOSCAMBIO WITH (nolock)
			WHERE MONEDA = HTC.MONEDA
				AND FECHA_COTIZACION = HTC.FECHA_COTIZACION
				AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
					AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
				)
			ORDER BY CODIGO_TIPO_CAMBIO
		)
		AND ((HTC.TZ_LOCK &lt; 100000000000000 OR HTC.TZ_LOCK >= 200000000000000)
			AND (HTC.TZ_LOCK &lt; 300000000000000 OR HTC.TZ_LOCK >= 400000000000000)
		)
	LEFT JOIN RRII_BITACORA_CREDEB AS B WITH (nolock) ON
		B.JTS_OID_SALDOS = CE.JTSOID
		AND B.TIPO_CARGO_IMPOSITIVO = 6
		AND B.FECHA = (
			SELECT TOP 1 FECHA
			FROM RRII_BITACORA_CREDEB
			WHERE JTS_OID_SALDOS = B.JTS_OID_SALDOS
				AND TIPO_CARGO_IMPOSITIVO = B.TIPO_CARGO_IMPOSITIVO
				AND FECHA &lt;= CD.FECHA_REAL_MOV
				AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
					AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
				)
		)
		AND ((B.TZ_LOCK &lt; 100000000000000 OR B.TZ_LOCK >= 200000000000000)
			AND (B.TZ_LOCK &lt; 300000000000000 OR B.TZ_LOCK >= 400000000000000)
		)
	LEFT JOIN CI_CARGOS_TARIFAS AS CT WITH (nolock) ON
		CT.ID_CARGO = FL.ID_CARGO
		AND CT.MONEDA = CD.MONEDA
		AND CT.SEGMENTO = ISNULL(B.SEGMENTO, 0)
		AND CT.ID_CLIENTE = 0
		AND CT.EMPRESA = 0
		AND CT.FECHA_DESDE = (
			SELECT TOP 1 FECHA_DESDE
			FROM CI_CARGOS_TARIFAS
			WHERE ID_CARGO = CT.ID_CARGO
				AND MONEDA = CT.MONEDA
				AND SEGMENTO = CT.SEGMENTO
				AND ID_CLIENTE = CT.ID_CLIENTE
				AND EMPRESA = CT.EMPRESA
				AND FECHA_DESDE &lt;= (CD.FECHA_REAL_MOV)
				AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
					AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
				)
			ORDER BY FECHA_DESDE DESC
		)
		AND CT.RANGO_HASTA = (
			SELECT TOP 1 RANGO_HASTA
			FROM CI_CARGOS_TARIFAS
			WHERE ID_CARGO = CT.ID_CARGO
				AND MONEDA = CT.MONEDA
				AND SEGMENTO = CT.SEGMENTO
				AND ID_CLIENTE = CT.ID_CLIENTE
				AND EMPRESA = CT.EMPRESA
				AND FECHA_DESDE = CT.FECHA_DESDE
				AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
					AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
				) 
		)
		AND ((CT.TZ_LOCK &lt; 100000000000000 OR CT.TZ_LOCK >= 200000000000000)
			AND (CT.TZ_LOCK &lt; 300000000000000 OR CT.TZ_LOCK >= 400000000000000)
		)
	LEFT JOIN CI_IMPUESTOS_TARIFAS AS CI WITH (nolock) ON
		CI.ID_IMPUESTO = FL.ID_IMPUESTO
		AND CI.MONEDA = CD.MONEDA
		AND CI.SEGMENTO = ISNULL(B.SEGMENTO, 0)
		AND CI.ID_CLIENTE = 0
		AND CI.EMPRESA = 0
		AND CI.FECHA_DESDE = (
			SELECT TOP 1 FECHA_DESDE
			FROM CI_IMPUESTOS_TARIFAS
			WHERE ID_IMPUESTO = CI.ID_IMPUESTO
				AND MONEDA = CI.MONEDA
				AND SEGMENTO = CI.SEGMENTO
				AND ID_CLIENTE = CI.ID_CLIENTE
				AND EMPRESA = CI.EMPRESA
				AND FECHA_DESDE &lt;= (CD.FECHA_REAL_MOV)
				AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
					AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
				)
			ORDER BY FECHA_DESDE DESC
		)
		AND CI.RANGO_HASTA = (
			SELECT TOP 1 RANGO_HASTA
			FROM CI_IMPUESTOS_TARIFAS
			WHERE ID_IMPUESTO = CI.ID_IMPUESTO
				AND MONEDA = CI.MONEDA
				AND SEGMENTO = CI.SEGMENTO
				AND ID_CLIENTE = CI.ID_CLIENTE
				AND EMPRESA = CI.EMPRESA
				AND FECHA_DESDE = CI.FECHA_DESDE
				AND ((TZ_LOCK &lt; 100000000000000 OR TZ_LOCK >= 200000000000000)
					AND (TZ_LOCK &lt; 300000000000000 OR TZ_LOCK >= 400000000000000)
				) 
		)
		AND ((CI.TZ_LOCK &lt; 100000000000000 OR CI.TZ_LOCK >= 200000000000000)
			AND (CI.TZ_LOCK &lt; 300000000000000 OR CI.TZ_LOCK >= 400000000000000)
		)
	LEFT JOIN CON_CREDITO_FISCAL_CREDEB AS CF WITH (nolock) ON
		CF.ALICUOTA_CREDEB = ISNULL(CT.TASA, CI.TASA)
		AND ((CF.TZ_LOCK &lt; 100000000000000 OR CF.TZ_LOCK >= 200000000000000)
			AND (CF.TZ_LOCK &lt; 300000000000000 OR CF.TZ_LOCK >= 400000000000000)
		)
	WHERE
		CE.LEGAL = ''
		AND CE.PERIODO = @periodo
),

COBROS_CREDEB_AGRUPADO AS (
SELECT 
	C.JTSOID,
	SUM(C.MONTO_CONVERTIDO_MN) AS MONTO_CONVERTIDO_MN,
	SUM(C.CREDEB_COMP) AS CREDEB_COMP
FROM COBROS_CREDEB AS C
group by c.JTSOID
)
 
SELECT
   concat('1',
   LEFT(ISNULL(RTRIM(cli.[Nombre de Persona]),'') + REPLICATE(' ', 30), 30),
   LEFT(ISNULL(concat(ltrim(rtrim(cab.CALLE)),' ',ltrim(rtrim(cab.NUMPUERTA)),' ', ltrim(rtrim(cab.APARTAMENTO)),' ',ltrim(rtrim(cab.PISO))),'') + REPLICATE(' ', 60),60),
   LEFT(ISNULL(ltrim(rtrim(cab.CODIGOPOSTAL)),'') + REPLICATE(' ', 8),8),
   LEFT(ISNULL(cab.CIUDADLOCALIDAD,'') + REPLICATE(' ', 30) ,30),
   CONVERT(VARCHAR(8), cab.FECHAEMISION, 112),
   --RIGHT(REPLICATE(' ', 30) + ISNULL(cab.NOMBRESUCURSAL,''),30),
   LEFT(ISNULL(cab.NOMBRESUCURSAL,'') + REPLICATE(' ', 30),30),
   CONVERT(VARCHAR(8), cab.FECHAdesde, 112),
   CONVERT(VARCHAR(8), cab.FECHAhasta, 112),
   LEFT(ISNULL(cab.TIPOCUENTA,'') + REPLICATE(' ', 30),30),
   RIGHT(replicate('0',12) + ISNULL(CAST(cab.CUENTA AS VARCHAR(12)),''), 12),
   LEFT(ISNULL(cab.moneda,'') + REPLICATE(' ', 20),20),
   LEFT(cab.RESUMENEN+ REPLICATE(' ', 20),20), 
   RIGHT(replicate('0',11) + ISNULL(CAST(cab.CUITCUIL AS VARCHAR(11)),''), 11),
   RIGHT(replicate('0',22) + ISNULL(CAST(cab.CBU AS VARCHAR(22)),''), 22),
   case when cab.saldoini>=0 then '+' else '-' end,
   RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(ABS(cab.saldoini), '0.00')),'.', ''), ''), 15),
   RIGHT(REPLICATE('0', 3) + ISNULL(CONVERT(VARCHAR(3), 
												(SELECT COUNT(1) 
												 FROM VW_CLI_PERSONAS vw (NOLOCK)
												 WHERE cab.CODIGOCLIENTE = vw.[Codigo de Cliente])
												),''), 3),
   LEFT(ISNULL(RTRIM(cli.[Nombre de Persona]),'') + REPLICATE(' ', 30), 30),
   RIGHT(REPLICATE('0', 11) + ISNULL( cli.[Numero de Documento],''), 11),
   LEFT(ISNULL((SELECT RTRIM([Nombre de Persona])
								   FROM (
											SELECT [Numero de Documento], [Nombre de Persona],
       										ROW_NUMBER() OVER (ORDER BY [Numero de Persona]) AS RowNum
											FROM VW_CLI_PERSONAS (NOLOCK)
											WHERE cab.CODIGOCLIENTE = [Codigo de Cliente]
  											AND [Titularidad] = 'C'
									) AS RankedData
									WHERE RowNum = 1),'') + REPLICATE(' ', 30), 30),
   RIGHT(REPLICATE('0', 11) + ISNULL((SELECT [Numero de Documento] 
							   FROM (
							   		SELECT [Numero de Documento],
       						   		ROW_NUMBER() OVER (ORDER BY [Numero de Persona]) AS RowNum
									FROM VW_CLI_PERSONAS (NOLOCK)
									WHERE cab.CODIGOCLIENTE = [Codigo de Cliente]
  									AND [Titularidad] = 'C'
									) AS RankedData
							 	WHERE RowNum = 1),''), 11),
   LEFT(ISNULL((SELECT RTRIM([Nombre de Persona])
								   FROM (
											SELECT [Numero de Documento], [Nombre de Persona],
       										ROW_NUMBER() OVER (ORDER BY [Numero de Persona]) AS RowNum
											FROM VW_CLI_PERSONAS (NOLOCK)
											WHERE cab.CODIGOCLIENTE = [Codigo de Cliente]
  											AND [Titularidad] = 'C'
									) AS RankedData
									WHERE RowNum = 2),'') + REPLICATE(' ', 30), 30),
   RIGHT(REPLICATE('0', 11) + ISNULL((SELECT [Numero de Documento] 
							   FROM (
							   		SELECT [Numero de Documento],
       						   		ROW_NUMBER() OVER (ORDER BY [Numero de Persona]) AS RowNum
									FROM VW_CLI_PERSONAS (NOLOCK)
									WHERE cab.CODIGOCLIENTE = [Codigo de Cliente]
  									AND [Titularidad] = 'C'
									) AS RankedData
							 	WHERE RowNum = 2),''), 11)
    ) AS Resultado,
    cab.jtsoid,
    1 AS indicador,
	null as "fechaReal",
	null as "fechaMov",
	null as "horaMov",
	null as "idDet"
FROM GRL_CAB_ENVIO_ESTCTA cab (NOLOCK)
JOIN GRL_ESTADOS_DE_CUENTA est ON CAB.JTSOID=est.JTSOID
join saldos sal on sal.jts_oid=est.JTSOID
INNER JOIN GRL_DET_ENVIO_ESTCTA det (NOLOCK) ON cab.jtsoid = det.jtsoid AND cab.PERIODO = det.periodo AND cab.idcab = det.idcab
INNER JOIN VW_CLI_PERSONAS cli (NOLOCK) ON cab.CODIGOCLIENTE= cli.[CODIGO de cliente] AND cli.titularIDAD = 'T'
INNER JOIN monedas mon (NOLOCK) ON cab.MONEDA=mon.c6400
WHERE cab.TIPOPRODUCTO = 2
AND est.CORREO_TRADICIONAL = 'S'
AND det.TIPOMOV = 'I'
and cab.periodo=@periodo
and sal.tz_lock=0
and sal.C1651 != 1
and sal.C1728 &lt;&gt; 'I'
AND NOT EXISTS(SELECT 1 FROM GRL_BLOQUEOS BLQ (NOLOCK) WHERE BLQ.SALDO_JTS_OID = SAL.JTS_OID AND BLQ.COD_BLOQUEO = 24)
AND EXISTS(
	SELECT 1 
	FROM SUSCRIPCIONES_RESUMEN SR (NOLOCK)
	WHERE 
		SR.JTS_OID_SALDO = sal.JTS_OID AND
		SR.ESTADO = 'S' AND 
		SR.TZ_LOCK LIKE '[04]%'
	)
 
UNION ALL
-- Segunda consulta (Detalles)
SELECT 
    concat('2',
        CONVERT(VARCHAR(8), det.FECHA_REAL_MOV, 112),
   LEFT(ISNULL(det.DESCTRANSACCION,'') + REPLICATE(' ', 30),30),
   RIGHT(replicate('0',10) + ISNULL(CAST(det.ASIENTO AS VARCHAR(10)),''), 10),
   (CASE WHEN det.IMPORTEDEBITO =0 AND det.IMPORTECREDITO &lt;&gt;0
   		THEN 'C'
   		ELSE 'D' END ),
   (CASE WHEN det.IMPORTEDEBITO =0 AND det.IMPORTECREDITO &lt;&gt;0
 
   		THEN RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(det.IMPORTECREDITO, '0.00')),'.', ''), ''), 15)
   		ELSE RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(det.IMPORTEdebito, '0.00')),'.', ''), ''), 15) END ),
   	case when (ISNULL(det.saldocalclinea, 0)) >=0 then '+' else '-' END, --'+',
   	RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(abs(det.saldocalclinea), '0.00')),'.', ''), ''), 15),
   	LEFT(ISNULL(Det.infoextendida,'') + replicate(' ',100), 100),
   	replicate(' ',259) 
    ) AS Resultado,
    cab.jtsoid,
    2 AS indicador,
	det.FECHA_REAL_MOV as "fechaReal",
	det.FECHAMOV as "fechaMov",
	det.HORA_MOV as "horaMov",
	det.IDDET as "idDet"
FROM GRL_CAB_ENVIO_ESTCTA cab (NOLOCK)
JOIN GRL_ESTADOS_DE_CUENTA est ON CAB.JTSOID=est.JTSOID
join saldos sal on sal.jts_oid=est.JTSOID
INNER JOIN GRL_DET_ENVIO_ESTCTA det (NOLOCK) ON cab.jtsoid = det.jtsoid AND cab.PERIODO = det.periodo AND cab.idcab = det.idcab
INNER JOIN VW_CLI_PERSONAS CLI (NOLOCK) ON cab.CODIGOCLIENTE=CLI.[CODIGO de cliente] AND cli.titularIDAD = 'T'
INNER JOIN monedas mon (NOLOCK) ON cab.MONEDA=mon.c6400

WHERE cab.TIPOPRODUCTO = 2
AND est.CORREO_TRADICIONAL = 'S'
AND DET.asiento &lt;&gt; 0
AND det.TIPOMOV = 'M'
and cab.periodo=@periodo
and sal.tz_lock=0
and sal.C1651 != 1
and sal.C1728 &lt;&gt; 'I'
AND NOT EXISTS(SELECT 1 FROM GRL_BLOQUEOS BLQ (NOLOCK) WHERE BLQ.SALDO_JTS_OID = SAL.JTS_OID AND BLQ.COD_BLOQUEO = 24)
AND EXISTS(
	SELECT 1 
	FROM SUSCRIPCIONES_RESUMEN SR (NOLOCK)
	WHERE 
		SR.JTS_OID_SALDO = sal.JTS_OID AND
		SR.ESTADO = 'S' AND 
		SR.TZ_LOCK LIKE '[04]%'
	)
 
UNION ALL
-- Tercera consulta (Pie)
SELECT
    concat('3',
       CONVERT(VARCHAR(8), cab.fechahasta, 112),
   case when (ISNULL(det.SALDOCALCLINEA, 0)) >=0 then '+' else '-' end,
   RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(( ABS(ISNULL(det.SALDOCALCLINEA, 0)) ), '0.00')), '.', ''),''), 15),
   RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(( ISNULL(C.MONTO_CONVERTIDO_MN, 0) ), '0.00')), '.', ''),''), 15),
   RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(( ISNULL(C.CREDEB_COMP, 0) ), '0.00')), '.', ''),''), 15),
   RIGHT(replicate('0', 15) + ISNULL(REPLACE(CONVERT(VARCHAR(15), FORMAT(( ISNULL(det.sircreb, 0) ), '0.00')), '.', ''),''), 15),
   replicate(' ',370)
    ) AS Resultado,
    cab.jtsoid,
    3 AS indicador,
	null as "fechaReal",
	null as "fechaMov",
	null as "horaMov",
	null as "idDet"
FROM GRL_CAB_ENVIO_ESTCTA cab (NOLOCK)
JOIN GRL_ESTADOS_DE_CUENTA est ON CAB.JTSOID=est.JTSOID
join saldos sal on sal.jts_oid=est.JTSOID
INNER JOIN GRL_DET_ENVIO_ESTCTA det (NOLOCK) ON cab.jtsoid = det.jtsoid  AND cab.PERIODO = det.periodo AND cab.idcab = det.idcab
INNER JOIN VW_CLI_PERSONAS CLI (NOLOCK) ON cab.CODIGOCLIENTE=CLI.[CODIGO de cliente] AND cli.titularIDAD = 'T'
INNER JOIN monedas mon (NOLOCK) ON cab.MONEDA=mon.c6400
LEFT JOIN COBROS_CREDEB_AGRUPADO AS C ON
	C.JTSOID = CAB.JTSOID
WHERE CAB.TIPOPRODUCTO = 2
AND EST.CORREO_TRADICIONAL = 'S'
AND DET.TIPOMOV = 'F'
AND CAB.PERIODO=@PERIODO
AND SAL.TZ_LOCK=0
