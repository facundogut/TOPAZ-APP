        <jsScript_type>0</jsScript_type>
        <jsScript_name>Transforma</jsScript_name>
        <jsScript_script>trans_Status = CONTINUE_TRANSFORMATION;

// Si la persona no esta en TOPAZ se la ignora
if (NUMEROPERSONAFJ == null)
	trans_Status = SKIP_TRANSFORMATION;

// Mappea valores de Impuesto a las Ganancias.
IMP_GANANCIAS = mapIGA(String(IMP_GANANCIAS), String(SECTOR))

