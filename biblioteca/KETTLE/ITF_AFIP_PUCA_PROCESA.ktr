      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Transforma</jsScript_name>
        <jsScript_script>var errores = [];

IMP_GANANCIAS = String(IMP_GANANCIAS);
IMP_IVA = String(IMP_IVA);
SECTOR = String(SECTOR);

// Valida que el CUIT sea numérico.
if ( !esNumerico(CUIT) ) {
	errores.push('CUIT no numérico. Recibido: ' + CUIT);
}


// Valida valores posibles de IGA
if ( !['AC','EX','NC','NI','  '].includes(IMP_GANANCIAS) ) {

	// Exento Público
	if ( ['02', '03'].includes(SECTOR) ) {
		errores.push('Campo IGA inválido para exento público. Recibido: ' + IMP_GANANCIAS);
		IMP_GANANCIAS = 'EX';
	} else {
		errores.push('Campo IGA inválido. Recibido: ' + IMP_GANANCIAS);
		IMP_GANANCIAS = 'NI'
	}

}

// Valida valores posibles de IVA
if ( !['AC','S ','NI','  ','EX','XN','NA','AN'].includes( IMP_IVA ) ){

	// Exento Publico
	if ( SECTOR == '02' ) {
		errores.push('Campo IVA inválido para exento público. Recibido: ' + IMP_IVA);
		IMP_IVA = 'EP'
	}
	// Persona Fisica 
	else if ( TIPOPERSONA == 'F' ) {
		errores.push('Campo IVA inválido para persona física. Recibido: ' + IMP_IVA); 
		IMP_IVA = 'NI'
	} 
	// Persona Juridica
	else if ( TIPOPERSONA == 'J' ) {
		errores.push('Campo IVA inválido para persona juridica. Recibido: ' + IMP_IVA);
		IMP_IVA = 'EX'
	// Si la persona no se encuentra en TOPAZ, asume persona física o jurídica por primer digito nro CUIT
	} else if ( esNumerico(CUIT) &amp;&amp; String(CUIT).substring(0,1) == '2' ) {
		errores.push('Campo IVA inválido para persona física. Recibido: ' + IMP_IVA); 
		IMP_IVA = 'NI'
	} else if ( esNumerico(CUIT) &amp;&amp; String(CUIT).substring(0,1) == '3' ) {
		errores.push('Campo IVA inválido para persona juridica. Recibido: ' + IMP_IVA);
		IMP_IVA = 'EX'
	} else {
		errores.push('Campo IVA inválido para tipo de persona desconocida. Se asumirá valor "NI" en la tabla. Recibido: ' + IMP_IVA);
		IMP_IVA = 'NI';
	}
}

// Une todos los errores detectados en un string.
var ERROR = '' + errores.join('; ');</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>//Script here

Array.prototype.includes = function(value) {
	return this.some( function(e) { return e == value } );
}

function esNumerico(numero) {
	return /[ ]*\d+[ ]*/.test(numero)
}</jsScript_script>
      </jsScript>
