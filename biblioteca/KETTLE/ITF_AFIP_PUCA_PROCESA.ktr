  <step>
    <name>Personas Fisicas y Juridicas en TOPAZ</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <sql>WITH CLIENTES AS (
	SELECT 
		CAST(d.NUMERODOCUMENTO AS VARCHAR(11)) AS NUMERODOCUMENTO,
		NUMEROPERSONAFJ,
		TIPOPERSONA,
		(SELECT SECTOR_CONTABLE FROM CLI_SECTORES WHERE SECTOR=PJ.SECTOR AND TZ_LOCK=0) AS SECTOR,
		ROW_NUMBER() OVER (PARTITION BY d.NUMERODOCUMENTO ORDER BY d.NUMEROPERSONAFJ DESC) AS RN,
		COALESCE(pf.ESTADO, pj.ESTADO) AS "ESTADO"
	FROM CLI_DocumentosPFPJ d WITH (NOLOCK)
	LEFT JOIN CLI_PERSONASFISICAS pf WITH (NOLOCK) ON d.NUMEROPERSONAFJ = pf.NUMEROPERSONAFISICA AND d.TIPOPERSONA = 'F'
	LEFT JOIN CLI_PERSONASJURIDICAS pj WITH (NOLOCK)ON d.NUMEROPERSONAFJ = pj.NUMEROPERSONAJURIDICA AND d.TIPOPERSONA = 'J'
	WHERE 
		d.TZ_LOCK LIKE '[04]%' AND
		(pf.TZ_LOCK LIKE '[04]%' OR pj.TZ_LOCK LIKE '[04]%')
)
SELECT 
	NUMERODOCUMENTO,
	NUMEROPERSONAFJ,
	TIPOPERSONA,
	SECTOR
FROM CLIENTES
WHERE RN = 1
ORDER BY NUMERODOCUMENTO ASC, (CASE WHEN ESTADO &lt;&gt; 1 THEN 0 ELSE 1 END) ASC</sql>
