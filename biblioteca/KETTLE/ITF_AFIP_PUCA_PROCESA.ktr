      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Inicio</jsScript_name>
        <jsScript_script>// Constante para usar como ID_TICKET
var CERO = 0;

// Constante de nombre de usuario
var USUARIO_MOD = getVariable('USUARIO_MOD', '');

Array.prototype.includes = function (value) {
	return this.some( function(e) { return e == value } );
}

// Funciones de mappeo
function mapIGA(valorArchivo, sector) {

	if ( ['02','03'].includes(sector) )
		return 'EX';
	else if ( ['AC'].includes(valorArchivo) )
		return 'AC';
	else if ( ['EX', 'NC'].includes(valorArchivo) )
		return 'EX';
	else if ( ['NI', '  '].includes(valorArchivo) )
		return 'NI';
	else
		return null;
}

function mapIVA(valorArchivo, tipoPersona, sector) {

	// Casos independientes de precondiciones
	switch(valorArchivo) {
		case 'AC':
		case 'S':
			return 'AC';
		case 'NI':
		case '  ':
			return 'NI';
	}
	
	// Si es exento público en TOPAZ.
	if (sector == '02' ) {
		return 'EP';
	}
	// Si es Exento
	if (['EX','XN'].indexOf(valorArchivo) != -1 || tipoPersona == 'J') {
		return 'EX';
	}
	
	// No alcanzados
	// PERSONA FISICA
	if (tipoPersona == 'F') {
		return 'NI';
	}	
}</jsScript_script>
