    <rowlimit>0</rowlimit>
    <sql>SELECT
	P.C6251 AS DESC_PRODUCTO,
	M.C6400 AS MONEDA_CORREO,
	S.C1620 AS FECHA_APERTURA,
	CASE WHEN S.CUENTA NOT IN (SELECT CUENTA FROM CHE_CHEQUERAS (nolock) WHERE TZ_LOCK = 0) THEN 'N'
	ELSE 'S'
	END TIENE_CHEQUERA,
	S.C1604 AS SALDO_ACT,
	CT.IMPORTE_APLICAR AS COSTO_COMISION
	
FROM
	PRODUCTOS P (nolock)
INNER JOIN SALDOS S (nolock) ON
	P.C6250 = S.PRODUCTO
INNER JOIN MONEDAS M (nolock) ON
	S.MONEDA = M.C6399,
	CI_CARGOS_TARIFAS CT (nolock)
	WHERE S.JTS_OID = ?
	AND CT.ID_CARGO =1231

</sql>
