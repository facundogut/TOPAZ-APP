    <quoteString>N</quoteString>
    <sql>declare 
@convenio1 INT = (SELECT numerico FROM PARAMETROSGENERALES WHERE CODIGO = 300), 
@convenio2 INT = (SELECT numerico FROM PARAMETROSGENERALES WHERE CODIGO = 301), 
@fecha VARCHAR(10) = '${P0_VALUE}';

SET @fecha = (SELECT CASE WHEN isdate(@fecha) = 0 THEN '18000101' ELSE @fecha END );

TRUNCATE TABLE ITF_RP_REPORTE;
INSERT INTO ITF_RP_REPORTE

SELECT DISTINCT
	isnull(conv.nomconvrec, 'No se encontro nombre de empresa.') AS empresa,
	convert(VARCHAR(10), p.fechaproceso, 120) as fechaProceso,
	isnull(conv.nomconvrec, 'No se encontro nombre de convenio.')  AS nombreConvenio,
	concat('MQ',replace(convert(VARCHAR(10), CAST(@fecha AS DATE), 3), '/',''), '.txt') AS nombreArchivo, 
   	convert(VARCHAR(10), CAST(ren.FECHA AS DATE), 120) AS fechaRendicion,
   	(SELECT cuenta FROM SALDOS (nolock) WHERE JTS_OID = 
			(SELECT cuentarec FROM CONV_CONVENIOS_REC (nolock) WHERE id_convrec = @convenio1)) AS cuentaRecaudacion,
	--------------------------
	isnull(convert(VARCHAR(10), cab.fecha_cobro_pago, 120), '---')  AS fechaRecaudacion,
	isnull(ren.TOTALIMPORTE,0) AS totalRecaudado,
	isnull(ren.TOTALREGISTROS, 0) AS cantComprobantes,
	isnull(ren.IMPORTE_COMISION, 0) AS impComision,
	isnull(ren.IMPORTE_IMPUESTOS, 0) AS totalImpuestos,
	isnull((CASE WHEN relEnte.COD_ENTE IN ('406','265') THEN ren.TOTAL_CARGO_ESPECIFICO ELSE 0 END),0) AS cargoEspecifico,
		
	isnull((ren.TOTALIMPORTE - ren.IMPORTE_COMISION - ren.IMPORTE_IMPUESTOS - isnull((CASE WHEN relEnte.COD_ENTE IN ('406','265') THEN ren.TOTAL_CARGO_ESPECIFICO ELSE 0 END),0)),0) AS importeRendido
				   
			FROM PARAMETROS p, REC_RENDICION (NOLOCK) ren
			JOIN CONV_CONVENIOS_REC  (NOLOCK) conv ON ren.CONVENIO = conv.Id_ConvRec 
			JOIN MOVIMIENTOS_CONTABLES (NOLOCK) mc ON ren.asiento_rendicion = mc.asiento AND mc.operacion = 2651 
			JOIN REC_AGENCIEROS (NOLOCK) cab ON cab.id_rendicion = ren.id_rendicion
			LEFT JOIN CONV_REL_ENTECONV (NOLOCK) relEnte  ON ren.convenio_padre = relente.id_convrec
     	    
		    WHERE 
		    	(ren.CONVENIO IN (@convenio1,@convenio2 ) OR ren.CONVENIO_PADRE IN (@convenio1,@convenio2 ) ) 
		  		AND ren.FECHA = CAST(@fecha AS DATE) 		  				  
		     	AND conv.Estado = 'A'
		     	AND cab.TZ_LOCK = 0 
		        AND ren.TZ_LOCK = 0 
		        AND conv.TZ_LOCK = 0 
                </sql>
