    <quoteString>N</quoteString>
    <sql>DECLARE @DIASAT NUMERIC(15)
SET @DIASAT = (SELECT NUMERICO FROM PARAMETROSGENERALES WITH(NOLOCK) WHERE CODIGO = 254)

INSERT INTO DEUDORES_4305
SELECT DISTINCT
-----4305
4305 AS CODIGO_DISENIO,
11 AS TIPO_ID,
MC.CUIT_CUIL_CDI AS NRO_ID,
LEFT(MC.Denominacion,55) AS DENOMINACION,
MC.Categoria_Cliente AS CATEGORIA,
CASE WHEN CLI.CODIGORESIDENCIA = 3 THEN 5 
WHEN CPF.SECTOR = 1 THEN 1 
WHEN CPF.SECTOR IN (3,5) THEN 3
WHEN CPF.SECTOR IN (6,7) THEN 2
WHEN CPF.SECTOR = 4 THEN 4 
END AS RES_SECTOR,
CASE WHEN CLI.CODIGORESIDENCIA = 3 THEN 0 
WHEN CPF.SECTOR = 3 THEN 2
WHEN CPF.SECTOR = 4 THEN 1
WHEN CPF.SECTOR = 5 THEN 3
ELSE 0 END AS GOBIERNO,
CASE WHEN CLI.CODIGORESIDENCIA = 3 THEN 00
WHEN CPF.SECTOR IN ('3', '5') THEN CONVERT(NUMERIC,P.ID_BCRA) ELSE 00 END AS PROVINCIA,
CASE WHEN LEN(MC.Clasificacion) = 0 OR MC.Clasificacion IS NULL THEN '0' 
WHEN MC.DEUDA_TOTAL &lt;&gt; 0 AND MC.DEUDA_TOTAL = MC.Deuda_GtiasPrefA THEN 11
ELSE
(SELECT TOP 1 RPI.NUM1 
     FROM RRI_PARAMETROS_DEF RPD WITH(NOLOCK)
     JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON RPI.CODIGO=RPD.CODIGO
     WHERE RPD.CODIGO=205 AND (RPI.ID4=MC.Clasificacion OR CONVERT(VARCHAR,CONVERT(NUMERIC(15),RPI.ID1))=(CASE WHEN LEN(MC.Clasificacion) = 0 THEN '0' ELSE ISNULL(MC.Clasificacion,'0') END)))
END AS SITUACION,
CASE 
	WHEN MC.Cliente_Vinc='N' THEN 0
	ELSE 1
	END AS VINCULACION,
ROUND(ISNULL(NAT.PREVISIONES,0)/1000, 0) AS PREV_ASIST_CRED,
0 AS SIN_USO,
---MC.PREVISION 
0 AS PROV_RESP_EVENT,
0 AS INCR_PREV_GRAL,
0 AS INCR_PREV_MIN,
ROUND(MC.Max_Asistencia/1000,0) AS MAX_ASISTENCIA,
ROUND(MC.DEUDA_TOTAL/1000,0) AS TOT_FINANCIACIONES,
CASE WHEN MC.Categoria_Cliente = 3 THEN 000 
WHEN (SELECT TOP 1 RPI.NUM1 
FROM RRI_PARAMETROS_DEF RPD WITH(NOLOCK)
JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON RPI.CODIGO=RPD.CODIGO
WHERE RPD.CODIGO=205 AND (RPI.ID4=MC.Clasificacion OR CONVERT(VARCHAR,CONVERT(NUMERIC(15),RPI.ID1))=(CASE WHEN LEN(MC.Clasificacion) = 0 THEN '0' ELSE ISNULL(MC.Clasificacion,'0') END))) = 11
THEN 000
ELSE MC.Cod_Activ_BCRA END AS ACT_PRINCIPAL,
0 AS INFO_TRANSITORIA,
CASE 
    WHEN MC.Deudor_Encuadrado_Ley25326='N' THEN 0
    ELSE 1
    END AS DEUDOR_ART_26,
CASE 
    WHEN MC.Clte_Refinanciado = 'N' THEN 0
    WHEN MC.Clte_Refinanciado = 'S' THEN 1
    WHEN MC.Clte_Refinanciado = '' THEN 9
    ELSE ''
END AS REFINANCIACIONES,
CASE 
	WHEN MC.Origen_Situacion='ATRASO' OR MC.Categoria_Cliente= 1 THEN 0 
	ELSE 1
	END AS RECAT_OBLIGATORIA,
CASE 
	----WHEN MC.Max_Atraso > @DIASAT THEN 1
	WHEN MC.Sit_Juridica='S' THEN 1
	WHEN MC.Sit_Juridica_Ext='S' THEN 1
	ELSE 0
	END AS SIT_JURIDICA,
0 AS IRREC_DISP_TEC,
CASE 
WHEN (SELECT TOP 1 RPI.NUM1 
FROM RRI_PARAMETROS_DEF RPD WITH(NOLOCK)
JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON RPI.CODIGO=RPD.CODIGO
WHERE RPD.CODIGO=205 AND (RPI.ID4=MC.Clasificacion OR CONVERT(VARCHAR,CONVERT(NUMERIC(15),RPI.ID1))=(CASE WHEN LEN(MC.Clasificacion) = 0 THEN '0' ELSE ISNULL(MC.Clasificacion,'0') END)))=1 THEN 0
WHEN (MC.Clte_Refinanciado = 'N' OR MC.Clte_Refinanciado IS NULL) AND (MC.Origen_Situacion='ATRASO' OR MC.Categoria_Cliente= 1) AND (MC.Sit_Juridica&lt;&gt;'S' OR MC.Sit_Juridica IS NULL
OR MC.Sit_Juridica_Ext&lt;&gt;'S' OR MC.Sit_Juridica_Ext IS NULL) THEN 0
WHEN LEN(MC.Max_Atraso) > 4 THEN 9999 ELSE MC.Max_Atraso END AS DIAS_ATRASO,
CONCAT(CASE 
WHEN sector_activ NOT IN (1,5,3,6,4) THEN 0
WHEN tamano_emp = 'NO MIPYMES' THEN 0
WHEN tamano_emp = 'MICRO' THEN 4
WHEN tamano_emp = 'PEQUEÑA' THEN 5
WHEN tamano_emp = 'MEDIANA TRAMO 1' THEN 6
WHEN tamano_emp = 'MEDIANA TRAMO 2' THEN 7
ELSE 0
END,
CASE 
WHEN tamano_emp NOT IN ('MICRO', 'PEQUEÑA', 'MEDIANA TRAMO 1', 'MEDIANA TRAMO 2') THEN 0
WHEN sector_activ = 0 THEN 0
WHEN sector_activ = 1 THEN 1  -- AGROPECUARIO
WHEN sector_activ = 5 THEN 2  -- INDUSTRIA Y MINERIA
WHEN sector_activ = 3 THEN 3  -- COMERCIO
WHEN sector_activ = 6 THEN 4 -- SERVICIOS
WHEN sector_activ = 4 THEN 5  -- CONSTRUCCION
ELSE 0
END) AS MIPYME,
CASE WHEN MC.Origen_Situacion='ATRASO' OR MC.Categoria_Cliente= 1 THEN 0 
WHEN MC.Origen_Situacion='DISCREPANCIA' THEN
ISNULL((SELECT TOP 1 RPI.NUM1 
     FROM RRI_PARAMETROS_DEF RPD WITH(NOLOCK)
     JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON RPI.CODIGO=RPD.CODIGO
     WHERE RPD.CODIGO=205 AND (RPI.ID4=MC.Sit_Prod_Banco OR CONVERT(VARCHAR,CONVERT(NUMERIC(15),RPI.ID1))=(CASE WHEN LEN(MC.Sit_Prod_Banco) = 0 THEN '0' ELSE ISNULL(MC.Sit_Prod_Banco,'0') END))),0) 
ELSE 0
END AS SIT_SIN_RECLASIF,
CONVERT(DATETIME,?,103) AS FECHA_PROCESO,
00
FROM MEMO_CABECERA MC WITH(NOLOCK)
INNER JOIN (SELECT MIN(TZ_LOCK) AS TZ_LOCK, NUMERODOCUMENTO FROM CLI_DocumentosPFPJ WITH(NOLOCK) GROUP BY NUMERODOCUMENTO) DOCT ON
DOCT.NUMERODOCUMENTO  = MC.CUIT_CUIL_CDI
INNER JOIN CLI_DocumentosPFPJ DOC WITH(NOLOCK) ON DOCT.NUMERODOCUMENTO = DOC.NUMERODOCUMENTO AND DOCT.TZ_LOCK = DOC.TZ_LOCK
INNER JOIN CLI_PERSONASFISICAS CPF WITH(NOLOCK) ON DOC.NUMEROPERSONAFJ = CPF.NUMEROPERSONAFISICA

INNER JOIN (SELECT MIN(TZ_LOCK) AS TZ_LOCK, NUMEROPERSONA, TITULARIDAD FROM CLI_ClientePersona P WITH(NOLOCK)
WHERE P.TITULARIDAD = (CASE WHEN (SELECT COUNT(1) FROM CLI_ClientePersona WITH(NOLOCK) WHERE NUMEROPERSONA = P.NUMEROPERSONA AND TITULARIDAD='T') > 0
THEN 'T' ELSE 'C' END) GROUP BY NUMEROPERSONA, TITULARIDAD) CPT ON CPT.NUMEROPERSONA = CPF.NUMEROPERSONAFISICA
INNER JOIN CLI_ClientePersona CP WITH(NOLOCK) ON CP.NUMEROPERSONA = CPF.NUMEROPERSONAFISICA
AND CP.TITULARIDAD = CPT.TITULARIDAD AND CP.TZ_LOCK = CPT.TZ_LOCK

INNER JOIN CLI_CLIENTES CLI WITH(NOLOCK) ON CLI.CODIGOCLIENTE = (CASE WHEN MC.CLIENTE = 0 OR MC.CLIENTE IS NULL THEN CP.CODIGOCLIENTE ELSE MC.CLIENTE END) AND CLI.TZ_LOCK = 0
INNER JOIN CLI_DIRECCIONES DIR WITH(NOLOCK) on dir.ID = CPF.NUMEROPERSONAFISICA 
AND dir.ORDINAL_DIR = (SELECT MIN(ORDINAL_DIR) FROM CLI_DIRECCIONES WITH(NOLOCK) WHERE ID = CP.NUMEROPERSONA AND TZ_LOCK = 0 AND TIPODIRECCION = 'PR')
AND dir.TIPODIRECCION = 'PR'
INNER JOIN CLI_PROVINCIAS P WITH(NOLOCK) ON P.CODIGOPAIS = DIR.PAIS AND P.DIM1 = DIR.PROVINCIA
LEFT JOIN (SELECT ope_upl_gdncuenta, OPE_UPL_Periodo, SUM(CONVERT(NUMERIC, CASE WHEN ope_peva_saldo = '.00' THEN '0' ELSE REPLACE(ISNULL(ope_peva_saldo,'0'),'.00','') END)) AS PREVISIONES FROM 
SALIDA_NAT WITH(NOLOCK) GROUP BY ope_upl_gdncuenta, OPE_UPL_Periodo) NAT ON NAT.ope_upl_gdncuenta = CONVERT(VARCHAR,MC.CUIT_CUIL_CDI) AND CONVERT(NUMERIC,SUBSTRING(NAT.OPE_UPL_Periodo, 1, 4)) = YEAR(MC.Fecha_Info) AND 
CONVERT(NUMERIC,SUBSTRING(NAT.OPE_UPL_Periodo, 5, 2)) = MONTH(MC.Fecha_Info)
WHERE MC.Fecha_Info= CONVERT(DATETIME,?,103) AND MC.TIPO_PERSONA = 'F'
AND MC.TZ_LOCK=0 AND CPF.TZ_LOCK = 0 AND DIR.TZ_LOCK = 0 AND P.TZ_LOCK = 0
AND (MC.DEUDA_TOTAL >= (SELECT IMPORTE FROM PARAMETROSGENERALES WITH(NOLOCK) WHERE CODIGO=784) OR 
(MC.DEUDA_TOTAL &lt; (SELECT IMPORTE FROM PARAMETROSGENERALES WITH(NOLOCK) WHERE CODIGO=784)) AND ((MC.Max_Asistencia > (SELECT IMPORTE FROM CRE_PARAMETROS WITH(NOLOCK) WHERE CODIGO = 609)*0.005) OR
MC.Max_Asistencia > (SELECT IMPORTE FROM CRE_PARAMETROS WITH(NOLOCK) WHERE CODIGO = 501)*0.4))</sql>
