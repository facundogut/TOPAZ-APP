    <connection>NBCH</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT COUNT(1) as HayRegistros 
FROM SALDOS S
JOIN PLANPAGOS PP ON S.JTS_OID = PP.SALDO_JTS_OID AND PP.TZ_LOCK = 0
JOIN TOPESPRODUCTO TP ON S.PRODUCTO = TP.CODPRODUCTO AND TP.TZ_LOCK = 0
JOIN CLI_ClientePersona CP ON CP.CODIGOCLIENTE = S.C1803 AND CP.TITULARIDAD = 'T' And CP.TZ_LOCK = 0
JOIN CLI_DocumentosPFPJ DFJ ON DFJ.NUMEROPERSONAFJ = CP.NUMEROPERSONA AND DFJ.TZ_LOCK = 0
JOIN CRE_SALDOS CS ON CS.SALDOS_JTS_OID = S.JTS_OID AND CS.TZ_LOCK = 0
JOIN CLI_CLIENTES CLI ON CLI.CODIGOCLIENTE = S.C1803 AND CLI.TZ_LOCK = 0
WHERE S.C1785 = 5 
AND S.TZ_LOCK = 0
AND (TP.CODIGO_ECOM = CONCAT('0',?) OR TP.CODIGO_ECOM = ?)
AND TP.INFORMA_ECOM = 'S'
AND CS.JURISDICCION &lt;&gt; 0 
AND CS.JURISDICCION IS NOT NULL
AND CS.Jurisdiccion &lt;&gt; '16'
AND S.C1621 = (SELECT fechaproceso FROM PARAMETROS)
AND (PP.C2309 + PP.C2310 + PP.IVAINTERES) > 0
AND ISNULL(CLI.CATEGORIARESULTANTE,'0') &lt; 6</sql>
