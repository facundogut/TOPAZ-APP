    <sql>DECLARE @codEnte VARCHAR(12) = '${P0_VALUE}';
DECLARE @fdesde VARCHAR(12) = '${P1_VALUE}';
DECLARE @fhasta VARCHAR(12) = '${P2_VALUE}';

DECLARE @fproc DATETIME = (SELECT fechaproceso FROM PARAMETROS (nolock));


SELECT
CASE WHEN isdate(@fdesde) = 1 THEN 1 ELSE 0 END AS fdesdeOk,
CASE WHEN isdate(@fdesde) = 1 AND @fdesde &lt;= @fproc THEN 1 ELSE 0 END AS fdesdeMenorfproc,
CASE WHEN isdate(@fhasta) = 1 THEN 1 ELSE 0 END AS fhastaOk,
CASE WHEN isdate(@fhasta) = 1 AND isdate(@fdesde) = 1 AND @fhasta >=  @fdesde THEN 1 ELSE 0 END AS fhastaMayorfdesde,
CASE WHEN isdate(@fhasta) = 1 AND @fhasta &lt;= @fproc THEN 1 ELSE 0 END AS fhastaMenorfproc,
CASE WHEN @codEnte IN ('265','406') AND COUNT(1) > 0 THEN 1 ELSE 0 END enteOk 

	FROM CONV_REL_ENTECONV (nolock) WHERE COD_ENTE = @codEnte;
</sql>
