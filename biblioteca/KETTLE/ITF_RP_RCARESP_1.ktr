DECLARE @codEnte VARCHAR(12) = '${P0_VALUE}';
DECLARE @fechaDesde VARCHAR(12) = '${P1_VALUE}';
DECLARE @fechaHasta VARCHAR(12) = '${P2_VALUE}';

DECLARE @nomFijo VARCHAR(2) = (SELECT CASE WHEN @codEnte = '265' THEN 'FC' ELSE 'FN' END);
DECLARE @fproc DATETIME = (SELECT fechaproceso FROM PARAMETROS (nolock));


IF (isdate(@fechaDesde) = 0 or isdate(@fechaHasta) = 0) --si fecha desde o fecha hasta es invalida la seteo asi para q no de error al castear
BEGIN
	SET @fechaDesde = '18000101';
	SET @fechaHasta = '18000101';
END 

SELECT 
	CONCAT(
		'${Top.Output}CONVENIOS/RCARESP/',@nomFijo,
		replace(convert(VARCHAR(10),CAST(@fproc AS DATETIME),3), '/','')
	)  AS rutaSalida,
	count(1) as cant_rendidos
FROM 
	CONV_REL_ENTECONV (nolock) relEnte
		JOIN REC_RENDICION (nolock) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN CONV_CONVENIOS_REC (nolock) conv  ON relEnte.id_convrec = conv.id_convrec
			WHERE 
				relEnte.COD_ENTE = @codEnte AND 
				relEnte.tz_lock = 0 AND 
				ren.TZ_LOCK = 0 AND 				
				ren.ESTADO = 'R' AND 				
				ren.FECHA BETWEEN CAST(@fechaDesde AS DATETIME) AND CAST(@fechaHasta AS DATETIME) AND
				conv.tz_lock = 0 
			


</sql>
