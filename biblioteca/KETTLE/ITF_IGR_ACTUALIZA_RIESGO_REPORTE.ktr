  <step>
    <name>ITF_MATRIZ_IGR_HISTORICO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <sql>DECLARE
	@RCO VARCHAR(200) = '${RCO}',
	@NOMBREARCHIVO VARCHAR(200) = 'REPORTE_${FECHARELOJ_F1}${HORA_F1}',
	@REPORTE VARCHAR(400) = '';

SET @REPORTE = CONCAT(@RCO,@NOMBREARCHIVO,'.csv');

WITH PERSONAS AS (
	SELECT
		'F' AS "PERSONA_TIPO",
		PF.NUMEROPERSONAFISICA AS "PERSONA_NUMERO",
		CONCAT(PF.APELLIDOPATERNO, PF.APELLIDOMATERNO, PF.PRIMERNOMBRE, PF.SEGUNDONOMBRE) AS "DENOMINACION",
		PF.PAIS_TRIBUTARIO AS "PAIS_TRIBUTARIO"
	FROM dbo.CLI_PERSONASFISICAS PF WITH (NOLOCK)
	--
	UNION ALL
	--
	SELECT
		'J' AS "PERSONA_TIPO",
		PJ.NUMEROPERSONAJURIDICA AS "PERSONA_NUMERO",
		PJ.RAZONSOCIAL AS "DENOMINACION",
		PJ.PAIS_TRIBUTARIO AS "PAIS_TRIBUTARIO"
	FROM dbo.CLI_PERSONASJURIDICAS PJ WITH (NOLOCK)
)
SELECT 
	FORMAT(h.FECHA_INGRESO, 'yyyy-MM-dd') AS 'FECHA_INGRESO',
	p.PERSONA_NUMERO AS 'PERSONA_NUMERO',
	TRIM(p.DENOMINACION) AS 'PERSONA_NOMBRE',
	h.IGR_OBTENIDO AS 'IGR_OBTENIDO',
	h.RIESGO_OBTENIDO AS 'RIESGO_OBTENIDO',
	TRIM(h.NUMERO_IDENTIFICACION) AS 'NUMERO_IDENTIFICACION',
	TRIM(h.CODIGO_ACTIVIDAD) AS 'CODIGO_ACTIVIDAD',
	TRIM(h.CONDICION_PERSONA) AS 'CONDICION_PERSONA',
	@REPORTE AS 'REPORTE',
	@RCO AS 'RCO',
	@NOMBREARCHIVO AS 'NOMBREACHIVO'
FROM dbo.ITF_MATRIZ_IGR_HISTORICO h
INNER JOIN PERSONAS p 
	ON p.PERSONA_NUMERO = h.NUMERO_PERSONA
	AND h.ID_EJECUCION = '${ID_EJECUCION}'
	AND h.RIESGO_OBTENIDO = 5 --Riesgo Alto. Dur√≠simo, como la vida misma.
</sql>
