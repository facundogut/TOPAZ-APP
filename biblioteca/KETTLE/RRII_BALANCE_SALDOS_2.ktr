  <step>
    <name>Traer datos txt</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>topaz_conec</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE @FECHA_PARAMETRO DATETIME;
SET @FECHA_PARAMETRO = CONVERT(DATETIME,?,103);
DECLARE @RECTIFICATIVA VARCHAR;
SET @RECTIFICATIVA = ?;
WITH CUENTAS AS (
SELECT 
	1 AS CANT_CUENTAS,
	@RECTIFICATIVA AS RECTIFICATIVA,
	SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(6)),1,6) AS CUENTA,
    -1 * (SUM(dbo.FN_RRII_ConvertirAMonNac(CBR.SALDOFINMESMO,CBR.MONEDA,@FECHA_PARAMETRO)))/1000 AS IMPORTE,
    @FECHA_PARAMETRO AS FECHA,
    CBR.MONEDA AS MONEDA,
    CASE 
        WHEN SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(2)), 1, 2) = '71' THEN 'D'
        WHEN SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(2)), 1, 2) = '72' THEN 'A'
       	ELSE 'B'
    END AS TIPO
FROM CO_BALANCE_RESULTADOS CBR WITH (NOLOCK)
INNER JOIN MONEDAS M WITH (NOLOCK) 
    ON CBR.MONEDA = M.C6399 
    AND M.TZ_LOCK = 0
WHERE 
    CBR.ID_BCE = 2
    AND CBR.FECHA = CONVERT(DATETIME, @fecha_parametro, 103)
    AND CBR.TIPO_BALANCE = 'M'
	AND SUBSTRING(CBR.CODIGO_INTERNO,5,2) != '00'
	AND SUBSTRING(CBR.CODIGO_INTERNO,1,6) != '450009'
	AND CBR.SALDOFINMESMO!=0
GROUP BY 
	CBR.CODIGO_INTERNO, 
	CBR.MONEDA, 
	CBR.FECHA	
),
DIFERENCIAS AS (
SELECT 
    SUM(CASE WHEN TIPO = 'D' THEN IMPORTE ELSE 0 END) + 
    SUM(CASE WHEN TIPO = 'A' THEN IMPORTE ELSE 0 END) AS DIF_ORDEN,
    SUM(CASE WHEN TIPO = 'B' THEN IMPORTE ELSE 0 END) AS DIF_BALANCE,
    TIPO
FROM CUENTAS
GROUP BY TIPO
),
RUBROS_CUENTAS_DIF AS (
SELECT
	1 as CANT_CUENTAS,
		
	@RECTIFICATIVA as RECTIFICATIVA,
	
	CASE WHEN TIPO IN ('A','D') AND DIF_ORDEN!=0 THEN 
		(SELECT ID1 FROM RRI_PARAMETROS_INF WHERE CODIGO=866)
	WHEN TIPO='B' AND DIF_BALANCE&lt;0 THEN 
		(SELECT ID1  FROM RRI_PARAMETROS_INF WHERE CODIGO=867 AND ID4='P')
	ELSE
		(SELECT ID1 FROM RRI_PARAMETROS_INF WHERE CODIGO=867 AND ID4='G')
	END AS CUENTA, 
	
	-1* CASE WHEN TIPO='B' THEN DIF_BALANCE ELSE DIF_ORDEN END AS IMPORTE,
	
	@FECHA_PARAMETRO AS FECHA,
	
	1 AS MONEDA,
	
	'X' AS TIPO
 	
FROM DIFERENCIAS
)
SELECT
	CANT_CUENTAS,
	RECTIFICATIVA,
	CUENTA,
	SUM(IMPORTE) AS IMPORTE,
	FECHA,
	MONEDA,
	TIPO
FROM RUBROS_CUENTAS_DIF 
GROUP BY CANT_CUENTAS,RECTIFICATIVA,CUENTA,FECHA,MONEDA,TIPO
UNION
SELECT 
	SUM(CANT_CUENTAS) AS CANT_CUENTAS,
	RECTIFICATIVA,
	CUENTA,
	SUM(IMPORTE) AS IMPORTE,
	FECHA,
	MONEDA,
	TIPO
FROM CUENTAS GROUP BY RECTIFICATIVA,CUENTA,FECHA,MONEDA,TIPO</sql>
