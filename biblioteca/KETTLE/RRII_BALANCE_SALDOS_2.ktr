  <step>
    <name>Suma cuentas balance</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>topaz_conec</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE @fecha_parametro AS DATETIME;
SET @fecha_parametro=CONVERT(DATETIME,?,103);
WITH SUMA AS (
    SELECT 
        -1 * (SUM(CBR.SALDOFINMESMO)*H.TIPO_CAMBIO_OFICIAL)/1000 AS IMPORTE,
        CBR.MONEDA
    FROM CO_BALANCE_RESULTADOS CBR WITH (NOLOCK)
    INNER JOIN MONEDAS M WITH (NOLOCK) 
        ON CBR.MONEDA = M.C6399 
        AND M.TZ_LOCK = 0
	INNER JOIN HISTORICOTIPOSCAMBIO H WITH(NOLOCK) ON
		H.MONEDA=CBR.MONEDA AND FECHA_COTIZACION= (SELECT MAX(FECHA_COTIZACION) FROM HISTORICOTIPOSCAMBIO WHERE MONEDA=H.MONEDA AND FECHA_COTIZACION&lt;=@fecha_parametro AND TZ_LOCK=0) AND
		H.TZ_LOCK=0
    WHERE 
        CBR.ID_BCE = 2
        AND CBR.FECHA = CONVERT(DATETIME, @fecha_parametro, 103)
        AND CBR.TIPO_BALANCE = 'M'
        AND SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(2)), 1, 2) NOT IN ('71', '72') 
		AND SUBSTRING(CBR.CODIGO_INTERNO,5,2) != '00'
		AND SUBSTRING(CBR.CODIGO_INTERNO,1,6) != '450009'
    GROUP BY CBR.CODIGO_INTERNO, CBR.MONEDA, CBR.FECHA,H.TIPO_CAMBIO_OFICIAL
)
SELECT 
	CASE WHEN SUM(IMPORTE)&lt;0 THEN
		CAST(SUM(IMPORTE) AS INT)
	ELSE
		FLOOR(SUM(IMPORTE))
	END AS SUMA_TOTAL
FROM SUMA</sql>
