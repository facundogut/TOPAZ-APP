    <connection>topaz_conec</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE @fecha_parametro AS DATETIME;
SET @fecha_parametro=CONVERT(DATETIME,?,103);
WITH SUMA AS (
    SELECT 
        -1 * SUM(CBR.SALDOFINMESMO) AS IMPORTE,
        CBR.MONEDA,
        CASE 
            WHEN SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(2)), 1, 2) = '71' THEN 'DEUDORAS'
            WHEN SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(2)), 1, 2) = '72' THEN 'ACREEDORAS'
        END AS TIPO
    FROM CO_BALANCE_RESULTADOS CBR WITH (NOLOCK)
    INNER JOIN MONEDAS M WITH (NOLOCK) 
        ON CBR.MONEDA = M.C6399 
        AND M.TZ_LOCK = 0
    WHERE 
        CBR.ID_BCE = 2
        AND CBR.FECHA = CONVERT(DATETIME, @fecha_parametro, 103)
        AND CBR.TIPO_BALANCE = 'M'
        AND SUBSTRING(CAST(CBR.CODIGO_INTERNO AS VARCHAR(2)), 1, 2) IN ('71', '72') 
		AND SUBSTRING(CBR.CODIGO_INTERNO,5,2) != '00'
    GROUP BY CBR.CODIGO_INTERNO, CBR.MONEDA, CBR.FECHA
)
SELECT 
    SUM(CASE WHEN TIPO = 'DEUDORAS' THEN IMPORTE ELSE 0 END) + 
    SUM(CASE WHEN TIPO = 'ACREEDORAS' THEN IMPORTE ELSE 0 END) AS SUMA
FROM SUMA</sql>
