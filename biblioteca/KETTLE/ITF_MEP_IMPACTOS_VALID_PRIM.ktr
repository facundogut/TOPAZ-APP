else {
	
	// Parsear parámetros para validar
	CANTIDAD_REGISTROS = parseInt(CANTIDAD_REGISTROS, 10);
	IMPORTE_TOTAL = new java.math.BigDecimal( java.lang.String.valueOf(IMPORTE_TOTAL) );
	
