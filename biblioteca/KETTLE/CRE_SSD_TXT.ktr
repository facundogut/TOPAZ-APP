	CONVERT(VARCHAR,DEUDA_TOTAL) + ',' +
	CONVERT(VARCHAR,DEUDA_CON_TOPE) + ',' +
	CONVERT(VARCHAR,DEUDA_REMANENTE) + ',' +
	CASE WHEN REQ_ADICIONALES = 'NR' THEN 'No Requerido' WHEN REQ_ADICIONALES = 'R' THEN 'Requerido' WHEN REQ_ADICIONALES = 'RC' THEN 'Requerido Cumplido' ELSE '' END AS LINEA
FROM CRE_SEGURO_DEUDAS_POR_CLIENTE WITH(NOLOCK)
