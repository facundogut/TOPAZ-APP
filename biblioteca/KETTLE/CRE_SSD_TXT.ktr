	CONVERT(VARCHAR,DNI) + ',' +
	CONVERT(VARCHAR,CUIL) + ',' +
	APELLIDO_NOMBRE + ',' +
	SEXO + ',' +
	CONVERT(VARCHAR(8), FECHA_NACIMIENTO, 112) + ',' +
	CASE WHEN INCLUIR_EXCLUIR = 'I' THEN 'Incluir' WHEN INCLUIR_EXCLUIR = 'E' THEN 'Excluir' ELSE '' END + ',' +
