    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE 
@fechaStr VARCHAR(8) = convert(VARCHAR(8),(SELECT fechaproceso FROM PARAMETROS (nolock)),112),
@fecha DATETIME = (SELECT fechaproceso FROM PARAMETROS (nolock));

SELECT
CONCAT('${Top.Output}INSSSEP/CPRESTAM/','INSSSEP',@fechaStr,'.txt') as 'RutaArchivo',
CONCAT('${Top.History}INSSSEP/CPRESTAM/','INSSSEP_',year(@fecha),'_',RIGHT(concat('0',month(@fecha)),2),'_',day(@fecha),'_', RIGHT(concat('0',datepart(hour,@fecha)),2) ,'.', RIGHT(concat('0',datepart(minute,@fecha)),2) ,'.', RIGHT(concat('0',datepart(second,@fecha)),2) ,'.txt') as 'RutaArchivoHist',
CONCAT('INSSSEP',@fechaStr,'.txt') as 'archivo';


</sql>
