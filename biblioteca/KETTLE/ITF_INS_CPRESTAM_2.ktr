    <connection>TopazDS</connection>
    <sql>
SELECT 
Campo AS 'Resultado',(Select CONVERT(VARCHAR(10),FECHAPROCESO,111) FROM PARAMETROS) AS fechaSistema, (Select CONVERT(VARCHAR(8),FECHAPROCESO,112) FROM PARAMETROS) AS fechaSistComp
FROM ( 
   SELECT DISTINCT CONCAT( 
    '16',-- 1 JURISDICCION
    --2 TIPO DE DOCUMENTO
	(CASE WHEN 
		cs.TIPO_DOCUMENTO IS NULL THEN
		'0' ELSE cs.TIPO_DOCUMENTO 
		END),--2 TIPO DE DOCUMENTO
   --3 NUMERO DOCUMENTO                   
   (SELECT 
   CASE WHEN DOC.NUM_DOC_FISICO IS NULL THEN REPLICATE ('0',8)
   ELSE
   RIGHT(REPLICATE('0',8) + CAST(ISNULL(DOC.NUM_DOC_FISICO,0) AS VARCHAR(8)), 8) 
   END
   	FROM VW_CLI_X_DOC VC 
   		JOIN CLI_DocumentosPFPJ DOC 
   			ON S.C1803 = VC.CODIGOCLIENTE
			AND doc.NUMERODOCUMENTO= VC.NUMERODOC

   ),--3 NUMERO DOCUMENTO
	--4 ASOCIACION
   (
   	SELECT RIGHT(REPLICATE('0',3) + CAST(ISNULL(TP.CODIGO_ECOM,0) AS VARCHAR (3)),3) 
   	FROM TOPESPRODUCTO TP WHERE TP.CODPRODUCTO = s.PRODUCTO AND TP.TZ_LOCK=0
   ),--4 ASOCIACION
   RIGHT(REPLICATE('0',3) + CAST(ISNULL(PP.C2300,0) AS VARCHAR(3)),3), --5 NRO DE CUOTA   
   RIGHT(REPLICATE('0',10) + REPLACE(CAST(ISNULL((PP.C2309+PP.C2310+PP.C2311+PP.C2312),0) 
   											AS VARCHAR(10)), '.', ''), 10),--6 IMPORTE
   CONVERT(VARCHAR(8), pp.C2302, 112),--Fecha 1er. Vto (AAAAMMDD)
   '3',--8 Tipo de Concepto
   RIGHT(replicate('0',4) + '1',4), --9 Tipo de Producto
   --10 Datos del producto
   CONCAT(
   	RIGHT(replicate('0',14) + CAST(ISNULL(S.OPERACION,0) AS VARCHAR(14)),14),
   	RIGHT(replicate('0',6) + CAST(ISNULL(S.ORDINAL,0) AS VARCHAR(6)),6),
   	RIGHT(replicate('0',5) + CAST(ISNULL(S.PRODUCTO,0) AS VARCHAR(5)),5)
   	),--10 Datos del producto
   	CONVERT(VARCHAR(8), s.C1620, 112),--Fecha Alta de la Operaci?n
   	'080000'--Hora de Alta de la Operaci?n PENDIENTE
                 
   ) AS Campo
    
    FROM CRE_SALDOS CS
    JOIN SALDOS S ON S.JTS_OID = CS.SALDOS_JTS_OID
    JOIN PLANPAGOS PP ON PP.SALDO_JTS_OID = S.JTS_OID
    WHERE CS.ID_CONVENIO = (SELECT numerico_1 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 500)
      AND CS.JURISDICCION = 16
      AND S.C1785 = 5
      AND (PP.C2309 > 0 OR PP.C2310 > 0)
      AND PP.C2302 &lt; (SELECT fechaproceso FROM PARAMETROS)
) AS resultado -- PARA COMENTAR
ORDER BY SUBSTRING(Campo, 2, 3), SUBSTRING(Campo, 1, 1) 
</sql>
