			 (format(DATEADD(MONTH, -1, CONVERT(DATE, @periodo + '01')), 'yyyyMM')=FORMAT((SELECT fechaproceso FROM PARAMETROS), 'yyyyMM')))AND UPPER(@reproceso)='N')  
        THEN 1
        ELSE 0
END
AS validaFecha</sql>
