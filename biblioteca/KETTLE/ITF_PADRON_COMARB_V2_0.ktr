    <sql>declare @periodo varchar(6)=?,@reproceso varchar(1)=?;
SELECT 
case
        WHEN (format(DATEADD(MONTH, 0, CONVERT(DATE, @periodo + '01')), 'yyyyMM')=FORMAT((SELECT fechaproceso FROM PARAMETROS), 'yyyyMM') AND UPPER(@reproceso)='S') or 
