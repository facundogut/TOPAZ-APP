    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var msg_err = "";

var arry = reg.split(";");

var cuit = arry[0];
var razonSocial = arry[1];
var jurisdiccion = arry[2];
var perido_aplicacion = arry[3];
var crc_contrib = arry[4];
var letraAlicuota = arry[5];

var arrylen = arry.length;

if (nroLinea == 1) {
  if (
    reproceso != "N" &amp;&amp;
    reproceso != "n" &amp;&amp;
    reproceso != "S" &amp;&amp;
    reproceso != "s"
  ) {
    msg_err = "Campo reproceso invalido. ";
  } else if ((reproceso == "N" || reproceso == "n") &amp;&amp; fechaRep > 0) {
    msg_err = "Ya se ha procesado para el periodo ingresado. ";
  } else if ((reproceso == "N" || reproceso == "n") &amp;&amp; fechaRep > 0) {
    msg_err = "Ya se ha procesado para el periodo ingresado. ";
  } else if (
    (reproceso == "N" || reproceso == "n") &amp;&amp;
    periodo != perido_aplicacion) {
    msg_err = "El periodo ingresado debe ser igual al del archivo. ";

  } else if ((reproceso == "S" || reproceso == "s") &amp;&amp; fechaRep == 0) {
    msg_err =
      "No se puede reprocesar ya que nunca se ha procesado para el periodo ingresado. ";
  } else if (
    archivo.substr(0, 3) != "PAD" ||
    (archivo.substr(8, 4) != ".TXT" &amp;&amp; archivo.substr(8, 4) != ".txt")
  ) {
    msg_err = "Nombre archivo con formato invalido. ";
    noLinea = false;
  } else if (
    isNaN(periodo) ||
    periodo.substr(0, 4) &lt; 1800 ||
    periodo.substr(0, 4) > 3500 ||
    periodo.substr(4, 2) &lt; 1 ||
    periodo.substr(4, 2) > 12
  ) {
    msg_err = "Campo periodo invalido. ";
  }
}

var descartoReg =
  arry.length != 6 ||
  isNaN(cuit) ||
  isNaN(jurisdiccion) ||
  isNaN(perido_aplicacion) ||
  perido_aplicacion.substr(0, 4) &lt; 1800 ||
  perido_aplicacion.substr(0, 4) > 3500 ||
  perido_aplicacion.substr(4, 2) &lt; 1 ||
  perido_aplicacion.substr(4, 2) > 12 ||
  isNaN(crc_contrib);
</jsScript_script>
