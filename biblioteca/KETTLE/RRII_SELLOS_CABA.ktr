    <sql>--Fecha parametro

DECLARE @fecha_parametro DATE = CONVERT(DATETIME,?,103); 
DECLARE @Id_inicio INTEGER = ISNULL((SELECT MAX(ID) FROM RRII_SELLOS_CABA WITH(NOLOCK)),0);
WITH CARGOS_IMP AS (
SELECT 
	IMP.ID_IMPUESTO AS ID_CAR_IMP
FROM CI_IMPUESTOS IMP WITH (nolock)
WHERE 
	IMP.TIPO_IMPUESTO = 11 
		AND IMP.TZ_LOCK=0
UNION 
SELECT CAR.ID_CARGO AS ID_CAR_IMP
FROM CI_CARGOS CAR (NOLOCK) 
WHERE
	CAR.TIPO_CARGO_IMPOSITIVO=11 AND
	CAR.TZ_LOCK=0
)

SELECT 
	FL.ASIENTO,
	FL.SUCURSAL_ASIENTO,
	FL.FECHA_PROCESO_ASIENTO,
	FL.ORDINAL,
	FL.TR_REAL,
	M.RUBROCONTABLE,
 
	@Id_inicio AS ID_INICIO,

	DOC.NUMERODOCUMENTO AS CUIT1,
 
	0 AS CUIT2,
    FL.MONTO_IMPONIBLE AS MONTO_CONTRATO,
    FL.MONTO_IMPONIBLE AS BASE_IMPONIBLE,
	--Cuando el id_cargo es 256 se meustra el monto facturable multiplicado por 2 porque se hace cargo el banco 50% y en ci factura solo queda el 50%
	CASE WHEN FL.ID_CARGO = 256 
		THEN (FL.MONTO_FACTURABLE*2)
		ELSE FL.MONTO_FACTURABLE
    END AS IMPORTE_RETENIDO,
   -- CAST(FL.MONTO_IMPONIBLE AS NUMERIC (11,2)) AS MONTO_CONTRATO,
  	--CAST(FL.MONTO_IMPONIBLE AS NUMERIC (11,2))  AS BASE_IMPONIBLE,
  	--CAST(FL.MONTO_FACTURABLE AS NUMERIC (11,2)) AS IMPORTE_RETENIDO,
 
    '139000' AS COD_EXENCION,
 
	RP.NUM1 AS CODIGO_ACTOS,
 
  	M.FECHAVALOR AS FECHA_CONTRATO,
	
--	CASE WHEN RP.CHAR1 IN ('CV','CD','CM') THEN
--	    Substring(CONCAT(RP.CHAR1,
--	                    CAST(CAST(SubString(V.CTA_CBU,4,4) AS NUMERIC) AS VARCHAR),
--	                    CAST(CAST(SubString(V.CTA_CBU,11,13) AS NUMERIC) AS VARCHAR),
--	                    CAST(ROW_NUMBER() OVER (PARTITION BY FL.ASIENTO ORDER BY FL.ORDINAL) AS VARCHAR(10))
--	                    ),1,15)
--	ELSE
--	    SubString(CONCAT(RP.CHAR1,
--	                    CAST(CAST(SubString(V.CTA_CBU,4,4) AS NUMERIC) AS VARCHAR),
--	                    (SELECT OPERACION_CUENTA 
--	                     FROM MOVIMIENTOS_CONTABLES 
--	                     WHERE ASIENTO = FL.ASIENTO AND 
--	                           FECHAPROCESO = FL.FECHA_PROCESO_ASIENTO AND 
--	                           SUCURSAL = FL.SUCURSAL_ASIENTO AND 
--	                           TRTIPO=62),
--	                    CAST(ROW_NUMBER() OVER (PARTITION BY FL.ASIENTO ORDER BY FL.ORDINAL) AS VARCHAR(10))
--	                    ),1,15)
--	END AS POLIZA_TRANS,

CASE WHEN RP.CHAR1 IN ('CV','CD','CM') THEN
    CASE WHEN RP.CHAR1 = 'CM' THEN
        Substring(CONCAT(RP.CHAR1,
                        RIGHT('0000' + CAST(FL.ASIENTO AS VARCHAR), 4),
                        CAST(ROW_NUMBER() OVER (PARTITION BY RP.CHAR1 ORDER BY FL.ASIENTO, FL.ORDINAL) AS VARCHAR(10))
                        ),1,15)
    ELSE
        
        Substring(CONCAT(RP.CHAR1,
                        CAST(CAST(SubString(V.CTA_CBU,4,4) AS NUMERIC) AS VARCHAR),
                        CAST(CAST(SubString(V.CTA_CBU,11,13) AS NUMERIC) AS VARCHAR)
                        ),1,15)
    END
ELSE
    CASE WHEN RP.CHAR1 = 'CM' THEN
      
       
        Substring(CONCAT(RP.CHAR1,
                        RIGHT('0000' + CAST(FL.ASIENTO AS VARCHAR), 4),
                        CAST(ROW_NUMBER() OVER (PARTITION BY RP.CHAR1 ORDER BY FL.ASIENTO, FL.ORDINAL) AS VARCHAR(10))
                        ),1,15)
    ELSE
       
        SubString(CONCAT(RP.CHAR1,
                        CAST(CAST(SubString(V.CTA_CBU,4,4) AS NUMERIC) AS VARCHAR),
                        (SELECT OPERACION_CUENTA 
                         FROM MOVIMIENTOS_CONTABLES 
                         WHERE ASIENTO = FL.ASIENTO AND 
                               FECHAPROCESO = FL.FECHA_PROCESO_ASIENTO AND 
                               SUCURSAL = FL.SUCURSAL_ASIENTO AND 
                               TRTIPO=62)
                        ),1,15)
    END
END AS POLIZA_TRANS,

   	V.CTA_CBU AS CBU
 
 
FROM CI_FACTURA F (NOLOCK)

INNER JOIN CI_FACTURA_LINEA FL (NOLOCK) ON
	FL.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO	
	AND FL.ASIENTO = F.ASIENTO	
	AND FL.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000))

INNER JOIN SALDOS S (NOLOCK) ON 
	S.JTS_OID=F.JTS_SALDO_CLIENTE

INNER JOIN CARGOS_IMP CAR (NOLOCK) ON
	CAR.ID_CAR_IMP=FL.ID_CARGO OR
	CAR.ID_CAR_IMP=FL.ID_IMPUESTO
 
INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO = F.ASIENTO AND
	A.ESTADO = 77 AND
	A.SUCURSAL = F.SUCURSAL_ASIENTO AND
	A.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
 
INNER JOIN VTA_SALDOS V WITH(NOLOCK) ON
	V.JTS_OID_SALDO=F.JTS_SALDO_CLIENTE
 
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO AND
	M.SUCURSAL = F.SUCURSAL_ASIENTO
	AND M.ASIENTO = F.ASIENTO
	AND M.DEBITOCREDITO='C'
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO=FECHAPROCESO
			AND M.SUCURSAL=SUCURSAL
			AND M.ASIENTO=ASIENTO
			AND M.DEBITOCREDITO=DEBITOCREDITO)
 
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803
	AND ((C.TZ_LOCK &lt; 300000000000000 OR C.TZ_LOCK >= 400000000000000) 
		AND (C.TZ_LOCK &lt; 100000000000000 OR C.TZ_LOCK >= 200000000000000))
 
INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND CP.TITULARIDAD='T'
	AND ((CP.TZ_LOCK &lt; 300000000000000 OR CP.TZ_LOCK >= 400000000000000) 
		AND (CP.TZ_LOCK &lt; 100000000000000 OR CP.TZ_LOCK >= 200000000000000))
 
LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON
	((DOC.tz_lock &lt; 300000000000000 OR DOC.tz_lock >= 400000000000000)
		AND (DOC.tz_lock &lt; 100000000000000 OR DOC.tz_lock >= 200000000000000))
	AND DOC.NUMEROPERSONAFJ = CP.NUMEROPERSONA	
	AND DOC.TIPODOCUMENTO = (						
						SELECT TOP 1 TIPODOCUMENTO					
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE
							((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000)
								AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))						
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000)
								AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))							
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
						)

LEFT JOIN RRI_PARAMETROS_INF RP WITH(NOLOCK) ON 
	RP.CODIGO = 508 AND
	FL.OPERACION = RP.ID1
 
LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO
 
WHERE
   	YEAR(M.FECHAVALOR) = YEAR(@fecha_parametro) AND
    MONTH(M.FECHAVALOR) = MONTH(@fecha_parametro)
	AND AE.ASIENTO_ORIGINAL IS NULL 							
 </sql>
    <outer_join>Y</outer_join>
