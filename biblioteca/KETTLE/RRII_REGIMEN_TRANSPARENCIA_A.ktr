--4
RT.TasaInteres,
--5
RT.Observaciones,
--6
RT.Moneda,
--7
RT.Empresa,
--8
RT.ImportePrevisto,
--9
FORMAT (RT.FechaVigencia, 'dd/MM/yyyy') AS FechaVigencia,
--10
RT.Cantidad
FROM VW_REGIMEN_TRANSPARENCIA RT WITH (NOLOCK)
WHERE CONVERT(DATETIME,?,103) BETWEEN RT.FechaDesde AND RT.FechaVigencia</sql>
