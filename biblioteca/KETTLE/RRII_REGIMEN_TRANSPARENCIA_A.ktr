    <sql>--CARGOS
SELECT
--11
	FORMAT (EOMONTH(CONVERT(DATETIME,?,103)), 'dd/MM/yyyy') AS FechaInformacion,
--2
RPI.CHAR1 AS CodigoPartida, 
--3
CT.IMPORTE_APLICAR AS ImporteVigente,
--4
(100*CT.TASA) AS TasaInteres,
--5
'0' AS Observaciones,
--6
NULL AS Moneda,
--7
0050 AS Empresa,
--8
0 AS ImportePrevisto, --no se encuentra en topaz, CT.IMPORTE_APLICAR con mes o dia +1
--9
CT.FECHA_HASTA AS FechaVigencia,
--10
CT.FECHA_DESDE AS FechaDesde,
--11
NULL AS Cantidad

FROM CI_CARGOS C WITH (NOLOCK)
JOIN CI_CARGOS_TARIFAS CT ON C.ID_CARGO = CT.ID_CARGO
	AND C.TZ_LOCK=CT.TZ_LOCK
JOIN RRI_PARAMETROS_INF RPI ON C.ID_CARGO = RPI.ID1 
WHERE RPI.CODIGO = 100
AND C.TZ_LOCK = 0
AND (SELECT CONVERT(DATETIME,?,103))
BETWEEN CT.FECHA_DESDE AND CT.FECHA_HASTA
OR (CT.FECHA_HASTA IS NULL AND CT.FECHA_DESDE IS NULL)

UNION
--PRODUCTOS
SELECT
--1
	FORMAT (EOMONTH(CONVERT(DATETIME,?,103)), 'dd/MM/yyyy') AS FechaInformacion,
--2
	R.CHAR1 AS CodigoPartida, 
--3
	NULL AS ImporteVigente,
--4
CASE WHEN R.ID1 = 3 THEN
	(SELECT (100*(POWER(((SELECT MIN(VRT.DIFERENCIALTASABASE) 
				FROM VTA_DEFINICION_TASAS VDT WITH (NOLOCK)
				JOIN VTA_DEFINICION_VISTA V
					ON VDT.CODIGORANGO = V.CODIGORANGOPAGO
					AND VDT.TZ_LOCK = V.TZ_LOCK
				JOIN VTA_RANGOS_TASAS VRT
					ON VRT.CODIGORANGO = VDT.CODIGORANGO
					AND VRT.TZ_LOCK = VDT.TZ_LOCK
				JOIN PRODUCTOS P 
					ON P.C6250 = V.PRODUCTO
					AND P.TZ_LOCK = V.TZ_LOCK
				WHERE V.MONEDA = 1
				AND P.C6252 = 3
				AND P.C6250 NOT IN (SELECT ID2 FROM RRI_PARAMETROS_INF WITH (NOLOCK) WHERE CODIGO =103 AND TZ_LOCK=0)
				AND VDT.FECHADESDE &lt;= (SELECT MAX (FECHADESDE) 
										FROM VTA_DEFINICION_TASAS 
										WHERE FECHADESDE &lt;= CONVERT(DATETIME,?,103))
				AND VDT.TZ_LOCK = 0
				AND VRT.DIFERENCIALTASABASE!=0
				))/365+1,365)-1)))
		
	WHEN R.ID1 = 2 THEN   

			(SELECT (100*(POWER(((SELECT MIN(VRT.DIFERENCIALTASABASE) 
			FROM VTA_DEFINICION_TASAS VDT WITH (NOLOCK)
			JOIN VTA_DEFINICION_VISTA V
				ON VDT.CODIGORANGO = V.CODIGORANGOPAGO
				AND VDT.TZ_LOCK = V.TZ_LOCK
			JOIN VTA_RANGOS_TASAS VRT
				ON VRT.CODIGORANGO = VDT.CODIGORANGO
				AND VRT.TZ_LOCK = VDT.TZ_LOCK
			JOIN PRODUCTOS P 
				ON P.C6250 = V.PRODUCTO
				AND P.TZ_LOCK = V.TZ_LOCK
			WHERE V.MONEDA = 1
			AND P.C6252 = 2
			AND P.C6250 NOT IN (SELECT ID2 FROM RRI_PARAMETROS_INF WITH (NOLOCK) WHERE CODIGO =104 AND TZ_LOCK=0)
			AND VDT.FECHADESDE &lt;= (SELECT MAX (FECHADESDE) 
									FROM VTA_DEFINICION_TASAS WITH (NOLOCK)
									WHERE FECHADESDE &lt;= CONVERT(DATETIME,?,103))
			AND VDT.TZ_LOCK = 0
			AND VRT.DIFERENCIALTASABASE!=0
			AND P.C6253 = 'N'
))/365+1,365)-1)))

	END AS TasaInteres,
--5
	'0' AS Observaciones,
--6
	NULL AS Moneda,
--7
	0050 AS Empresa,
--8
	0 AS ImportePrevisto, --no se encuentra en topaz, CT.IMPORTE_APLICAR con mes o dia +1
--9
	NULL AS FechaVigencia,
--10
	NULL AS FechaDesde,
--11
	COUNT(*) AS CANTIDAD
		
FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
INNER JOIN SALDOS S 
	ON G.SALDOS_JTS_OID = S.JTS_OID
	AND G.MONEDA = S.MONEDA
   AND G.TZ_LOCK = S.TZ_LOCK
INNER JOIN RRI_PARAMETROS_INF R
	ON G.TZ_LOCK = R.TZ_LOCK
	AND R.ID1 = S.C1785
INNER JOIN PRODUCTOS P 
	ON P.C6250 = S.PRODUCTO
	AND P.TZ_LOCK = G.TZ_LOCK
WHERE R.CODIGO = 101
AND G.TZ_LOCK = 0
AND G.MONEDA = 1
AND G.FECHA = CONVERT(DATETIME,?,103)
AND (S.PRODUCTO NOT IN (SELECT R.ID2 FROM RRI_PARAMETROS_INF R WITH (NOLOCK) WHERE R.CODIGO=103 and R.TZ_LOCK=0)
	AND S.C1785 IN (SELECT ID1 FROM RRI_PARAMETROS_INF WITH (NOLOCK) WHERE CODIGO = 101 AND TZ_LOCK=0))
AND (S.PRODUCTO NOT IN (SELECT R.ID2 FROM RRI_PARAMETROS_INF R WITH (NOLOCK) WHERE R.CODIGO=104 and R.TZ_LOCK=0)
	AND S.C1785 IN (SELECT ID1 FROM RRI_PARAMETROS_INF WITH (NOLOCK) WHERE CODIGO = 101 AND TZ_LOCK=0))

GROUP BY 
	R.ID1,
	R.CHAR1

UNION ALL

--TARJETAS DE CREDITO 
SELECT
--1
	FORMAT (EOMONTH(CONVERT(DATETIME,?,103)), 'dd/MM/yyyy') AS FechaInformacion,
--2
R.CHAR1 AS CodigoPartida, 
--3
NULL AS ImporteVigente,
--4
NULL AS TasaInteres,
--5
'0' AS Observaciones,
--6
NULL AS Moneda,
--7
0050 AS Empresa,
--8
0 AS ImportePrevisto, --no se encuentra en topaz, CT.IMPORTE_APLICAR con mes o dia +1
--9
NULL AS FechaVigencia,
--10
NULL AS FechaDesde,
--11
	COUNT(T.USUARIO) AS Cantidad

FROM TJC_SALDOS T WITH (NOLOCK)
INNER JOIN RRI_PARAMETROS_INF R
	ON T.ADMINISTRADORA = R.ID1
	AND T.TZ_LOCK = R.TZ_LOCK
INNER JOIN TJC_ADINTAR_DEUDASPLAS TAD 
	ON T.ADMINISTRADORA = TAD.ADMINISTRADORA
	AND TAD.TZ_LOCK = T.TZ_LOCK
WHERE R.CODIGO = 102
AND R.TZ_LOCK=0
AND T.ADMINISTRADORA IN (SELECT R.ID1 FROM RRI_PARAMETROS_INF R WITH (NOLOCK) WHERE R.CODIGO=102 AND R.TZ_LOCK=0)
AND TAD.CONDICION = 'T'
AND TAD.FECHA_GENERACION = CONVERT (DATETIME,?,103)
GROUP BY 
	R.CHAR1,
	R.ID1</sql>
