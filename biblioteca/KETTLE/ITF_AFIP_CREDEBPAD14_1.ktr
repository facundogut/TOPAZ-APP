  <step>
    <name>Execute SQL script</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>top</connection>
    <execute_each_row>Y</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>Y</replace_variables>
    <quoteString>Y</quoteString>
    <sql>DECLARE 
		@p_cuit NUMERIC(11) = ?,
        @p_cbu varchar(22) = ?,
        @p_beneficio NUMERIC(5) = ?,
        @p_fechaVigencia DATETIME = convert(DATETIME,substring('?',0,11),102),--(SELECT TOP 1 fecha_vig FROM ITF_BEN_FISCAL_CREDEB ORDER BY ID desc), 
		@p_fechaproceso datetime=(SELECT TOP 1 fechaproceso FROM PARAMETROS),
		@p_segmento NUMERIC(3)=?
		
DECLARE @v_count_padron		NUMERIC(2) = 0,
		@v_cbu_padron		NUMERIC(22),
		@v_fecha_padron		DATETIME,
		@v_beneficio_padron	NUMERIC(5),
		@v_jts_oid			NUMERIC(10),
		@v_padron_mipyme	VARCHAR(1)		

SELECT TOP 1 @v_count_padron= count(1),@v_cbu_padron = cbu, @v_fecha_padron = FECHA_vigencia, @v_beneficio_padron = codigo_beneficio 
FROM ITF_PADRON_FISCAL_CREDEB (nolock)
WHERE CUIT = @p_cuit
GROUP BY cbu, FECHA_vigencia, codigo_beneficio

SELECT TOP 1 @v_jts_oid = jts_oid_saldo 
FROM VTA_saldos (nolock) 
WHERE cta_cbu = @p_cbu

UPDATE SALDOS
SET CREDEB_Segmento = @p_segmento
WHERE JTS_OID = @v_jts_oid

IF @v_count_padron > 0 AND (@p_cbu &lt;&gt; @v_cbu_padron
					         OR @p_fechaVigencia &lt;&gt; @v_fecha_padron 
					         OR @p_beneficio &lt;&gt; @v_beneficio_padron 
					         )
	BEGIN
	    SELECT @v_padron_mipyme = b1.PADRON_MiPyme
	    FROM RRII_BITACORA_CREDEB b1 (nolock)
		JOIN (	SELECT JTS_OID_SALDOS, max(FECHA) AS max_fecha 
				FROM RRII_BITACORA_CREDEB (nolock) 
				GROUP BY JTS_OID_SALDOS
			) b2 ON b1.JTS_OID_SALDOS = b2.JTS_OID_SALDOS AND b1.FECHA = b2.max_fecha
	    WHERE b1.jts_oid_saldos = @v_jts_oid
	    
	    UPDATE dbo.RRII_BITACORA_CREDEB
		SET PADRON_BENEF_FISCAL = 'S'
			, PADRON_MiPyme = @v_padron_mipyme
			, TZ_LOCK = 0
			, SEGMENTO = @p_segmento
		WHERE JTS_OID_SALDOS = @v_jts_oid AND FECHA = @p_fechaproceso AND TIPO_CARGO_IMPOSITIVO = 6
		
		IF @@ROWCOUNT = 0
   		BEGIN
		    INSERT INTO RRII_BITACORA_CREDEB (TZ_LOCK, JTS_OID_SALDOS, FECHA, SEGMENTO, TIPO_CARGO_IMPOSITIVO, PADRON_BENEF_FISCAL, PADRON_MiPyme)
			VALUES (0, @v_jts_oid, @p_fechaproceso, @p_segmento, 6, 'S', @v_padron_mipyme)
   		END
	END
ELSE IF @v_count_padron = 0
	BEGIN
		begin try
			UPDATE dbo.RRII_BITACORA_CREDEB
			SET PADRON_BENEF_FISCAL = 'S'
				, PADRON_MiPyme = 'N'
				, TZ_LOCK = 0
				, SEGMENTO = @p_segmento
			WHERE JTS_OID_SALDOS = @v_jts_oid AND FECHA = @p_fechaproceso AND TIPO_CARGO_IMPOSITIVO = 6
			
			IF @@ROWCOUNT = 0
		   		BEGIN
				    INSERT INTO  RRII_BITACORA_CREDEB(
				        tz_lock,
				        jts_oid_saldos,
				        fecha,
				        segmento,
				        tipo_cargo_impositivo,
				        padron_benef_fiscal,
				        padron_mipyme
				    )
				    VALUES (
				        0,
				        @v_jts_oid,
				        @p_fechaproceso, 
				        @p_segmento,
				        6,
				        'S',
				        'N'
				    )
		   		END
		end try
		BEGIN CATCH
		END CATCH
	END</sql>
