      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>function esFechaValida(fechaStr) {
    // Verificar el formato correcto usando una expresión regular que permita ceros opcionales
    const regexFecha = /^(\d{1,2})\/(\d{1,2})\/(\d{4})$/;
    if (!regexFecha.test(fechaStr)) {
        return false;
    }

    // Descomponer la fecha en día, mes y año
    const [ , diaStr, mesStr, anioStr ] = fechaStr.match(regexFecha);
    const dia = parseInt(diaStr, 10);
    const mes = parseInt(mesStr, 10) - 1; // Los meses en JavaScript son 0-11
    const anio = parseInt(anioStr, 10);

    // Crear un objeto de fecha
    const fecha = new Date(anio, mes, dia);

    // Verificar que la fecha es válida
    if (fecha.getFullYear() === anio &amp;&amp; 
        fecha.getMonth() === mes &amp;&amp; 
        fecha.getDate() === dia) {
        return true;
    } else {
        return false;
    }
}

function isNumeric(n) {
  return !isNaN(parseFloat(n)) &amp;&amp; isFinite(n);
}

function esFechaMayor(fch1, fch2) {
    if (!esFechaValida(fch1) || !esFechaValida(fch2)) {
        return false; // Retornar false si cualquiera de las dos fechas no es válida
    }

    const [dia1, mes1, anio1] = fch1.split('/').map(Number);
    const [dia2, mes2, anio2] = fch2.split('/').map(Number);

    const fecha1 = new Date(anio1, mes1 - 1, dia1);
    const fecha2 = new Date(anio2, mes2 - 1, dia2);

    return fecha2 > fecha1;
}


function convertirFecha(fecha) {
    const anio = fecha.substring(0, 4);
    const mes = fecha.substring(5, 7);
    const dia = fecha.substring(8, 10);
    return dia + '/' + mes + '/' + anio;
}</jsScript_script>
      </jsScript>
