    <sql>DECLARE @cuit varchar(11) = ?,
        @cbu varchar(22) = ?,
        @beneficio varchar(5) = ?,
        @fechaVigencia DATETIME = TRY_CONVERT(DATE,?,  102)

SELECT 
    CASE WHEN @cuit IN (SELECT [Número DOCUMENTO identificativo] 
						FROM VW_DOCUMENTOS_PFPJ) THEN @cuit ELSE null END AS cuitValidado,
    CASE WHEN @cbu IN (SELECT cta_cbu 
					   FROM VTA_SALDOS) THEN @cbu ELSE null END AS cbuValidado,
    CASE WHEN @beneficio IN (SELECT CODIGO_BENEFICIO 
							 FROM CON_CREDEB_BENEFICIOS) THEN @beneficio ELSE null END AS beneficioValidado,
    CASE WHEN @fechaVigencia &lt;= FECHAPROCESO THEN @fechaVigencia ELSE null END AS fechaVigenciaValidada,
	CASE WHEN @cuit IN (SELECT [Número DOCUMENTO identificativo] 
						FROM VW_DOCUMENTOS_PFPJ) THEN (SELECT DISTINCT TOP 1cp.CODIGOCLIENTE 
													   FROM CLI_DocumentosPFPJ pfj JOIN CLI_ClientePersona cp ON pfj.NUMEROPERSONAFJ = cp.NUMEROPERSONA
													   WHERE cp.TZ_LOCK = 0 
													   AND cp.TITULARIDAD = 'T' 
													   AND pfj.NUMERODOCUMENTO = @cuit ) ELSE null END AS codCliValidado,	 
	CASE WHEN @cuit IN (SELECT [Número DOCUMENTO identificativo] 
						FROM VW_DOCUMENTOS_PFPJ) THEN (SELECT DISTINCT TOP 1 cp.NUMEROPERSONA 
													   FROM CLI_DocumentosPFPJ pfj JOIN CLI_ClientePersona cp ON pfj.NUMEROPERSONAFJ = cp.NUMEROPERSONA
													   WHERE cp.TZ_LOCK = 0 
													   AND cp.TITULARIDAD = 'T' 
													   AND pfj.NUMERODOCUMENTO = @cuit ) ELSE null END AS numPersonaValidado,
	CASE WHEN @cuit IN (SELECT [Número DOCUMENTO identificativo] 
						FROM VW_DOCUMENTOS_PFPJ) THEN (SELECT TOP 1 [Apellidos y Nombres/Razón Social] 
													   FROM VW_DOCUMENTOS_PFPJ 
													   where [Número DOCUMENTO identificativo] = @cuit ) ELSE null END AS nomPersonaValidado,
	CASE WHEN @beneficio IN (SELECT CODIGO_BENEFICIO 
							 FROM CON_CREDEB_BENEFICIOS) THEN (SELECT TOP 1 CODIGO_SEGMENTO 
															 FROM CON_CREDEB_BENEFICIOS 
															 WHERE CODIGO_BENEFICIO=@beneficio) ELSE null END segmentoValidado,
	CASE WHEN @beneficio IN (SELECT CODIGO_BENEFICIO 
							 FROM CON_CREDEB_BENEFICIOS) THEN (SELECT TOP 1 ALICUOTA 
															 FROM Valores_alicuota va, CON_CREDEB_BENEFICIOS bf 
															 WHERE va.categoria_impuesto=bf.CODIGO_SEGMENTO 
															 AND bf.CODIGO_BENEFICIO=@beneficio) ELSE null END alicuotaValidada,
    FORMAT(@fechaVigencia, 'dd/MM/yyyy') AS FechaEnFormatoVarchar,
    CASE WHEN @cuit IN (SELECT d.[Número Documento Identificativo]
						FROM VW_DOCUMENTOS_PFPJ d (nolock)
						JOIN cli_clientepersona cp (nolock) ON d.[Número de la Persona] = cp.NUMEROPERSONA AND cp.TZ_LOCK = 0
						JOIN saldos s (nolock) ON s.C1803 = cp.CODIGOCLIENTE
						JOIN vta_saldos vs (nolock) ON s.JTS_OID = vs.JTS_OID_SALDO
						WHERE vs.CTA_CBU = @cbu) THEN @cuit ELSE NULL END AS cuitPerteneceACBU,
	FECHAPROCESO AS FECHA_PROCESO_RECORTADA	
FROM PARAMETROS (NOLOCK)					</sql>
