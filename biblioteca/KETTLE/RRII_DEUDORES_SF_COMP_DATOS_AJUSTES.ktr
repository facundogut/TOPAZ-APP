    <quoteString>N</quoteString>
    <sql>WITH Totales AS (
    SELECT 
        TIPO_ID,
        NRO_ID,
        TIPO_ASIST_CRED,
        FECHA_PROCESO,
        SUM(DEUDA_VENCIDA_SGTIA) AS TotalVencida,
SUM(DEUDA_TOTAL_SGTIA) as deuda_total, 
SUM(DEUDA_VENCIDA_GTIAB) as  TotalVencidaB,
SUM(DEUDA_TOTAL_GTIAB) as  deuda_totalB,
SUM(DEUDA_VENCIDA_GTIAA) as TotalVencidaA,
SUM(DEUDA_TOTAL_GTIAA) as deuda_totalA
   FROM DEUDORES_4306_COMP WITH(NOLOCK)
WHERE FECHA_PROCESO = CONVERT(DATETIME,?,103)
    GROUP BY TIPO_ID, NRO_ID, TIPO_ASIST_CRED, FECHA_PROCESO
)
 
UPDATE b
SET b.SIN_GAR_CAP_INT_NOPREV_VENC = t.TotalVencida,  
b.SIN_GAR_CAP_INT_NOPREV_TOT= t.deuda_total,
b.GAR_B_CAP_INT_NOPREV_VENC = t.TotalVencidaB,
b.GAR_B_CAP_INT_NOPREV_TOT = t.deuda_totalB,
b.GAR_A_CAP_INT_VENC = T.TotalVencidaA,
b.GAR_A_CAP_INT_TOT = T.deuda_totalA
FROM DEUDORES_4306 b
INNER JOIN Totales t ON
    t.TIPO_ID = b.TIPO_ID AND
    t.NRO_ID = b.NRO_ID AND
    t.TIPO_ASIST_CRED = b.TIPO_ASIST_CRED AND
    t.FECHA_PROCESO = b.FECHA_PROCESO
WHERE (b.SIN_GAR_CAP_INT_NOPREV_VENC &lt;&gt; t.TotalVencida or 
b.SIN_GAR_CAP_INT_NOPREV_TOT &lt;&gt; t.deuda_total or 
 b.GAR_B_CAP_INT_NOPREV_VENC &lt;&gt; t.TotalVencidaB or
 b.GAR_B_CAP_INT_NOPREV_TOT &lt;&gt; t.deuda_totalB or 
 b.GAR_A_CAP_INT_VENC &lt;&gt; T.TotalVencidaA or
 b.GAR_A_CAP_INT_TOT &lt;&gt; T.deuda_totalA)
AND b.FECHA_PROCESO = CONVERT(DATETIME,?,103)</sql>
