    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>UPDATE D5
SET D5.TOT_FINANCIACIONES = (ISNULL(D6.TOTALD6,0) + ISNULL(D7.TOTALD7,0))
FROM DEUDORES_4305 D5
LEFT JOIN (SELECT SUM(ISNULL(GAR_A_CAP_INT_TOT, 0) + ISNULL(GAR_B_CAP_INT_NOPREV_TOT, 0) + 
ISNULL(GAR_B_INT_PREV_TOT, 0) + ISNULL(SIN_GAR_CAP_INT_NOPREV_TOT, 0) + ISNULL(SIN_GAR_INT_PREV_TOT, 0)) AS TOTALD6,
TIPO_ID, NRO_ID, FECHA_PROCESO
FROM DEUDORES_4306 WITH(NOLOCK) 
WHERE FECHA_PROCESO = CONVERT(DATETIME,?,103)
GROUP BY TIPO_ID, NRO_ID, FECHA_PROCESO) D6 
ON D6.TIPO_ID = D5.TIPO_ID AND D6.NRO_ID = D5.NRO_ID AND D6.FECHA_PROCESO = D5.FECHA_PROCESO
LEFT JOIN (SELECT SUM(ISNULL(RESP_EVENT_GAR_A, 0) + ISNULL(RESP_EVENT_GAR_B, 0) + ISNULL(RESP_EVENT_SIN_GAR, 0) + 
ISNULL(GAR_OTORG_GAR_A, 0) + ISNULL(GAR_OTORG_GAR_B, 0) + ISNULL(ADEL_CC_GAR_A, 0) + ISNULL(ADEL_CC_GAR_B, 0) + 
ISNULL(ADEL_CC_SIN_GAR, 0) + ISNULL(GAR_OTORG_SIN_GAR, 0) + ISNULL(MONTO_IRREC_FUERA_BAL, 0) + 
ISNULL(FIN_EXTERIOR, 0) + ISNULL(DIF_ADQ_CARTERA, 0)) AS TOTALD7,
TIPO_ID, NRO_ID, FECHA_PROCESO 
FROM DEUDORES_4307 WITH(NOLOCK) 
WHERE FECHA_PROCESO = CONVERT(DATETIME,?,103)
GROUP BY TIPO_ID, NRO_ID, FECHA_PROCESO) D7 
ON D7.TIPO_ID = D5.TIPO_ID AND D7.NRO_ID = D5.NRO_ID AND D7.FECHA_PROCESO = D5.FECHA_PROCESO
WHERE D5.FECHA_PROCESO = CONVERT(DATETIME,?,103)</sql>
