    <quoteString>N</quoteString>
    <sql>WITH COTIZACIONES AS (
SELECT MONEDA, MAX(FECHA_COTIZACION) AS FECHA_COTIZACION FROM HISTORICOTIPOSCAMBIO WITH(NOLOCK) WHERE FECHA_COTIZACION &lt;= CONVERT(DATETIME,?,103) GROUP BY MONEDA
)

INSERT INTO DEUDORES_4301
SELECT DISTINCT
----- 4301
4301 AS CODIGO_DISENIO,

    ROUND((
        SELECT ISNULL(SUM(ISNULL(SALDO_FINAL,0)),0)
        FROM (
            SELECT 
                CASE 
                    WHEN GSD.MONEDA &lt;&gt; 1 THEN 
                        ISNULL(GSD.SALDO_AJUSTADO,0) * /*(
                            SELECT TOP 1 H.TIPO_CAMBIO_OFICIAL
                            FROM HISTORICOTIPOSCAMBIO H WITH(NOLOCK)
                            WHERE H.FECHA_COTIZACION &lt;= MC.Fecha_Info
                            AND H.MONEDA = GSD.MONEDA
                            ORDER BY H.FECHA_COTIZACION DESC
                        )*/
                        ISNULL((SELECT H.TIPO_CAMBIO_OFICIAL
						FROM HISTORICOTIPOSCAMBIO H WITH(NOLOCK)
						INNER JOIN COTIZACIONES COT WITH(NOLOCK) ON COT.MONEDA = H.MONEDA AND H.FECHA_COTIZACION = COT.FECHA_COTIZACION
						WHERE H.MONEDA = GSD.MONEDA),0)
                    ELSE ISNULL(GSD.SALDO_AJUSTADO,0)
                END AS SALDO_FINAL
            FROM GRL_SALDOS_DIARIOS GSD WITH(NOLOCK)
			INNER JOIN PLANCTAS PC WITH(NOLOCK) ON PC.C6326 = GSD.RUBROCONTABLE
            WHERE PC.COD_INTERNO_IFRS = '711045' 
               OR PC.COD_INTERNO_IFRS = '715045' 
               --OR GSD.RUBROCONTABLE LIKE '%715023%'
        ) Sal
    )/1000,0) AS INT_DOC_NO_DEV,
 ISNULL((
                SELECT SUM(GAR_PREF_A_CAP_INT + 
                          GAR_PREF_B_CAP_INT_NOPREV +  
                          SIN_GAR_CAP_INT_NOPREV)
                FROM DEUDORES_4302 D4 WITH(NOLOCK)
                WHERE D4.CONCEPTO = 2
            ), 0) +  ISNULL((
                SELECT SUM(SIN_GAR_CAP_INT_NOPREV)
                FROM DEUDORES_4302 D4 WITH(NOLOCK)
                WHERE D4.CONCEPTO = 4
            ), 0) AS TOT_FIN_SALDOS_MEN,
 ISNULL((
                SELECT SUM(GAR_PREF_A_CAP_INT + 
                          GAR_PREF_B_CAP_INT_NOPREV +  
                          SIN_GAR_CAP_INT_NOPREV)
                FROM DEUDORES_4302 D4 WITH(NOLOCK)
                WHERE D4.CONCEPTO = 2
            ), 0) +  ISNULL((
                SELECT SUM(SIN_GAR_CAP_INT_NOPREV)
                FROM DEUDORES_4302 D4 WITH(NOLOCK)
                WHERE D4.CONCEPTO = 4
            ), 0) +
    ISNULL((
        SELECT SUM(TOT_FINANCIACIONES)
        FROM DEUDORES_4305 WITH(NOLOCK) WHERE FECHA_PROCESO = CONVERT(DATETIME,?,103)
    ), 0) AS TOT_FINANCIACIONES,
'' SIN_USO,
CONVERT(DATETIME,?,103) as FECHA_PROCESO
FROM MEMO_CABECERA MC WITH(NOLOCK)
WHERE MC.Fecha_Info= CONVERT(DATETIME,?,103)
AND MC.TZ_LOCK=0</sql>
