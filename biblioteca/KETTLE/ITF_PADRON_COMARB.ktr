    <sql>--select distinct vw.numeropersona as numeroPersona
--		, convert(NUMERIC,VW.NUMERODOC) as NUMERODOC
--		, vw.tipopersona as tipo_persona
--FROM VW_CLIENTES_PERSONAS VW
--inner join cli_clientes cli on vw.codigocliente=cli.codigocliente
--WHERE VW.TITULARIDAD='T'
