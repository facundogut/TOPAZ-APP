    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Transforma</jsScript_name>
        <jsScript_script>

// Itera por cada campo de la fila
for (var idx_campo = 0; idx_campo &lt; lista_campos.size(); idx_campo++) {
	
	// Aplica unicamente a Strings con valor no nulo
    if (lista_campos.get(idx_campo).getType() == org.pentaho.di.core.row.ValueMetaInterface.TYPE_STRING &amp;&amp; row[idx_campo] != null) {

		// Parsea objeto String de Java a objeto String de javascript.
        var string_actual = String(row[idx_campo]);
        
        // Matchea con cada caracter y reemplaza con el mappeo correspondiente. 
		// Si el caracter no se encuentra en el mapa, le asigna un espacio
        var string_reemplazo = string_actual.replace(/./g, function(match) {
            return mapaCaracteres[match] || ' ';
        });

        // Asigna al campo en la fila el valor mappeado con tipo de dato String de Java
        row[idx_campo] = java.lang.String.valueOf(string_reemplazo);
    }
}

