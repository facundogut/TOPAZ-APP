	// Si algun campo es de tipo String, se debe aplicar el mappeo de caracteres por cada caracter
	if ( lista_campos.get( idx_campo ).getType() == org.pentaho.di.core.row.ValueMetaInterface.TYPE_STRING &amp;&amp; row[ idx_campo ] != null ) {
		
		// Obtengo el String a reemplazar como array de caracteres
		var string_actual = String( row[ idx_campo ] );
		var string_reemplazo = '';
		
		// Reemplaza cada caracter
		for (var idx_caracter = 0; idx_caracter &lt; string_actual.length; idx_caracter++)
			string_reemplazo += mapaCaracteres[ string_actual.charAt( idx_caracter ) ] || ' ';
		
		// Asigna string reemplazado a la fila.
		row[ idx_campo ] = java.lang.String.valueOf( string_reemplazo );
		
	}

}
*/</jsScript_script>
      </jsScript>
