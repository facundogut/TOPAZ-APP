        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Ajuste importe
IMPORTE_ACRED = (Number(IMPORTE_ACRED) / 100).toFixed(2);

//Campos Acreditacion Sin Uso
var CONTRAPARTE_JTS_OID = null;
var FECHA_VALOR = null;

//Campo Normaliza Respuesta del impactador
var JTS_BANDEJA = null;

//Validaciones
var PARAR = false;

//Validaciones contra cuenta vista
if (PARAR != true)
{
	if (EXISTE_CUENTA == null)
	{
		COD_RESPUESTA = '002';
		DETALLE = ' Cuenta Vista Inexistente en Core. ';
		PARAR = true;
	}
	else if (COINCIDE_MONEDA != 'S')
	{
		COD_RESPUESTA = '005';
		DETALLE = ' Moneda NO coincide con Cuenta Vista. ';
		PARAR = true;
	}
	else if (CUENTA_VINCULADA == null)
	{
		COD_RESPUESTA = '010';
		DETALLE = ' Cuenta no vinculada al CUIT indicado. ';
		PARAR = true;
	}
	else if (CUENTA_CANCELADA == 'S')
	{
		COD_RESPUESTA = '015';
		DETALLE = ' Cuenta Cancelada. ';
		PARAR = true;
	}
}

//Validacion Fecha Valor
if (PARAR != true)
{
	if (FECHA_IGUAL_PROCESO != 'S')
	{
		FECHA_VALOR = FECHA_ACRED;
	}
}

//Validaciones de Parametros
if (PARAR != true)
{
	if (MONEDA_PERMITIDA == null)
	{
		COD_RESPUESTA = '009';
		DETALLE = ' Moneda NO Permitida. ';
		PARAR = true;
	}
	else if (ADMINISTRADORA_PERMITIDA == null)
	{
		COD_RESPUESTA = '008';
		DETALLE = ' Administradora NO Permitida. ';
		PARAR = true;
	}
	else if (PRODUCTO_EXCLUIDO != null)
	{
		COD_RESPUESTA = '011';
		DETALLE = ' Cuenta NO Permitida (Producto Excluido). ';
		PARAR = true;
	}
	else if (SATELITE == null || CAUSAL == null)
	{
		COD_RESPUESTA = '016';
		DETALLE = ' No se encuentra Satelite y Causal en parametros de Interfaz. ';
		PARAR = true;
	}
}

</jsScript_script>
