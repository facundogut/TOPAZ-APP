  <step>
    <name>Get Transferencias Monobanco</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <sql>begin
	declare 
	@FECHA_PROCESO datetime = (select top 1 FECHAPROCESO from PARAMETROS), 
	@CLAVE_PROCESO varchar(50) = (select top 1 ALFA_2 from ITF_MASTER_PARAMETROS where CODIGO_INTERFACE = 131 and ALFA_1 = 'ID PROCESO');
	with MONOBANCO as 
	(
	select 
	NRO_REG, 
	case
		when COD_ACRED = '01' then 2 
		when COD_ACRED = '02' then 3 
		else null
	end as TIPO_PRODUCTO, 
	NRO_CUENTA_ACRED, 
	COD_TIPO_MOV, 
	FECHA_ACRED, 
	try_convert(datetime,FECHA_ACRED) as 'FECHA_CONVERTIDA', 
	IMPORTE_ACRED, 
	COD_ADMIN, 
	COD_MONEDA, 
	SUCURSAL_CUENTA, 
	NRO_CUIT, 
	COD_RESPUESTA, 
	DETALLE 
	from ITF_AD_CREDICOM_AUX 
	where COD_RESPUESTA is null and COD_ACRED in ('01','02') 
	), 
	CLIENTES_PERSONAS as 
	(
	select 
	ccp.CODIGOCLIENTE, 
	cdp.NUMERODOCUMENTO 
	from CLI_ClientePersona ccp 
	inner join CLI_DocumentosPFPJ cdp on cdp.NUMEROPERSONAFJ = ccp.NUMEROPERSONA and left(cdp.TZ_LOCK,1) in (0,4) 
	where left(ccp.TZ_LOCK,1) in (0,4) 
	group by ccp.CODIGOCLIENTE, cdp.NUMERODOCUMENTO 
	)
	select 
	m.NRO_REG, --Clave para actualizar codigo respuesta y detalle
	m.FECHA_ACRED, --FECHA_ASIENTO para impacto masivo
	m.IMPORTE_ACRED, --IMPORTE para impacto masivo
	m.COD_MONEDA, --MONEDA para impacto masivo
	m.COD_RESPUESTA, --Codigo de Respuesta a Actualizar
	m.DETALLE, --Detalle a Actualizar
	s.CUENTA as 'EXISTE_CUENTA', --NULL si no existe, dato si existe
	case
		when m.COD_MONEDA = s.MONEDA then 'S' 
		else 'N'
	end as 'COINCIDE_MONEDA', --Indica con S si coincide la moneda del archivo con la del saldo de la cuenta indicada
	s.JTS_OID as 'SALDO_JTS', --ORIGEN_JTS_OID para impacto masivo
	case
		when s.C1651 = '1' then 'S' 
		else 'N' 
	end as 'CUENTA_CANCELADA', --Indica con S si la cuenta esta Cancelada
	cp.NUMERODOCUMENTO as 'CUENTA_VINCULADA', --NULL si no tiene cuenta vinculada para ese CUIT, dato si tiene
	@CLAVE_PROCESO as 'CLAVE_PROCESO', --Clave de Proceso para control de Traba Contable
	case 
		when m.FECHA_CONVERTIDA = @FECHA_PROCESO then 'S' 
		else 'N' 
	end as 'FECHA_IGUAL_PROCESO', --Indica con S si la fecha asiento es igual a la fecha proceso 
	impm.NUMERICO_1 as 'MONEDA_PERMITIDA', --NULL si la moneda NO esta permitida
	impp.NUMERICO_1 as 'PRODUCTO_EXCLUIDO', --NULL si el producto NO esta excluido
	tma.COD_ADMINISTRADORA as 'ADMINISTRADORA_PERMITIDA', --NULL si la administradora NO esta permitida
	impc.ALFA_2 as 'SATELITE', --Indica Satelite para impacto masivo, NULL si no hay parametria
	impc.ALFA_3 as 'CAUSAL' --Indica Causal para impacto masivo, NULL si no hay parametria
	from MONOBANCO m 
	left join SALDOS s on s.SUCURSAL = m.SUCURSAL_CUENTA and s.CUENTA = m.NRO_CUENTA_ACRED and s.C1785 = m.TIPO_PRODUCTO and left(s.TZ_LOCK,1) in (0,4) 
	left join CLIENTES_PERSONAS cp on cp.CODIGOCLIENTE = s.C1803 and try_convert(numeric(20,0),cp.NUMERODOCUMENTO) = try_convert(numeric(20,0),m.NRO_CUIT) 
	left join ITF_MASTER_PARAMETROS impm on impm.NUMERICO_1 = m.COD_MONEDA and impm.CODIGO_INTERFACE = 131 and impm.ALFA_1 = 'MONEDA PERMITIDA' and left(impm.TZ_LOCK,1) in (0,4) 
	left join ITF_MASTER_PARAMETROS impp on impp.NUMERICO_1 = s.PRODUCTO and impp.CODIGO_INTERFACE = 131 and impp.ALFA_1 = 'PRODUCTO EXCLUIDO' and left(impp.TZ_LOCK,1) in (0,4) 
	left join TJC_MAESTRO_ADMINISTRADORAS tma on tma.COD_ADMINISTRADORA = m.COD_ADMIN and left(tma.TZ_LOCK,1) in (0,4) 
