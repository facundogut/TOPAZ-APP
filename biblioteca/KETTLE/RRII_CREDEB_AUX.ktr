    <sql>
--Fecha parametro
DECLARE @fecha_parametro DATE = CONVERT(DATETIME,'${fecha}',103);
DECLARE @fecha_hoy DATE = (SELECT FECHAPROCESO FROM PARAMETROS);
--

SELECT 
ISNULL(CODIGO_REGIMEN, 0) CODIGO_REGIMEN, 
TIPO_DOCUMENTO, 
NUMERO_DOCUMENTO, 
CBU, 
CAST(sum(IMPORTE_IMPUESTO) AS NUMERIC(15,2)) IMPORTE_IMPUESTO,
CAST(sum(BASE_IMPONIBLE)  AS NUMERIC(15,2)) BASE_IMPONIBLE,
ISNULL(MODALIDAD_MOVIMIENTO, 0 ) MODALIDAD_MOVIMIENTO, 
CODIGO_BENEFICIO,
CONVERT( VARCHAR(8), FECHA_GENERACION, 112) FECHA_GENERACION,
PERIODO_GENERADO

----------------------------------------
--ANIO_RECAUDACIONES, 
--MES_RECAUDACIONES, 
--CUOTA_RECAUDACIONES, 
--SUCURSAL,
--ASIENTO

FROM (
----CREDEB MOVIMIENTOS
SELECT 
	@fecha_hoy AS FECHA_GENERACION,
	F.ASIENTO AS ASIENTO,
	F.FECHA_PROCESO_ASIENTO AS FECHA_PROCESO,
	F.SUCURSAL_ASIENTO AS SUCURSAL, 
	concat(YEAR(M.FECHACONTABLE) , RIGHT(CONCAT('0',MONTH(M.FECHACONTABLE)), 2)) PERIODO_GENERADO,
	YEAR(M.FECHACONTABLE) AS ANIO_RECAUDACIONES,
	MONTH(M.FECHACONTABLE) AS MES_RECAUDACIONES,
	CASE
		WHEN DAY(M.FECHACONTABLE) &lt;= 7 THEN 1
		WHEN DAY(M.FECHACONTABLE) &lt;= 15 THEN 2
		WHEN DAY(M.FECHACONTABLE) &lt;= 22 THEN 3
		ELSE 4
	END AS CUOTA_RECAUDACIONES,
	CASE WHEN RPII.NUM1 IS NULL THEN
		RPI.NUM1 
	ELSE
		RPII.NUM1 
	END AS CODIGO_REGIMEN, 
	CASE WHEN DOC.TIPODOCUMENTO IN ('CUIL','CUIL') THEN
			86
		WHEN DOC.TIPODOCUMENTO='CDI' THEN
			87
		ELSE
			80
	END AS TIPO_DOCUMENTO,
	DOC.NUMERODOCUMENTO AS NUMERO_DOCUMENTO,
	V.CTA_CBU AS CBU,
	CASE		
		WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
			THEN
				(FL.MONTO_FACTURABLE)		
			ELSE			
				(FL.MONTO_FACTURABLE) * ISNULL(MI.TIPO_CAMBIO_MOV, 0)			
	END AS IMPORTE_IMPUESTO,
	CASE		
		WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
		THEN
			(FL.MONTO_IMPONIBLE)		
		ELSE			
			(FL.MONTO_IMPONIBLE) * ISNULL(MI.TIPO_CAMBIO_MOV, 0)			
	END AS BASE_IMPONIBLE,
	isnull(MM.MODALIDAD_MOVIMIENTO,0) AS MODALIDAD_MOVIMIENTO, --COMO PODEMOS LLEGAR A LA MODALIDAD?
	CASE WHEN BF.CODIGO_BENEFICIO IS NULL THEN 
		0
	ELSE
		BF.CODIGO_BENEFICIO
	END	AS CODIGO_BENEFICIO
		
FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK) 
	
INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND F.ASIENTO = GDC.MOV_ASIENTO 
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)) 
	
INNER JOIN CI_FACTURA_LINEA AS FL WITH (nolock) ON	
	FL.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO	
	AND FL.ASIENTO = F.ASIENTO	
	AND FL.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO	
	AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 6
			AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)))) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 6
			AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))))
	)
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  )	

	
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
 	AND M.SUCURSAL = F.SUCURSAL_ASIENTO
	AND M.ASIENTO = F.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO = FECHAPROCESO
			AND M.SUCURSAL = SUCURSAL
			AND M.ASIENTO = ASIENTO
			AND M.DEBITOCREDITO = DEBITOCREDITO)
			
LEFT JOIN RRII_CREDEB_MODALIDAD_MOVIMIENTO MM WITH (NOLOCK) ON
	MM.COD_TRANSACCION = M.COD_TRANSACCION

INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO = M.ASIENTO AND
	A.ESTADO = 77 AND
	A.SUCURSAL = M.SUCURSAL AND
	A.FECHAPROCESO = M.FECHAPROCESO

INNER JOIN VTA_SALDOS AS V WITH (nolock) ON
	V.JTS_OID_SALDO = S.JTS_OID
	AND ((V.tz_lock &lt; 300000000000000 OR V.tz_lock >= 400000000000000)
		AND (V.tz_lock &lt; 100000000000000 OR V.tz_lock >= 200000000000000))

INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803
	AND ((C.tz_lock &lt; 300000000000000 OR C.tz_lock >= 400000000000000)
		AND (C.tz_lock &lt; 100000000000000 OR C.tz_lock >= 200000000000000))

INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND CP.TITULARIDAD = 'T'
	AND ((CP.tz_lock &lt; 300000000000000 OR CP.tz_lock >= 400000000000000)
		AND (CP.tz_lock &lt; 100000000000000 OR CP.tz_lock >= 200000000000000))

LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON
	((DOC.tz_lock &lt; 300000000000000 OR DOC.tz_lock >= 400000000000000)
		AND (DOC.tz_lock &lt; 100000000000000 OR DOC.tz_lock >= 200000000000000))
	AND DOC.NUMEROPERSONAFJ = CP.NUMEROPERSONA	
	AND DOC.TIPODOCUMENTO = (						
						SELECT TOP 1 TIPODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE
							((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000)
								AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))						
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000)
								AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))							
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
						)
-- PARA OBTENER CODIGO ACTIVIDAD CLIENTE
LEFT JOIN VW_ACT_ECO CAE WITH(NOLOCK) ON
	CAE.NUMERO_DOCUMENTO = DOC.NUMERODOCUMENTO AND 
	CAE.COD_ACTIVIDAD = 702010 AND
	CAE.TIPO_ACTIVIDAD = 'Primaria'
						
						
--PARA OBTENER SEGMENTO
INNER JOIN RRII_BITACORA_CREDEB BC WITH(NOLOCK) ON 
	BC.JTS_OID_SALDOS = S.JTS_OID AND
	BC.TIPO_CARGO_IMPOSITIVO = 6 AND
	BC.FECHA = (SELECT MAX(FECHA) FROM RRII_BITACORA_CREDEB 
			WHERE FECHA &lt;= M.FECHACONTABLE AND
				JTS_OID_SALDOS = BC.JTS_OID_SALDOS AND
				TIPO_CARGO_IMPOSITIVO = 6)
				
LEFT JOIN RRI_PARAMETROS_INF RPII WITH(NOLOCK) ON
	RPII.CODIGO=505 AND
	RPII.ID1 = CAE.COD_ACTIVIDAD AND
		(RPII.ID2 = FL.ID_CARGO OR
			RPII.ID2 = FL.ID_IMPUESTO) AND
	--Puede pertenecer a bf y a Mp
	RPII.ID5 = (CASE WHEN (BC.PADRON_BENEF_FISCAL='S' AND BC.PADRON_MiPyme='S') THEN
				'BM'
			WHEN BC.PADRON_BENEF_FISCAL='S' THEN
				'BF'
			WHEN BC.PADRON_MiPyme='S' THEN 
				'MP'
			ELSE
				'X'
			END)

			
LEFT JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON
	RPI.CODIGO = 502 AND
	RPI.ID1 = BC.SEGMENTO AND
		(RPI.ID2 = FL.ID_CARGO OR
			RPI.ID2 = FL.ID_IMPUESTO) AND
	--Puede pertenecer a bf y a Mp
	RPI.ID5 = (CASE WHEN (BC.PADRON_BENEF_FISCAL='S' AND BC.PADRON_MiPyme='S') THEN
				'BM'
			WHEN BC.PADRON_BENEF_FISCAL='S' THEN
				'BF'
			WHEN BC.PADRON_MiPyme='S' THEN 
				'MP'
			ELSE
				'X'
			END)
			
LEFT JOIN ITF_BEN_FISCAL_CREDEB BF WITH(NOLOCK) ON 
	BF.CUIT = DOC.NUMERODOCUMENTO AND
	BF.CBU = V.CTA_CBU AND
	BF.SEGMENTO = BC.SEGMENTO AND
	BF.FECHA_VIG = (SELECT MAX(FECHA_VIG) 
					FROM ITF_BEN_FISCAL_CREDEB WITH(NOLOCK) 
					WHERE CUIT = BF.CUIT AND
						CBU = BF.CBU AND
						SEGMENTO = BF.SEGMENTO AND
						FECHA_VIG &lt;= M.FECHACONTABLE)

						
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MI WITH (nolock) ON	
	MI.ASIENTO = F.ASIENTO	
	AND MI.SUCURSAL = F.SUCURSAL_ASIENTO	
	AND MI.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO AND
	MI.TIPO_CAMBIO_MOV!=0 AND
	MI.ASIENTO_MOV= (SELECT MAX(ASIENTO_MOV) FROM CON_MOVIMIENTOS_IMPUESTOS_ME WITH(NOLOCK) 
						WHERE ASIENTO=MI.ASIENTO 
							AND SUCURSAL=MI.SUCURSAL 
							AND FECHAPROCESO=MI.FECHAPROCESO 
							AND TIPO_CAMBIO_MOV=MI.TIPO_CAMBIO_MOV) AND
	((MI.tz_lock &lt; 300000000000000 OR MI.tz_lock >= 400000000000000)
		AND (MI.tz_lock &lt; 100000000000000 OR MI.tz_lock >= 200000000000000))

LEFT JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO

WHERE
	((GDC.tz_lock &lt; 300000000000000 OR GDC.tz_lock >= 400000000000000) 
	AND (GDC.tz_lock &lt; 100000000000000 OR GDC.tz_lock >= 200000000000000))
	AND YEAR(M.FECHACONTABLE) = YEAR(@fecha_parametro)
	AND MONTH(M.FECHACONTABLE) = MONTH(@fecha_parametro)
		
UNION ALL

--CREDEB MOVIMIENTOS EXTORNADOS
SELECT 
	@fecha_hoy AS FECHA_GENERACION,
	F.ASIENTO AS ASIENTO,
	F.FECHA_PROCESO_ASIENTO AS FECHA_PROCESO,
	F.SUCURSAL_ASIENTO AS SUCURSAL, 
	concat(YEAR(M.FECHACONTABLE) , RIGHT(CONCAT('0',MONTH(M.FECHACONTABLE)), 2)) PERIODO_GENERADO,
	YEAR(M.FECHACONTABLE) AS ANIO_RECAUDACIONES,
	MONTH(M.FECHACONTABLE) AS MES_RECAUDACIONES,
	CASE
		WHEN DAY(M.FECHACONTABLE) &lt;= 7 THEN 1
		WHEN DAY(M.FECHACONTABLE) &lt;= 15 THEN 2
		WHEN DAY(M.FECHACONTABLE) &lt;= 22 THEN 3
		ELSE 4
	END AS CUOTA_RECAUDACIONES,
	CASE WHEN RPII.NUM1 IS NULL THEN
		RPI.NUM1 
	ELSE
		RPII.NUM1 
	END AS CODIGO_REGIMEN,
	CASE WHEN DOC.TIPODOCUMENTO IN ('CUIL','CUIL') THEN
			86
		WHEN DOC.TIPODOCUMENTO='CDI' THEN
			87
		ELSE
			80
	END AS TIPO_DOCUMENTO,
	DOC.NUMERODOCUMENTO AS NUMERO_DOCUMENTO,
	V.CTA_CBU AS CBU,
	CASE		
		WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
			THEN
				(-1)*ABS(FL.MONTO_FACTURABLE)		
			ELSE			
				(-1)*ABS(FL.MONTO_FACTURABLE) * ISNULL(MI.TIPO_CAMBIO_MOV, 0)			
	END AS IMPORTE_IMPUESTO,
	CASE		
		WHEN F.MONEDA = (SELECT TOP 1 C6399 FROM MONEDAS WITH (nolock) WHERE C6403 = 'N' AND TZ_LOCK = 0)
		THEN
			(-1)*ABS(FL.MONTO_IMPONIBLE)
		ELSE			
			(-1)*ABS(FL.MONTO_IMPONIBLE) * ISNULL(MI.TIPO_CAMBIO_MOV, 0)			
	END AS BASE_IMPONIBLE,
	isnull(MM.MODALIDAD_MOVIMIENTO,0) AS MODALIDAD_MOVIMIENTO, --COMO PODEMOS LLEGAR A LA MODALIDAD?
	CASE WHEN BF.CODIGO_BENEFICIO IS NULL THEN 
		0
	ELSE
		BF.CODIGO_BENEFICIO
	END	AS CODIGO_BENEFICIO
		
FROM GRL_DETALLE_CONTABILIDAD GDC WITH(NOLOCK) 
	
INNER JOIN SALDOS S WITH(NOLOCK) ON
	GDC.SALDO_ORIG_SUCURSAL = S.SUCURSAL
	AND GDC.SALDO_ORIG_MONEDA = S.MONEDA
	AND GDC.SALDO_ORIG_PRODUCTO = S.PRODUCTO
	AND GDC.SALDO_ORIG_OPERACION = S.OPERACION
	AND GDC.SALDO_ORIG_ORDINAL = S.ORDINAL
	AND GDC.SALDO_ORIG_CUENTA = S.CUENTA
	AND ( (S.tz_lock &lt; 300000000000000 OR S.tz_lock >= 400000000000000) 
	AND (S.tz_lock &lt; 100000000000000 OR S.tz_lock >= 200000000000000)  )

INNER JOIN CI_FACTURA F WITH(NOLOCK) ON 
	F.SUCURSAL_ASIENTO = GDC.MOV_SUCURSAL
	AND F.JTS_SALDO_CLIENTE = S.JTS_OID 
	AND F.ASIENTO = GDC.MOV_ASIENTO 
	AND F.FECHA_PROCESO_ASIENTO = GDC.MOV_FECHA_PROCESO
	AND ( (F.tz_lock &lt; 300000000000000 OR F.tz_lock >= 400000000000000) 
	AND (F.tz_lock &lt; 100000000000000 OR F.tz_lock >= 200000000000000)) 
	
INNER JOIN CI_FACTURA_LINEA AS FL WITH (nolock) ON	
	FL.SUCURSAL_ASIENTO = F.SUCURSAL_ASIENTO	
	AND FL.ASIENTO = F.ASIENTO	
	AND FL.FECHA_PROCESO_ASIENTO = F.FECHA_PROCESO_ASIENTO	
	AND ((FL.ID_IMPUESTO IN (SELECT ID_IMPUESTO FROM CI_IMPUESTOS WITH (nolock) WHERE TIPO_IMPUESTO = 6
			AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000)))) 	
		OR (FL.ID_CARGO IN (SELECT ID_CARGO FROM CI_CARGOS WITH (nolock) WHERE TIPO_CARGO_IMPOSITIVO = 6
			AND ((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000) AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))))
	)
	AND F.TR_REAL = FL.TR_REAL
	AND F.TR_VIRTUAL = FL.TR_VIRTUAL
	AND F.TR_TIPO = FL.TR_TIPO
	AND (GDC.CARGOS_ID_CARGO=FL.ID_CARGO 
		OR GDC.CARGOS_ID_IMPUESTO=FL.ID_IMPUESTO)
	AND ( (FL.tz_lock &lt; 300000000000000 OR FL.tz_lock >= 400000000000000) 
	AND (FL.tz_lock &lt; 100000000000000 OR FL.tz_lock >= 200000000000000)  )	
	
INNER JOIN CON_ASIENTOS_EXTORNADOS AS AE WITH (nolock) ON	
	AE.ASIENTO_ORIGINAL = F.ASIENTO	
	AND AE.SUCURSAL_ORIGINAL = F.SUCURSAL_ASIENTO	
	AND AE.FECHAPROCESO_ORIGINAL = F.FECHA_PROCESO_ASIENTO

	
INNER JOIN MOVIMIENTOS_CONTABLES M WITH(NOLOCK) ON 
	M.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO
	AND M.SUCURSAL = F.SUCURSAL_ASIENTO
	AND M.ASIENTO = F.ASIENTO
	AND M.DEBITOCREDITO = GDC.MOV_SIGNO
	AND M.JTS_OID = (SELECT TOP 1 JTS_OID FROM MOVIMIENTOS_CONTABLES WITH (NOLOCK) 
		WHERE M.FECHAPROCESO = FECHAPROCESO
			AND M.SUCURSAL = SUCURSAL
			AND M.ASIENTO = ASIENTO
			AND M.DEBITOCREDITO = DEBITOCREDITO)

LEFT JOIN RRII_CREDEB_MODALIDAD_MOVIMIENTO MM WITH (NOLOCK) ON
	MM.COD_TRANSACCION = M.COD_TRANSACCION

INNER JOIN ASIENTOS A WITH(NOLOCK) ON
	A.ASIENTO = M.ASIENTO AND
	A.ESTADO = 77 AND
	A.SUCURSAL = M.SUCURSAL AND
	A.FECHAPROCESO = M.FECHAPROCESO

INNER JOIN VTA_SALDOS AS V WITH (nolock) ON
	V.JTS_OID_SALDO = S.JTS_OID
	AND ((V.tz_lock &lt; 300000000000000 OR V.tz_lock >= 400000000000000)
		AND (V.tz_lock &lt; 100000000000000 OR V.tz_lock >= 200000000000000))
		
INNER JOIN CLI_CLIENTES AS C WITH (nolock) ON	
	C.CODIGOCLIENTE = S.C1803
	AND ((C.tz_lock &lt; 300000000000000 OR C.tz_lock >= 400000000000000)
		AND (C.tz_lock &lt; 100000000000000 OR C.tz_lock >= 200000000000000))

INNER JOIN CLI_ClientePersona AS CP WITH (nolock) ON	
	CP.CODIGOCLIENTE = C.CODIGOCLIENTE	
	AND CP.TITULARIDAD = 'T'
	AND ((CP.tz_lock &lt; 300000000000000 OR CP.tz_lock >= 400000000000000)
		AND (CP.tz_lock &lt; 100000000000000 OR CP.tz_lock >= 200000000000000))

LEFT JOIN CLI_DOCUMENTOSPFPJ AS DOC WITH (nolock) ON
	((DOC.tz_lock &lt; 300000000000000 OR DOC.tz_lock >= 400000000000000)
		AND (DOC.tz_lock &lt; 100000000000000 OR DOC.tz_lock >= 200000000000000))
	AND DOC.NUMEROPERSONAFJ = CP.NUMEROPERSONA	
	AND DOC.TIPODOCUMENTO = (						
						SELECT TOP 1 TIPODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE
							((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000)
								AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))						
							AND	NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ						
						ORDER BY TIPODOCUMENTO						
						)
	AND DOC.NUMERODOCUMENTO = (						
						SELECT TOP 1 NUMERODOCUMENTO						
						FROM CLI_DocumentosPFPJ WITH (nolock)						
						WHERE							
							((tz_lock &lt; 300000000000000 OR tz_lock >= 400000000000000)
								AND (tz_lock &lt; 100000000000000 OR tz_lock >= 200000000000000))							
							AND NUMEROPERSONAFJ = DOC.NUMEROPERSONAFJ							
							AND TIPODOCUMENTO = DOC.TIPODOCUMENTO						
						ORDER BY NUMERODOCUMENTO						
						)
-- PARA OBTENER CODIGO ACTIVIDAD CLIENTE
LEFT JOIN VW_ACT_ECO CAE WITH(NOLOCK) ON
	CAE.NUMERO_DOCUMENTO = DOC.NUMERODOCUMENTO AND 
	CAE.COD_ACTIVIDAD = 702010 AND
	CAE.TIPO_ACTIVIDAD = 'Primaria'
						
						
--PARA OBTENER SEGMENTO
INNER JOIN RRII_BITACORA_CREDEB BC WITH(NOLOCK) ON 
	BC.JTS_OID_SALDOS = S.JTS_OID AND
	BC.TIPO_CARGO_IMPOSITIVO = 6 AND
	BC.FECHA = (SELECT MAX(FECHA) FROM RRII_BITACORA_CREDEB 
			WHERE FECHA &lt;= M.FECHACONTABLE AND
				JTS_OID_SALDOS = BC.JTS_OID_SALDOS AND
				TIPO_CARGO_IMPOSITIVO = 6)
				
LEFT JOIN RRI_PARAMETROS_INF RPII WITH(NOLOCK) ON
	RPII.CODIGO=505 AND
	RPII.ID1 = CAE.COD_ACTIVIDAD AND
		(RPII.ID2 = FL.ID_CARGO OR
			RPII.ID2 = FL.ID_IMPUESTO) AND
	--Puede pertenecer a bf y a Mp
	RPII.ID5 = (CASE WHEN (BC.PADRON_BENEF_FISCAL='S' AND BC.PADRON_MiPyme='S') THEN
				'BM'
			WHEN BC.PADRON_BENEF_FISCAL='S' THEN
				'BF'
			WHEN BC.PADRON_MiPyme='S' THEN 
				'MP'
			ELSE
				'X'
			END)

			
LEFT JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) ON
	RPI.CODIGO = 502 AND
	RPI.ID1 = BC.SEGMENTO AND
		(RPI.ID2 = FL.ID_CARGO OR
			RPI.ID2 = FL.ID_IMPUESTO) AND
	--Puede pertenecer a bf y a Mp
	RPI.ID5 = (CASE WHEN (BC.PADRON_BENEF_FISCAL='S' AND BC.PADRON_MiPyme='S') THEN
				'BM'
			WHEN BC.PADRON_BENEF_FISCAL='S' THEN
				'BF'
			WHEN BC.PADRON_MiPyme='S' THEN 
				'MP'
			ELSE
				'X'
			END)

LEFT JOIN ITF_BEN_FISCAL_CREDEB BF WITH(NOLOCK) ON 
	BF.CUIT = DOC.NUMERODOCUMENTO AND
	BF.CBU = V.CTA_CBU AND
	BF.SEGMENTO = BC.SEGMENTO AND
	BF.FECHA_VIG = (SELECT MAX(FECHA_VIG) 
					FROM ITF_BEN_FISCAL_CREDEB WITH(NOLOCK) 
					WHERE CUIT = BF.CUIT AND
						CBU = BF.CBU AND
						SEGMENTO = BF.SEGMENTO AND
						FECHA_VIG &lt;= M.FECHACONTABLE)
						
LEFT JOIN CON_MOVIMIENTOS_IMPUESTOS_ME AS MI WITH (nolock) ON	
	MI.ASIENTO = F.ASIENTO	
	AND MI.SUCURSAL = F.SUCURSAL_ASIENTO	
	AND MI.FECHAPROCESO = F.FECHA_PROCESO_ASIENTO AND
	MI.TIPO_CAMBIO_MOV!=0 AND
	MI.ASIENTO_MOV= (SELECT MAX(ASIENTO_MOV) FROM CON_MOVIMIENTOS_IMPUESTOS_ME WITH(NOLOCK) 
						WHERE ASIENTO=MI.ASIENTO 
							AND SUCURSAL=MI.SUCURSAL 
							AND FECHAPROCESO=MI.FECHAPROCESO 
							AND TIPO_CAMBIO_MOV=MI.TIPO_CAMBIO_MOV) AND
	((MI.tz_lock &lt; 300000000000000 OR MI.tz_lock >= 400000000000000)
		AND (MI.tz_lock &lt; 100000000000000 OR MI.tz_lock >= 200000000000000))


WHERE
	((GDC.tz_lock &lt; 300000000000000 OR GDC.tz_lock >= 400000000000000) 
	AND (GDC.tz_lock &lt; 100000000000000 OR GDC.tz_lock >= 200000000000000))
	AND AE.ASIENTO_ORIGINAL IS NOT NULL AND
	MONTH(AE.FECHAPROCESO_ORIGINAL) = MONTH(@fecha_parametro) AND
	AE.FECHAPROCESO BETWEEN dateadd(day, 1, eomonth(@fecha_parametro, -1)) AND @fecha_hoy
	
) xd GROUP BY CODIGO_REGIMEN, TIPO_DOCUMENTO, NUMERO_DOCUMENTO, CBU, MODALIDAD_MOVIMIENTO, CODIGO_BENEFICIO, FECHA_GENERACION, PERIODO_GENERADO</sql>
