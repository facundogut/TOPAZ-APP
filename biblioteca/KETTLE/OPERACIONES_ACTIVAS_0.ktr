    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT 
	S.JTS_OID AS JTS_OID,
    FORMAT(GSD.FECHA, 'ddMMyyyy') AS fecha,
    MD.suc_producto AS Sucursal,
    CASE WHEN MD.tipo_doc IN ('CUIT', 'CUIL', 'CDI') THEN 11 ELSE 99 END AS TD,
    MD.num_doc AS ND,
    0 AS 'TOP',
    MD.nombre_apellido AS NOM,
    MD.tipo_garantia_desc AS CGA,
    MD.tipo_gtia_bcra AS TGA,
    CASE WHEN M.C6399 NOT IN (1, 2) THEN 3 ELSE M.C6399 END AS CMO,
    MD.saldo_capital AS CAP,
    MD.int_deven_cobrar_cont AS INTC,
    MD.int_deven_cobrar_cont AS INTDP,
    MD.prevision AS PRE,
    MD.clasificacion AS CLA,
    MD.fecha_1ervto_impago AS FEC,
    MD.dias_atraso AS ATR,
    MD.capital_original AS CORI,
    MD.fecha_alta_oper AS FORI,
    MD.int_adelantados AS IDOC,
    MD.monto_acuerdo_cc AS TACUER,
    MD.fecha_alta_ac_cc AS FACUER,
    MD.plazo_ac_cc AS PACUER,
    MD.dias_consec_sdo_deud AS DSD,
    MD.operacion AS NOP,
    0 AS CODLIN,
    MD.admin_tarj_cred AS ADMTC,
	0 AS FULTC,
    -- Obtener fecha de ?ltimo pago directamente en la consulta principal
    (SELECT MAX(FECHA_ULT_PAGO) 
       FROM TJC_SALDOS_RRII WITH (NOLOCK) 
      WHERE FECHA_GENERACION &lt; GSD.FECHA
        AND USUARIO = S.CUENTA 
        AND TZ_LOCK = 0) AS FUP,
    -- Obtener IUP directamente en la consulta principal
    (SELECT TOP 1 SALDO_MN + (SALDO_ME * (SELECT TOP 1 tcventacomun 
                                          FROM MONEDAS WITH (NOLOCK)  
                                          WHERE C6399 = 2 
                                             AND TZ_LOCK = 0))
       FROM TJC_HISTORICO_PAGOS WITH (NOLOCK) 
      WHERE FECHA = (SELECT MAX(FECHA_ULT_PAGO) 
                       FROM TJC_SALDOS_RRII WITH (NOLOCK)  
                      WHERE FECHA_GENERACION &lt; GSD.FECHA 
                        AND USUARIO = S.CUENTA 
                        AND TZ_LOCK = 0)
        AND USUARIO = S.CUENTA) AS IUP,
    MC.categoria_cliente AS TCART,    
    GSD.RUBROCONTABLE AS CCIC,
    0 AS OCID,
    0 AS CC36,
    0 AS TPG,	    
    MD.clasificacion AS CLAS6,
    0 AS OCDP,
    0 AS OASP,
    0 AS CLAAB,
    '000' AS CUABO
    , 0 AS VALTAS
    , 0 AS CLAIN
    , 0 AS CODCRED
    , 0 AS LIM
    , 0 AS FECLIM
    , 0 AS CTANALI
    , 0 AS CODES
    , 0 AS PAGMIN
    , 0 AS METORI
FROM 
    SALDOS S WITH (NOLOCK)
    INNER JOIN GRL_SALDOS_DIARIOS GSD WITH (NOLOCK) 
        ON S.JTS_OID = GSD.SALDOS_JTS_OID 
		AND GSD.FECHA= try_CONVERT(DATE, ?, 103)
        AND GSD.tz_lock NOT BETWEEN 100000000000000 AND 200000000000000 
        AND GSD.tz_lock NOT BETWEEN 300000000000000 AND 400000000000000
    INNER JOIN MEMO_DETALLE MD WITH (NOLOCK) 
        ON MD.Fecha_Info = GSD.FECHA 
        AND md.SALDO_JTS_OID=s.JTS_OID
        AND MD.Cliente = S.C1803
   	INNER JOIN MEMO_CABECERA MC WITH (NOLOCK) 
   		ON MC.Fecha_Info=MD.Fecha_Info
   	    AND MC.Cliente=MD.Cliente 
    INNER JOIN MONEDAS M WITH (NOLOCK) 
        ON S.MONEDA = M.C6399 
        AND M.C6399 = MD.Cod_Moneda
        AND M.tz_lock NOT BETWEEN 100000000000000 AND 200000000000000 
        AND M.tz_lock NOT BETWEEN 300000000000000 AND 400000000000000
    INNER JOIN PRODUCTOS PRD WITH (NOLOCK) 
        ON S.PRODUCTO = PRD.C6250 
        AND PRD.C6252 IN (5, 6) 
        AND PRD.C6255 != 0 
       AND PRD.tz_lock NOT BETWEEN 100000000000000 AND 200000000000000 
       AND PRD.tz_lock NOT BETWEEN 300000000000000 AND 400000000000000
WHERE S.C1785 IN (5, 6) 
    -- AND DATEDIFF(DAY, S.C1621, S.C1627) > 0
    AND S.tz_lock NOT BETWEEN 100000000000000 AND 200000000000000 
    AND S.tz_lock NOT BETWEEN 300000000000000 AND 400000000000000 </sql>
