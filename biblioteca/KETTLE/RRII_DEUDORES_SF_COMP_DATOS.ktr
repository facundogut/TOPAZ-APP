    <quoteString>N</quoteString>
    <sql>INSERT INTO dbo.DEUDORES_4306_COMP
SELECT 
TIPO_ID,
NRO_ID,
TIPO_ASISTENCIA,
OPERACION,
MAX(CPA_NUEVO) AS CPA_NUEVO,
MAX(COD_MONEDA) AS COD_MONEDA,
CASE WHEN TIPO_ASISTENCIA IN (1,10) THEN CONVERT(DATETIME,?,103) 
WHEN TIPO_ASISTENCIA NOT IN (1,10) AND MAX(Fecha_1erVto_Impago) IS NOT NULL THEN MAX(Fecha_1erVto_Impago)
ELSE MAX(Fecha_Alta_Oper) END AS FECHA_ALTA,
CASE WHEN TIPO_ASISTENCIA IN (1,2,10) THEN SUM(DEUDA_TOTAL) ELSE SUM(CAPITAL_ORIGINAL) END AS CAPITAL_ORIGINAL,
SUM(DEUDA_TOTAL) AS DEUDA_TOTAL,
CASE WHEN MAX(PLAZO) = 0 THEN 1 ELSE MAX(PLAZO) END AS PLAZO,
MAX(TIPO_TASA) AS TIPO_TASA,
CASE WHEN MAX(TIPO_TASA) = 3 THEN 'OTRAS' ELSE '' END AS DESC_TASA,
MAX(FRECUENCIA_TASA) AS FRECUENCIA_TASA,
MAX(TNA) AS TNA,
----MAX(CFT_TNA) 
MAX(TNA) AS CFT_TNA, 
MAX(SIST_AMORTIZACION) AS SIST_AMORTIZACION,
CASE WHEN MAX(SIST_AMORTIZACION) = 4 THEN 'OTROS' ELSE '' END DESC_SIST_AMORTIZACION,
MAX(FREC_AMORT) AS FREC_AMORT,
CASE WHEN MAX(FREC_AMORT) = 5 THEN 'OTROS' ELSE '' END AS DESC_FREC_AMORT,
SUM(VENC_GTIA_A) AS VENC_GTIA_A,
SUM(DEUDA_GTIA_A) AS DEUDA_GTIA_A,
SUM(VENC_GTIA_B) AS VENC_GTIA_B,
SUM(DEUDA_GTIA_B) AS DEUDA_GTIA_B,
SUM(INTVENC_GTIA_B) AS INTVENC_GTIA_B,
SUM(INT_GTIA_B) AS INT_GTIA_B,
SUM(VENC_SINGTIA) AS VENC_SINGTIA,
SUM(DEUDA_SINGTIA) AS DEUDA_SINGTIA,
SUM(INTVENC_SINGTIA) AS INTVENC_SINGTIA,
SUM(INT_SINGTIA) AS INT_SINGTIA,
CASE WHEN TIPO_ASISTENCIA IN (1,10) THEN CONVERT(DATETIME,?,103) 
WHEN TIPO_ASISTENCIA NOT IN (1,10) AND MAX(Fecha_1erVto_Impago) IS NOT NULL THEN MAX(Fecha_1erVto_Impago)
ELSE MAX(Fecha_Alta_Oper) END AS FECHA_1ER_VTOIMP,
MAX(FCAMBIOESTADOSUSPENSO) AS FCAMBIOESTADOSUSPENSO,
----MAX(TNA_ORIGINAL) 
MAX(TNA) AS TNA_ORIGINAL,
CASE WHEN TIPO_ASISTENCIA IN (1,10) THEN CONVERT(DATETIME,?,103) 
WHEN TIPO_ASISTENCIA NOT IN (1,10) AND MAX(Fecha_1erVto_Impago) IS NOT NULL THEN MAX(Fecha_1erVto_Impago)
WHEN TIPO_ASISTENCIA NOT IN (1,10) AND MAX(Fecha_Alta_Oper) >= MAX(FECHA_TASA_VAR)THEN MAX(Fecha_Alta_Oper) 
ELSE MAX(FECHA_TASA_VAR) END AS FECHA_TASA_VAR,
FECHA_PROCESO,
MAX(JTS_OID)
FROM (
SELECT DISTINCT
DE.TIPO_ID,
DE.NRO_ID,
CASE 
WHEN CPI.TIPO_ASISTENCIA IS NOT NULL THEN CPI.TIPO_ASISTENCIA
ELSE 99 END AS TIPO_ASISTENCIA,
CASE 
WHEN S.C1785 = 2 THEN REPLICATE('0',13-LEN(MD.CUENTA_CLI)) + CONVERT(VARCHAR,MD.CUENTA_CLI) + 'C'
WHEN CONVERT(NUMERIC,P.C6826) = 1 AND S.C1665 = 0 THEN REPLICATE('0',14-(LEN(MD.OPERACION)+LEN(MD.DESGLOSE))) + CONVERT(VARCHAR,MD.OPERACION) + CONVERT(VARCHAR,MD.DESGLOSE)
WHEN CONVERT(NUMERIC,P.C6826) = 1 AND S.C1665 &lt;&gt; 0 THEN REPLICATE('0',13-LEN(SC.CUENTA)) + CONVERT(VARCHAR,SC.CUENTA) + 'C'
WHEN P.C6800 = 'T' THEN REPLICATE('0',2) + CONVERT(VARCHAR,CASE WHEN S.ORDINAL = 1 THEN 36
WHEN S.ORDINAL = 2 THEN 20 WHEN S.ORDINAL = 3 THEN 54 WHEN S.ORDINAL = 8 THEN 11 END) + REPLICATE('0',7) + M.C6402
ELSE (CASE WHEN CPI.TIPO_ASISTENCIA IS NOT NULL THEN CONVERT(VARCHAR,CPI.TIPO_ASISTENCIA) ELSE '99' END) + REPLICATE('0',14-(LEN(CASE WHEN CPI.TIPO_ASISTENCIA IS NOT NULL THEN CONVERT(VARCHAR,CPI.TIPO_ASISTENCIA) ELSE '99' END)+LEN(MD.OPERACION)+LEN(MD.DESGLOSE))) + CONVERT(VARCHAR,MD.OPERACION) + CONVERT(VARCHAR,MD.DESGLOSE)
END AS OPERACION,

DIR.CPA_NUEVO,
M.C6402 AS COD_MONEDA,
MD.Fecha_Alta_Oper,
ROUND((MD.Capital_Original)/1000,0) AS CAPITAL_ORIGINAL,
ROUND((MD.saldo_capital + MD.int_deven_cobrar_cont + MD.otros_accesorios + MD.iva_financiado + MD.rg_financiado + MD.seguro_financ)/1000,0) AS DEUDA_TOTAL,

CASE WHEN P.C6252 = 5 AND ROUND(DATEDIFF(MM,MD.FECHA_ALTA_OPER, MD.FECHA_VTO),0) >= 1 THEN ISNULL(ROUND(DATEDIFF(MM,MD.FECHA_ALTA_OPER,MD.FECHA_VTO),0),0) 
WHEN P.C6252 = 5 AND ROUND(DATEDIFF(MM,MD.FECHA_ALTA_OPER,MD.FECHA_VTO),0) = 0 THEN 1
ELSE ROUND(DATEDIFF(MM,MD.FECHA_ALTA_OPER,CONVERT(DATETIME,?,103)),0) END AS PLAZO,
CASE 
WHEN ISNULL(MD.TASA_NOM_ANUAL,0) = 0 THEN 3
WHEN S.C1690 = 'F' THEN 1
WHEN S.C1690 = 'V' THEN 2
ELSE 3 END AS TIPO_TASA,
CASE WHEN S.C1690 = 'F' OR S.C1690 = 'V' THEN ''
WHEN S.C1690 = 'M' THEN 'Tasa Mixta' 
ELSE '' END AS DESCRIPCION_TASA,
----S.C1643 AS FRECUENCIA_TASA,
CASE WHEN S.C1690 = 'F' THEN 0 ELSE 0 END AS FRECUENCIA_TASA,
CASE 
WHEN ISNULL(MD.TASA_NOM_ANUAL,0) = 0 THEN 0.01 
ELSE CONVERT(NUMERIC(5,2), MD.TASA_NOM_ANUAL) END AS TNA,
CASE WHEN ISNULL(MD.CFT_TNA,0) = 0 THEN 0.01 ELSE CONVERT(NUMERIC(5,2), MD.CFT_TNA) END AS CFT_TNA,
CASE WHEN P.C6259 = 'F' THEN 1
WHEN P.C6259 = 'H' THEN 2
WHEN P.C6259 = 'Q' THEN 3
WHEN P.C6259 = ' ' OR P.C6259 IS NULL THEN 9
ELSE 4 END AS SIST_AMORTIZACION,
CASE WHEN P.C6259 = 'F' THEN 'Francés'
WHEN P.C6259 = 'H' THEN 'Alemán'
WHEN P.C6259 = 'Q' THEN 'Pago íntegro de capital e intereses al vencimiento'
WHEN P.C6259 = ' ' OR P.C6259 IS NULL THEN 'No aplicable'
ELSE 'Otros' END AS DESC_SIST_AMORTIZACION,
CASE WHEN S.C1642/30 = 1 THEN 1
WHEN S.C1642/30 = 2 THEN 2
WHEN S.C1642/30 = 3 THEN 3
WHEN S.C1642/30 = 6 THEN 4
WHEN S.C1642 = 0 OR S.C1642 IS NULL THEN 9
ELSE 5 END AS FREC_AMORT,
CASE WHEN S.C1642/30 = 1 THEN 'Mensual'
WHEN S.C1642/30 = 2 THEN 'Bimestral'
WHEN S.C1642/30 = 3 THEN 'Trimestral'
WHEN S.C1642/30 = 6 THEN 'Semestral'
WHEN S.C1642 = 0 OR S.C1642 IS NULL THEN 'No aplicable'
ELSE 'Otros' END AS DESC_FREC_AMORT,
CASE WHEN MD.TIPO_GTIA_BCRA = 2 AND D.SITUACION NOT IN (1, 11,21,22, 23,5) THEN ROUND((MD.CAPITAL_VENCIDO + ISNULL(HD.ICMORA_DEVEN_VIGENTE_CONT,0))/1000,0) ELSE 0 END AS VENC_GTIA_A,
CASE WHEN MD.TIPO_GTIA_BCRA = 2 THEN ROUND((MD.saldo_capital + MD.int_deven_cobrar_cont + MD.iva_financiado + MD.rg_financiado + MD.seguro_financ)/1000,0) ELSE 0 END AS DEUDA_GTIA_A,

CASE WHEN MD.TIPO_GTIA_BCRA = 1 AND D.SITUACION NOT IN (1, 11,21,22, 23,5) THEN ROUND((MD.CAPITAL_VENCIDO + ISNULL(HD.ICMORA_DEVEN_VIGENTE_CONT,0))/1000,0) ELSE 0 END AS VENC_GTIA_B,
CASE WHEN MD.TIPO_GTIA_BCRA = 1 THEN ROUND((MD.saldo_capital + MD.iva_financiado + MD.rg_financiado + MD.seguro_financ)/1000,0) ELSE 0 END AS DEUDA_GTIA_B,

0 AS INTVENC_GTIA_B,
0 AS INT_GTIA_B, 

CASE WHEN (MD.Tipo_Gtia_BCRA IS NULL OR MD.Tipo_Gtia_BCRA = 0) AND D.SITUACION NOT IN (1, 11,21,22, 23,5) THEN ROUND((MD.Capital_Vencido + ISNULL(HD.ICMORA_DEVEN_VIGENTE_CONT,0))/1000,0) ELSE 0 END AS VENC_SINGTIA,
CASE WHEN (MD.Tipo_Gtia_BCRA IS NULL OR MD.Tipo_Gtia_BCRA = 0) THEN ROUND((MD.saldo_capital + MD.int_deven_cobrar_cont + MD.otros_accesorios + MD.iva_financiado + MD.rg_financiado + MD.seguro_financ)/1000,0) ELSE 0 END AS DEUDA_SINGTIA,
0 AS INTVENC_SINGTIA,
0 AS INT_SINGTIA,

CASE WHEN MD.Fecha_1erVto_Impago &lt;= CONVERT(DATETIME,?,103) THEN MD.Fecha_1erVto_Impago ELSE NULL END AS Fecha_1erVto_Impago,
S.FCAMBIOESTADOSUSPENSO, ---S.C1813,
CASE WHEN ISNULL(MD.TNA_Original,0) = 0 THEN 0.01 ELSE CONVERT(NUMERIC(5,2), MD.TNA_Original) END AS TNA_ORIGINAL,
ISNULL(HIST.FECHA, MD.Fecha_Alta_Oper) AS FECHA_TASA_VAR,
CONVERT(DATETIME,?,103) AS FECHA_PROCESO,
S.JTS_OID
FROM MEMO_DETALLE MD WITH(NOLOCK)
INNER JOIN MEMO_CABECERA MC WITH(NOLOCK) ON MC.CUIT_CUIL_CDI = MD.NUM_DOC AND MC.Fecha_Info = MD.Fecha_Info AND MC.TIPO_PERSONA = 'J'
INNER JOIN DEUDORES_4306 DE WITH(NOLOCK) ON DE.NRO_ID = MD.Num_Doc
INNER JOIN DEUDORES_4305 D WITH(NOLOCK) ON D.NRO_ID = CONVERT(VARCHAR,MD.Num_Doc) AND D.FECHA_PROCESO = MD.Fecha_Info
INNER JOIN PRODUCTOS P WITH(NOLOCK) ON P.C6250 = MD.Producto
INNER JOIN CLI_DIRECCIONES DIR WITH(NOLOCK) ON DIR.ID = MD.Suc_Producto 
AND DIR.FORMATO = 'S' AND DIR.TIPODIRECCION = 'R'
INNER JOIN MONEDAS M WITH(NOLOCK) ON M.C6399 = MD.Cod_Moneda
LEFT JOIN CRE_PROD_INSTNBCH CPI WITH(NOLOCK) ON CPI.PRODUCTO = MD.PRODUCTO AND CPI.TIPO_INST_NBCH = (CASE WHEN EXISTS (
SELECT 1 FROM CRE_PROD_INSTNBCH with(nolock) WHERE PRODUCTO = MD.PRODUCTO AND TIPO_INST_NBCH = ISNULL(MD.TIPO_INST_NBCH, 0)) 
THEN ISNULL(MD.TIPO_INST_NBCH, 0) ELSE 0 END)
LEFT JOIN SALDOS S WITH(NOLOCK) ON S.JTS_OID = MD.SALDO_JTS_OID
LEFT JOIN SALDOS SC WITH(NOLOCK) ON SC.JTS_OID = S.C1665
LEFT JOIN (SELECT MAX(FECHAPROCESOMOV) AS FECHA, SALDOS_JTS_OID FROM BS_HISTORIA_PLAZO WITH(NOLOCK)
WHERE TIPOMOV = 'S' AND TZ_LOCK = 0 GROUP BY SALDOS_JTS_OID) HIST ON HIST.SALDOS_JTS_OID = S.JTS_OID
LEFT JOIN HISTORICO_DEVENGAMIENTO HD WITH(NOLOCK) ON HD.SALDO_JTS_OID = MD.SALDO_JTS_OID AND HD.FECHA = MD.Fecha_Info
WHERE MD.Fecha_Info = CONVERT(DATETIME,?,103) AND DE.FECHA_PROCESO = CONVERT(DATETIME,?,103)
AND (CASE 
WHEN CPI.TIPO_ASISTENCIA IS NOT NULL THEN CPI.TIPO_ASISTENCIA
ELSE 99 END) = DE.TIPO_ASIST_CRED
AND DE.TIPO_ASIST_CRED != 31
AND MC.CUIT_CUIL_CDI NOT IN (SELECT CUIL FROM CLI_MAESTRO_FALLECIDOS WITH(NOLOCK) WHERE TZ_LOCK = 0 AND ESTADO = 'P'
AND FECHA_ALTA &lt;= CONVERT(DATETIME,?,103)) 
) T
GROUP BY
TIPO_ID,
NRO_ID,
TIPO_ASISTENCIA,
OPERACION,
FECHA_PROCESO
HAVING (SUM(DEUDA_TOTAL)>0)</sql>
