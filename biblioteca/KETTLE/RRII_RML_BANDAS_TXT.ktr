    <rowlimit>0</rowlimit>
    <sql>DECLARE @FECHA_PARAMETRO AS DATETIME;
SET @FECHA_PARAMETRO = CONVERT(DATETIME,?,103);
 
SELECT
(SELECT P.ALFA FROM PARAMETROSGENERALES P WHERE P.CODIGO=787) AS EMPRESA,
CONVERT(VARCHAR(8), EOMONTH(S.FECHA), 112) AS FECHA_INFORMACION,
CONVERT(VARCHAR(8), CONVERT(DATETIME, @FECHA_PARAMETRO, 103), 112) AS FECHA_INFORME,
(SELECT RPF.CHAR1 FROM RRI_PARAMETROS_INF RPF WHERE RPF.CODIGO=1 AND RPF.ID1=S.MONEDA AND RPF.TZ_LOCK = 0) AS MONEDA,
CASE 
  WHEN (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, @FECHA_PARAMETRO, 103))) &lt;= 29 THEN
    RPI.NUM2*10 + 1
  WHEN (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, @FECHA_PARAMETRO, 103))) >= 30 AND (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, '31/08/2024', 103))) &lt;= 59 THEN
    RPI.NUM2*10 + 2
  WHEN (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, @FECHA_PARAMETRO, 103))) >= 60 AND (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, '31/08/2024', 103))) &lt;= 89 THEN
    RPI.NUM2*10 + 3
  WHEN (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, @FECHA_PARAMETRO, 103))) >= 90 AND (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, '31/08/2024', 103))) &lt;= 179 THEN
    RPI.NUM2*10 + 4
  WHEN (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, @FECHA_PARAMETRO, 103))) >= 180 AND (convert(NUMERIC(10), SS.C1627)) - (convert(NUMERIC(10), CONVERT(DATETIME, '31/08/2024', 103))) &lt;= 365 THEN
    RPI.NUM2*10 + 5
  ELSE
    RPI.NUM2*10 + 6
  END AS PARTIDA_RML,
S.RUBROCONTABLE AS PARTIDA_RML_Y_EMENT,
isnull(BA.PARTIDA,0) AS PARTIDA_ATM,
isnull(B12.PARTIDA, 0) AS PARTIDA_B12,
isnull(BM.PARTIDA, 0) AS PARTIDA_Mipymes,
isnull((SUM(BA.DISPENSADO) + sum(B12.SALDO) + sum(BM.SALDO)), 0) AS SALDO_RML,
CASE 
      WHEN S.MONEDA&lt;&gt;2 
      THEN SUM(S.SALDO_AJUSTADO * ISNULL(H1.ARBITRAJE_COMPRA,0))
      WHEN S.MONEDA=999 
      THEN SUM(S.SALDO_AJUSTADO * ISNULL(H2.TIPO_CAMBIO_COMPRA,0))                           
      ELSE S.SALDO_AJUSTADO
END AS SALDO_DIARIO_RML
 
FROM 
GRL_SALDOS_DIARIOS S WITH(NOLOCK)
LEFT JOIN SALDOS SS WITH(NOLOCK) ON SS.SUCURSAL=S.SUCURSAL
                                 AND SS.MONEDA=S.MONEDA
                                 AND SS.JTS_OID = S.SALDOS_JTS_OID
                                 AND SS.TZ_LOCK=0
INNER JOIN RRI_PARAMETROS_INF RPI WITH(NOLOCK) 
                 ON RPI.ID1 = SS.PRODUCTO 
                 AND RPI.TZ_LOCK = 0
INNER JOIN RRI_PARAMETROS_DEF RPD WITH(NOLOCK) 
                 ON RPD.CODIGO = RPI.CODIGO 
                 AND RPD.CODIGO = 207 
                 AND RPD.TZ_LOCK = 0
LEFT JOIN HISTORICOTIPOSCAMBIO H1 WITH(NOLOCK) ON 
  H1.MONEDA= S.MONEDA AND 
  H1.FECHA_COTIZACION = (SELECT TOP 1 FECHA_COTIZACION 
              FROM HISTORICOTIPOSCAMBIO WITH(NOLOCK) 
              WHERE FECHA_COTIZACION&lt;=S.FECHA
                AND TZ_LOCK=0
              ORDER BY H1.FECHA_COTIZACION DESC) AND
  H1.TZ_LOCK=0
LEFT JOIN HISTORICOTIPOSCAMBIO H2 WITH(NOLOCK) ON 
  H2.MONEDA= S.MONEDA AND 
  H2.FECHA_COTIZACION = (SELECT TOP 1 FECHA_COTIZACION 
              FROM HISTORICOTIPOSCAMBIO WITH(NOLOCK) 
              WHERE FECHA_COTIZACION&lt;=SS.C1620
                AND TZ_LOCK=0
              ORDER BY H2.FECHA_COTIZACION DESC) AND
  H2.TZ_LOCK=0
 
LEFT JOIN RML_BANDAS_ATM BA WITH(NOLOCK) ON 
	BA.FECHA = S.FECHA
	AND BA.PARTIDA NOT IN (0)
LEFT JOIN RML_BANDAS_AHORA_12 B12 WITH(NOLOCK) ON
	B12.FECHA = S.FECHA
	AND B12.PARTIDA NOT IN (0)
LEFT JOIN RML_BANDAS_MIPYME BM WITH(NOLOCK) ON
	BM.FECHA = S.FECHA
	AND BM.PARTIDA NOT IN (0)
 
WHERE S.FECHA=@FECHA_PARAMETRO AND 
S.TZ_LOCK=0 AND
SS.C1785 = 4
GROUP BY
S.MONEDA,
S.FECHA,
S.SALDO_AJUSTADO,
S.RUBROCONTABLE,
RPI.NUM2,
SS.C1627,
BA.PARTIDA,
B12.PARTIDA,
BM.PARTIDA</sql>
