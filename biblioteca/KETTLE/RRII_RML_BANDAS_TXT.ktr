        <jsScript_script>function padIzquierda(value, length) {
    var str = String(value || ""); 
    while (str.length &lt; length) {
        str = '0' + str;
    }
    return str;
}

function padLeftWithZeros(value, length) {
    if (value == null) return padIzquierda("", length);
    
    var str = String(value);
    if (str[0] === '-') {
        var numericPart = str.substr(1);
        return '-' + padIzquierda(numericPart, length - 1);
    } else {
        return padIzquierda(str, length);
    }
}


function rellenarDerecha(texto, longitudTotal) {
   
    texto = texto != null ? String(texto) : "";
    if (texto.length >= longitudTotal) {
        return texto;
    }
    var espacios = "";
    for (var i = 0; i &lt; (longitudTotal - texto.length); i++) {
        espacios += " ";
    }
    return texto + espacios;
}





