        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>if (COELSAEC_CODIGO == 200) {
  if (TIPO_CONSULTA == 'C' &amp;&amp; COELSAEC_CODIGO_COELSA == 22 &amp;&amp; COELSAEC_CUENTA_ESTADO == 'ACTIVA') {
