  } else if (TIPO_CONSULTA == 'C' &amp;&amp; (COELSAEC_CODIGO_COELSA != 22 || (COELSAEC_CODIGO_COELSA == 22 &amp;&amp; COELSAEC_CUENTA_ESTADO == 'INACTIVA'))){
      CODIGO_RETORNO = 11
  } else if (TIPO_CONSULTA == 'A' &amp;&amp; (COELSAEC_CODIGO_COELSA == 1000 || COELSAEC_CODIGO_COELSA == 1100)) {
      CODIGO_RETORNO = 12 // OK
  } else if (TIPO_CONSULTA == 'A' &amp;&amp; !(COELSAEC_CODIGO_COELSA == 1000 || COELSAEC_CODIGO_COELSA == 1100)){ 
      CODIGO_RETORNO = 13
  } else if (TIPO_CONSULTA == 'B' &amp;&amp; COELSAEC_CODIGO_COELSA == 1200) {
      CODIGO_RETORNO = 14 // OK
  } else if (TIPO_CONSULTA == 'B' &amp;&amp; COELSAEC_CODIGO_COELSA != 1200) {
      CODIGO_RETORNO = 15
  }

  if ([11, 13, 15].indexOf(CODIGO_RETORNO) == -1) {
    estado_MENSAJE = 'OK: '+ COELSAEC_MENSAJE;
  } else {
    estado_MENSAJE = 'ERROR: '+ COELSAEC_MENSAJE;
  }
} else {
	estado_MENSAJE = 'ERROR: '+ COELSAEC_MENSAJE;
}</jsScript_script>
