    <sql>
DECLARE 
	@convPadre VARCHAR (12) = ?,
	@fechaRen VARCHAR(12) = ?;

	
SET @convPadre = CASE WHEN ISNUMERIC(@convPadre) = 1 THEN @convPadre ELSE '-1' END;
SET @fechaRen = CASE WHEN ISDATE(@fechaRen) = 1 THEN @fechaRen ELSE '18000101' END;
	

	SELECT 
	   CASE WHEN count(*) > 0 THEN 1 ELSE 0 END estaRendido,	
	   CASE WHEN @fechaRen = '18000101' THEN 0 ELSE 1 END fRenOk			
	FROM
		REC_RENDICION ren  
			JOIN CONV_CONVENIOS_REC conv ON conv.Id_ConvRec = ren.CONVENIO OR conv.Id_ConvRec = ren.CONVENIO_PADRE				
			JOIN REC_LIQUIDACION liq  ON ren.ID_RENDICION = liq.ID_RENDICION
			WHERE 
				ren.TZ_LOCK = 0 AND 
				conv.TZ_LOCK = 0 AND 
				liq.TZ_LOCK = 0 AND 
				liq.ESTADO = 'R' AND 
				conv.Id_ConvRec = @convPadre AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) AND 
				conv.FORMATO_RENDICION = 	
			(
			SELECT opcioninterna 
			FROM OPCIONES 
			WHERE 
			NUMERODECAMPO = 45146 AND 
			IDIOMA = 'E' AND 
			upper(DESCRIPCION) LIKE '%FORMATO ESTANDAR 2%'
			)
		</sql>
    <outer_join>N</outer_join>
