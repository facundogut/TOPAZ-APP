    <connection>top</connection>
    <sql>DECLARE @convPadre VARCHAR(12) = '${P0_VALUE}';
DECLARE @fechaRen VARCHAR(12) = '${P1_VALUE}';
DECLARE @canal VARCHAR (5) = '${P2_VALUE}';
 

SELECT linea, xd.archivo, canal, fechacarga FROM 
(

SELECT DISTINCT 
cab.archivo,
cab.fechacarga,
	conv.canal,
	concat(
		'0',
		RIGHT(concat('000',CASE WHEN relConv.codigoEmpresa IS NULL THEN relEnte.COD_ENTE ELSE relConv.codigoEmpresa END),3),
		'SECHEEP    ',
		CONVERT(VARCHAR(8),(SELECT FECHAPROCESO FROM PARAMETROS (NOLOCK)),112),
		REPLICATE('0', 56)
	) linea
	FROM CONV_REL_ENTECONV (nolock) relEnte 
		JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
		JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON  conv.Id_ConvRec = ren.convenio --OR conv.Id_ConvPadre = ren.convenio_padre
		LEFT JOIN RelConvPadreNomArchivo (nolock) relConv ON relConv.ConvenioPadre = ren.convenio
		JOIN SUC_FICTICIA_CONV_CANALES (NOLOCK) suc ON suc.CANAL = conv.Canal
			WHERE  
			  	relEnte.ID_CONVREC = @convPadre AND
				ren.ESTADO = 'R' AND  
				cab.tz_lock = 0 AND 
				conv.tz_lock = 0 AND 
				ren.Tz_Lock = 0 AND
				(conv.Canal = @canal OR  @canal = '99') AND 
				ren.FECHA = CAST(@fechaRen AS DATETIME) 

UNION ALL 

SELECT DISTINCT	
cab.archivo,
cab.fechacarga,
	conv.canal,
	concat(
		'0',
		RIGHT(concat('000',CASE WHEN relConv.codigoEmpresa IS NULL THEN relEnte.COD_ENTE ELSE relConv.codigoEmpresa END),3),
		'SECHEEP    ',
		CONVERT(VARCHAR(8),(SELECT FECHAPROCESO FROM PARAMETROS (NOLOCK)),112),
		REPLICATE('0', 56)
	) linea
	
FROM CONV_REL_ENTECONV (nolock) relEnte 
		JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
		JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab ON cab.id_liquidacion = liq.id_liquidacion
		JOIN CONV_CONVENIOS_REC (NOLOCK) conv ON  conv.Id_ConvRec = ren.convenio --OR conv.Id_ConvPadre = ren.convenio_padre
		LEFT JOIN RelConvPadreNomArchivo (nolock) relConv ON relConv.ConvenioPadre = ren.convenio
			WHERE  
		  	conv.Id_ConvRec = @convPadre AND
			ren.ESTADO = 'R' AND  
			cab.tz_lock = 0 AND
			conv.tz_lock = 0 AND 
			ren.Tz_Lock = 0 AND
