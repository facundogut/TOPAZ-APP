    <sql>DECLARE @fecha DATETIME;
SET @fecha = CONVERT(DATETIME,?,103);

DECLARE @anio INT = YEAR(@fecha) - 1;
DECLARE @mes INT = MONTH(@fecha);
DECLARE @ultimo_dia_anio_anterior CHAR(6) = CAST(@anio AS CHAR(4)) + '12';
DECLARE @fecha_anio_mes CHAR(6) = FORMAT(CONVERT(DATE, @fecha), 'yyyyMM');

-- Tabla derivada para los d?as h?biles
WITH UltimoDiaHabil AS (
    SELECT DISTINCT 
    	CONVERT(DATETIME, DBO.ULTIMODIAHABIL(G.FECHA), 103) AS FECHA_HABIL
    FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
    INNER JOIN PLANCTAS P WITH (NOLOCK) ON 
    	P.C6326 = G.RUBROCONTABLE
    WHERE MONTH(G.FECHA) IN (10, 11, 12)
      AND YEAR(G.FECHA) = @anio
      AND G.TZ_LOCK = 0
      AND P.C6304 IN (8, 9)
)
SELECT 
    G.FECHA AS FECHA_DE_INFORME,
    CONVERT(DATETIME, EOMONTH(@fecha), 103) AS FECHA_DE_INFORMACION,
    '20300000' AS CUENTA_ENT,
    '203000' AS CUENTA,
    G.MONEDA,
    SUM(G.SALDO_AJUSTADO_MN) AS SALDO_ACUM,
    0 AS SaldoAcumPGNME,
    0 AS SDO_PROM,
    0 AS SDO_PROM_MON_OR,
    NULL AS SdoPromPGNME,
    0 AS SdoAcumMonOr,
    NULL AS CATEGORIA,
    0 AS COEFACTUALIZACION,
    NULL AS BALANCE_BANDA
FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
INNER JOIN PLANCTAS P WITH (NOLOCK) ON 
	P.C6326 = G.RUBROCONTABLE AND 
	G.TZ_LOCK = P.TZ_LOCK
INNER JOIN MONEDAS M WITH (NOLOCK) ON 
	G.MONEDA = M.C6399 AND 
	G.TZ_LOCK = M.TZ_LOCK
INNER JOIN ESTADO_BALANCE EB WITH (NOLOCK) ON 
	EB.ANIOMES = @ultimo_dia_anio_anterior
INNER JOIN AUDITORIA_BALANCE_MENSUAL ABM WITH (NOLOCK) ON 
	EB.ANIOMES = ABM.ANIOMES
WHERE EB.ABIERTO_CERRADO = 'C'   
	AND G.FECHA IN (SELECT ISNULL(FECHA_HABIL,'18000101') FROM UltimoDiaHabil)
  	AND ABM.AuditadoS_N = 'N'
GROUP BY G.MONEDA,G.FECHA

UNION ALL

SELECT 
    G.FECHA AS FECHA_DE_INFORME,
    CONVERT(DATETIME, EOMONTH(@fecha), 103) AS FECHA_DE_INFORMACION,
    '20400000' AS CUENTA_ENT,
    '204000' AS CUENTA,
    G.MONEDA,
    SUM(G.SALDO_AJUSTADO_MN) AS SALDO_ACUM,
    0 AS SaldoAcumPGNME,
    0 AS SDO_PROM,
    0 AS SDO_PROM_MON_OR,
    NULL AS SdoPromPGNME,
    0 AS SdoAcumMonOr,
    NULL AS CATEGORIA,
    0 AS COEFACTUALIZACION,
    NULL AS BALANCE_BANDA
FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
INNER JOIN PLANCTAS P WITH (NOLOCK) ON 
	P.C6326 = G.RUBROCONTABLE AND 
	G.TZ_LOCK = P.TZ_LOCK
INNER JOIN MONEDAS M WITH (NOLOCK) ON 
	G.MONEDA = M.C6399 AND 
	G.TZ_LOCK = M.TZ_LOCK
INNER JOIN ESTADO_BALANCE EB WITH (NOLOCK) ON 
	EB.ANIOMES = @fecha_anio_mes
INNER JOIN AUDITORIA_BALANCE_MENSUAL ABM WITH (NOLOCK) ON 
	EB.ANIOMES = ABM.ANIOMES
WHERE @mes >= 4
  AND G.FECHA IN (SELECT ISNULL(FECHA_HABIL,'18000101') FROM UltimoDiaHabil)
  AND EB.ABIERTO_CERRADO = 'C'
  AND ABM.AuditadoS_N = 'N'
GROUP BY G.MONEDA,G.FECHA

UNION ALL

SELECT 
    G.FECHA AS FECHA_DE_INFORME,
    CONVERT(DATETIME, EOMONTH(@fecha), 103) AS FECHA_DE_INFORMACION,
    '20500000' AS CUENTA_ENT,
    '205000' AS CUENTA,
    G.MONEDA,
    CASE 
        WHEN SUM(G.SALDO_AJUSTADO_MN) >= 0 THEN SUM(G.SALDO_AJUSTADO_MN) / 2
        ELSE SUM(G.SALDO_AJUSTADO_MN)
    END AS SALDO_ACUM,
    0 AS SaldoAcumPGNME,
    0 AS SDO_PROM,
    0 AS SDO_PROM_MON_OR,
    NULL AS SdoPromPGNME,
    0 AS SdoAcumMonOr,
    NULL AS CATEGORIA,
    0 AS COEFACTUALIZACION,
    NULL AS BALANCE_BANDA
FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
INNER JOIN PLANCTAS P WITH (NOLOCK) ON P.C6326 = G.RUBROCONTABLE AND G.TZ_LOCK = P.TZ_LOCK
INNER JOIN MONEDAS M WITH (NOLOCK) ON G.MONEDA = M.C6399 AND G.TZ_LOCK = M.TZ_LOCK
WHERE P.C6304 IN (8, 9)
  AND G.FECHA IN (SELECT ISNULL(FECHA_HABIL,'18000101') FROM UltimoDiaHabil)
  AND G.TZ_LOCK = 0
GROUP BY G.MONEDA,G.FECHA;
</sql>
