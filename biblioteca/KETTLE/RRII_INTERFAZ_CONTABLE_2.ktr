    <sql>DECLARE @fecha DATETIME;
DECLARE @fechaFinMes DATETIME;
SET @fecha = CONVERT(DATETIME,?,103);
SET @fechaFinMes = EOMONTH(@fecha);

WITH UltimoDiaHabil AS (

	SELECT DISTINCT CONVERT(DATETIME, DBO.ULTIMODIAHABIL(G2.FECHA), 103) AS FECHAHABIL
	FROM GRL_SALDOS_DIARIOS G2 WITH (NOLOCK)
	INNER JOIN PLANCTAS P2 WITH (NOLOCK) ON 
		P2.C6326 = G2.RUBROCONTABLE
	WHERE 
		MONTH(G2.FECHA) IN (10,11,12)
		AND YEAR(G2.FECHA) = YEAR(@fecha) - 1
)

-- Secci?n 204
SELECT 
    G.FECHA AS FECHA_DE_INFORME,
    @fechaFinMes AS FECHA_DE_INFORMACION,
    '20400000' AS CUENTA_ENT,
    '204000' AS CUENTA,
    G.MONEDA AS MONEDA,
    SUM(G.SALDO_AJUSTADO_MN) AS SALDO_ACUM,
    0 AS SaldoAcumPGNME,
    0 AS SDO_PROM,
    0 AS SDO_PROM_MON_OR,
    NULL AS SdoPromPGNME,
    0 AS SdoAcumMonOr,
    0 AS CATEGORIA,
    0 AS COEFACTUALIZACION,
    NULL AS BALANCE_BANDA
FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
INNER JOIN PLANCTAS P WITH (NOLOCK)
    ON P.C6326 = G.RUBROCONTABLE
    AND P.TZ_LOCK = G.TZ_LOCK
    AND P.C6304 IN (8,9)
INNER JOIN MONEDAS M WITH (NOLOCK)
    ON M.C6399 = G.MONEDA
    AND M.TZ_LOCK = G.TZ_LOCK
INNER JOIN ESTADO_BALANCE EB WITH (NOLOCK)
    ON EB.ANIOMES =  FORMAT(CONVERT(DATE, @fecha), 'yyyyMM')
    AND EB.ABIERTO_CERRADO = 'C'
INNER JOIN AUDITORIA_BALANCE_MENSUAL ABM WITH (NOLOCK)
    ON ABM.ANIOMES = EB.ANIOMES
    AND ABM.AuditadoS_N = 'S'
WHERE MONTH(@fecha) >= 4
AND G.FECHA IN (SELECT ISNULL(FECHAHABIL,'18000101') FROM UltimoDiaHabil)
AND G.TZ_LOCK = 0
GROUP BY G.MONEDA,G.FECHA

UNION ALL

-- Secci?n 205
SELECT 
    G.FECHA AS FECHA_DE_INFORME,
    @fechaFinMes AS FECHA_DE_INFORMACION,
    '20500000' AS CUENTA_ENT,
    '205000' AS CUENTA,
    G.MONEDA AS MONEDA,
    CASE 
        WHEN SUM(G.SALDO_AJUSTADO_MN) >= 0 THEN 
            (SUM(G.SALDO_AJUSTADO_MN) / 2)
        ELSE 
            SUM(G.SALDO_AJUSTADO_MN)
    END AS SALDO_ACUM,
    0 AS SaldoAcumPGNME,
    0 AS SDO_PROM,
    0 AS SDO_PROM_MON_OR,
    NULL AS SdoPromPGNME,
    0 AS SdoAcumMonOr,
    0 AS CATEGORIA,
    0 AS COEFACTUALIZACION,
    NULL AS BALANCE_BANDA
FROM GRL_SALDOS_DIARIOS G WITH (NOLOCK)
INNER JOIN PLANCTAS P WITH (NOLOCK)
    ON P.C6326 = G.RUBROCONTABLE
    AND P.TZ_LOCK = G.TZ_LOCK
    AND P.C6304 IN (8,9)
INNER JOIN MONEDAS M WITH (NOLOCK)
    ON M.C6399 = G.MONEDA
    AND M.TZ_LOCK = G.TZ_LOCK
WHERE G.FECHA IN (SELECT ISNULL(FECHAHABIL,'18000101') FROM UltimoDiaHabil)
AND MONTH(@fecha) > 3
AND G.TZ_LOCK = 0
GROUP BY G.MONEDA,G.FECHA;</sql>
