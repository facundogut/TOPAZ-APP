    <rowlimit>0</rowlimit>
    <sql>SELECT DISTINCT 
    CONVERT(VARCHAR,CODIGO_DISENIO) + ';' +
    CASE WHEN ISNULL(INT_DOC_NO_DEV, 0) = 0 THEN '' ELSE CONVERT(VARCHAR,INT_DOC_NO_DEV) END + ';' +
    CASE WHEN ISNULL(TOT_FIN_SALDOS_MEN, 0) = 0 THEN '' ELSE 
	(SELECT CONVERT(VARCHAR,SUM(CASE WHEN CONCEPTO = 2 THEN GAR_PREF_A_CAP_INT + 
	GAR_PREF_B_CAP_INT_NOPREV + SIN_GAR_CAP_INT_NOPREV
	WHEN CONCEPTO = 4 THEN SIN_GAR_CAP_INT_NOPREV ELSE 0 END))
    FROM DEUDORES_4302 WITH(NOLOCK) WHERE 
    FECHA_PROCESO=CONVERT(DATETIME,?,103)) END + ';' +
    ---CONVERT(VARCHAR,CEILING(TOT_FIN_SALDOS_MEN/1000)) END + ';' +
    CASE WHEN ISNULL(TOT_FINANCIACIONES, 0) = 0 THEN '' ELSE 
	CONVERT(VARCHAR,(SELECT SUM(CASE WHEN CONCEPTO = 2 THEN GAR_PREF_A_CAP_INT + 
	GAR_PREF_B_CAP_INT_NOPREV + SIN_GAR_CAP_INT_NOPREV
	WHEN CONCEPTO = 4 THEN SIN_GAR_CAP_INT_NOPREV ELSE 0 END)
    FROM DEUDORES_4302 WITH(NOLOCK) WHERE 
    FECHA_PROCESO=CONVERT(DATETIME,?,103)) + 
	(SELECT SUM(TOT_FINANCIACIONES) FROM DEUDORES_4305 WITH(NOLOCK)
	WHERE FECHA_PROCESO=CONVERT(DATETIME,?,103))) END + ';' +
---CONVERT(VARCHAR,CEILING(TOT_FINANCIACIONES/1000)) END + ';' +
    CONVERT(VARCHAR,SIN_USO) 
    /* + ';' +
    CONVERT(VARCHAR,FECHA_PROCESO,112) + ';' +
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' +
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' */
AS LINEA
FROM DEUDORES_4301 WITH(NOLOCK)
WHERE 
    FECHA_PROCESO=CONVERT(DATETIME,?,103)

UNION ALL

SELECT DISTINCT
    CONVERT(VARCHAR,CODIGO_DISENIO) + ';' +
    /*';' + 
    ';' + 
    ';' + 
    ';' + 
    CONVERT(VARCHAR,FECHA_PROCESO,112) + ';' +
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + 
    ';' + */
    CONVERT(VARCHAR,CONCEPTO) + ';' +
    CONVERT(VARCHAR,CATEGORIA) + ';' +
    CONVERT(VARCHAR,SITUACION) + ';' +
    CASE WHEN ISNULL(GAR_PREF_A_CAP_INT, 0) = 0 OR CONCEPTO = 4 THEN '' ELSE CONVERT(VARCHAR,GAR_PREF_A_CAP_INT) END + ';' +
    CASE WHEN ISNULL(GAR_PREF_B_CAP_INT_NOPREV, 0) = 0 OR CONCEPTO = 4 THEN '' ELSE CONVERT(VARCHAR,GAR_PREF_B_CAP_INT_NOPREV) END + ';' +
    CASE WHEN ISNULL(GAR_PREF_B_INT_PREV, 0) = 0 OR CONCEPTO IN (2,4) THEN '' ELSE CONVERT(VARCHAR,GAR_PREF_B_INT_PREV) END + ';' +
    CASE WHEN ISNULL(SIN_GAR_CAP_INT_NOPREV, 0) = 0 THEN '' ELSE CONVERT(VARCHAR,SIN_GAR_CAP_INT_NOPREV) END + ';' +
    CASE WHEN ISNULL(SIN_GAR_INT_PREV, 0) = 0 OR CONCEPTO IN (2,4) THEN '' ELSE CONVERT(VARCHAR,SIN_GAR_INT_PREV) END + ';' +
    CONVERT(VARCHAR,CANT_DEUDORES) 
FROM DEUDORES_4302 WITH(NOLOCK)
WHERE 
    FECHA_PROCESO=CONVERT(DATETIME,?,103)

UNION ALL

SELECT DISTINCT 
