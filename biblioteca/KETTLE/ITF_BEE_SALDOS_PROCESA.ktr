    <connection>TopazDS</connection>
    <sql>DECLARE @fechaAnterior VARCHAR(8);
SET @fechaAnterior = (SELECT convert(VARCHAR(8), dbo.diaHabil(Dateadd(Day,-1,fechaproceso),'B'),112) FROM PARAMETROS);

DECLARE @fechaProximaHabilUltimoDiaMes VARCHAR(8);
SET @fechaProximaHabilUltimoDiaMes = (SELECT convert(VARCHAR(8), dbo.diaHabil(Dateadd(Day,1,EOMONTH(@fechaAnterior)),'B'),112));

SELECT 
    S.C1785 AS 'TIPO_CUENTA',
	S.MONEDA AS 'MONEDA',
	RIGHT(VS.CTA_REDLINK, 19) AS 'NUMERO_CUENTA',
	VS.CTA_CBU AS 'CBU',
	S.C1624 AS 'FECHA_ULT_MOV',
	@fechaProximaHabilUltimoDiaMes AS 'FECHA_PROX_CAPIT',
	d.SALDO_CON_FECHA_VALOR AS 'SALDO_CON_FECHA_VALOR',
	S.C2627 AS 'SALDO_BLOQUEADO',
	S.C1683 AS 'ACUERDOS',
	s.JTS_OID AS 'JTS_OID',
	@fechaAnterior AS 'FECHA_ANTERIOR'
