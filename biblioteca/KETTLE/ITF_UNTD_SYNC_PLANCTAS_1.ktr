    <sql>DECLARE @fecProceso DATE;
SET @fecProceso = (SELECT fechaproceso FROM PARAMETROS)
 
SELECT codCapCont AS COD_CAPITULO_CONTABLE,
       codRubCont AS COD_RUBRO_CONTABLE,
       codMdaCont AS COD_MONEDA_CONTABLE,
       nroCtaCont AS NRO_CTA_CONTABLE,
       nroSubCtaCont AS NRO_SUBCTA_CONTABLE,
       SUBSTRING(Nombre,1,55) AS NOMBRE_SUBCTA,
       FORMAT(FecVig,'yyyyMMdd') AS FECHA_VIGENCIA_HASTA,
       RUBRO AS SUBCTA_CONTABLE,
       '0' DIG_VERIF_CTA_BCRA,
       FORMAT(@fecProceso,'yyyyMMdd') AS FECHA_PROCESO,
       ' ' AS COD_INGRESO_CTRO_COSTO,       
       Moneda_tabla AS MONEDA_TABLA,       
       'N' AS CTA_POZO
FROM (
    --Mda Nacional
    SELECT SUBSTRING(CONVERT(VARCHAR, A.C6326), 1, 1) AS codCapCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 2, 1) AS codRubCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 3, 1) AS codMdaCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 4, 3) AS nroCtaCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 7, 4) AS nroSubCtaCont,
           A.C6300 AS Nombre,
           CASE WHEN A.c6304 = 'I' THEN @fecProceso ELSE '20991230' END AS FecVig,
           A.C6326 AS RUBRO,
           'N' AS Moneda_tabla
    FROM PLANCTAS A
    WHERE C6302 = 'N' AND A.TZ_LOCK = 0 
    UNION
    --Opera ambos tipos de Mdas
    SELECT SUBSTRING(CONVERT(VARCHAR, A.C6326), 1, 1) AS codCapCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 2, 1) AS codRubCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 3, 1) AS codMdaCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 4, 3) AS nroCtaCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 7, 4) AS nroSubCtaCont,
           A.C6300 AS Nombre,
           CASE WHEN A.c6304 = 'I' THEN @fecProceso ELSE '20991230' END AS FecVig,
           A.C6326 AS RUBRO,           
           ' ' AS Moneda_tabla
           FROM PLANCTAS A
           WHERE C6302 = ' ' AND A.TZ_LOCK = 0 
    UNION
    --Mda Extranjera
    SELECT SUBSTRING(CONVERT(VARCHAR, A.C6326), 1, 1) AS codCapCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 2, 1) AS codRubCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 3, 1) AS codMdaCont,SUBSTRING(CONVERT(VARCHAR, A.C6326), 4, 3) AS nroCtaCont,
           SUBSTRING(CONVERT(VARCHAR, A.C6326), 7, 4) AS nroSubCtaCont,
           A.C6300 AS Nombre,
           CASE WHEN A.c6304 = 'I' THEN @fecProceso ELSE '20991230' END AS FecVig,
           A.C6326 AS RUBRO, 
           'E' AS Moneda_tabla
    FROM PLANCTAS A
    WHERE C6302 = 'E' AND A.TZ_LOCK = 0 
) AS Resultado;</sql>
