    <connection>NBCH</connection>
    <sql>DECLARE @convPadre VARCHAR(5) = (SELECT NUMERICO_1 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 476);
DECLARE @fechaRen VARCHAR(12) = '${P0_VALUE}';

DECLARE @fproc DATETIME = (SELECT fechaproceso FROM PARAMETROS (nolock));


SELECT
@fechaRen FREN,
CASE WHEN isdate(@fechaRen) = 1 AND @fechaRen &lt;= @fproc THEN 1 ELSE 0 END AS fRenOk,
CASE WHEN COUNT(1) > 0 THEN 1 ELSE 0 END enteOk 

	FROM CONV_REL_ENTECONV (nolock) 
	WHERE id_convrec = @convPadre AND cod_ente IN ('FA0','855') 
</sql>
