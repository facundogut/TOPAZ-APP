    <rowlimit>0</rowlimit>
    <sql>
DECLARE @convPadre VARCHAR(5) = (SELECT NUMERICO_1 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 476);
DECLARE @convHijo VARCHAR(5) = (SELECT NUMERICO_2 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 476);
DECLARE @fechaRen VARCHAR(12) = '${P0_VALUE}';

DECLARE @fproc DATETIME = (SELECT fechaproceso FROM PARAMETROS (nolock));


IF (isdate(@fechaRen) = 0) begin SET @fechaRen = '18000101'; end --si fecha desde o fecha hasta es invalida la seteo asi para q no de error al castear

SELECT 
	CASE WHEN count(1) > 0 THEN 1 ELSE 0 END  as cant_rendidos,
	concat('${Top.Output}CONVENIOS/RMRCIA/B', right(concat('000',@convPadre),3), RIGHT(@fechaRen,4)) nomRep
FROM CONV_REL_ENTECONV (nolock) relEnte
		RIGHT JOIN REC_RENDICION (NOLOCK) ren ON relEnte.ID_CONVREC = ren.CONVENIO OR relEnte.ID_CONVREC = ren.CONVENIO_PADRE
		JOIN CONV_CONVENIOS_REC conv  (NOLOCK) ON conv.Id_ConvRec = ren.convenio --OR relEnte.ID_CONVREC = conv.Id_ConvPadre		
		JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.id_rendicion = liq.id_rendicion
			WHERE  
			 	(ren.CONVENIO_PADRE = @convPadre OR ren.CONVENIO = @convHijo) AND
				ren.ESTADO = 'R' AND 
				ren.Tz_Lock = 0 AND
				ren.FECHA = CAST(@fechaRen AS DATETIME) </sql>
