        <jsScript_script>//Script here
COD_ERROR = 0;
ERROR = null;




// Validaciones

if ( EXISTE_CABEZAL == 1 )
	setError(10, 'Lote ya procesado');

else if ( CONVENIO_EXISTE == null )
	setError(11, 'No existe Convenio');

else if ( CONVENIO_DEBAUT != 1 )
	setError(12, 'Convenio no es Debito Automatico');

else if ( ESTADO_CONVENIO != 'A' )
	setError(13, 'Estado del convenio distinto de Activo');

else if ( DIA_CIERRE_VALIDO == 0 )
	setError(14, 'El convenio tiene un dia de cierre inválido');

else if ( CONVENIO_CORRECTO == null )
	setError(15, 'Convenio no relacionado a empresa');

else if ( CANT_REG_STAGE == 0 )
	setError(16, 'Lote sin ordenes individuales');

//else if ( CANTIDAD_CONVENIOS_STAGE == 1 &amp;&amp; Number(CONVENIO_STAGE) != Number(CONVENIO) )
//	setError(17, 'Convenio del lote distinto al informado en la solicitud');

else if ( CANTIDAD_PERIODOS_STAGE != 1 )
	setError(18, 'Se encontraron inconsistencias en los períodos informados en el lote');

else if ( isNaN(PERIODO_STAGE) == true )
	setError(19, 'Período no numérico');

else if ( PERIODO_VALIDO == 0)
	setError(20, 'Período inválido');

else if ( Number(PERIODO_STAGE) &lt; Number(date2str(FECHATOP, 'yyyyMM')))
	setError(21, 'Período menor al año y mes de la fecha de proceso');

else if ( Number(CANTIDAD_REGISTROS_PARAM) != Number(CANT_REG_STAGE) )
	setError(37, 'Cantidad de ordenes incorrecta');

else if ( row[ rowMeta.indexOfValue("SUMA_IMPORTES_PARAM") ].compareTo( row[ rowMeta.indexOfValue("SUM_IMPORTE_STAGE") ] ) != 0 )
	setError(38, 'Sumatoria de importes incorrecta');

if (FECHA_CORTE_CARGADA == false) {
	setVariable("FECHACORTE", FECHA_CORTE, 'r');
	FECHA_CORTE_CARGADA = true;
}
</jsScript_script>
