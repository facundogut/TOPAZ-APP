    <sql>SELECT
	COUNT(*) AS 'CANT_REG_STAGE',
	SUM(importe) AS 'SUM_IMPORTE_STAGE',
	COUNT(DISTINCT convenio) AS 'CANTIDAD_CONVENIOS_STAGE',
	COUNT(DISTINCT periodo) AS 'CANTIDAD_PERIODOS_STAGE',
	MAX(periodo) AS 'PERIODO_STAGE',
	MAX(convenio) AS 'CONVENIO_STAGE'
FROM OrdenDebitoAutomaticoLote
WHERE idLote = '${ID_LOTE}'</sql>
