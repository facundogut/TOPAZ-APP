  <step>
    <name>Convenio existe? Corresponde a la empresa?</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>DECLARE
@cuit NUMERIC(15,0) = ?,
@ccu NUMERIC(15,0) = ?,
@periodo VARCHAR(6) = ?;

DECLARE
@periodo_valido INT = ISDATE(@periodo + '01');

DECLARE
@ultimo_dia_mes DATE = (
	CASE 
		WHEN @periodo_valido = 1 THEN EOMONTH(@periodo + '01')
		ELSE NULL
	END
);

SELECT 
	TOP 1
	ccr.Id_ConvRec AS CONVENIO_EXISTE, -- Nulo => Convenio inexistente | No Nulo => Convenio existe
	ct.TpoContrato AS CONVENIO_DEBAUT, -- Distinto de 1 => Convenio no es debito automatico | Igual a 1 => Convenio es debito automatico
	CASE
		WHEN ccr.Cuit = @cuit THEN Id_ConvRec
		ELSE NULL
	END AS CONVENIO_CORRECTO, -- Nulo => El convenio es orden de débito pero no corresponde a la empresa | No Nulo => El convenio es orden de débito y es de la empresa
	Estado AS ESTADO_CONVENIO,
	@periodo_valido AS PERIODO_VALIDO,
	CASE
		WHEN (DiaCierreDA >= 1 AND DiaCierreDA &lt;= 28) OR DiaCierreDA = 99 THEN 1
		ELSE 0
	END AS DIA_CIERRE_VALIDO,
	CASE 
		WHEN @periodo_valido = 1 AND ((DiaCierreDA >= 1 AND DiaCierreDA &lt;= 28) OR DiaCierreDA = 99) THEN dbo.diaHabil(
			CAST(
				(CASE 
					WHEN DiaCierreDA = 99 THEN @ultimo_dia_mes
					ELSE (@periodo + FORMAT(DiaCierreDA, '00'))
				END) AS DATE
			), 
			'D'
		)
		ELSE NULL
	END AS FECHA_CORTE
FROM CONV_CONVENIOS_REC ccr 
LEFT JOIN CONV_TIPOS ct ON ct.Id_TpoConv = ccr.Id_TpoConv AND ct.TZ_LOCK = 0
WHERE ccr.Id_ConvRec = @ccu AND ccr.TZ_LOCK = 0</sql>
