    <sql>DECLARE 
@convenio INT = '${P0_VALUE}', 
@fecha VARCHAR(10) = '${P1_VALUE}',
@ente VARCHAR(11);

SET @fecha = (SELECT CASE WHEN isdate(@fecha) = 0 THEN '18000101' ELSE @fecha END );
SET @ente = (SELECT TOP 1 cod_ente FROM CONV_REL_ENTECONV WHERE ID_CONVREC = @convenio AND tz_lock = 0);

DROP TABLE IF EXISTS #ITF_RP_REPORTE_SECHEEP
DROP TABLE IF EXISTS #ITF_IMPUESTOS_REC_SECHEEP
TRUNCATE TABLE ITF_RP_REPORTE_SECHEEP
TRUNCATE TABLE ITF_RP_REPORTE_SECHEEP_SUBTOT

SELECT * INTO #ITF_RP_REPORTE_SECHEEP FROM 
(
SELECT DISTINCT  
	cab.id,
	ren.asiento_rendicion, 
	ren.NOMBRE_CONVENIO_PADRE AS empresa,
	convert(VARCHAR(10), p.fechaproceso, 120) as fechaProceso,
	conv.NomConvRec AS nombreConvenio,
	concat('T',RIGHT(concat('000',@convenio), 3), RIGHT(convert(VARCHAR(8), ren.FECHA,112),4), '.txt') AS nombreArchivo, 
	convert(VARCHAR(10), CAST(ren.FECHA AS DATE), 120) AS fechaRendicion,
	(SELECT cuenta FROM SALDOS (nolock) WHERE JTS_OID = 
			(SELECT cuentarec FROM CONV_CONVENIOS_REC (nolock) WHERE id_convrec = @convenio)) AS cuentaRecaudacion,
	--------------------------
	convert(VARCHAR(10), cab.FECHA_COBRANZA, 120)  AS fechaRecaudacion,
	ren.TOTALIMPORTE AS totalRecaudado,
	ren.TOTALREGISTROS AS cantComprobantes,
	ren.IMPORTE_COMISION AS impComision,
	--desglose impuestos
	isnull((CASE WHEN @ente IN ('406','265') THEN ren.TOTAL_CARGO_ESPECIFICO ELSE 0 END),0) AS cargoEspecifico,	
	
    (ren.TOTALIMPORTE - ren.IMPORTE_COMISION - ren.IMPORTE_IMPUESTOS - isnull((CASE WHEN @ente IN ('406','265') THEN ren.TOTAL_CARGO_ESPECIFICO ELSE 0 END),0)) AS importeRendido
	
	FROM PARAMETROS p,  REC_RENDICION (NOLOCK) ren 
	JOIN MOVIMIENTOS_CONTABLES (NOLOCK) mc ON REN.ASIENTO_RENDICION = MC.ASIENTO AND REN.SUCURSAL_RENDICION = MC.SUCURSAL AND REN.FECHA = MC.FECHAPROCESO
	JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.ID_RENDICION = liq.ID_RENDICION
	JOIN CONV_CONVENIOS_REC  (NOLOCK) conv ON ren.CONVENIO = conv.Id_ConvRec -- OR A.CONVENIO_PADRE = B.Id_ConvPadre
	JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab  ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
	LEFT JOIN CONV_REL_ENTECONV (NOLOCK) relEnte  ON relente.id_convrec = conv.Id_ConvRec --ren.convenio_padre = relente.id_convrec OR ren.convenio= relente.id_convrec
	WHERE 
				(conv.Id_ConvPadre = @convenio OR conv.Id_ConvRec = @convenio) 
		  	AND ren.FECHA = CAST(@fecha AS DATE) 			
	     	AND conv.Estado = 'A'
	     	AND cab.TZ_LOCK = 0 
	        AND liq.TZ_LOCK = 0 
	        AND ren.TZ_LOCK = 0 
	        AND conv.TZ_LOCK = 0 
	        
UNION ALL 


SELECT DISTINCT 
	cab.id,
	ren.asiento_rendicion, 
	ren.NOMBRE_CONVENIO_PADRE AS empresa,
	convert(VARCHAR(10), p.fechaproceso, 120) as fechaProceso,
	conv.NomConvRec AS nombreConvenio,
	concat('T',RIGHT(concat('000',@convenio), 3), RIGHT(convert(VARCHAR(8), ren.FECHA,112) ,4), '.txt') AS nombreArchivo, 
   	convert(VARCHAR(10), CAST(ren.FECHA AS DATE), 120) AS fechaRendicion,
   	(SELECT cuenta FROM SALDOS (nolock) WHERE JTS_OID = 
			(SELECT cuentarec FROM CONV_CONVENIOS_REC (nolock) WHERE id_convrec = @convenio)) AS cuentaRecaudacion,
	--------------------------
	convert(VARCHAR(10), cab.FECHACARGA, 120)  AS fechaRecaudacion,
	ren.TOTALIMPORTE AS totalRecaudado,
	ren.TOTALREGISTROS AS cantComprobantes,
	ren.IMPORTE_COMISION AS impComision,
	isnull((CASE WHEN @ente IN ('406','265') THEN ren.TOTAL_CARGO_ESPECIFICO ELSE 0 END),0) AS cargoEspecifico,
		
	(ren.TOTALIMPORTE - ren.IMPORTE_COMISION - ren.IMPORTE_IMPUESTOS - isnull((CASE WHEN @ente IN ('406','265') THEN ren.TOTAL_CARGO_ESPECIFICO ELSE 0 END),0)) AS importeRendido
				   
			FROM PARAMETROS p, REC_RENDICION (NOLOCK) ren
			JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.ID_RENDICION = liq.ID_RENDICION
			JOIN CONV_CONVENIOS_REC  (NOLOCK) conv ON ren.CONVENIO = conv.Id_ConvRec -- OR A.CONVENIO_PADRE = B.Id_ConvPadre
			JOIN REC_CAB_RECAUDOS_CAJA (NOLOCK) cab  ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
			LEFT JOIN CONV_REL_ENTECONV (NOLOCK) relEnte  ON relente.id_convrec = conv.Id_ConvRec--ren.convenio_padre = relente.id_convrec OR ren.convenio= relente.id_convrec
     	    
		    WHERE 
		    		(conv.Id_ConvPadre = @convenio OR conv.Id_ConvRec = @convenio) 
		  	   AND ren.FECHA = CAST(@fecha AS DATE) 
		  	   	AND conv.Estado = 'A'
		     	AND cab.TZ_LOCK = 0 
		        AND liq.TZ_LOCK = 0 
		        AND ren.TZ_LOCK = 0 
		        AND conv.TZ_LOCK = 0 
) xd        
	               
	        
--TRAIGO IMPUESTOS

select 
	CONCEPTO, sum(CAPITALREALIZADO) CAPITALREALIZADO, MC.ASIENTO
	INTO #ITF_IMPUESTOS_REC_SECHEEP
FROM MOVIMIENTOS_CONTABLES MC 
JOIN ASIENTOS A on A.ASIENTO = mc.ASIENTO and mc.FECHAPROCESO = a.FECHAPROCESO and mc.SUCURSAL = a.SUCURSAL
JOIN REC_RENDICION REN ON  REN.ASIENTO_RENDICION = mc.ASIENTO and mc.FECHAPROCESO = REN.FECHA and mc.SUCURSAL = REN.SUCURSAL_RENDICION
LEFT OUTER JOIN CONV_CONVENIOS_REC CONV on CAST(RIGHT(A.DESCRIPCION, 3) AS INT) = Id_ConvRec
WHERE 
--mc.OPERACION = 2651 and -- NUMERO DE OPERACION
a.ESTADO = 77 AND -- ESTADO APROBADO
cuenta = (SELECT cuenta FROM SALDOS (nolock) WHERE JTS_OID = 
			(SELECT cuentarec FROM CONV_CONVENIOS_REC (nolock) WHERE id_convrec = @convenio)) AND-- CUENTA DEL CLIENTE
 capitalrealizado > 0 and
 mc.FECHAPROCESO = @fecha AND
 REN.FECHA = @fecha  ---//parametro entrada
GROUP BY CONCEPTO,  MC.ASIENTO

--CARGO EN TABLA AUX PARA JASPER

INSERT INTO ITF_RP_REPORTE_SECHEEP
SELECT 
empresa,
fechaProceso,
nombreConvenio,
nombreArchivo, 
fechaRendicion,
cuentaRecaudacion,
fechaRecaudacion,
totalRecaudado,
cantComprobantes,
impComision,
cargoEspecifico,
importeRendido,
--desglose impuestos
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'Credito por Convenio Recaudado' AND ASIENTO = a.asiento_rendicion ), 0) AS 'Credito por Convenio Recaudado',
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'IMPUESTO AL CRÉDITO LEY 25413' AND ASIENTO = a.asiento_rendicion ), 0) AS 'IMPUESTO AL CRÉDITO LEY 25413' ,
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'RETENCION DE IVA' AND ASIENTO = a.asiento_rendicion ), 0) AS 'RETENCION DE IVA' ,
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'CANTIDAD DE MOVIMIENTOS CONVEN' AND ASIENTO = a.asiento_rendicion ), 0) AS 'CANTIDAD DE MOVIMIENTOS CONVEN' ,
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'IVA sobre comisiones' AND ASIENTO = a.asiento_rendicion ), 0) AS  'IVA sobre comisiones',
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'Impuesto al Débito Ley 25413' AND ASIENTO = a.asiento_rendicion ), 0) AS 'Impuesto al Débito Ley 25413',
isnull((SELECT capitalrealizado FROM #ITF_IMPUESTOS_REC_SECHEEP WHERE CONCEPTO = 'Impuesto al Débito Ley 25413 I' AND ASIENTO = a.asiento_rendicion ), 0) AS 'Impuesto al Débito Ley 25413 I'

 FROM #ITF_RP_REPORTE_SECHEEP a
 
 
INSERT INTO ITF_RP_REPORTE_SECHEEP_SUBTOT
SELECT   
	conv.NomConvRec AS nombreConvenio,
	conv.id_convrec,
	ren.asiento_rendicion,
	convert(VARCHAR(10), DET.FECHA_COBRANZA, 120)  AS fechaRecaudacion,
    SUM(det.IMPORTE) AS totalRecaudado,
	COUNT(det.ID_CABEZAL) AS cantComprobantes,
	---COMISIONES
	SUM(det.IMPORTE)*(SELECT tasa FROM PARAMETROSGENERALES (NOLOCK) WHERE codigo = 788) AS comisiones,
	SUM(det.IMPORTE)*(SELECT tasa FROM PARAMETROSGENERALES (NOLOCK) WHERE codigo = 788)*(SELECT tasa FROM PARAMETROSGENERALES (NOLOCK) WHERE codigo = 789)  AS IVAcomision,
	SUM(det.IMPORTE)*(SELECT tasa FROM PARAMETROSGENERALES (NOLOCK) WHERE codigo = 790)  AS retencion
	
	 
	FROM   REC_RENDICION (NOLOCK) ren 
	JOIN REC_LIQUIDACION (NOLOCK) liq ON ren.ID_RENDICION = liq.ID_RENDICION
	JOIN CONV_CONVENIOS_REC  (NOLOCK) conv ON ren.CONVENIO = conv.Id_ConvRec -- OR A.CONVENIO_PADRE = B.Id_ConvPadre
	JOIN REC_CAB_RECAUDOS_CANAL (NOLOCK) cab  ON cab.ID_LIQUIDACION = liq.ID_LIQUIDACION
	JOIN REC_DET_RECAUDOS_CANAL (NOLOCK) det ON det.id_cabezal = cab.id
	LEFT JOIN CONV_REL_ENTECONV (NOLOCK) relEnte  ON relente.id_convrec = conv.Id_ConvRec --ren.convenio_padre = relente.id_convrec OR ren.convenio= relente.id_convrec
	WHERE 
				(conv.Id_ConvPadre = @convenio OR conv.Id_ConvRec = @convenio) 
		  		AND ren.FECHA = CAST(@fecha AS DATE) 	   
	     	AND conv.Estado = 'A'
	     	AND cab.TZ_LOCK = 0 
	        AND liq.TZ_LOCK = 0 
	        AND ren.TZ_LOCK = 0 
	        AND conv.TZ_LOCK = 0 
	        AND conv.id_convrec IN (500,574,620)
GROUP BY 	conv.id_convrec, 	ren.asiento_rendicion, conv.NomConvRec ,DET.FECHA_COBRANZA

DROP TABLE IF EXISTS #ITF_RP_REPORTE_SECHEEP
DROP TABLE IF EXISTS #ITF_IMPUESTOS_REC_SECHEEP</sql>
