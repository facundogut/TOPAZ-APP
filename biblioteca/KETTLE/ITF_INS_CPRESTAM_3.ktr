  <step>
    <name>Defino ruta Archivo</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT CONCAT('${Top.Output}INSSSEP/CPRESTAM/','INSSSEP',?,'.txt') as 'RutaArchivo',
       CONCAT('INSSSEP',?,'.txt') as 'archivo',
isnull((
SELECT sum(PP.C2309+PP.C2310+PP.C2311+PP.C2312)
FROM CRE_SALDOS CS
    JOIN SALDOS S ON S.JTS_OID = CS.SALDOS_JTS_OID
    JOIN PLANPAGOS PP ON PP.SALDO_JTS_OID = S.JTS_OID AND PP.TZ_LOCK=0
    WHERE CS.ID_CONVENIO = (SELECT numerico_1 FROM ITF_MASTER_PARAMETROS WHERE CODIGO = 500) --joaco lo cambio el 28/05/2024, porque estaba harcodeado en 313 y le parecio que no estaba correcto
      AND CS.JURISDICCION = 16
      AND S.C1785 = 5
      AND (PP.C2309 > 0 OR PP.C2310 > 0)
     AND PP.C2302 &lt; (SELECT fechaproceso FROM PARAMETROS) 

),0) AS importeTotal


</sql>
    <outer_join>N</outer_join>
    <replace_vars>Y</replace_vars>
    <parameter>
      <field>
        <name>archivo</name>
        <type>String</type>
      </field>
      <field>
        <name>archivo</name>
