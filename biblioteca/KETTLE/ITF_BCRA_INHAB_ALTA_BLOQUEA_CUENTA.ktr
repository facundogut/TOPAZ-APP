  <step>
    <name>Consulta cuentas</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
	DOC.NUMEROPERSONAFJ AS "PERSONA_NUMERO",
	S.JTS_OID AS "SALDO_JTS_OID",
	S.SUCURSAL AS "SALDO_SUCURSAL",
	S.MONEDA AS "SALDO_MONEDA",
	S.PRODUCTO AS "SALDO_PRODUCTO",
	S.CUENTA AS "SALDO_CUENTA",
	CASE
		WHEN S.JTS_OID IS NOT NULL THEN 1
		ELSE 0
	END AS "SALDO_ENCONTRADO",
	(
		SELECT 
			COALESCE( MAX(GB.ORDINAL_BLOQUEO), -1 ) + 1 
		FROM GRL_BLOQUEOS GB WITH (NOLOCK) 
		WHERE 
			GB.SALDO_JTS_OID = S.JTS_OID AND
			GB.COD_BLOQUEO = 14
	) AS "BLOQUEO_ORDINAL",
	CASE
		WHEN S.C1679 = 1 AND EXISTS(
			SELECT 1
			FROM GRL_BLOQUEOS GB WITH (NOLOCK)
			WHERE 
				GB.SALDO_JTS_OID = S.JTS_OID AND
				GB.COD_BLOQUEO = 14 AND
				GB.ESTADO IN (0,1)
		) THEN 1
		ELSE 0
	END AS "SALDO_BLOQUEO_ACTIVO_EXISTE"
FROM SALDOS S WITH (NOLOCK)
LEFT JOIN CLI_CLIENTEPERSONA CCP WITH (NOLOCK) ON CCP.CODIGOCLIENTE = S.C1803 AND CCP.TZ_LOCK LIKE '[04]%'
LEFT JOIN CLI_CLIENTES CC WITH (NOLOCK) ON CC.CODIGOCLIENTE = CCP.CODIGOCLIENTE AND CC.TZ_LOCK LIKE '[04]%'
LEFT JOIN CLI_DOCUMENTOSPFPJ DOC WITH (NOLOCK) ON CCP.NUMEROPERSONA = DOC.NUMEROPERSONAFJ AND DOC.TZ_LOCK LIKE '[04]%'
WHERE 
	DOC.NUMERODOCUMENTO = ? AND
	(CASE (DOC.TIPOPERSONA) 
		WHEN 'F' THEN (SELECT ESTADO FROM CLI_PERSONASFISICAS PF WITH (NOLOCK) WHERE PF.NUMEROPERSONAFISICA = DOC.NUMEROPERSONAFJ) 
		WHEN 'J' THEN (SELECT ESTADO FROM CLI_PERSONASJURIDICAS PJ WITH (NOLOCK) WHERE PJ.NUMEROPERSONAJURIDICA = DOC.NUMEROPERSONAFJ) 
	END) = 0 AND
	S.TZ_LOCK LIKE '[04]%' AND
	S.C1785 = 2 AND
	S.C1651 &lt;&gt; 1 AND -- No interesan cuentas canceladas
	CC.SUBDIVISION1 &lt;&gt; '02' --No interesan clientes públicos.
ORDER BY S.JTS_OID DESC</sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
