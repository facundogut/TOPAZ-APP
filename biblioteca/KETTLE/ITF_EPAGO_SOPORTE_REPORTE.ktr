    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
trans_Status = CONTINUE_TRANSFORMATION;

var CODIGO_ERROR
var ERROR
var ESTADO_STAGE
var ESTADO_DESCRIPCION

switch( String(ESTADO_CODIGO) ) {
	case 'P': 
		ERROR = null;
		CODIGO_ERROR = 0
		ESTADO_STAGE = 'PAGO_REALIZADO'
		ESTADO_DESCRIPCION = 'Pago realizado con éxito';
		count_procesados++;
		suma_acreditado = suma_acreditado.add( getImporte() );
		break;
	case 'E': 
		ERROR = 'El pago sufrió un error en el procesamiento y no se pudo realizar => ' + COMENTARIO;
		CODIGO_ERROR = 400;
		ESTADO_STAGE = 'PAGO_NO_REALIZADO'
		ESTADO_DESCRIPCION = 'Pago rechazado por la bandeja de acreditaciones';
		count_errores++; 
		suma_no_acreditado = suma_no_acreditado.add( getImporte() );
		break;
	case 'I': 
		ERROR = 'El pago no fue procesado => ' + COMENTARIO;
		CODIGO_ERROR = 401;
		ESTADO_STAGE = 'PAGO_NO_REALIZADO'
		ESTADO_DESCRIPCION = 'Pago ignorado por la bandeja';
		count_sin_procesar++; 
		suma_no_acreditado = suma_no_acreditado.add( getImporte() );
		break;
	default: 
		suma_no_acreditado = suma_no_acreditado.add( getImporte() );
		if (ESTADO_CODIGO == null) {
			trans_Status = SKIP_TRANSFORMATION;
			count_errores++;
		} else {
			ERROR = 'Pago con estado desconocido => ' + COMENTARIO;
			CODIGO_ERROR = 402;
			ESTADO_STAGE = 'DESCONOCIDO';
			ESTADO_DESCRIPCION = 'Estado de movimiento en bandeja desconocido';
			count_otros++;
		}

		break;
}</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Item_0</jsScript_name>
        <jsScript_script>//Script here

var mc = new java.math.MathContext(2, java.math.RoundingMode.FLOOR );

var count_procesados = 0;
var count_errores = 0;
var count_sin_procesar = 0;
var count_otros = 0;

var suma_acreditado = new java.math.BigDecimal(0, mc);
var suma_no_acreditado = new java.math.BigDecimal(0, mc);

const getCampo = function(campo) {
	var idx = rowMeta.indexOfValue(String(campo));
	if (idx == -1)
		return null;
	return row[idx];
}

const getImporte = function() {
	return getCampo('IMPORTE') || java.math.BigDecimal.ZERO;
}</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>2</jsScript_type>
        <jsScript_name>Item_1</jsScript_name>
        <jsScript_script>//Script here

setVariable('CANT_PROCESADOS', count_procesados, 'r');
setVariable('CANT_ERRORES', count_errores, 'r');
setVariable('CANT_SIN_PROCESAR', count_sin_procesar, 'r');
setVariable('CANT_OTROS', count_otros, 'r');
setVariable('CANT_NOT_OK', count_errores + count_sin_procesar + count_otros, 'r');

setVariable('SUMA_ACREDITADO', suma_acreditado, 'r');
setVariable('SUMA_NO_ACREDITADO', suma_no_acreditado, 'r');</jsScript_script>
      </jsScript>
    </jsScripts>
