        <jsScript_script>//Script here

const filenamePattern = /^311\.\d{8}\.txt$/i

const parseDate = function (date, format) {
  var parsedDate;
  try {
    parsedDate = str2date( date, format, 'es', 'AGT');
  } catch (ex) {
    return null;
  }
 
  var reversedDate = date2str( parsedDate, format, 'es', 'AGT');
 
  if ( reversedDate == date) {
    return parsedDate;
  } else {
    return null;
  }
}

const abortar = function( mensaje ) {
	writeToLog('e', 'Ocurrió un error => ' + String(mensaje) );
	setVariable('ERROR', String(mensaje), 'r');
	trans_Status = ERROR_TRANSFORMATION;
	_step_.setErrors(1);
}</jsScript_script>
