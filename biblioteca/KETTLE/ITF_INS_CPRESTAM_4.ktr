    <sql>DECLARE 
@archivoAdjunto VARCHAR (50) = ?,
@fechaProc VARCHAR (10) = ?,
@periodo VARCHAR (6); 

SELECT @periodo =  RIGHT(replace(convert(VARCHAR(10),dateadd(month, 1, CAST(@fechaProc AS DATE)), 104),'.',''), 6)

SELECT 
	concat(	
	'Por la presente hacemos llegar el archivo con la información de cuotas de prestamos a descontar correspondiente al período ',@periodo,
	' de los agentes que prestan servicios en esa Institución. Se adjunta el reporte con el detalle de la cantidad de los registros y el importe total que contiene el archivo, para vuestro control.') as mail_msj,
	456 as parametro,
	
	'Informacion operaciones de prestamos empleados INSSSEP' asunto,
	--(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 456) AS mensaje,  no es necesario guardar, se setea arriba
	--(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 457) AS Email_Sender,
	--(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 458) AS EMAIL_ENVIAR,
	--(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 459) AS Email_Pass,
	--(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 462) AS Email_Sender_Name,
   	--(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 460) AS SMTP_Server,
	--(SELECT NUMERICO_1 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 461) AS SMTP_Port
	
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 470) AS Email_Sender,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 471) AS EMAIL_ENVIAR,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 472) AS Email_Pass,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 475) AS Email_Sender_Name,
   	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 473) AS SMTP_Server,
	(SELECT NUMERICO_1 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 474) AS SMTP_Port</sql>
