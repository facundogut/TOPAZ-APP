    <sql>DECLARE @v_fechaproceso DATETIME
SELECT @v_fechaproceso = FECHAPROCESO FROM parametros (nolock)

MERGE RRII_BITACORA_CREDEB AS TARGET
USING (
		SELECT 	0 AS TZ_LOCK, b1.JTS_OID_SALDOS AS JTS_OID_SALDOS
				, @v_fechaproceso AS FECHA, 0 AS SEGMENTO, b1.TIPO_CARGO_IMPOSITIVO AS TIPO_CARGO_IMPOSITIVO
				, 'N' AS PADRON_BENEF_FISCAL, b1.PADRON_MiPyme AS PADRON_MiPyme
		FROM RRII_BITACORA_CREDEB b1 (nolock)
		JOIN (	SELECT JTS_OID_SALDOS, max(FECHA) AS max_fecha 
			 	FROM RRII_BITACORA_CREDEB (nolock) 
			 	GROUP BY JTS_OID_SALDOS
			) b2 ON b1.JTS_OID_SALDOS = b2.JTS_OID_SALDOS AND b1.FECHA = b2.max_fecha
		WHERE 	b1.JTS_OID_SALDOS NOT IN (SELECT jts_oid_saldo 
										FROM VTA_saldos (nolock) 
										WHERE cta_cbu IN (SELECT CONVERT(VARCHAR,cbu) FROM ITF_PADRON_FISCAL_CREDEB_aux (nolock))
										)
		) AS SOURCE
ON TARGET.JTS_OID_SALDOS = SOURCE.JTS_OID_SALDOS 
	AND TARGET.FECHA = SOURCE.FECHA 
	AND TARGET.TIPO_CARGO_IMPOSITIVO = SOURCE.TIPO_CARGO_IMPOSITIVO
WHEN MATCHED THEN 
	UPDATE SET	TARGET.SEGMENTO = SOURCE.SEGMENTO,
				TARGET.PADRON_BENEF_FISCAL = SOURCE.PADRON_BENEF_FISCAL,
				TARGET.PADRON_MiPyme = SOURCE.PADRON_MiPyme,
				TARGET.TZ_LOCK = SOURCE.TZ_LOCK
WHEN NOT MATCHED THEN
	INSERT (TZ_LOCK, JTS_OID_SALDOS, FECHA, SEGMENTO, TIPO_CARGO_IMPOSITIVO, PADRON_BENEF_FISCAL, PADRON_MiPyme)
	VALUES (SOURCE.TZ_LOCK, SOURCE.JTS_OID_SALDOS, SOURCE.FECHA, SOURCE.SEGMENTO, SOURCE.TIPO_CARGO_IMPOSITIVO, SOURCE.PADRON_BENEF_FISCAL, SOURCE.PADRON_MiPyme);

truncate table ITF_PADRON_FISCAL_CREDEB
insert into ITF_PADRON_FISCAL_CREDEB(CUIT,cbu,codigo_beneficio,FECHA_vigencia)
select CUIT,cbu,codigo_beneficio,FECHA_vigencia from ITF_PADRON_FISCAL_CREDEB_aux
truncate table ITF_PADRON_FISCAL_CREDEB_aux

--borro registros de mas de un a√±o
DELETE FROM RRII_BITACORA_CREDEB
WHERE fecha &lt; DATEADD(YEAR, -1, @v_fechaproceso)

DELETE FROM ITF_BEN_FISCAL_CREDEB
WHERE fechaproceso &lt; DATEADD(YEAR, -1, @v_fechaproceso)

DELETE FROM ITF_BEN_FISCAL_CREDEB_INCONSISTENCIAS
WHERE fecha_proceso &lt; DATEADD(YEAR, -1, @v_fechaproceso)</sql>
