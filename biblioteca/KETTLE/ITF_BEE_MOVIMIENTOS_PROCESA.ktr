    <connection>TopazDS</connection>
    <sql>DECLARE @fechaAnterior VARCHAR(8);
SET @fechaAnterior = (SELECT convert(VARCHAR(8), dbo.diaHabil(Dateadd(Day,-1,fechaproceso),'B'),112) FROM PARAMETROS);
						
SELECT 
	'CONFOR' AS 'TIPO_REGISTRO',
	'311' AS 'BANCO',
	S.C1785 AS 'TIPO_CUENTA',
	S.MONEDA AS 'MONEDA',
	RIGHT(VS.CTA_REDLINK, 19) AS 'CTA_REDLINK_PBF',
	VS.CTA_CBU AS 'CBU',
	MC.FECHAPROCESO AS 'FECHA_IMPUTACION',
	MC.FECHACONTABLE AS 'FECHA_CONTABLE',
	MC.DEBITOCREDITO AS 'TIPO_OPERACION',
	REPLACE(CAST(MC.CAPITALREALIZADO AS VARCHAR(17)), '.', '') AS 'MONTO',
	MC.ASIENTO AS 'REFERENCIA',
	MC.COD_TRANSACCION AS 'COD_TRANSACCION',
	MC.CONCEPTO AS 'CONCEPTO',
	@fechaAnterior AS 'FECHA_ANTERIOR'
FROM VTA_SALDOS VS (NOLOCK)
	JOIN SALDOS S (NOLOCK) ON S.JTS_OID = VS.JTS_OID_SALDO AND VS.TZ_LOCK=0
	JOIN MOVIMIENTOS_CONTABLES MC (NOLOCK) ON MC.SALDO_JTS_OID = S.JTS_OID
	JOIN ASIENTOS A (NOLOCK) ON A.ASIENTO = MC.ASIENTO AND A.SUCURSAL = MC.SUCURSAL AND a.FECHAPROCESO=MC.FECHAPROCESO
WHERE VS.OPERABE = 'S' AND A.ESTADO = 77 AND MC.FECHAPROCESO = @fechaAnterior
ORDER BY VS.CTA_REDLINK ASC</sql>
