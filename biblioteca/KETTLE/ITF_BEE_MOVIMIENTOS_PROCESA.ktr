        <jsScript_script>// Definición de variables
trans_Status = CONTINUE_TRANSFORMATION;
var REGISTRO = '';
var rutaArchivo = '';
var SECUENCIA = '';

// Tipo registro: Detalle
esRegistro = true;

// Adecuaciones

if (!CTA_REDLINK_PBF.equals(CTAAUX)) {
	REGAUX = 1;
	CTAAUX = CTA_REDLINK_PBF;
} else {
	REGAUX = (REGAUX&lt;=9999) ? REGAUX+1 : 0 ;
};

TIPO_CUENTA = mapaTipoCuenta[parseInt(TIPO_CUENTA, 10)]
if (TIPO_CUENTA == null) ERROR += 'TIPO DE CUENTA NO ESPERADO. ';

MONEDA = mapaTipoMoneda[parseInt(MONEDA, 10)]
if (TIPO_CUENTA == null) ERROR += 'TIPO DE MONEDA NO ESPERADO. ';


// Contar registros; si hay error abortar ejecución.
if (ERROR == '') {

	// Contar registros detalle
	REGOK++

	// Adecuar campos
	CTA_REDLINK_PBF = rpad(trim(CTA_REDLINK_PBF), ' ', 19);
	CBU = lpad(CBU, '0', 23);
	FECHA_IMPUTACION = date2str(FECHA_IMPUTACION, 'yyyyMMdd');
	FECHA_CONTABLE = date2str(FECHA_CONTABLE, 'yyyyMMdd');
	MONTO = lpad(MONTO, '0', 17);
	SECUENCIA = lpad(REGAUX, '0', 4);
	REFERENCIA = lpad(REFERENCIA, '0', 13);

	// Elaborar ruta archivo
	var dia = substr(FECHA_ANTERIOR, 6, 2);
	var mes = substr(FECHA_ANTERIOR, 4, 2);
	rutaArchivo = RCO + 'RBEMC' + dia + mes + '0311.DAT';
	
} else {
	ERROR = 'Cuenta: ' + CTA_REDLINK_PBF + ' - Error/es: ' + ERROR;
	setVariable('msg_res', ERROR, 'r');
	writeToLog('e', ERROR);
	trans_Status = ERROR_TRANSFORMATION;
}
</jsScript_script>
