SELECT
	concat('ADINTAR USUARIOS - Listado de Tarjetas con errores ',convert(VARCHAR, (select fechaproceso FROM PARAMETROS (nolock)),111)) as asunto,
--	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 169) AS mensaje,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 170) AS Email_Sender,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 171) AS EMAIL_ENVIAR,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 172) AS Email_Pass,
	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 173) AS Email_Sender_Name,
   	(SELECT ALFA_3 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 174) AS SMTP_Server,
	(SELECT NUMERICO_1 FROM ITF_MASTER_PARAMETROS (nolock) WHERE CODIGO = 174) AS SMTP_Port</sql>
