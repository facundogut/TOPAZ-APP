  <step>
    <name>Traer datos</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>TopazConect</connection>
    <rowlimit>0</rowlimit>
    <sql>
DECLARE @fecha DATETIME; 
DECLARE @fechaMesAnterior DATETIME;
SET @fecha = CONVERT(DATETIME,?, 103);
SET @fechaMesAnterior = dbo.ULTIMODIAHABIL(DATEADD(MONTH, -1, @fecha));

SELECT DISTINCT
    V.FECHA AS FECHA_DE_INFORME,
    EOMONTH(@fecha, 0) AS FECHA_DE_INFORMACION,
    LEFT(CAST(V.RUBROCONTABLE AS VARCHAR(15)), 6) AS CUENTA,
    V.RUBROCONTABLE AS CUENTA_ENT,
    V.CAT_BCRA AS CATEGORIA,
    V.MONEDA_NUM1 AS MONEDA,
--Saldo Acum
    CASE WHEN V.MONEDA =1 THEN
    	-1*SUM(V.SALDO_AJUSTADO)
    WHEN V.MONEDA IN (999,988) THEN
    	-1*SUM(V.SALDO_AJUSTADO * ISNULL(H1.TIPO_CAMBIO_OFICIAL,0))--dbo.FN_ObtenerTipoCambioOficial(V.MONEDA, V.FECHA_COT))
    ELSE 
    	-1*SUM(V.SALDO_AJUSTADO) * ISNULL(H2.TIPO_CAMBIO_OFICIAL,0) --dbo.FN_ObtenerTipoCambioOficial(V.MONEDA,V.FECHA)
    END AS SaldoAcum,
--Saldo Acum mon or
    CASE WHEN V.MONEDA IN (1,2) THEN
    	-1*SUM(V.SALDO_AJUSTADO)
    WHEN V.MONEDA IN (999,988) THEN
    	-1*SUM(V.SALDO_AJUSTADO * ISNULL(H1.TIPO_CAMBIO_COMPRA,0))
   ELSE
        ISNULL(H2.ARBITRAJE_COMPRA,0) * ROUND(SUM(ISNULL(V.SALDO_AJUSTADO, 0)), 2) * -1
    END AS SDO_ACUM_MON_OR,
--Sdo Prom
    CASE WHEN V.MONEDA =1 THEN
    	-1*SUM(V.PROMEDIO_DIARIO)
    WHEN V.MONEDA IN (999,988) THEN
    	-1*SUM(V.PROMEDIO_DIARIO * ISNULL(H1.TIPO_CAMBIO_COMPRA,0))
    ELSE
    	-1*SUM(V.PROMEDIO_DIARIO) * ISNULL(H2.TIPO_CAMBIO_COMPRA,0)
    END AS SDO_PROM,
--Sdo Prom Mon Or
    CASE WHEN V.MONEDA IN (1,2) THEN
    	-1*SUM(V.PROMEDIO_DIARIO)
    WHEN V.MONEDA IN (999,988) THEN
    	-1*SUM(V.PROMEDIO_DIARIO * ISNULL(H1.TIPO_CAMBIO_COMPRA,0))
   ELSE
        ISNULL(H2.ARBITRAJE_COMPRA,0) * ROUND(SUM(ISNULL(V.PROMEDIO_DIARIO, 0)), 2) * -1 
    END AS SdoPromMonOr,
--Categoria
	1 AS CATEGORIA,
--SaldoAcumPGNME
	CASE WHEN V.MONEDA IN (1,999,988) THEN 0
	ELSE
		-1*SUM(V.SALDO_AJUSTADO * ISNULL(H3.TIPO_CAMBIO_COMPRA,0))
	END AS SDO_ACUM_PGNME,
--SdoPromPGNME
	CASE WHEN V.MONEDA IN (1,999,988) THEN 0
	ELSE
		-1*SUM(V.PROMEDIO_DIARIO * ISNULL(H3.TIPO_CAMBIO_COMPRA,0))
	END AS SDO_PROM_PGNME,
--COEFACTUALIZACION
	M.CODIGO_MERCADO AS COEFACTUALIZACION,
--BALANCE BANDA
	0 AS BALANCE_BANDA,
	
	ISNULL(V.RUBROCONTABLE, 0) AS CUENTA_VALIDADA


FROM
    vw_SaldosAjustados V WITH(NOLOCK)
INNER JOIN MONEDAS M WITH(NOLOCK) ON
	M.C6399=V.MONEDA AND
	M.TZ_LOCK=0
LEFT JOIN HISTORICOTIPOSCAMBIO H1 WITH(NOLOCK) ON 
	H1.MONEDA= V.MONEDA AND 
	H1.FECHA_COTIZACION = (SELECT TOP 1 FECHA_COTIZACION 
							FROM HISTORICOTIPOSCAMBIO WITH(NOLOCK) 
							WHERE FECHA_COTIZACION&lt;=V.FECHA_COT 
								AND TZ_LOCK=0
							ORDER BY H1.FECHA_COTIZACION DESC) AND
	H1.TZ_LOCK=0
	
LEFT JOIN HISTORICOTIPOSCAMBIO H2 WITH(NOLOCK) ON 
	H2.MONEDA= V.MONEDA AND 
	H2.FECHA_COTIZACION = (SELECT TOP 1 FECHA_COTIZACION 
							FROM HISTORICOTIPOSCAMBIO WITH(NOLOCK) 
							WHERE FECHA_COTIZACION&lt;=V.FECHA
								AND TZ_LOCK=0
							ORDER BY H1.FECHA_COTIZACION DESC) AND
	H2.TZ_LOCK=0

LEFT JOIN HISTORICOTIPOSCAMBIO H3 WITH(NOLOCK) ON
    H3.MONEDA = V.MONEDA 
    AND H3.FECHA_COTIZACION = (SELECT MAX(FECHA_COTIZACION) FROM HISTORICOTIPOSCAMBIO 
    						WHERE FECHA_COTIZACION &lt;= @fechaMesAnterior
    							AND MONEDA=V.MONEDA
    							AND TZ_LOCK=0)
    AND H3.TZ_LOCK = 0
  	
WHERE 
    V.FECHA BETWEEN DATEADD(MONTH, DATEDIFF(MONTH, 0, @fecha), 0) AND @fecha AND
	V.SALDO_AJUSTADO!=0
GROUP BY
    LEFT(CAST(V.RUBROCONTABLE AS VARCHAR(15)), 6),
    V.RUBROCONTABLE,
    V.CAT_BCRA,
    V.MONEDA_NUM1,
    V.MONEDA,
    V.FECHA,
    M.CODIGO_MERCADO,
    H1.TIPO_CAMBIO_OFICIAL,
    H2.TIPO_CAMBIO_OFICIAL,
    H1.TIPO_CAMBIO_COMPRA,
    H2.TIPO_CAMBIO_COMPRA,
    H2.ARBITRAJE_COMPRA
</sql>
