if (ORIGEN_MAIL == 'default')
	ERROR += ' Debe Proporcionar un mail de origen en ORIGEN_MAIL. ';

if (ORIGEN_NOMBRE == 'default')
	ERROR += ' Debe Proporcionar un nombre de origen en ORIGEN_NOMBRE. ';

if (ASUNTO == 'default')
	ERROR += ' Debe Proporcionar un asunto en ASUNTO. ';

if (CUERPO == 'default')
	ERROR += ' Debe Proporcionar un cuerpo en CUERPO. ';

if (AUTENTICACION != 'N' &amp;&amp; AUTENTICACION != 'A' &amp;&amp; AUTENTICACION != 'T' &amp;&amp; AUTENTICACION != 'S')
	ERROR += ' Debe Proporcionar un tipo de autenticacion valido, N nula A simple T TLS S SSL. ';

if (ERROR != '')
	abortar(ERROR);

//Adecuacion
if (FIN == false)
{
	if (COPIAS == 'default' || COPIAS == '')
		COPIAS = null;
	
	if (RUTA_ADJUNTOS == 'default' || RUTA_ADJUNTOS == '')
		RUTA_ADJUNTOS = null;
	
	if (WILDCARD_ADJUNTOS == 'default' || WILDCARD_ADJUNTOS == '')
		WILDCARD_ADJUNTOS = null;
	
	if (SERVER_USER == 'default' || SERVER_USER == '')
		SERVER_USER = null;
	
	if (SERVER_PASS == 'default' || SERVER_PASS == '')
		SERVER_PASS = null;
}

//Agrega NODO al cuerpo
CUERPO += '&lt;div style="background-color: #00274d; color: #ffffff; padding: 20px; text-align: center; justify-content: center; margin-top: 20px;"> &lt;img src="logoNBCH.png" alt="Logo NBCH"/> &lt;p style="text-align: left; font-size: xx-small;">Correo enviado desde TOPAZ Core. Nodo: ' + NODO + '&lt;/p> &lt;p style="text-align: left; font-size: xx-small;">NBCH Meteorito Integration Team&lt;/p>  &lt;/div>';
</jsScript_script>
