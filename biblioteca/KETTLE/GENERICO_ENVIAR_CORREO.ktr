        <jsScript_script>trans_Status = CONTINUE_TRANSFORMATION; //Estado default (Permite arrojar error por el javascript)

var NODO = getVariable("IP_SERVIDOR","NODATA");
var ERROR = '';

//Parametros Obligatorios
if (SERVER_HOST == 'default')
	ERROR += ' Debe Proporcionar un host de SMTP en SERVER_HOST. ';

