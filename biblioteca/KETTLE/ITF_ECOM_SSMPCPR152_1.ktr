    <rowlimit>0</rowlimit>
    <sql>SELECT RIGHT(concat('052',--Asociación
			  RIGHT(REPLICATE('0', 2) + CAST(cre.JURISDICCION AS VARCHAR(2)), 2),--Jurisdicción
			  RIGHT(REPLICATE('0', 8) + CAST(DOC.NUM_DOC_FISICO AS VARCHAR(8)), 8),--Número de Documento Físico
			  CASE 
        	  WHEN DOC.TIPO_DOC_FISICO = 'DNI' THEN 3
        	  WHEN DOC.TIPO_DOC_FISICO = 'LE' THEN 1
        	  WHEN DOC.TIPO_DOC_FISICO = 'LC' THEN 2
        	  ELSE 0 -- Valor por defecto si no coincide con ninguno de los anteriores
    		  END,--Tipo de Documento Físico
			  '0001',--Tipo de Producto
			  RIGHT(REPLICATE('0', 14) + CAST(s.OPERACION AS VARCHAR(14)), 14),
			  RIGHT(REPLICATE('0', 6) + CAST(s.ORDINAL AS VARCHAR(6)), 6),--Datos del producto
			  RIGHT(REPLICATE('0', 5) + CAST(s.PRODUCTO AS VARCHAR(5)), 5),
			  RIGHT(REPLICATE('0', 1) + CAST(PP.C2303 AS VARCHAR(1)), 1),--Tipo de Cuota
			  RIGHT(REPLICATE('0', 3) + CAST(PP.C2300 AS VARCHAR(3)), 3),--Número de Cuota
			  RIGHT(REPLICATE('0', 9) +CAST(CAST(((pp.c2304 + pp.c2305)*100) AS numeric(9)) AS VARCHAR(9)),9)--Importe 
			  ),56) as linea, CONVERT(VARCHAR(19), pp.c2313, 120) as fecha_pago, (select CONVERT(VARCHAR(19), fechaproceso, 120) from parametros) as fechaproceso, 'N' as estado
fROM SALDOS S 
JOIN PLANPAGOS PP ON PP.SALDO_JTS_OID = S.JTS_OID
JOIN VW_CLI_X_DOC VC ON S.C1803 = VC.CODIGOCLIENTE
JOIN CLI_DocumentosPFPJ DOC ON doc.NUMERODOCUMENTO= VC.NUMERODOC
JOIN TOPESPRODUCTO TP ON TP.CODPRODUCTO = S.PRODUCTO
JOIN cre_saldos cre ON cre.SALDOS_JTS_OID=S.JTS_OID
join cre_bcra_cendeu bcra on bcra.codigocliente=S.c1803

WHERE (TP.CODIGO_ECOM = '52' OR TP.CODIGO_ECOM = '052')
AND S.C1785 = 5
AND (PP.C2309 = 0 OR PP.C2310 = 0)
AND cre.JURISDICCION&lt;&gt;16
AND S.TZ_LOCK=0
AND PP.TZ_LOCK=0
AND DOC.TZ_LOCK=0
AND TP.TZ_LOCK=0
AND cre.TZ_LOCK=0
AND bcra.SITUACION_BCRA&lt;=6
</sql>
