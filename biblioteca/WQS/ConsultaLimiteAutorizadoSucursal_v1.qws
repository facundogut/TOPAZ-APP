<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceQuery>
    <uri>/usogeneral/limiteAutorizadoSucursal</uri>
    <serviceDescription>Consula limite autorizado por sucursales</serviceDescription>
    <title>Consula limite autorizado por sucursales</title>
    <querys>
        <query id="1" name="grupo1" type="MULTIPLE">
            <sql>SELECT
				NF.NIVEL AS nivel
				,NF.DESCRIPCION AS descripcionNivel
				,NF.LIM_AUTORIZADO AS limiteAutorizado
				,NF.SUCURSAL AS sucursal
				FROM ITF_MASTER_NIV_FACULTADES NF
				WHERE NF.TZ_LOCK=0
				ORDER BY NF.SUCURSAL ASC
			</sql>
            <input/>

            <output>
            
	            <level groupby="" name="Datos1">
	                
	                    <outParameter description="" type="STRING">
	                        <queryname>nivel</queryname>
	                        <servicename>nivel</servicename>
	                    </outParameter>
	                    <outParameter description="" type="STRING">
	                        <queryname>descripcionNivel</queryname>
	                        <servicename>descripcionNivel</servicename>
	                    </outParameter>
	                    <outParameter description="" type="LONG">
	                        <queryname>limiteAutorizado</queryname>
	                        <servicename>limiteAutorizado</servicename>
	                    </outParameter>
	                    <outParameter description="" type="INTEGER">
	                        <queryname>sucursal</queryname>
	                        <servicename>sucursal</servicename>
	                    </outParameter>
	                
                </level>
	                
          
            </output>
            
        </query>
        
    </querys>
</serviceQuery>
