<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<serviceQuery>
    <uri>/clientes/personaJuridicabyIdPersona/{idPersona}</uri>
    <serviceDescription>Consulta persona jurídica por idPersona</serviceDescription>
    <title>PersonaJuridiabyIdPersona</title>
    <querys>
        <query id="1" name="query1" type="MULTIPLE">
            <sql>
SELECT DISTINCT isnull(teb.JTS_BITACORA,0) AS JTS_BITACORA, pj.NUMEROPERSONAJURIDICA, 'J' AS tipoPersona, ts.codigo_sociedad AS codSociedad, ts.descripcion AS descSociedad,

-- documentoIdentificativo
(CASE WHEN dpj.TIPODOCUMENTO IS NOT NULL THEN dpj.TIPODOCUMENTO ELSE '' END) AS TIPODOCUMENTO,
(CASE WHEN dpj.TIPODOCUMENTO IS NOT NULL THEN dpj.TIPODOCUMENTO ELSE '' END) AS DESCR_DOCUMENTO,
(CASE WHEN dpj.NUMERODOCUMENTO IS NOT NULL THEN dpj.NUMERODOCUMENTO ELSE '' END) AS NUMERODOCUMENTO,
(CASE WHEN dpj.COD_PAIS_ORIGEN IS NOT NULL THEN dpj.COD_PAIS_ORIGEN ELSE 0 END) AS COD_PAIS_ORIGEN,
(CASE WHEN dpj.COD_PAIS_ORIGEN IS NOT NULL THEN (SELECT NOMBREPAIS FROM CLI_PAISES WHERE CODIGOPAIS=dpj.COD_PAIS_ORIGEN) ELSE '' END) AS DESCR_PAIS_DOC,
   
-- datos documentoFisico  
(CASE WHEN dpj.TIPO_DOC_FISICO IS NOT NULL THEN dpj.TIPO_DOC_FISICO ELSE '' END) AS TIPO_DOC_FISICO,
(CASE WHEN dpj.TIPO_DOC_FISICO IS NOT NULL THEN dpj.TIPO_DOC_FISICO ELSE '' END) AS DESCR_DOCUMENTO_FISCO,
(CASE WHEN dpj.NUM_DOC_FISICO IS NOT NULL THEN dpj.NUM_DOC_FISICO ELSE '' END) AS NUM_DOC_FISICO,
(CASE WHEN dpj.COD_PAIS_ORIGEN IS NOT NULL THEN dpj.COD_PAIS_ORIGEN  ELSE 0 END) AS cod_pais_origen_docF,
(CASE WHEN dpj.COD_PAIS_ORIGEN IS NOT NULL THEN (SELECT NOMBREPAIS FROM CLI_PAISES WHERE CODIGOPAIS=dpj.COD_PAIS_ORIGEN) ELSE '' END) AS DESCR_PAIS_DOC_FISCO,
(CASE WHEN dpj.NRO_TRAMITE IS NOT NULL THEN dpj.NRO_TRAMITE  ELSE '0' END) AS NRO_TRAMITE,
(CASE WHEN dpj.EJEMPLAR IS NOT NULL THEN dpj.EJEMPLAR ELSE '' END) AS EJEMPLAR,
(CASE WHEN CAST (dpj.FECHAVENCIMIENTO AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,dpj.FECHAVENCIMIENTO,126)  END ) AS FECHAVENCIMIENTO, 
--
(CASE WHEN pj.PAISRESIDENCIA IS NOT NULL THEN pj.PAISRESIDENCIA  ELSE 0 END) AS PAISRESIDENCIA,
(CASE WHEN pj.PAISRESIDENCIA IS NOT NULL THEN (SELECT NOMBREPAIS FROM CLI_PAISES WHERE CODIGOPAIS=pj.PAISRESIDENCIA) ELSE '' END) AS DESCR_PAIS_RECIDENCIA,
(CASE WHEN CAST (pj.FECHACONSTITUCION AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,pj.FECHACONSTITUCION,126)  END ) AS FECHACONSTITUCION,      
(CASE WHEN CAST (pj.FECHAVENCIMIENTO AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,pj.FECHAVENCIMIENTO,126)  END ) AS FECHAVENCIMIENTO,      
(CASE WHEN CAST (pj.FECHAALTA AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,pj.FECHAALTA,126)  END ) AS FECHAALTA,  
(CASE WHEN CAST (pj.FECHAULTIMAACTUALIZACION AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,pj.FECHAULTIMAACTUALIZACION,126)  END ) AS FECHAULTIMAACTUALIZACION,  
(CASE WHEN pj.CODIGODERESIDENCIA IS NOT NULL THEN pj.CODIGODERESIDENCIA ELSE '' END) AS CODIGODERESIDENCIA,
(CASE WHEN pj.CODIGODERESIDENCIA IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO = 1481 AND IDIOMA= 'E' AND OPCIONINTERNA=pj.CODIGODERESIDENCIA) ELSE '' END) AS DESCR_RESIDENCIA,
(CASE WHEN pj.NIVEL_APERTURA IS NOT NULL THEN pj.NIVEL_APERTURA ELSE 0 END) AS NIVEL_APERTURA,
(CASE WHEN pj.NIVEL_APERTURA IS NOT NULL  THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=2045 AND IDIOMA='E' AND OPCIONINTERNA=pj.NIVEL_APERTURA) ELSE '' END)AS DESCR_APERTURA, 
(CASE WHEN pj.CANTIDAD_EMPLEADOS IS NOT NULL THEN pj.CANTIDAD_EMPLEADOS  ELSE 0 END) AS CANTIDAD_EMPLEADOS,
(CASE pj.CONC_ACREEDORES WHEN 'S' THEN 'true' WHEN 'N' THEN 'false' ELSE '' END) AS CONCURSO_ACREEDORES,

(CASE WHEN pj.SECTOR IS NOT NULL THEN pj.SECTOR  ELSE 0 END) AS SECTOR,
(CASE WHEN pj.SECTOR IS NOT NULL THEN (SELECT DESCRIPCION FROM CLI_SECTORES WHERE SECTOR=pj.SECTOR)ELSE '' END) AS DESCRIPCION_SECTOR, 

(CASE WHEN pj.NIF IS NOT NULL THEN pj.NIF ELSE '' END) AS NIF,
(CASE WHEN pj.MOTIVO_VINC IS NOT NULL THEN pj.MOTIVO_VINC ELSE '' END) AS MOTIVO_VINC,
(CASE WHEN pj.MOTIVO_VINC IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=33274 AND OPCIONINTERNA=pj.MOTIVO_VINC) ELSE '' END) AS DESCR_MOTIVO_VINC,
(CASE WHEN pj.SUC_ALTA IS NOT NULL THEN pj.SUC_ALTA  ELSE 0 END) SUC_ALTA,
(CASE WHEN pj.SUC_ALTA IS NOT NULL THEN (SELECT  NOMBRESUCURSAL  FROM SUCURSALES WHERE  SUCURSAL= pj.SUC_ALTA)ELSE '' END) AS NOMBRESUCURSAL,

(CASE WHEN teb.tam_actual IS NOT NULL THEN teb.tam_actual ELSE '' END) AS tamanioEmp,

(CASE pj.CFI WHEN 'S' THEN 'true' WHEN 'N' THEN 'false' ELSE '' END)  AS FCI,
(CASE WHEN pj.NIVEL_RIESGO IS NOT NULL THEN pj.NIVEL_RIESGO ELSE 0 END) AS NIVEL_RIESGO,
(CASE WHEN pj.NIVEL_RIESGO IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=33358 AND IDIOMA='E' AND OPCIONINTERNA=pj.NIVEL_RIESGO) ELSE '' END) AS DESCR_NIVEL_RIESGO,
(CASE WHEN pj.RAZONSOCIAL IS NOT NULL THEN pj.RAZONSOCIAL ELSE '' END) AS RAZONSOCIAL,
(CASE WHEN pj.NOMBREFANTASIA IS NOT NULL THEN pj.NOMBREFANTASIA ELSE '' END) AS NOMBREFANTASIA,
(CASE WHEN pj.IGA IS NOT NULL THEN pj.IGA ELSE '' END) AS IGA,
(CASE WHEN pj.IGA IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=4606 AND IDIOMA='E' AND OPCIONINTERNA=pj.IGA) ELSE '' END) AS DESCR_IGA,
(CASE WHEN pj.MONOTRIBUTO IS NOT NULL THEN pj.MONOTRIBUTO ELSE '' END) AS MONOTRIBUTO,
(CASE WHEN pj.MONOTRIBUTO IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=2021 AND IDIOMA='E' AND OPCIONINTERNA=pj.MONOTRIBUTO) ELSE '' END) AS DESCR_MONOTRIBUTO,
(CASE WHEN pj.IVA IS NOT NULL THEN pj.IVA ELSE '' END) AS IVA,
(CASE WHEN pj.IVA IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=4608 AND IDIOMA='E' AND OPCIONINTERNA= pj.IVA)ELSE '' END) AS DESCR_IVA,

 



--"iibbCaba": cambio estructura (nombre de campos)

--(CASE WHEN pj.IIBB_CABA_PERC IS NOT NULL THEN pj.IIBB_CABA_PERC ELSE '' END) AS IIBB_CABA_PERC,
--(CASE WHEN pj.IIBB_CABA_ALI IS NOT NULL THEN pj.IIBB_CABA_ALI  ELSE 0 END) AS IIBB_CABA_ALI,  
(CASE WHEN pj.IIBB_CABA_PERCEPCION_COND IS NOT NULL THEN pj.IIBB_CABA_PERCEPCION_COND ELSE '' END) AS IIBB_CABA_PERC,
(CASE WHEN pj.IIBB_CABA_PERCEPCION_ALI IS NOT NULL THEN pj.IIBB_CABA_PERCEPCION_ALI  ELSE 0 END) AS IIBB_CABA_ALI, 

 

--"iibbCorrientes": 
(CASE WHEN pj.IIBB_CORRIENTES_RECAUD IS NOT NULL THEN pj.IIBB_CORRIENTES_RECAUD ELSE '' END)  AS IIBB_CORRIENTES_RECAUD,
(CASE WHEN pj.IIBB_CORRIENTES_ALI IS NOT NULL THEN pj.IIBB_CORRIENTES_ALI ELSE 0 END) AS IIBB_CORRIENTES_ALI,

 

--Lista Negra
(CASE WHEN pj.INCL_LISTA_NEGRA IS NOT NULL THEN pj.INCL_LISTA_NEGRA ELSE '' END) AS INCL_LISTA_NEGRA,
(CASE WHEN pj.INCL_LISTA_NEGRA IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=1906 AND IDIOMA='E' AND OPCIONINTERNA=pj.INCL_LISTA_NEGRA)ELSE ''END) AS DESCR_LISTA_NEGRA,

 

--Lista Blanca
(CASE (SELECT COUNT(*) FROM CLI_LISTA_BLANCA WHERE TIPO_DOCUMENTO=dpj.TIPODOCUMENTO AND DOCUMENTO= dpj.NUMERODOCUMENTO) WHEN 0 THEN 'false' ELSE 'true' END) AS listaBlanca,

(CASE pj.IN_INAES WHEN 'N' THEN 'false' WHEN 'S' THEN 'true' ELSE '' END) AS IN_INAES,
(CASE pj.IN_UIF WHEN 'N' THEN 'false' WHEN 'S' THEN 'true' ELSE '' END) AS IN_UIF,
(CASE pj.SUJETO_UIF WHEN 'N' THEN 'false' WHEN 'S' THEN 'true' ELSE '' END) AS SUJETO_UIF,
(CASE pj.DEC_JURADA WHEN 'N' THEN 'false' WHEN 'S' THEN 'true' ELSE '' END) AS DEC_JURADA,
(CASE pj.INSC_UIF WHEN 'N' THEN 'false' WHEN 'S' THEN 'true' ELSE '' END) AS INSC_UIF,

 

(CASE WHEN pj.NRO_MAT_INAES IS NOT NULL THEN pj.NRO_MAT_INAES ELSE 0 END) AS NRO_MAT_INAES,
(CASE WHEN pj.NUMERO IS NOT NULL THEN pj.NUMERO ELSE 0 END) AS NUMERO,
(CASE WHEN pj.FOLIO IS NOT NULL THEN pj.FOLIO ELSE '' END) AS FOLIO,
(CASE WHEN pj.LIBRO IS NOT NULL THEN pj.LIBRO ELSE 0 END) AS LIBRO,
(CASE WHEN pj.ANIO IS NOT NULL THEN pj.ANIO ELSE 0 END) AS ANIO,
(CASE WHEN pj.OBSERVACIONES IS NOT NULL THEN pj.OBSERVACIONES ELSE '' END) AS OBSERVACIONES,
(CASE  pj.ESTADO WHEN '1' THEN 'false' WHEN '0' THEN 'true' ELSE '' END) AS  habilitado,
(CASE WHEN pj.ESTADO IS NOT NULL THEN pj.ESTADO ELSE 0 END) AS ID_ESTADO,
(CASE WHEN pj.ESTADO IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=33367 AND IDIOMA='E' AND OPCIONINTERNA=pj.ESTADO) ELSE '' END) AS DESCR_ESTADO, 
(CASE WHEN pj.FORMA_LLEGADA_BANCO IS NOT NULL THEN  pj.FORMA_LLEGADA_BANCO ELSE 0 END) AS FORMA_LLEGADA_BANCO,
(CASE WHEN pj.FORMA_LLEGADA_BANCO IS NOT NULL THEN (SELECT DESCRIPCION FROM OPCIONES WHERE NUMERODECAMPO=2009 AND IDIOMA='E' AND OPCIONINTERNA= pj.FORMA_LLEGADA_BANCO) ELSE '' END) AS DESCR_FORM_ACCESO_BANCO,

 

--Actividad economica
(CASE WHEN ae.CODIGO_ACTIVIDAD IS NOT NULL THEN CAST(ae.CODIGO_ACTIVIDAD AS INT) ELSE 0 END) AS CODIGO_ACTIVIDAD,
(CASE WHEN ae.CODIGO_ACTIVIDAD IS NOT NULL THEN (SELECT DESCRIPCION FROM CLI_Cod_Act_AFIP WHERE CODIGO_ACT_AFIP=ae.CODIGO_ACTIVIDAD) ELSE '' END) AS DESCR_ACTIVIDAD,
(CASE WHEN ae.CODIGO_ACTIVIDAD IS NOT NULL THEN (SELECT ORDINAL_ACTIVIDAD FROM CLI_ACTIVIDAD_ECONOMICA WHERE CODIGO_PERSONA_CLIENTE = ae.CODIGO_PERSONA_CLIENTE AND ORDINAL_ACTIVIDAD = ae.ORDINAL_ACTIVIDAD) ELSE '' END) AS ORDINAL_ACTIVIDAD,
(CASE 
	WHEN ae.ORDEN_AFIP = 1 OR ae.ORDINAL_ACTIVIDAD = 1 THEN 'Primaria' 
    ELSE 'Secundaria'
        END) AS TipoActividad,
 

--idBCRA
    (CASE WHEN         (SELECT bcra.CODIGO_BCRA 
         FROM CLI_Cod_Act_BCRA bcra 
            JOIN cli_cod_act_afip afip ON bcra.CODIGO_BCRA=afip.CODIGO_BCRA
            WHERE afip.CODIGO_ACT_AFIP = ae.CODIGO_ACTIVIDAD  
        ) 
     IS NOT NULL THEN 
        
            (SELECT bcra.CODIGO_BCRA FROM CLI_Cod_Act_BCRA bcra 
                JOIN cli_cod_act_afip afip ON bcra.CODIGO_BCRA=afip.CODIGO_BCRA
                WHERE afip.CODIGO_ACT_AFIP = ae.CODIGO_ACTIVIDAD
               )
                    
     ELSE 0 END 
    ) AS idBCRA,    
    
(CASE ae.VERIFICADO_AFIP WHEN  'N' THEN 'false' WHEN 'S' THEN 'true' ELSE '' END) AS verificadoAFIP,
(CASE WHEN CAST (ae.FECHA_VERIFICADO AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,ae.FECHA_VERIFICADO,126)  END ) AS FECHA_VERIFICADO,      
(CASE WHEN ISNUMERIC(ae.INICIO_ACTIVIDAD) = 1 THEN CAST(ae.INICIO_ACTIVIDAD AS NUMERIC)  ELSE NULL END) AS INICIO_ACTIVIDAD,
(CASE WHEN CAST (ae.FECHA_INSCRIPCION_CNA AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,ae.FECHA_INSCRIPCION_CNA,126)  END ) AS FECHA_INSCRIPCION_CNA,      
(CASE WHEN CAST (ae.FECHA_VENCIMIENTO_CNA AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,ae.FECHA_VENCIMIENTO_CNA,126)  END ) AS FECHA_VENCIMIENTO_CNA, 
(CASE WHEN ae.IERIC IS NOT NULL THEN ae.IERIC ELSE 0 END) AS IERIC,
(CASE WHEN CAST (ae.FECHA_VENC_IERIC AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,ae.FECHA_VENC_IERIC,126)  END ) AS FECHA_VENC_IERIC, 
(CASE WHEN CAST (ae.FECHA_INICIO_IERIC AS VARCHAR(100)) IS NULL  THEN NULL ELSE CONVERT(varchar,ae.FECHA_INICIO_IERIC,126)  END ) AS FECHA_INICIO_IERIC, 
(CASE WHEN ae.CARACTER_IERIC IS NOT NULL THEN ae.CARACTER_IERIC ELSE '' END)  AS CARACTER_IERIC,
(CASE WHEN ae.CONSEJO_PROFESIONAL IS NOT NULL THEN ae.CONSEJO_PROFESIONAL ELSE '' END) AS CONSEJO_PROFESIONAL,
(CASE WHEN ae.NRO_MAT_PROFESIONAL IS NOT NULL THEN ae.NRO_MAT_PROFESIONAL ELSE 0 END) AS NRO_MAT_PROFESIONAL
 
FROM CLI_PERSONASJURIDICAS pj
JOIN CLI_DocumentosPFPJ dpj ON pj.NUMEROPERSONAJURIDICA=dpj.NUMEROPERSONAFJ
LEFT JOIN CLI_ACTIVIDAD_ECONOMICA ae ON ae.CODIGO_PERSONA_CLIENTE=pj.NUMEROPERSONAJURIDICA AND 
					((ae.TZ_LOCK &lt; 300000000000000 OR ae.TZ_LOCK &gt;= 400000000000000) AND (ae.TZ_LOCK &lt; 100000000000000 OR ae.TZ_LOCK &gt;= 200000000000000))
JOIN CLI_TIPO_SOCIEDAD ts ON pj.TIPOSOCIEDAD = ts.CODIGO_SOCIEDAD
LEFT JOIN CLI_ClientePersona cp ON cp.NUMEROPERSONA = pj.NUMEROPERSONAJURIDICA AND cp.TITULARIDAD = 'T' AND 
					((cp.TZ_LOCK &lt; 300000000000000 OR cp.TZ_LOCK &gt;= 400000000000000) AND (cp.TZ_LOCK &lt; 100000000000000 OR cp.TZ_LOCK &gt;= 200000000000000))
LEFT JOIN CRE_TAM_EMP_BITACORA teb ON teb.CLIENTE = cp.CODIGOCLIENTE AND 
					((teb.TZ_LOCK &lt; 300000000000000 OR teb.TZ_LOCK &gt;= 400000000000000) AND (teb.TZ_LOCK &lt; 100000000000000 OR teb.TZ_LOCK &gt;= 200000000000000))


WHERE  pj.NUMEROPERSONAJURIDICA = ? --ALPHA: 1795276 DESA: 22228

AND ((pj.TZ_LOCK &lt; 300000000000000 OR pj.TZ_LOCK &gt;= 400000000000000) AND (pj.TZ_LOCK &lt; 100000000000000 OR pj.TZ_LOCK &gt;= 200000000000000))
AND ((dpj.TZ_LOCK &lt; 300000000000000 OR dpj.TZ_LOCK &gt;= 400000000000000) AND (dpj.TZ_LOCK &lt; 100000000000000 OR dpj.TZ_LOCK &gt;= 200000000000000))
AND ((ts.TZ_LOCK &lt; 300000000000000 OR ts.TZ_LOCK &gt;= 400000000000000) AND (ts.TZ_LOCK &lt; 100000000000000 OR ts.TZ_LOCK &gt;= 200000000000000))
ORDER BY isnull(teb.JTS_BITACORA,0) DESC</sql>
