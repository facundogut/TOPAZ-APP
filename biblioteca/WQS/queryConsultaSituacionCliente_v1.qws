<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceQuery>
    <uri>/clientes/situacionCliente/{idCliente}</uri>
    <serviceDescription>Consulta Situacion NBCH y BCRA cliente</serviceDescription>
    <title>Consulta Situacion NBCH y BCRA cliente</title>
    <querys>
        <query id="1" name="CLIT010" type="MULTIPLE">
            <sql>SELECT CATEGORIARESULTANTE, CATEGORIA_OFICIAL FROM CLI_CLIENTES WHERE CODIGOCLIENTE=? AND TZ_LOCK=0</sql>
            <input>
                <inParameter description="Codigo Cliente" type="INTEGER">idCliente</inParameter>
            </input>
            <output>
                <level groupby="" name="Categoria">
                    <outParameter description="situacionNBCH" type="STRING">
                        <queryname>CATEGORIARESULTANTE</queryname>
                        <servicename>situacionNBCH</servicename>
                    </outParameter>
                    <outParameter description="situacionBCRA" type="STRING">
                        <queryname>CATEGORIA_OFICIAL</queryname>
                        <servicename>situacionBCRA</servicename>
                    </outParameter>
                </level>
            </output>
        </query>
    </querys>
</serviceQuery>
